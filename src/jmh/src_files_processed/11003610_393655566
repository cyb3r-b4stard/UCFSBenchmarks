import java.util.*; 

public class Triangles {

    public static void main(String[]args){
    
    Scanner console = new Scanner(System.in); 
    
    System.out.println(); 
    
    System.out.println("What is side length a?"); 
    int SideA = console.nextInt(); 
    System.out.println("What is side lenth b?"); 
    int SideB = console.nextInt();
    System.out.println("What is side lenth c? "); 
    int SideC = console.nextInt();
    
   
    System.out.println(); 
    
    double A2 = SquareSides(SideA); 
   
  

}

    public static double WholeEquation(int SideA, int SideB, int SideC){
       double y = EQ( double y);
        
    }
    public static double SquareSides(int SideA, int SideB, int SideC){
        double A2= Math.pow(SideA, 2); 
        return A2;
        double B2= Math.pow(SideB, 2);
        double C2= Math.pow(SideC, 2);
        
    }
        
    public static double EQ(int A2, int B2, int C2){ 
        int y= (A2 + B2 - C2);
        return y; 
    
    }
    
 
}
