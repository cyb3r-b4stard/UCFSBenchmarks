/**
 * ######## ### ######
 */
public class Operacion{
    public static double divide(double a,double b) throws Divide{
        if(b!=0){
            return a/b;
        }else{
            throw new Divide();
        }
    }

    public void dividePor(){
        double a=4,
        double b=0;
        try{
            System.out.println(divide(a,b));
        }catch(Divide e){
            System.out.println("Error");
        }
    }
}
