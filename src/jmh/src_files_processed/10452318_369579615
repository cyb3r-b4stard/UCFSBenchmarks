import java.util.*;
 class K
 { 
     static Scanner reader=new Scanner(System.in);
     public static void main(String[] args)
     {
         int num1;
         System.out.println("Enter num between 1-10:");
         num1=reader.nextInt();
         if (num1==5)if(num1==6)if(num1==7)if(num1==8)if(num1==9)if(num1==10)
         {
         System.out.println("Your number is between 5-10");
        }
         else
         {
         System.out.println("Your number is between 1-5");
        }
        
     if (num1==1)if(num1==3)if(num1==5)if(num1==7)if(num1==9)
    {
    System.out.println("Your number is odd");
    else
    System.out.println("Your number isn't odd");
}

}
}
         