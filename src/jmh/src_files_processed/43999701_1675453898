       fam_income = 0.00;
       state = " ";
    }
    
    public void setData(){
        
        fam_id = f;
        race = r;
        num_child = n;
        fam_income = i;
        state = s;
    }
    
    public getData(){
        return fam_id, race, num_child, fam_income, state;
    }
    
    
    public class CalculateData(int f, int n){
        
        double AverageInc;
        
