import becker.robots.*;

public class RobotBsp1
{
    public (LeftDancer(City city, int zeile, int spalte, direction dir, int anzahlDinge)
    {
     super(city, zeile, spalte, dir,);
        
    }
    
    public static void fahre()
    {
        City leoben = new City();
        Leftdancer Tom = new Leftdancer (Leoben, 1, 2, Direction.EAST)
        Tom.dance()
        
        new Wall(leoben,1,1,Direction.NORTH);
        new Wall(leoben,1,2,Direction.NORTH);
        new Wall(leoben,1,2,Direction.EAST);
        new Wall(leoben,2,2,Direction.EAST);
        new Wall(leoben,2,2,Direction.SOUTH);
        new Wall(leoben,2,1,Direction.SOUTH);
        new Wall(leoben,2,1,Direction.WEST);
        
        
        
        
        new Wall(leoben,4,4,Direction.NORTH);
        new Wall(leoben,4,5,Direction.EAST);
        new Wall(leoben,5,5,Direction.EAST);
        new Wall(leoben,5,5,Direction.SOUTH);
        new Wall(leoben,5,4,Direction.SOUTH);
        new Wall(leoben,5,4,Direction.WEST);
        new Wall(leoben,4,4,Direction.WEST);
        
        new Thing(leoben,1,2);
        new Thing(leoben,1,2);
        new Thing(leoben,1,2);
        new Thing(leoben,1,2);
        new Thing(leoben,1,2);
        new Thing(leoben,1,2);
        new Thing(leoben,1,2);
        new Thing(leoben,1,2);
    }
    public viod dance()
    {this.turnLeft();
     super.move();
     this.turnRight();
     super.move();
     this.turnRight();
     super.move();
     this.turnRight();







}
