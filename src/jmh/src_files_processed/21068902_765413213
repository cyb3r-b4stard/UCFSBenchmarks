import java.io.*;
public class CourseGrade
{
public static void main(String[] args)
{
int examWeight=70;
int labWeight=20;
int hwWetght=10;
double examScore;
double labScore;
double hwScore;
BufferedReader br=new BufferedReader (new InputStreamReader(System.in));
System.out.println("enter your exam grade");
examScore)=Double.parseDouble(br.readLine());
System.out.println("enter your lab grade");
labScore=Double.parseDouble(br.readLine());
System.out.println("enter your homeworkgrade");
hwScore=Double.parseDouble(br.readLine());
examScore=examScore*(examWeight/100.0);
labScore=labScore*(labWeight/100.0);
hwScore=hwScore*(hwWetght/100.0);
finalGrade=examScore+labScore+hwScore;
System.out.println("your final grade is"+finalGrade);

}
}
   