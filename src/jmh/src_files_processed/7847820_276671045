public class Kuh{
        private int gewicht;
        private String name;
        
        
        public Kuh (String nameH, int gewichtH);
        name=nameH;
        gewicht=gewichtH;
       
    
}


public void setName(String Name);

{
    return Name}
    
    
    public void getName (String Name);
    
public void setGewicht (int Gewicht);

{
    return Gewicht}
    public void getGewicht (int Gewicht);
    
    
  

