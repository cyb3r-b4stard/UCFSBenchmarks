import java.util.ArrayList;
public class Club
{
    private ArrayList<Membership> collection;

    /**
     * Constructor for objects of class Club
     */
    public Club()
    {
        ArrayList collection;
        collection = new ArrayList();

    }

    /**
     * Add a new member to the club's list of members.
     * @param member The member object to be added.
     */
    public void join(Membership member)
    {
        collection.add(member);
    }

    /**
     * @return The number of members (Membership objects) in
     *         the club.
     */
    public int numberOfMembers()
    {
        return collection.size();
    }

    /**
     * Determine the number of members who joined in the
     * given month.
     * @param month The month we are interested in.
     * @return The number of members who joined in that month.
     */
    public int joinedInMonth(int month)
    {
        

        for(Membership inMonth: collection){
int numberInMonth;
            if(inMonth.getMonth() == month){
                numberInMonth += 1;
            }
            if(inMonth.getMonth() != month && inMonth.getMonth() > 12 || inMonth.getMonth() < 1){
            }
            else{
                System.out.println("Error! Month must be an int 1-12!");
            }
return numberInMonth;
        }
        return void;
    }
}