public void divider()
{
    String a=JOptionPane.showInputDialog ("input value h:");
    String b=JOptionPane.showInputDialog ("input value i:");
    String c=JOptionPane.showInputDialog ("input value j:");
    String str1= "a";
    Integer h = Integer.valueOf(str1);
    String str2= "b";
    Integer i = Integer.valueOf(str2);
    String str3= "c";
    Integer j = Integer.valueOf(str3);

    if (h < i && h < j)
    {
       JOptionPane.showMessageDialog (null, "h is the youngest");
    }
    if(i < h && i < j)
    {
        JOptionPane.showMessageDialog (null, "i is the youngest");
    }
    if(j < h && j < i)
    {
        JOptionPane.showMessageDialog (null, "j is the youngest");
    }
    
}
