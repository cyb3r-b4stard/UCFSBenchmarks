/**
 * 
 * @###### ####### #########
 * @####### #/#/##
 */
public class Equilateral extends Triangle
{
	private int sideA;
	private int sideB;
	private int sideC;

	public Equilateral(int a, int b, int c)
	{
	    super(a, b, c);
		sideA = a;
		sideB = b;
		sideC = c;
	}

	public int getSideA()
	{
		return sideA;
	}
	
}
}