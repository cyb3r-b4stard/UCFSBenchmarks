
public class isaacissocool
{
 
    public static String toUpperCase(String str)
{
    int length = str.getLength();
    for(i = 0; i < length; i++){
        char temp = charAt(i);
        Character.toUpperCase(str);
        Character.toString(str);
        String sum = "";
        String sum += str;
    }
    return sum;
}

}