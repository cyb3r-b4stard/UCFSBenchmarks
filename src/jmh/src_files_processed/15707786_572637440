import java.util.Scanner;
public class RecursivePalindrome
{
    RecursivePalindrome()
    {
        
    }
    
    public void findPalindrome(x)
    {
    }
    
    public String getPalindrome(String x)
    {
        return findPalindrome(x);
    }
}