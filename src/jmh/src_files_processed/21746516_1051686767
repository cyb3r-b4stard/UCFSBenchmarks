import java.util.*;
class pranav
{
    public static void main(String[]args)
    {
        int temperature;
        Scanner sc=new Scanner (System.in);
        System.out.println("enter your choice");
        int ans=sc.nextInt();
        int sp=sc.nextInt();
        switch(ans)
        {
            case 1:
            System.out.println("enter temperature");
            int c=sc.nextInt();
            System.out.println("enter temperature");
            System.out.println("cel to fahr ="+5/9*(c+32));
            break;
        }
        {
            case2:
            System.out.println("enter temperature");
            int f=sc.nextInt();
            System.out.println("fahr to cel ="+5/9*(f-32));
            break;
        }
        {
            default:
             System.out.println("invalid chice");
            }
            
        
        }
        
