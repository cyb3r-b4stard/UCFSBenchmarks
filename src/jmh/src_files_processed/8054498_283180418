public class MedicamentoQuimoterápico
{
 private String nome,composto,laboratorio,descrição;
 public MedicamentoQuimoterápico(String nome,String composto,String laboratorio,String descricao)
 {
     this.nome=nome;
     this.composto=composto;
     this.laboratorio=laboratorio      
     this.descricao=descricao 
 }
 }