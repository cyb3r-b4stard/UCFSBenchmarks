import java.util.Scanner;
/**
 * ##### # ########### ## ##### ######### ####.
 * 
 * @###### (#### ####) 
 * @####### (# ####### ###### ## # ####)
 */
public class OddorEven
{ public static void main(String[] args)
    {   int a;
        Scanner inputa = new Scanner(System.in);
        a = inputa.nextInt();
        if(a!=((2)*a)
        {
            System.out.println("This number is odd");
        }
        else
        {
            System.out.println("This number is even");
        }
        
        
    }
   
}