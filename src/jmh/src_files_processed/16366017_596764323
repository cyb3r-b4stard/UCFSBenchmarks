public class Feld
{
    private int x;

    /**
     * Constructor for objects of class F
     */
    public Feld(int x,int y)
   { 

     int[] listex new = int[20];
     int[] listey new = int[20];
   for (int a=0 ; a<20 ; a++)
   { listex[a]=x*(a+1);
     listey[a]=y*(a+1);

    }

    
}
 }
