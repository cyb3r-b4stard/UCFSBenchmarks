
public class Warrior
{
    private int health;
    private int attack;

    public Warrior()
    {
        health = 3;
        attack = 2;
    }
    
    public void showinfo()
    {
        System.out.println("Warrior");
        System.out.println("attack = " + attack);
        System.out.println("health = " + health);
    }
    
    public int sethealth(int sethealth)
    {
        return sethealth;
        
        sethealth ** health;
    }
}