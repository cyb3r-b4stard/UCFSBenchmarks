
public class euler20
{
    public static int getDigits(long numba)
    {
        int digimon = 1;
        while(numba > 9)
        {    
        numba /= 10;
        digimon++;    
    }
        return digimon;
    }
    
    
    
    
    
    public static int getSum(long number)
    {
        int digits = getDigits(number);
        int array = new int[digits];
        for(int i = 0;i < digits;i++)
        {
            array[i] = (long)Math.pow(10,i + 1);
        
        
        
        return 0;
    }
    
    
    
    
    
    public static long getFactorial(int i)
    {
        int fact = 1; 
        for(int a = 2;a <= i;a++)
         {
             fact *= a;
            }
            return fact;
        }
    
    
    public static void main(String [] args)
    {
        int sumOfDigits = 0;
        long currentFact;
        int numb = 1;
        while(sumOfDigits < 100)
        {
            currentFact = getFactorial(numb);
            numb = getSum(currentFact);
        }
    }
}
