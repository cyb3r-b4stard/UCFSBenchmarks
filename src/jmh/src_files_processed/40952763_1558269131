java.util.Calendar;
public class Author
{
    String name;
    Calendar bDay = new Calendar;
    public Author()
    {
        name = null;
    }
    public String getname(){
        return name;
    }
    public Calendar getbDay(){
        return bDay;
    }
    public void setname(String newname){
        name = newname;
    }
    public void setbDay(Calendar newbDay){
        bDay = newbDay;
    }
}