import java.util.*;
import java.io.*;

public class driver
{
    private int x;

    /**
     * Constructor for objects of class driver
     */
    public static void main()throws IOException
    {
        String fileName;
        String line;
        Scanner in = new Scanner(System.in);
        StringTokenizer stok;
        String lastName,firstName,address,clientID,petName,type,lastShot,lastVisit;
        int visits,numbPets,weight;
        double balance;

        Client [] List = new Client[5];
        Pets [] petList = new Pets[50];
        
        System.out.println("Please enter the file location: ");
        fileName = in.nextLine();
        
        File file = new File(fileName);
        Scanner inputFile = new Scanner(file);
        
       for(int j=0;j<5;j++)
        {
        line = inputFile.nextLine();
        
        stok = new StringTokenizer(line,",");
        
        lastName = stok.nextToken();
        firstName = stok.nextToken();
        address = stok.nextToken();
        clientID = stok.nextToken();
        visits = Integer.parseInt(stok.nextToken());
        balance = Double.parseDouble(stok.nextToken());
        numbPets = Integer.parseInt(stok.nextToken());
        
        List[j] = new Client(lastName,firstName,address,clientID,visits,balance,numbPets);  
        
        for(int i=0;i<numbPets;i++)
        {
           line = inputFile.nextLine();
           stok = new StringTokenizer(line,",");
           
           petName = stok.nextToken();
           type = stok.nextToken();
           weight = Integer.parseInt(stok.nextToken());
           lastShot = stok.nextToken();
           lastVisit = stok.nextToken();
           
           petList[i] = new Pets(petName,type,weight,lastShot,lastVisit
        }
        
       }
       System.out.println(List[0].lastN);
       System.out.println(List[1].lastN);
       System.out.println(petList[2].name);
       System.out.println(List[3]);
       System.out.println(List[4]);
    }
}