public class Interface
{
    public static void main(String[] args)
    {
        Dog d = new Dog(true, "Wooly", "Shaggy");
        Cat c = new Cat(false, "Fuzzy", "Kitty");
        Robin r = new Robin(false, true, "Batman");
        Crow cr = new Crow(true, true, "Clap-Trap")
    }
}