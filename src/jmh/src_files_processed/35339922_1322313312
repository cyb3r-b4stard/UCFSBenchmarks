import java.util.*;
class ifelse
{
    Scanner obj=new Scanner (System.in);
    int a,b,c;
    void main()
    {
        System.out.println("enter a and b");
        a=obj.nextInt();
        b=obj.nextInt();
        if (a>b)
        c=a+b;
        system.out.println("c="+c)
        else
        c=a-b;
        System.out.println("c="+c)
       
    }
}
        