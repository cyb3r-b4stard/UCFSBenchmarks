
/**
 * ##### # ########### ## ##### ####### ####.
 * 
 * @###### (#### ####) 
 * @####### (# ####### ###### ## # ####)
 */

public class Scanner
{
  

    /**
     * Constructor for objects of class Scanner
     */
    public Scanner(System.in){}
    
    public static String LeerString()
    {
     Scanner scan=new Scanner(System.in);
     return scan.next();
    }
      
    public static int LeerNumero()
    {
     return scan.nextInt();
    }
}