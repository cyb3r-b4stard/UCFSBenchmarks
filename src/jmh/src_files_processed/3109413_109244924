import java.util.ArrayList;
public class Purse
{
    public Purse()
    {
        coins = new ArrayList<String>();
    }

    public void addCoin(String coinName)
    {
        coin.add(coinName);
    }

    public String toString()
    {
        String coinsInPurse="";
        for(String c:coins){
            coinsInPurse+=c;

        }
        return coinsInPurse;
    }  

    public boolean sameCoins(Purse other)
    {
        if(this.coins.size()!=other.coins.size()){
            return false;
        }else{
            int dimes=0;
            int nickels=0;
            int quarters=0;
            int pennies=0;
            int dimes1=0;
            int nickels1=0;
            int quarters1=0;
            int pennies1=0;
            for(String a:coins){
                if(a.equalsIgnoreCase("quarter"));
                quarters++;
                else if(a.equalsIngoreCase("dime"));
                dimes++;
                else if(a.equalsIngoreCase("nickel"));
                nickels++;
            }else{
                pennies++;
            }
            for(String :coins){
                if(b.equalsIgnoreCase("quarter"));
                quarters1++;
                else if(b.equalsIngoreCase("dime"));
                dimes1++;
                else if(b.equalsIngoreCase("nickel"));
                nickels1++;
            }else{ 
                pennies1++;
            }
            if(quarters==quarters1 && nickels==nickels1 && pennies==pennies1 && dimes==dimes1){
                return true;
            }else{
                return false;

            }
        }
        private ArrayList<String> coins;

    }
}
