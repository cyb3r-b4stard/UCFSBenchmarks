/**
 * ####.####
 * ##### ######
 * 
 * #### ##### ###### # ####'# ###########
 */
public class Card
{
    private int num;
    private char suit
    public Card(int num, char suit)
    {
        this.num = num;
        this.suit = suit;
    }
    public String toString()
    {
        return Integer.toString(num) + suit;
    }
}