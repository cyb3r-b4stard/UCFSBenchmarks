public class Champion
{
    public String Name;
    public double Health = 1;
    public double AD;
    public double AP;
    Champion Target;
    
    
    
    public void setTarget (Champion pTarget)
    {
        Target=pTarget;
    }
        
    public void AutoAttack ()
    {
        Target.Health = Target.Health-AD;
        public void Deathcheck();
    }
    
    public void Deathcheck()
    {
          if(Target.Health==0||Target.Health<0)
        {
            System.out.println(Name+ " has slain " + Target.Name);
        }
    }
}