
import java.io.*; 

public class ProgErrors
{
    static final char  BLANK;
    static final int ONE = 5;
    
    public static void main ( String[] args)
    {
        int a, b, cc,
        a = one + 5;
        b = a + BLANK;
        cc = a + ONE * 2;
        a + cc = b;
        ONE = b + c;
        System.out.println("a = " a + ", b =" + b + " , cc = " + cc);
    }
}
