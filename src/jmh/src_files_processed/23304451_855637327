 * @###### (####### #########) 
 * @####### (##-#-##)
 */
public class Chapter6
{
    
        public static double computeShippingCost (int nJars)
        {
            int nCartons = (nJars + 11) / 12; 
            int totalOunces = ((nJars * 5) + (nCartons * 9) / 16);
            int lbs = ((nJars * 1) + (nCartons * 1) + totalOunces);
            return (int) (((1.44 * nCartons) + (0.96 * lbs) + 3.00 )));
        }

    
    
    
    
}
 



