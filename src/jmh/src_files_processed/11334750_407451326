import java.awt.*;
import java.applet.*;

public class Chapter5GraphicsLab extends Applet{
    public void paint(Graphics g){
        int width = 0, height = 0;
        
        while(width <= getWidth() && height <= getHeight() ){
            g.drawLine(0, height, width, getHeight();
        }
    }
}
