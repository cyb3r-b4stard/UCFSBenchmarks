public class Angabe18
{
   public void test()
   {
       double d = 0.0;
       while(d != 1.0)
       {
           d+=0.1;
           System.out.println(d);
       }
   }
   
   public void test2()
   {
       double d = 0.0;
       while(d != 0.0=;
       {
           d++;
           System.out.println(d/10);
       }
   }
}