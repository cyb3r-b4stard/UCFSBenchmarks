
public class MyCalculator extends BasicCalculator1
{
    public MyCalculator(int num1, int num2)
    {
        System.out.println(super());
    }
    
    public int mutiply()
    {
        return getNum1() * getNum2();
    }
    
    public double divide()
    {
        return getNum1() / getNum2();
    }
    
}