 * @####### 
 */
public class ifSwitch
{
    public void käse()
    {
   
        int i;
        i=5;
    
    switch(i)
    {
        case i<7 ;
        {
            System.out.println("Weil die Zahl kleiner als 7 ist wird sie um 2 erhöht");
            i = i+2;
          
        };
        case i = 7
        {
            System.out.println("Die Zahl ist 7");break;
        }
        
        System.exit;
    }
    
}