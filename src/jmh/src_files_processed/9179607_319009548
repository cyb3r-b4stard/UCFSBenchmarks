
/**
 * ##### # ########### ## ##### ### ####.
 * 
 * @###### (#### ####) 
 * @####### (# ####### ###### ## # ####)
 */
public class App
{
   public void executar () {
       Circle meuCirculo = new Circle();
       Square meuSquare=new Square();
       Triangle meuTriangle=new Triangle();
      
       meuCirculo.makeVisible();
       meuCirculo.changeColor("red");
       meuCirculo.changeSize(50);
    
       meuSquare.makeVisible();
       meuSquare.changeColor("bluee");
       meuSquare.changeSize(100);
       
       meuTriangle.makeVisible();
       meuTriangle.changeColor("black");
       meuTriangle.changeSize(50,50);
    
        meuCirculo.moveHorizontal(10);
        meuSquare.moveHorizontal(11);
        meuTriangle.moveHorizontal(130);
        
        meuCircle.moveDown();
        meuSquare.moveDown();
        meuTriangle.moveDown();
        
        
        
    }
}
    
    

    
    
    
}