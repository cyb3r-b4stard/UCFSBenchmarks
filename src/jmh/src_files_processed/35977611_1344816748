class add
{
    void input(int x, int y);
    
{   int z=x+y;
    int a=x-y;
    int b=x*y;
    int c=x/y;
    System.out.println("The Total Is")+z);
    System.out.println("The Total Is")+a);
    System.out.println("The Total Is")+b);
    System.out.println("The Total Is")+c);
            
       }
}