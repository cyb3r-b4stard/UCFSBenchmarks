public class SpeiseKarte
{

private Speise speise0;
private Speise speise1;
private Speise speise2;

public SpeiseKarte()
{

}

public void anbieten(Speise neu)
{
    if(neu != null)
    {
        if(speise0 == null)
        {
            speise0=neu;
        }
        else
        {
            if(speise1 == null)
            {
                speise1=neu;
            }
            else
            {
                if(speise2 == null)
                {
                    speise2=neu;
                }  
                else
                {
                    System.out.print("Error Alle Belegt");
                }
            }
        }
}



















}