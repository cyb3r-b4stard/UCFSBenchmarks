public class bubble
{
    static void Sort(int [] a)
    {
        for(int i=a.length-1; i>0 ; i--)
        {
            for(int j=0; j<i; j++)
            {
                if(a[j] < a[j]+1)
                {
                    swap.swap1(a,j,j+1);
                    print.printArray(a);
                }
            }
        }
    }
    System.out.println();
    System.out.print("Ergebniss: ");
    print.printArray(a);
}