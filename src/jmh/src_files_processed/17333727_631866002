

public class Banco
{
   private long numeroCuenta;
   private String rutCliente;
   private int saldo;
   private int dinero;
   
   
   public Banco()
   {
       numeroCuenta = 0;
       rutCliente = "Sin rut"; 
       saldo = 0;
   }
   public Banco(int numeroCuenta,String rutCliente, int saldo)
   {
       this.numeroCuenta = numeroCuenta;
       this.rutCliente = rutCliente;
       this.saldo = saldo;
    }
   public void depositarDinero(int dinero)
   {
       saldo = saldo + dinero;
       
   }
   public void girarDinero(int dinero)
   {
       saldo = saldo - dinero;
   }
   public void printBanco(int dinero)
   {
       System.out.println("----Datos de Cuenta----");
       System.out.println("Numero de cuenta  :" + numeroCuenta);
       System.out.println("Rut Cliente       :" + rutCliente);
       System.out.println("Saldo             :" + saldo);       
       System.out.println("-------------------------");
   }
   
