
/**
 * ##### # ########### ## ##### ################ ####.
 * 
 * @###### (#### ####) 
 * @####### (# ####### ###### ## # ####)
 */
public class salariedEmployee extends Personnel
{
    public String position;
    public double salary;
    public int ID;
   public salariedEmployee(String wName,int age1,char gender1,String job,double pay,int id)
   {
       super( wName, age1, gender1);
       position=job;
       salary=pay;
       ID=id;
   }
   public String getPosition()
   {
       return position;
   }
   public double getSalary()
   {
       return salary;
   }
   public int getID()
   {
       return ID;
   }
   public void setPosition(String job)
   {
      position=job;
   }
   public void setSalary(double pay)
   {
       salary=pay;
   }
   public void setID()
   {
       id=Math.random*(1)(450355400-123011150)+1);
   }
}