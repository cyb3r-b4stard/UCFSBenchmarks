public class Billamount
{
   private double Billamount;
   public void getBillamount(double Billamount)
   {
    {
        System.out.println("Lunch tip : " + (double) (Billamount*.15);
    } 
    {
        System.out.println("Dinner tip: " + (double) (Billamount*.20);
    }
   } 
}    
           