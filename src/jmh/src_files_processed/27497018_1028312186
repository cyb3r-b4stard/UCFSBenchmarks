public class StringPractice
{
    private static String st="blarglelglalglglglgl";
    public static int countEs()
    {
        int count=0;
        for (int i=0; i<st.length(); i++)
        {
            if ((st.substring(i,i+1)).equals("e"))
            {
                count++;
            }
        }
        return count;
    }
