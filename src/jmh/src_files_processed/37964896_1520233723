import java.util.Arrays;
import java.util.Arrays;
import java.util.ArrayList;
import java.util.Collection;
public class Testttt {

    

    public static void main (String[] params) {
    
         permisArchitectes("A1234");


    } 

    public static boolean permisArchitectes (String permis){

        boolean valide = false;

        if( permis.length() == 5 && (permis.charAt(0) == 'A' || permis.charAt(0) == 'T') ){

            valide =true;

            for (int i = 1; i < permis.length(); i++){

                if(permis.charAt(i) < '0' || permis.charAt(i) > '9'){

                    valide = false;

                    i = permis.length();

                }

            
            }

        
        }

    
        return valide;

    }
    
    public static boolean permisPsychologues (String permis){

        boolean valide = false;

        if( permis.length() == 8 && permis.charAt(5)=='-'){

            valide =true;

            for (int i = 0; i < 5; i++){

                if(permis.charAt(i) < '0' || permis.charAt(i) > '9' ||){

                    valide = false;

                    i = permis.length();

                }

            
            }
            
            for (int i = 6; i < permis.lenght(); i++){

                if(permis.charAt(i) < '0' || permis.charAt(i) > '9' ||){

                    valide = false;

                    i = permis.length();

                }

            
            }

        
        }

    
        return valide;

    }

    public static boolean bon(String permis ){

        boolean bon = false;

        
        if(permis.length()== 5 &&( permis.charAt(0) == 'A' || permis.charAt(0) == 'R'||permis.charAt(0) == 'S' ||permis.charAt(0) == 'Z')){

            bon = true;

            for(int i =1; i<permis.length();i++){

                if(! (permis.charAt(i) >= '0' && permis.charAt(i) <= '9')){

                    bon = false;

                }
            
            }
        }

        return bon;
    }
    public static boolean estValide( String Y){
        boolean correct = false; 
        if ((Y.length())%3==0){

            if ((Y.substring(Y.length()-3).equals("UAG") || Y.substring(Y.length()-3).equals("UAA") || Y.substring(Y.length()-3).equals("UGA") && ((Y.indexOf("AUG")==0)||(Y.indexOf("UUG")==0)||(Y.indexOf("GUG")==0)) )){

                correct = true;

            }

            if(Y.indexOf("UAA") !=  Y.length()-3){
                correct = false;
            }  
        }

        return correct;
    }
}
