public class P5A
{
    public static void main(String[] args)
    {

        APImage blank;
        blank = new APImage(500,200);
        
        for (Pixel p : blank)

              Pixel p = blank.getPixel(x,y);
              p.setGreen(255);
              p.setRed(0);
              p.setBlue(0);
               
            }
