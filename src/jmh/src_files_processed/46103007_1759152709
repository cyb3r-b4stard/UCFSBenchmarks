
public class Number
{
    public static void printOdds(int max)
    {
        for (int i = 1; i<= max; i+=2)
        {
            System.out.println(i);
        }
    }
    
    public static boolean primeCheck(int num)
    (
        for (int i = 2; i < num; i++)
            {
                return false;
            }
        
    )
}