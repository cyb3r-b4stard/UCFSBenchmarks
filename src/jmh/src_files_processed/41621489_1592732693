public class Addition
{   public static void main()
    {
            int a=5,
            int b=7;
            int sum, sub;
            System.out.println("The value of a="+a);
            System.out.println("The value of b="+b);
            sum=a+b
            System.out.println("sum="+sum);
            sub=a-b;
            System.out.println("Subtraction of"+a+" and "+b+"="+sub);
        }
    }
  