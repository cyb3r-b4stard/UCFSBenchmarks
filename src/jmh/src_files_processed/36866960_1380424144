public class StarwarsNameGenerator
{
    public static void main(String[] args)
    {
        NameGenerator gen = new NameGenerator("Sean", "McLane", "doggo", 
            "Los Angeles", "Mini Clubman", "chair", "country");
        System.out.print(gen.SwFirstGen() + " ");
        System.out.print(gen.SwLastGen() + " the ");
        System.out.println(gen.SwTitleGen())
        
    }
}