import java.util.ArrayList;

public class Bubblesort
{
  
    ArrayList<Integer> arrlist = new ArrayList<Integer>();
    public int Variable1;
    public int Variable2;
    
    public Bubblesort()
{
    ArrayList<Integer> arrlist = new ArrayList<Integer>();
}

public void addlist(int Zahl)
{
    arrlist.add(Zahl);
}

public void clearlist()
{
    arrlist.clear();
}

public void showlist()
{
    if(arrlist.isEmpty())
    {
        System.out.println("Es befinden sich keine Werte in der Arraylist.");
    }
    else
    {
        System.out.println("Werte der Arraylist: - " + arrlist + " -");
    }
}

public void bubble(int x)
{
    int retval=arrlist.get(3)
    arrlist.get(x) = Variable1;
}

























}