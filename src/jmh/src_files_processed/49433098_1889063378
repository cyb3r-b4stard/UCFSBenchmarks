import java.util.*;
public class Rent
{
    public void main
   {
        Scanner p=new Scanner(System.in);
        int d,r;
        System.out.println("Enter number of days");
        d=p.nextInt();
        if(d<=5)
        {
            r=d*500;
        }
        else if(d>=6 && d<=10)
        {
            r=(5*500)=(d-5)*400;
        }
        else if(d>=10)
        {
            r=(5*500)+(5*400)+(d-10)*200;
        }
        System.out.println("Rent= Rs."+r)
    }
}