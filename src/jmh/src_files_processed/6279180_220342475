public class Skola
{
    private Trida[] tridy;
    private String nazev;
    
    public Skola(String n)
    {
        this.nazev = n;
        this.tridy = new Trida[20];
    }
    
    public void vypisZakuSkoly()
    {
        for (int i=0; i<30; i++)
        {
         
            if (this.ridy[i] != null )
            {
                this.tridy[i].vypisZaku;
            }
            i++;
        }
    }
}