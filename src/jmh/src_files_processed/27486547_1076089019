import java.util.*;
class decode
{
    public static void main()throws Exception
    {
        Scnner sc = new Scanner(System.in);
        System.out.println("enter code");
        String s1 = sc.nextLine();
        StringBuffer br = new StringBuffer(s1);
        br.reverse();
        int l = br.length;
        int s = 0;
        String ans = "";
        for(int i = 0; i<l; i++)
        {
            String c = "" + charAt(i);
            s = s*10 + Integer.parseInt(c);
            c = "";
            if((s >= 65 && s <= 90)||( s >= 97 && s<=122)||s==32)
            {
                ans+ = (char)(s);
                s = 0;
                continue;
            }
        }
        System.out.println("encoded message=" + "/t"+ s1);
        System.out.pritnln("the decoded message = " + "/t" + ans);
    }
}