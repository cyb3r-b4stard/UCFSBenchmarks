public class Garten
{
    private Rechteck stamm;
    private Kreis blätter;
    private Kreis apfel1;
    private Kreis apfel2;
    private Kreis apfel3;
    private Dreieck hausdach;
    private Rechteck haus;
    private Rechteck wurm;
    
    /**
     * Erzeuge ein Exemplar der Klasse Zeichnung
     */
    public zeichnung()
    {
    
    }
    
     /**
     * Zeichne die Zeichnung.
     */public void zeichne()
     {
    
        stamm = new Rechteck ();
        stamm.sichtbarMachen();
        stamm.farbeAendern("braun");
        stamm.groesseAendern(30,100);
        
        blätter = new Kreis ();
        blätter.sichtbarMachen();
        blätter.farbeAendern(grün);
        blätter.groesseAendern(50);
        
        apfel1 = new Kreis ();
        apfel1.sichtbarMachen();
        apfel1.farbeAendern("rot");
        apfel1.groesseAendern(5);
     
        apfel2 = new Kreis ();
        apfel2.sichtbarMachen();
        apfel2.farbeAendern("rot");
        apfel2.groesseAendern(5);
        
        apfel3 = new Kreis ();
        apfel3.sichtbarMachen();
        apfel3.farbeAendern("rot");
        apfel3.groesseAendern(5);
        
        haus = new Rechteck ();
        haus.sichtbarMachen();
        haus.farbeAendern("blau");
        haus.groesseAendern(80,30);
        
        hausdach = new Dreieck ();
        hausdach.sichtbarMachen();
        hausdach.farbeAendern("braun");
        hausdach.groesseAendern(80,20);
        
        wurm = new rechteck ();
        wurm.sichtbarMachen();
        wurm.farbeAendern("braun");
        wurm.groesseAendern(100,5);
        wurm.
        
    }
    /**
     * Ändere die Darstellung in action
     */
    public void action()
    {
        {
      apfel1.langsamVertikalBewegen(90);
      wurm.langsamHorizontalBewegen(100)
            
            
            
            
            
}