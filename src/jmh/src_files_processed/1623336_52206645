
/**
 * ##### # ########### ## ##### ############# ####.
 * 
 * @###### (#### ####) 
 * @####### (# ####### ###### ## # ####)
 */
public class GameCharacter
{
    private String name;
    private String race;
    private int level;

    /**
     * Constructor for objects of class GameCharacter
     */
    public GameCharacter(String name, String race, int level)
    {
       this.name = name;
       this.race = race;
       this.level = level;
    }

public String toString()
{
    return name + ", " race + ", level:" + level; 
}
}