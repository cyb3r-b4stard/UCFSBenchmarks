public class Found
{
    public static void main(String [] args) {
        System.out.println("\f");
        int [] a = new int [10];
            for(int i=0; i<a.length; ++i) {
                a[i] = 10-i;
                System.out.println("a["+i+"] = " + a[i]);
            }
        }
    public static boolean isFound(int [] a, int i) {
          return a[].contains(a, i);          
      }
    }