
public class SavingAccount extends Account
{
    private double minBalance;
    private double balPenalty;
    private int maxWithdraws;
    priavte double transFee;
    
    public SavingAccount(double b, String n, double ir, int accNum, double mb,double bp, int mw, double tf)
    { super(b, n,ir,accNum);
        minBalance=mb;
        balPenalty=bp;
        maxWithdraws=mw;
        transFee= tf;
    }
        
   
}