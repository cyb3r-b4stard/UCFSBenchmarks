
public class Point
{
    private double x;
    private double y;

    /**
     * Constructor for objects of class Point
     */
    public Point(double initialX, double initialY)
    {
        x = initialX;
        y = initialY;
    }  

    public void setX(double setX)
    {
        x= setX;
    }

    public void setY(double setY)
    {
        y = setY;
    }

    public double getX()
    {
        return x;
    }

    public double getY()
    {
        return y;
    }

    public String toString()
    {
        return "("+ x + ","+ y+")";

    }

    public Point other (double x, double y)
    {
        Point other = new Point(5.00, 6.00);
        other.x = x;
        other.y = y;
    }

    public Point p1 (double x, double y)
    {
        Point p1 = new Point(8.00, 10.00);
        p1.x = x;
        p1.y = y;
    }

    public Point p2 (double x, double y)
    {
        Point p2 = new Point(2.00, 2.00);
        p2.x = x;
        p2.y = y;
    }

    public double distanceTo(double x, double y)
    {
        double distanceTo;
        distanceTo = sqrt(((p1.x - other.x)pow2)+(p1.y-other.ypow2));
        return distanceTo;

    }

    public static double showDistance(double p1, double p2)
    {
        System.out.println ("The distance between (" + p1 + ") and (" + p2 +") is "+point.distanceTo+".");
    }
}