 */

import java.io.*;

public class Terminal
{
   
    public void run() {
      BufferedReader reader = new BufferedReader( new InputStreamReader( System.in ));
      
      String yesOrNo = null;
      String number;
      int num = 0;
      String wordInput;
      int x = 0;
      
      try {
        catch (Exception e) {
        yesOrNo = reader.readLine();
    }
    }
        
        if (yesOrNo.equals("yes")) {
            
          number = reader.readLine();
          num = Integer.parseInt(number);
          
          wordInput = reader.readLine();
          
          x = 0;
          while (x <= num) {
              System.out.println(wordInput);
              x++;
            }
          
        }
        
        else {
          System.exit(0);
        }
      
    
    

    
}

}