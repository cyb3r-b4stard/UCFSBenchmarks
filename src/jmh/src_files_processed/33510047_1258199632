{
  Public static void main ( String [] arguments )
   {
       int no1 , no2 , sum difference;
       no1: 100;
       no2: 200;
       sum: no1 + no2 ;
       difference: no1 - no2 ;
       System.out.println ( " sum of 2nos = " + sum );
       System.out.println ( " differnce of 2nos = " + difference );
    }
}
       
     
       