import java.util.Scanner; 
public class Vrbanac_Ross_HW_BranchingWarmUp { 
public static void main(String[] args) { 
    Scanner keyboard = new Scanner(System.in); 
    
    System.out.println("Enter the number"); 
    int number = keyboard.nextInt(); 
    if (number < 0) 
    System.out.println("The number is negative"); 
   
    System.out.println("Enter a second number"); 
    int second = keyboard.nextInt(); 
    if (second == 0) 
    System.out.println("The number is zero"); 
    
    System.out.println("Enter a third number, a double, for the class average."); 
    double class_avg = keyboard.nextDouble(); 
    if (class_avg >= 65) 
    System.out.println("A passing grade"); 
    
    
    System.out.println("give me a number");
    int number2 = keyboard.nextInt();
    if (number2 % 2 == 0)
    System.out.println("the number is even");
    else 
    System.out.println("the number is odd");
    
    System.out.println("give me a float value that will represent a grade");
    float grade = keyboard.nextFloat();
    if (grade >= 95.9)
    System.out.println("The grade is above an 95.9 if the grade is an A.");
    else if (grade >=84.7 && grade <=86.9)
    System.out.println("The grade is within the B range");
    
    System.out.println("Give me a temperature");
    double temperature = keyboard.nextDouble();
    if (temperature > 78)
    System.out.println("higher than 78 degrees");
    else if (temperature <= 78) 
    System.out.println("less than or equal to 78 degrees");
    
    if (temperature % 2 != 0 && temp > 0)
    System.out.println("yes; positive and odd");
    else if (temperature % 2 == 0 && temp < 0)
    System.out.println("no; not positive and odd");
    
    System.out.println("Enter a letter grade");
    char letterGrade = keyboard.nextChar();
    if (letterGrade = A)
    System.out.println("Yes, you chose A");
    else if (letterGrade = B)
    System.out.println("Yes, you chose B");
    else if (letterGrade = C)
    System.out.println("Yes, you chose C");
    else if (letterGrade = D)
    System.out.println("Yes, you chose D");
    else if (letterGrade = F)
    System.out.println("Yes, you chose F");
    
    System.out.println("input a number");
    int a = keyboard.nextInt();
    System.out.println("input another number, please");
    int b = keyboard.nextInt();
    max();
} 
public static int max(int a, int b){
    if (a > b)
    System.out.println(a);
    else if (b > a)
    System.out.println(b);
} 
 
