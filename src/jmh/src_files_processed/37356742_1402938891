import java.util.*;
class project
{
    public static void main()
    {
        Scanner sc=new Scanner(System.in);
        int b=2;
        System.out.println("enter number for n");
        int m=sc.nextInt();
        int n=sc.nextInt();
        
        for(m=0;m<n&&n<100;m++)
        {
           int a=sc.nextInt();
           a=(n%b=0);
           System.out.println(+a+"number is even");
        }
    }
}