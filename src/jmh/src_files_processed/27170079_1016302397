class Rechteck {
    double laenge;
    double breite;

    Rechteck(double l, double b) {
        laenge = l;
        breite = b;
    }

    void setzeLaenge(double l) {
        laenge = 5.0;
  
    }

    void setzeBreite(double b) {
        breite = 2.0; 
    }

    double gibLaenge() {
        return laenge;
    }
    
    double gibBreite() {
        return breite;
    }

    void berechneUmfang(double l, double b) {
        Umfang = 2 * laenge + 2 * breite;
    }
    
   void berechneFlaeche(double l, double b) {
        Flaeche = laenge * breite;
    }
    
   void datenAusgeben() {
        System.out.println ("laenge" = laenge);
        System.out.println ("breite" = breite);
        System.out.println ("Umfang" = Umfang);
        System.out.println ("Flaeche" = Flaeche);
    }
         
    void tauscheLaengeBreite() {
        if (breite > laenge) {
            ???
        }
    }
}