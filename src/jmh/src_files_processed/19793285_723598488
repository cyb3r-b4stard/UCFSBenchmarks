
/**
 * @###### ###### #
 * @####### ##.##.####
 */
public class Würfelsimulator
{
    int wuerfelergebnisse[];

    wuerfelergebnisse = new int[29];

    public Würfelsimulator()
    {
        for(int i = 0; i < 30; i++)
        {
            wuerfelergebnisse[i]= (int) (Math.random()*6+1);

        }
    }
}
