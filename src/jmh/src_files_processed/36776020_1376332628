
import java.awt.*;
import javax.swing.*;


public class First_Yagnik_Sujal_2Q3 extends JApplet
{
    public void paint(Graphics g)
    {
        resize(700,350);
        setBackground(Color.BLUE);
        g.setColor(Color.red);
        g.drawRect(100,100,150,50);
        g.setColor(Color.green);
        g.drawLine(400,50,400,200);
        g.setColor(Color.Blue);
        g.drawString("Sujal Yagnik" 100,250);
        
    }
}