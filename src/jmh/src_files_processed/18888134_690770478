public class KFZ extends Landfahrzeug {
String Motorart;
double Tankvolumen;
super(220,5,4); 
public void MotorStarten (){
    System.out.println ("MotorStarten");
}
public void MotorAbstellen () {
    System.out.println ("MotorAbstellen");
}
 public KFZ () {
     
    }
    public KFZ (String m, double t){
     Motorart = m;
     Tankvolumen = t;
     super(220,5,4);
        
        
    }
}