class Palindrome
{
    public static void main(int n)
    {
        int r,c,d;
        r=0;
        c=n;
        do
        {
            d=n%10;
            r=r*10+d;
            n=n/10;
        
        While (n!=0);
        if(r==c)
        {
            System.out.println("palindrome");
            Else
            System.Out.println("not palindrome");
        }
    }
}