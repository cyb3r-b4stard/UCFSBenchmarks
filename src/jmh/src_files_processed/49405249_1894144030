import java.awt.Color;
import java.util.ArrayList;

public class Lollipop extends Rect
{
    private Circle c;
    private Rect r;
    private int x;
    private int y;

    
    
    public Lollipop()
    {
        r = new Rect();
        r.changeSize(50, 1);
        r.setX(75)
        c = new Circle(25);
        c.setX(75);
        c.setY(40);
        x=48;
        y=40;
        
        r.makeVisible();
        
        c.makeVisible();
    }
    
    public void changeCol(String col)
    {
        c.changeColor(col);
    }
    
    public int getX()
    {
        return x;
    }
    
    public int getY()
    {
        return y;
    }
    
    
    
}