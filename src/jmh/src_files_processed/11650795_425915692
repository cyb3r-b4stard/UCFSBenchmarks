public class GCD {
    public static void main (String args[ ])
    {
        int smaller, larger, dividend, divisor, quotient, remainder, GCD, temp;
        
        ConsoleIO keyboard = new ConsoleIO();
        System.out.print("Enter first integer (smaller) -->");
        smaller=keyboard.readInt();
        System.out.print("Enter second integer (larger) -->");
        larger=keyboard.readInt();
        if(smaller > larger)
        { temp=smaller; smaller=larger; lrger=temp;}
        
        dividend=larger;
        divisor=smaller;
        quotient=0;
        remainder=99999;
        
        while remainder > 0) {
            quotient=dividend/divisor;
            dividend=divisor;
            divisor=remainder;
        }
        GCD = dividend;
        System.out.print("The GCD is: ");
        System.out.print1n(GCD);
    }
}