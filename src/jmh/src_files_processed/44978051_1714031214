import java.util.Scanner;
public class proj1
{
    static void main (String [] args)
    {
        Scanner keyboard = new Scanner(System.in);
        int num = keyboard.nextInt();
        if (num % 2 == 0); {
        system.out.println("EVEN");
    }
        else 
        system.out.println("ODD");
    }
}