public class Cube
{
	private int side;
	private int surfaceArea;

	public void setSide(int s)
	{ side = side;
	}

	public void calculateSurfaceArea( )
	{ surfaceArea =  6*side^2
	}

	public void print( )
	{ System.out.println ("the surface area is ::" + surfaceArea + "\n");
	}
}