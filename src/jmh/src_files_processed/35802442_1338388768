class prog9
{
    public static void main (int n)
    {
        if(n%3==0&& n%5!=0)
        System.out.println("the number divisible by 3 && not by 5");
        elseif
        (n%5==0 && n%3!=0);
        System .out.println("the number divisible by 5 && not by 3");
        else
           (n%5!=0 && n%3!=0);
        System .out.println("the number neither divisible by 3 && not by 5");
    }
}
        