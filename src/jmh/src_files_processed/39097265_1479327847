  public static void main(String[] args)
{
{
  int 1stD = (int) (1+Math.random() *8);
  int 2ndD = (int)(Math.random() *8);
  int 3rdD = (int)(Math.random() *8);
  int 4to6D = (int)(Math.random() *743);
  int 7to11D = (int)(Math.random() *10000);
  System.out.println("A random number is "+"1stD"+"2ndD"+"3rdD"+" - "+4to6D +" - "+7to11D);
}
}