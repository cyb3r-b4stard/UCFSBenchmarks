import javax.swing.*;
public class LinearEquation{
    private int a;
    private int b;
    private int c;
    private int d;
    private int e;
    private int f;
    public int getA(){
        return a;
    }
    public int getB(){
        return b;
    }
    public int getC(){
        return c;
    }
    public int getD(){
        return d;
    }
    public int getE(){
        return e;
    }
    public int getF(){
        return f;
    }
    public boolean isSolvable(){
        int sol = (a*b)-(b*c)
        if(sol=0){
            JOptionPane.showMessageDialog(null, "The equation has no solution.");
            return false;
        }
    }
    public int linear(){
        a = 0;
        b = 0;
        c = 0;
        d = 0;
        e = 0;
        f = 0;
        return 1;
    }
    public int linear(int a, int b, int c, int d){
        this.a = a;
        this.b = b;
        this.c = c;
        this.d = d;
        this.e = e;
        this.f = f;
        return 1;
    }
    public String toString(){
        String output = new String();
        output = "";
        return output;
    }
}