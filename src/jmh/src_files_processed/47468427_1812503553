public class NumberToWords {
	public static void main(String[] args) {
	  String[] TensArray = {"twenty","thirty","forty","fifty","sixty","seventy","eighty","ninety");
	  String[] OnesArray = {"one","two","three","four","five","six","seven","eight","nine","ten","eleven","twelve","thirteen","fourteen","fifteen","sixteen","seventeen","eighteen","nineteen"}
	   
	  System.out.print("Number: ") 
	  int Input = in.nextInt();
	  int Tens = 0;
	  int Ones = 0;
	  while (Input != -1){
	  
	    if (Input > 99) {
	      int Hundreds = input / 100;
	       System.out.print(onesArray[Hundred] + " " + "hundred");
	    
	     if (Input % 10 != 0 && Input / 10 != 0){
	        System.print.out(" " + "and" + " ");
	        Input = Input - Hundreds * 100; 
	        
	        if (Input > 19) {
	         Tens = Input / 10; 
	         Ones = Input % 10;
	         System.out.print(TensArray[Tens] + " " + OnesArray[Ones])
	        
	       }
	        
	      }
	    
	   }   
	}
}