 */
public class codierung
{
   public static int[] binaercode(int n){
        int index =0;
        int f = n ;
       int[] folge = new int[16]; 
       while(f != 0){
           int z;
           z = f%2;
           f = f/2;
           
           folge[index] = z;
           index++;
        
        }
        return folge;
        for(int i : folge[]){
            System.out.print(folge[i]);
        
        }
    
    }
    
    
}