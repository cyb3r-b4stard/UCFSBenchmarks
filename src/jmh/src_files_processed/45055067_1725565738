import java.util.Scanner;
/**
 * ####: ###### ##########
 * ##: ########
 */
public class ArrayManip
{
    public static void main(String[] args)
    {
        testHasDuplicates();
        int times = 10;
        int [][] cases =    {
                                {1, 2, 3, 4, 5, 6}, 
                                {2342, 234,2, 35, 234, 234, 1}, 
                                {12, 12412, 12, 444, 1},
                                {},
                                {1}
                            };
        for (int case = 0; case < case.length; case++)
        {
            System.out.println(hasDuplicates(case));
        }
        System.out.println(hasDuplicates());
    }
    public static boolean searchNum(int[] myArray, int num)
    {
        boolean numIsInArray = false;
        for (int numFromArray: myArray)
        {
            if (num == numFromArray)
            {
                numIsInArray = true;
            }
        }
        return numIsInArray;
    }
    public static void testSearchNum()
    {
        String promtSizeArray = "Please enter the size of your array";
        String promtNextNumber = "Please enter number %d / %d";
        String promtKeyNumber = "Please enter the number to search for";
        String searchSuccess = "That number presents in array";
        String searchFailure = "That number doesn`t present in array";
        System.out.println(promtSizeArray);
        Scanner keyboard = new Scanner(System.in);
        int sizeOfArray = keyboard.nextInt();
        int[] arrayOfNumbers = new int[sizeOfArray];
        for (int count = 0; count < arrayOfNumbers.length; count++)
        {
            System.out.printf(promtNextNumber + "\n", (count + 1), arrayOfNumbers.length);
            arrayOfNumbers[count] = keyboard.nextInt();
        }
        System.out.println(promtKeyNumber);
        int keyNumber = keyboard.nextInt();
        boolean searchResult = searchNum(arrayOfNumbers, keyNumber);
        if (searchResult)
            System.out.println(searchSuccess);
        else
            System.out.println(searchFailure);
    }
    public static boolean hasDuplicates(int[] listA)
    {
        for (int count1 = 0 ; count1 < listA.length; count1 ++)
        {
            for (int count2 = count1 + 1; count2 < listA.length; count2++)
            {
                if (listA[count1] == listA[count2])
                {
                    return true;
                }
            }
        }
        return false;
    }
    public static void testHasDuplicates()
    {
        String promtSizeArray = "Please enter the size of your array";
        String promtNextNumber = "Please enter number %d / %d";
        String testSuccess = "That array has duplicates";
        String testFailure = "That array has no duplicates";
        System.out.println(promtSizeArray);
        Scanner keyboard = new Scanner(System.in);
        int arraySize = keyboard.nextInt();
        int[] array = new int[arraySize];
        for (int count = 0; count < array.length; count++)
        {
            System.out.printf(promtNextNumber + "\n", (count + 1), array.length);
            array[count] = keyboard.nextInt();
        }
        System.out.println();
        boolean searchResult = hasDuplicates(array);
        if (searchResult)
            System.out.println(testSuccess);
        else
            System.out.println(testFailure);
    }
}