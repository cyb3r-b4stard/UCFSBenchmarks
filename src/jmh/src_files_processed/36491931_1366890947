import java.util.*;
class palindrom
{
    void main()
    {String str,rev="";
        int l,i;
        Scanner Sc=new Scanner(System.in);
        System.out.println("Enter a string");
        str=Sc.nextLine();
        l=str.length();
        for(i=l-1;i>=0;i--)
        rev=rev+str.charAt(i);
        
        System.out.println("palindrome STRING");
else
System.out.println("not a palindrome STRING");
}
}
        