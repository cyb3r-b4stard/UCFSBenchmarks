    {
        natuerlicheZahlen = new int [8];
    }
    
    public void zahlenAusgeben()
    {
    System-out.println("Vorhandene Zahlen in deinem Array sind: ");
        for (int zahl = 0; zahl<natuerlicheZahlen.length; zahl++){
         System.out.printeln(natuerlicheZahlen[zahl]);
        }
    }
    
    /**
