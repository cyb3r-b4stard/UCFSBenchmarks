import java.util.Scanner;
class rainbow
{
    public static void main (String args[])
    {
        Scanner sc = new Scanner (System.in);
        System.out.println ("Enter any no. between 1 to 7");
        int s = sc.nextInt();
        switch (s);{
            Case1: System.out.println ("Violet");
            break;
            Case2: System.out.println ("Indigo");
            break;
            Case3: System.out.println ("Blue");
            break;
            Case4: System.out.println ("Green");
            break;
            Case5: System.out.println ("Yellow");
            break;
            Case6: System.out.println ("Orange");
            break;
            Case7: System.out.println ("Red");
            break;
        }
    }
}
