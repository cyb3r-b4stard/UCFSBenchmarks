public class Liste{
    public Listenelement erster;
    
    public Liste(Listenelement ersterNeu){
        erster = ersterNeu;
    }
    public void hinteneinfuegen(Knoten k){
        if(erster = a){
        k.setnext(a);
        erster = k;
    }
    else{
        erster.hinteneinfuegen(Knoten k);
    }
}