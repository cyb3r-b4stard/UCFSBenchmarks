package Unit5;


public class MixedNumber extends Fraction
{
    private int whole;
    
    public MixedNumber()
    {
        super ( 1 , 1 );
        whole = 1;
    }
    
    public MixedNumber ( int d )
    {
        super ( 1 , d );
        whole = 1;
    }
    
    public MixedNumber ( Fraction f )
    {
        super ( f.getNum() , f.getDen() );
        whole = 1;
        
    }
    
    public MixedNumber ( int w , Fraction f )
    {
        super ( f.getNum() , f.getDen() );
        whole = w;
    }
    
    public Fraction getFrac ()
    {
        return super( this.getNum() , this.getDen() );
    }
}