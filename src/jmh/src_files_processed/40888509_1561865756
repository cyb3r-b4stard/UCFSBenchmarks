public class Afspeellijst
{
    private int aantalLiedjes;
public Afspeellijst (int aantalLiedjes)
{
    this.aantalLiedjes=aantalLiedjes;
}

public double totaleSpeelduur()
{
    double totaleSpeelduur;
    totaleSpeelduur=aantalLiedjes*double speelduur;
    return totaleSpeelduur;
}

}