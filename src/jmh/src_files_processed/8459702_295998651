 */
public class Wurfwaffe extends Waffe    
{
    public int angriffswert;

    public Wurfwaffe(int angriffswert)
    {
        this.angriffswert=angriffswert;
        super(angriffswert);
    }
    
    public int getKampfwert()
    {
        return angriffswert;
    }
  
}