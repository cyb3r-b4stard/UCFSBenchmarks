

public class Retangulo
{
  private double retangulo,area,base,altura,perimetro;
  public Retangulo (double base1, double altura1)
   {
     base = base1;
     altura = altura1;
   }
  public double getBase() 
   {
      return base;
   }
  public double getAltura()
   {
      return altura;
   }
  public void setBase(double base1)
   {
      base = base1;
   }
  public void setAltura(double altura1)
   {
      altura = altura1
   }
  public double areaRetangulo()
   {
     return (base*altura) 
   }
}