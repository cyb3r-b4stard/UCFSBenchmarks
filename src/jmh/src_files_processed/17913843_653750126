public class q4
{
    public static void main(String[]args){
        int[]a = {17,11,20,34,5,10,8,19,55,11,13};
        int value = 12;
        imt[]b =  filter(a,value);
    }

    static int[] filter(int[]a, int value) {
        int arraySize = 0;
        
        for(int i=0; i<a.length; i ++) {
            if(a[i] < value)
                }       arraySize++;
        }
        int b[] = new int[arraySize];
        for(int j=0; j<arraySize; j++) {
            if(a[j]>value)
                a[j] = b[j];
           
        }
        return b;
    }
    
}