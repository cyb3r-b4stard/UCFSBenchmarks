package Chapter3;

import java.util.Random;

public class RandomhoneNumber
{
    public static void main (String[]args)
    {
        int digit1, digit2, digit3, digit4, digit5, 
        digit6, digit7, digit8, digit9;
        Random generator = new Random();
        
        digit1 = generator.nextInt(8);
        digit2 = generator.nextInt(8);
        digit3 = generator.nextInt(8);
        digit4 = generator.nextInt(7);
        digit5 = generator.nextInt(4);
        digit6 = generator.nextInt(1);
        digit7 = generator.nextInt(9);
        digit8 = generator.nextInt(9);
        digit9 = generator.nextInt(9);
        
        System.out.print ("Phone number: " + digit1 + digit2 + 
        digit3 + "-" + digit4 + digit5 + digit6 + "-" digit7 + 
        digit8 + digit9);
    }
}
        
