
public abstract class ComunicacionInalambrica extends TipoComunicacion 
{
    private int frecuencia;
    private String antena;
    public ComunicacionInalambrica(int frecuencia,String antena){
        super(velocidad,distancia)
        this.frecuencia=frecuencia;
        this.antena=antena;
    }
    public abstract void getAntena();
    public abstract void getFreciencia();
}