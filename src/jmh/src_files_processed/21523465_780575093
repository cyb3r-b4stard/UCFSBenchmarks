 */
public class NameThatCelebrity
{
   String s1="Allan Alda";
   String Allan=s1.substring(2,7);
   System.out.println("Allan");
   
   String s2="John Wayne";
   String John=s2.substring(2,7);
   System.out.println("John");
   
   String s3="Gregory Peck";  
   String Gregory=s3.substring(2,7);
   System.out.println("Gregory");
   
}