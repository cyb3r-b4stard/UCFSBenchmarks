import edu.duke.*;
import java.util.*;

public class TestVignereCipher {
    public void encrypt(){
        FileResource fr = new FileResource();
        VigenereCipher vigenereCipher = new VigenereCipher({17, 14, 12, 4});
        for (String line: fr.lines()){
            String str = vigenereCipher.encrypt(line.toString());
            System.out.println(str);
        }
    }
}