public class GradingQuizzes
{
   public static void main(String [] args) throws IOException 
     {
        BufferedReader f = new BufferedReader(new FileReader("test2.in"));


        PrintWriter out = new PrintWriter(new BufferedWriter(new
                    FileWriter("test2.out")));

        StringTokenizer st = new StringTokenizer(f.readLine());
       final int SALESPEOPLE;
       int sum;
       int questions;
       int answers;
       String phrase = "";
       
       
      Scanner scan = new Scanner(System.in);
    System.out.println("How many questions are on the quiz?");
    questions = scan.nextInt();
    System.out.println("number of questions =" +questions);
    int [] correctAnswers= new int [questions];
    for (int i=0; i<correctAnswers.length; i++)
    {
        System.out.println("Enter the key" + (i+1) +":");
        correctAnswers[i] = Integer.parseInt(st.nextToken());
    }
    BufferedReader g = new BufferedReader(new FileReader("Test2.in"));
    StringTokenizer br = new StringTokenizer(g.readLine());
    int quizNumber = Integer.parseInt(br.nextToken());
    double Correct=0;
    double Wrong=0;
    for (int i=0; i<correctAnswers.length; i++)
    {
        System.out.println("Enter the students answers to be graded");
        answers = scan.nextInt();
        
        if (answers== correctAnswers[i])
        {
          Correct++;
        }
        else
        {
           Wrong++; 
        }
    }
   
    double Percent = Correct/questions;
    System.out.println("The number correct is" +Correct);
    System.out.println(Correct);
    System.out.println(questions);
    System.out.println(Percent);
    System.out.println("The percent correct is" +Percent*100+"%");
        System.out.println("Grade another quiz? (y/n)");
        phrase = scan.nextLine();
         System.out.println("Abigail Higginbotham")
  }
    

   
   
