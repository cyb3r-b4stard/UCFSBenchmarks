import java.awt.*;
import java.applet.Applet;
/**
 * ##### # ########### ## ##### ####### ####.
 * 
 * @###### (#### ####) 
 * @####### (# ####### ###### ## # ####)
 */
public class Picture extends Applet
{
   public void paint (Graphics page)
   {
       setBackground(Color.lightGray);
       page.setColor.cyan;       
       page.drawRect(500,250,450,250);
       page.fillRect(500, 250, 450, 250);
   }
}