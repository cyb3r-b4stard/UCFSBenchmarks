/**
 * ### ####### ##### ## ####### ## #### ####### #######
 */
public class Student
{
    private String Name;
    private String PhoneNumber;
    private Subject subject;
    /**
     * Constructor 
     */
    public Student(String Name,String PhoneNumber,subject)
    {
        this.Name = Name;
        this.PhoneNumber = PhoneNumber;
        subject = new Subject();
    }
    
    /**
     * Display student information
     */
    public void Display()
    {
        System.out.println("Student Name:" + getName() + "Student PhoneNumber:" + getPhoneNumber() + "Subject:" + subject.getName());
    }
    
    /**
     * Set Name
     */
    public void setName()
    {
        this.Name = Name;
    }
    
    /**
     * get Name
     */
    public String getName()
    {
        return Name;
    }
    
    /**
     * 
     */
    public void setPhoneNumber()
    {
        this.PhoneNumber = PhoneNumber;
    }
    
    /**
     * 
     */
    public String getPhoneNumber()
    {
        return PhoneNumber;
    }    
}