import sum.werkzeuge.*;
import sum.kern.*;
/**
 * @###### ####### ########, ##### ### ######
 * @####### 
 */
public class Hauptprogramm
{
    Bildschirm b;
    Schaaaahatz schatz;
    Schuuuuuh schuh;
    Stieeehfel stiefel;
    xXgoldene_KuuuuhgelXx kugel;
    
    public Hauptprogramm()
    {
         b = new Bildschirm();
         schatz = new Schaaaahatz(b);
         schuh = new Schuuuuuh(b);
         stiefel = new Stieeehfel(b);
         kugel = new xXgoldene_KuuuuhgelXx(b);
         
    }
    public Verstecken_der_saaaahaachen();
    {
    schatz.Verstecke();
    schuh.Verstecke();
    stiefel.Verstecke();
    kugel.Verstecke();
    }
}