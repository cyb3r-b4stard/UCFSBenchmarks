
/**
 * ### ########## ##### ########## # ########## ## # ########## ############## ######.
 * ## ##### ### ########## ####### ######## ## ### #######.
 * 
 * @###### ####### #####
 * @####### ####.##.##
 */
public class Instructor extends LabClass
{
    private String name;
    private String id;
    private String address;
    private String phone;
    private String email;

    /**
     * Create a new Instructor with a given name and ID number.
     */
    public Instructor(int totalStudents, String fullName, String instructorID, String addr, String ph, String mail)
    {
        super(int totalStudents, fullName);
        name = fullName;
        id = InstructorID;        
        address = addr;
        phone = ph;
        email = mail;
    }

    /**
     * Return the full name of this Instructor.
     */
    public String getName()
    {
        return name;
    }

    /**
     * Return the ID of this Instructor.
     * Method getInstructorID return the ID of this Instructor 
     * allotted for taaking a LabClass. 
     */
    public String getInstructorID()
    {
        return id;
    }
    
    /**
     * Return the address this Instructor.
     * Method getAddress return the address of this Instructor 
     * for the LabClass. 
     */ 
    public String getAddress()
    {
        return address;
    }

    /**
     * Return the id this Instructor.
     * Method getPhone return the address of this Instructor. 
     * This is stored in the private field phone 
     * of the class Instructor.
     */ 
    public String getPhone()
    {
        return phone;
    }
    
        /**
     * Return the id this Instructor.
     * Method getEmail return the address of this Instructor. 
     * This is stored in the private field email 
     * of the class Instructor.
     */ 
    public String getEmail()
    {
        return email;
    }
}
