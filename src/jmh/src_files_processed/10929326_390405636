public class Arbeiter extends Mitarbeiter
{
    private double arbeitsstunden;
    private double lohnsatz;
    
    public void setStunden(double pStunden)
    {
        arbeitsstunden = pStunden;
    }
    
    public void setLohnsatz (double pLohnsatz)
    {
        lohnsatz = pLohnsatz;
    }
    
    public double getStunden()
    {
        return arbeitsstunden;
    }
    
    public double getLohnsatz()
    {
        return lohnsatz;
    }
    
    public double berechneLohn()
    {
        arbeitsstunden * lohnsatz = lohn;
    }
    
    
}