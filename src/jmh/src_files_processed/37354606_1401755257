import java.util.*;
class sprog
{
    public static void main()
    {
        Scanner in=new Scanner(System.in);
        int c;
        System.out.println("enter a number");
        c=in.nextInt;
        swith(c);
        {
            case1:
            System.out.println("mon");
            break;
            case 2:
            System.out.println("tue"):
            break;
            default:
            System.out.println("wrong inputs");
        }
    }
}
           