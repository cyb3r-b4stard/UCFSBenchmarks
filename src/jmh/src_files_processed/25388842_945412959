    { 
        n1 = one;
        n2= two;

    }

    public void setNums(int n1, int n2)
    {
        

    }

    public int getLargest()
    {
        if(one>two)
           return one;
        else 
           return two;
           
    }

    public int getSmallest()
    {
        if (two > one)
        return two;
        else
        return one;-
    }

