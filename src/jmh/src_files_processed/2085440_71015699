public class dart1
{
    Bildschirm derBildschirm;
    Stift meinStift;

    public dart1()
    {
        derBildschirm = new Bildschirm();
        meinStift = new Stift();
    }

    public void fuehreAus()
    {
        meinStift.bewegeBis(10,390);
        meinStift.runter();
        meinStift.bewegeBis(40;380);
        meinStift.zeichneKreis(2); 
        meinStift.hoch();
        meinStift.bewegeBis();
	    