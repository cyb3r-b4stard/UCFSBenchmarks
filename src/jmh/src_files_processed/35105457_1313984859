public class Pasajero
{
    private String rut;
    private String contrasenia;
    private String nombreCompleto;
    private boolean esNacional;
    private String email;
    
    public Pasajero()
    {
    }
    public Pasajero(String rut, String contrasenia, String nombreCompleto,
    boolean esNacional, String email)
    {
        this.setRut(rut);
        this.setContrasenia(contrasenia);
        this.setNombreCompleto(nombreCompleto);
        this.setEsNacional(esNacional);
        this.setEmail(email);
    }
    
    public String getRut()
    {
        return this.rut;
    }
    public String getContrasenia()
    {
        return this.contrasenia;
    }
    public String getNombreCompleto()
    {
        return this.nombreCompleto;
    }
    public boolean getEsNacional()
    {
        return this.esNacional;
    }
    public String getEmail()
    {
        return this.email;
    }
    
    public void setRut(String nuevoRut)
    {
        this.rut = nuevoRut;
    }
    public void setContrasenia(String nuevaContrasenia)
    {
        this.contrasenia = nuevaContrasenia;
    }
    public void setNombreCompleto(String nuevoNombreCom)
    {
        this.nombreCompleto = nuevoNombreCom;
    }
    public void setEsNacional(boolean nuevaEsNacional)
    {
        if(nuevaEsNacional == chileno == true )
