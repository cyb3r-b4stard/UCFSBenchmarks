import java.util.Scanner; 
public class FactorialAH
{
 public static void main (String[] args)
 {
     int count = 1;
     int product = 1;
     int number; 
     
     Scanner scan = new Scanner (System.in);
     
     System.out.println("Enter a non-negative whole number");
     number = scan.nextInt();
     
    
     while (number < 0)
     {
         System.out.println("you can not enter a negative number, you must enrter a positive number");
         number+;
        }  
     while (count<= number)
     {
         product *= count;
         count ++;
        
     }
     
     System.out.print(number+ "! = " + product);
    }
}
     