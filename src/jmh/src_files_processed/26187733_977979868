import java.util.Scanner;

public class GoodMorning 
{
    public static void main(String[] args)
    {
        int count = 0;   		
        int num
        System.out.print("The number " + num);
        while (num >= 1) 
        {
            num = num / 2;
            count = count + 1;
        }
        System.out.println(" can be divided by two " + count + " times");

    }
}


        