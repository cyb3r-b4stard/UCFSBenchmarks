import java.util.*;
import java.lang.*;
import java.io.*;

public class Loops
{
    public static void main (String[] args)
    {
        int numberOfTimes = 10;
        for (int index=0; index<numberOfTimes; index++)
        {
            System.out.println("Hello World");
        }
        for (int index=1; index<=numberOfTimes; index++)
        {
            System.out.println("Hello World");
        }
        for (int index=0; index<numberOfTimes; index=index+1)
        {
            System.out.println("Hello World");
        }
        int index=0;
        while (index<numberOfTimes)
        {
            System.out.println("Hello World!");
            index++;
        }
    }
    public static void printHelloWorld(int numberOfTimes)
    {
        for (int index=0; index<numberOfTimes; index++)
        {
            System.out.println("Hello World");
        }
    }
    public static void printHelloWorld(int numberOfTimes)
    {
        for (int index=0; index<=numberOfTimes; index++)
        {
            System.out.println("Hello World");
        }
    }
    public static void printHelloWorld(int numberOfTimes)
    {
        for (int index=0; index<numberOfTimes; index=index+1)
        {
            System.out.println("Hello World");
        }
    }
    public static void printHelloWorld(int numberOfTimes)
    {
        int index=0;
        while (index<numberOfTimes)
        {
            System.out.println("Hello World!");
            index++;
        }
    }
    public static void printIntegersTo(int limit)
    {
        {
            printIntegersTo(10);
        }
    }
 }
}
