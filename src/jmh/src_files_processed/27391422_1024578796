class OOP_2
{
    
    private String Nis;
    private String Nama;
    private String Alamat;
    private String Telepon;
    void displayData()     
     {
        System.out.println ("Nis       :" +this.Nis);
        System.out.println ("Nama      :" +this.Nama);
        System.out.println ("Alamat    :" +this.Alamat);
        System.out.println ("Telepon   :" +this.Telepon);
    }
    
    public static void main (String[] args)
    {
        OOP_1 TEMAN = new OOP_2();  
        TEMAN.Nis ="87987";
        TEMAN.Nama ="Andi Maulana";
        TEMAN.Alamat = "Jl. angsana No 88 Jakarta";
        TEMAN.Telepon = "021-434493943";
        System.out.println ("Nis       :" +TEMAN.Nis);
        System.out.println ("Nama      :" +TEMAN.Nama);
        System.out.println ("Alamat    :" +TEMAN.Alamat);
        System.out.println ("Telepon   :" +TEMAN.Telepon);
        TEMAN.displayData();
        
    };
    