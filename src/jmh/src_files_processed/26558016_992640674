
/**
 * ##### # ########### ## ##### ####_#_##### ####.
 * 
 * @###### (#### ####) 
 * @####### (# ####### ###### ## # ####)
 */
public class Joke_O_Matic
{
    String setup[] = {"How do you make 7 an even number?", 
        "Why are ghosts bad liars?", "What dog can jump higher than a building?"}
    
    String punchLine[] = {"Take the s out.", 
        "Because you can see right through them.", "Any dog, buildings can't jump."}
    public static void main(String args[])
    {
        System.out.print(setup[0] + "\n" + punchLine[0] + "\n");
        System.out.print(setup[1] + "\n" + punchLine[1] + "\n");
        System.out.print(setup[2] + "\n" + punchLine[2] + "\n");
    }
}