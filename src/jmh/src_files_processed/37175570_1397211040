import java.io.*;
import java.util.Scanner;
public class proj3 {
    public static void main ( String [] args) {
        Scanner input = new Scanner(System.in);
        int Num1,Num2,Num3,tempnum;
        System.out.println("Enter the first number in your dataset");
        Num1=input.nextInt();
        System.out.println("Now enter the second");
        Num2=input.nextInt();
        System.out.println("...And the last one");
        Num3=input.nextInt();
        if (Num1 > Num2 && Num2 > Num3) {
            System.out.println("The order is"+Num3 +Num2 +Num1);
        }
                else if (Num2 > Num1 && Num3 > Num2) {
                    System.out.println("The order is"+Num1 +Num2 +Num3);
                }
                else if (Num3 > Num2 && Num1 > Num3){
                            System.out.println("The order is"+Num2 +Num3 +Num1);
                        
                    
                }         
                else if (Num2 > Num1 && Num1 > Num3) {
                    System.out.println("The order is"+Num3 +Num1 +Num2);
            } 
                else if  (Num1 > Num3 && Num3 > Num2) {
                    System.out.println("The order is" +Num2 +Num3 +Num1);
            
        }        
                else if (Num3 > Num1 && Num1 > Num2) {
                    System.out.println("The order is" +Num2 +Num1 +Num3);
    }           
}                  
         
        
        
        
        
   