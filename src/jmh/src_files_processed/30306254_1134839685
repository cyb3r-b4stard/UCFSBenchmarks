public class March1Tester
{
    public static void main(String args[])
    {
        System.out.println(crch(12));
    }
    public static int crch(int n)
    {
        if(n<=0)
        {
        System.out.print(n);
        }
        else
        {
            crch(n/3);
            System.out.print("," + n);
        }
        else
        return 0;
    }
}