 */
public class sumRange
{
  public static void main(String[] args)
    {
        int x = 75, y = 50;
        if (y < x){
        System.out.println("ERROR" + 0);
    }
        else{
            System.out.println(((x-y)(x-y) + 1) / 2);
        }
      
    }
}