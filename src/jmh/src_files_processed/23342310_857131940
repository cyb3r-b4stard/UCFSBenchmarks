        int result2 = 2 / (4 - 3) + ((5 * 4 % 3 * 5 - 6) * 2) - 5;   
        int i = 10 , j = 20 , k ;
        k = ++i - j--;
        System.out.println(result1);
        System.out.println(result2);
        System.out.println(k +"\n");
        
