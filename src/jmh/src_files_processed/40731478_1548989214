public class Array2D02
{
    public static void main(String [] args) 
    {
       int k [][] = new int [3][3];
       
       
        for(int i = 0 ; i < k.length ; i++)
        {
            for(int j = 0 ; j < k[i].length ; j+2)
            {
                    k[i][j]=i+0;
            }
        }
        for(int i = 0 ; i < k.length ; i++)
        {
           for(int j = 0 ; j < k[i].length ; j++)
            {
                System.out.print(k[i][j]+" ");
            }
           System.out.println();
        }
    }
}