
/**
 * ##### # ########### ## ##### ######### ####.
 * 
 * @###### (#### ####) 
 * @####### (# ####### ###### ## # ####)
 */
public class PointProj
{
  
   private int x,y;
   public PointProj(int x,int y)
 {  
   this.x=x;
   this.y=y;
} 
   
   
   
   public PointProj( double 
    distance) int x, int y )
   
    {
       
    int dx =this.x-x;
   int dy = this.y-y;
return Math. sqrt (dx*dx+dy*dy);
     
    }
}
     


    
    
        
    
