
/**
 * ##### # ########### ## ##### ######## ####.
 * 
 * @###### (#### ####) 
 * @####### (# ####### ###### ## # ####)
 */
public class Complejo
{
    private double a;
    private double b;
    
    /**
     * Constructor for objects of class complejo
     */
    public Complejo()
    {
        a=0;
        b=0;
        
    }
    public Complejo(double x, double y)
    { 
        a=x;
        b=y;
        
    }
    public Complejo (Complejo z)
    {
        this.a = z.a;
        this.b = z.b;
    }
    public double getReal()
    {
        return a   ;
    }
     public double getImaginario()
    {
        return b ;
    }
     public Complejo sumar (Complejo z)
    {
        Complejo S = new Complejo(); 
        S.a = this.a + z.a;
        S.b = this. b + z.b;
        return S;
    }
      public Complejo restar (Complejo z)
    {
        Complejo S = new Complejo(); 
        S.a = this.a - z.a;
        S.b = this. b - z.b;
        return S;
    }
     public Complejo multiplicar (Complejo z)
    {
         Complejo S = new Complejo();
         S.a = this.a * z.a - this.b * z.b ;
         S.b = this.b * z.a + z.b * this.a ;
         return S;     
    }    
     public complejo dividir (complejo z)
    {
       Complejo S = new Complejo () ;
       S.a= (this.a * z.a + this.b * z.b)/(z.a*z.a)+(z.b*z.b)
       S.b= (this.b * z.a - this.a * z.b)/(z.a*z.a)+(z.b*z.b)
       return S
    }
    /**
     * An example of a method - replace this comment with your own
     * 
     * @param  y   a sample parameter for a method
     * @return     the sum of x and y 
     */
    public int sampleMethod(int y)
    {
        /return x + y;
    }
}
