
import java.io.*;
public class RepetitionsTest
{
    public static void main(String [] args){
        Week5Repetitions app = new Week5Repetitions();
        int start = 0;
        in end = 0;
        
        BufferReader br = new BufferedReader(new InputStreamReader(System.in));
        
        System.put.print("Enter starting number: ");
        
        try{
            start = Integer.parseInt(br.readLine());
        }catch (I0Exception ioe) {
            System.out.println("IO error trying to read first number!");
            System.exit(1);
        }
        System.out.print("Enter ending number: ")
        
        try{
            end = Integer.parseInt(br.readLine());
        }catch (I0Exception ioe) {
            System.out.println("IO error trying to read first number!");
            System.exit(1);
        }
        
        app.whileStatement(Start,end);
       
}
}