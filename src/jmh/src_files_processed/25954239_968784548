import java.io.*;
public class AAA{
    public static void main (String[]args) throws IOException{
        BufferedReader var = new BufferedReader (new InputStreamReader (System.in));
        
        String y;
        int num1, num2, num3;
        
        System.out.print ("Enter The First Number :");
        y = var.readLine();
        num1 = Integer.parseInt (y);
        
        System.out.print ("Enter The Second Number :");
        y = var.readLine();
        num2 = Integer.parseInt (y);
        
        System.out.print ("Enter The third Number :");
        y = var.readLine();
        num3 = Integer.parseInt (y);
        
        
        if (num1) > (num2 > num3)
        System.out.print(num1 + " IS GREATER THAN" + num2 + "AND " + num3);
            
        else if (num2 > num1)(num2 > num3)
        System.out.print(num2 + " IS GREATER THAN " + num1 + " AND " + num3);
        
        else
        System.out.print(num3 + " IS GREATER THAN " + num1 + " AND " + num2);
        
    }
}