public class Ex4 {

  public static void main(String[] args) {
      
      
      double[] input = new double[args.length];
      for (int i = 0; i < args.length; i++){
          input[i] = double.parseDouble(args[i]);
          System.out.println(input[i]);
          
        }
        
      
      
  }
}