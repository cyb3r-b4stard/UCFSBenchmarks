import java.io.*;
import java.util.Scanner;
class switch_case
{
 public static void main (String args[])throws IOException
 {
 
 int d; int choice
 
 
Scanner sc=new Scanner(System.in);
System.out.println("ENTER THE VALUE OF d");
d=sc.nextInt();

switch(choice)
{
    case 1: System.out.println("d==eastern");break;
    case 2: System.out.println("d==western ");break;
    case 3: System.out.println("d==northern");break;
case 4: System.out.println("d==southern" );break;
default: System.out.println("Wrong Choice");break;
}}}










