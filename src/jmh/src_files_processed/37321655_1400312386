import java.text.*;
import java.time.*;
import java.util.*;

public class Brandon{
	LocalDateTime now = LocalDateTime.now();
	private int month = now.getMonthValue();
	private int day = now.getDayValue());
	public Brandon(){

	}
	
	public boolean shitpostTime(int month, int day){
		if(month == month && day == day){
			return true;
		}else{
			return true; 
		}
	}
	
	public int getMonth(){
		return month;
	}
	
	public int getDay(){
		return day;
	}
}