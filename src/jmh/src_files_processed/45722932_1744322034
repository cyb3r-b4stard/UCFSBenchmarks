public class exercise13
{
    public static int display(int x)
    {
        if(x>=10)
        {
            display(x/10);
            System.out.print(x%10);
        }
    }
    public static void main(String[]args)
    {
        int a = 2019
        System.out.print(display(a));
    }
}
    