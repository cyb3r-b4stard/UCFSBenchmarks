/**
 * ####### ####### ## ##### ###### #### # ###### #### 
 */
public class Weapons
{
    public Weapon getWeapon(int number)
    {
        Weapon w1 = new Weapon("null",0,0,0,0);
        if(number=1)
            w1=("sword",1,0,0,0);
        if(number=2)
            w1=("fire sword",1,1,0,0);
    }
}