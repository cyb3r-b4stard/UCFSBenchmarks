
public class Lab0b
{
	public static void main ( String[] args )
	{

		byte myByte = 127;

		
		

		System.out.println("
		System.out.println("*        Integer Types          *");
		System.out.println("*                               *");
		System.out.println("*myByte (8 bit) = "+myByte+"\t\t*");
		System.out.println("*myShort (16 bit) = "32123"\t\t*");
		System.out.println("*myInt (32 bit) = "90877"\t\t*");
	}
}