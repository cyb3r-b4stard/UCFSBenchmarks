import java.util.*;
/**
 * ##### #### - ##########
 */
public class Line extends Record
 {
    private String result;
    /**
     * Constructor for objects of class Line
     */
    public Line(int number)
    {
        super(number);
    }
    public void spin(Reels reels)
    {         
        result = reels.getResult(number);
    }
    public int payOff()
    {
        int payoff = 0;
        for()
        {
            
        }
        return payoff;
    }
    public String toString()
    {
        return result;
    }
    
}