public class TwoPlayerBlackJack
{
    private DeckOfCards myDeck;
    public TwoPlayerBlackJack(Card c)
    {
       myDeck = new DeckOfCards();
    }
    public int getCardValue(Card c)
    {
        String cardRank = c.getRank();
        int cardValue;
        try
        {
            cardValue =  Integer.parseInt(cardRank);
        }
        catch(Exception e)
        {
            if(cardRank.equals("Ace"))
            {
                cardValue = 1;
            }
            else
            {
                cardValue = 10;
            }
        }
        return cardValue;  
    }
