import java.util*
class Array{
public static void main(String args[]){

int arr[]=new int[7]; 
arr[0]=5;
arr[1]=10;
arr[2]=15;
arr[3]=40;
arr[4]=45;
arr[5]=50;
arr[6]=55;
for(int i=0;i<7;i++)
{
System.out.println(arr[i]);
}

Arrays.sort(arr);



for(int i=0;i<arr.length;i++)
System.out.println(arr[i]);

}
}