
public class Perro
{
    private String color;
    private String raza;
    private  int tamano;
    private  int peso;
    private Perro( String color, String raza , String tamano , String peso)
    {
       color (color);
       raza (raza);
       tamano (tamano);
       peso (peso);

    }
    private Perro()
    {
       color = "coffe";
       raza = "doberman";
       tamano = 120;
       peso = 67;

    }
    private void setColor(String color)
    {
        this.color;
    }
    private String getColor()
    {
       return color;
    }
    
}