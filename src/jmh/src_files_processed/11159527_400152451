public class PiggyBank
{
    private int myPennies;
    private int myNickels;
    private int myDimes;
    private int myQuarters;
    
    public PiggyBank()
    {
        myPennies = 0;
        myNickels = 0;
        myDimes = 0;
        myQuarters = 0;
    }
    public PiggyBank(int quarters, int dimes, int nickels, int pennies)
    {
        myPennies = pennies;
        myNickels = nickels;
        myDimes = dimes;
        myQuarters = quarters;
    }
    public void addMoney(int quarters, int dimes, int nickels, int pennies)
    { 
        myPennies += pennies;
        myNickels += nickels;
        myDimes += dimes;
        myQuarters += quarters;
    }
    public int getPennies()
    {
        return myPennies;
    }
    public int getNickels()
    {
        return myNickels;
    }
    public int getDimes()
    {
        return myDimes;
    }
    public int getQuarters()
    { 
        return myQuarters;
    }
    public double calculateMula()
    { 
        double total= myQuarters * 0.25, myDimes * 0.10, myNickels * 0.05, myPennies *0.01
        return total
    }