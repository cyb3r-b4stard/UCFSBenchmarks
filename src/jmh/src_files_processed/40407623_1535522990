public class Figur{
    private String form;
    private String farbe;
    
    public Figur(String fo, String fa){
        form=fo;
        farbe=fa;
    }
    
    public String formGeben(){
        return form;
    }
    
    public String farbeGeben(){
        return farbe;
    }
    
    public void datenAusgeben(){
        System.out.println(formGeben()", "farbeGeben());
    }
}