public class VCR implements PlayMovie
{
    int timeToRewind;
    
    public VCR(int iTimeToRewind)
    {
        timeToRewind = iTimeToRewind;
    }
    
    public int getRewindTime()
    {
        return timeToRewind;
    }
    
    public void play()
    {
        System.out.println("Napoleon Dynamite");
    }
    
    public boolean skipPreviews()
    {
        return false;
    }
    
    public String toString()
    {
        return timeToRewind " mins";
    }
}