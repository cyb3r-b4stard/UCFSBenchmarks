     */
    public Ex1()
    {
        a=1;
        b=1;
    }

    public Ex1(double a, double b){
        this.a=a;
        this.b=b;
    }
    
    public Ex1(Ex1 c){
        
    }
    
    public Ex1 conjugado(){
        rerturn new Ex1(this.a, this.b);
    }
}