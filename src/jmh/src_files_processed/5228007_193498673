   public void loop3()
   {
   char[] charArray ={ 'a','b','c','d','e','f','g','h','i','j','k','l','m','n','o','p','q','r','s','t','u','v','w','x','y','z'};
   
        for(int i=1; i<= 26;i++)
        {
            System.out.print("\n");  

            for(int j =0; j<i; j++)
            {

                System.out.print(charArray[j]);
            }
        }
        System.out.println("\n"+"-----------------------------");

        System.out.println("Loop 4 Output:");
    } 

