    public static void main (String [] args)
    {
        System.out.print("Loop 1 output (lb = 1, ub = 10, incr = 1)");
        for( int x = 1; x <= 10; x = x+1)
        {
            
            System.out.print("\n");
            System.out.print(x);
           
           
        }
         System.out.println("\n"+"-----------------------------");
        }
         
