public class Complex{
    double x;
    double y;
    
    Complex (double a, double b){
        this.x=x;
        this.y=y;
    }
    
    public double calculModul (Complex c){
        double d = Math.sqrt(this.x*c.x+this.y*c.y);
        return d;
    }
    Complex getSuma (Complex z){
        Complex rez = new Complex (0,0);
        rez.x=this.x+z.x;
        rez.y=this.y+z.y;
        return rez;
    }
    }

}