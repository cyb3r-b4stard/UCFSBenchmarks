public class Alignment{
    public static void main(String[] args){
        System.out.println("Sum: " + %10.2f + "45");
        System.out.println("Difference: %1.2f -5");
        System.out.println("Product: 500");
        System.out.println("Average: 22.50");
        System.out.println("Distance: 5");
        System.out.println("Maximum: 25");
        System.out.println("Minimum: 80");
    }
}