import java.util.*;
/**
 * ##### # ########### ## ##### ########## ####.
 * 
 * @###### (#### ####) 
 * @####### (# ####### ###### ## # ####)
 */
public class Schoolklas {
  private ArrayList<Kind> lijst;
  private ArrayList<tentamen> lijst2;
  
  public Schoolklas() {
    lijst = new ArrayList<Kind>();
    lijst2 = new ArrayList<tentamen>();
  }

  public void printOverzicht() {
    System.out.println( "Overzicht van de kinderen" );      
    for( Kind k : lijst ) {
      System.out.println( k );
    }
  }
    public void printOverzichtCijfers() {
    System.out.println( "Overzicht de vakken en cijfers" );      
    for( tentamen b : lijst2 ) {
      System.out.println( b );
    }
  }   
  
 
  
  public void schrijfIn( Kind k,lijst b ) {
    lijst.add( k );
    lijst.add(b)
  }   
   
  }   
}