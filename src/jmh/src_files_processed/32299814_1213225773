
public class RGBColor
{
    private int _red;
    private int _green;
    private int _blue;
    
     public int getBlue(){
        }

     public int getGreen(){
        }
        
     public int getRed(){
        }
    

     
    
    
    
    
    public RGBColor(int red, int green, int blue){
         _red = 0 ;
        _green = 0 ;
        _blue = 0;
        
       
        setRed(red) ;
        setGreen(green) ;
        setBlue(blue);
        
        

 
        
         if ( (getRed() == 0 ) || (getGreen() == 0)|| (getBlue () ==0)) {
              RGBColor(){};
    }
}
           
    public RGBColor(RGBColor other){
        
         setRed(other.getRed ());
         setGreen(other.getGreen());
         setBlue(other.getBlue ());
    }
    

    public boolean equals(RGBColor other){
        }
    public double convertToGrayscale(){
        }
    public void invert(){
        }
    public void mix(RGBColor other){
        }
        public void setRed(int num){
        }
    public void setBlue(int num){
        }
    public void setGreen(int num){
        }
    public java.lang.String toString(){
        }

    
    
    

    
    
}