public class findAGene {
    public String findGeneSimple(String dna){
        int startP = dna.indexOf("ATG");
        int endP = dna.indexOf("TAA", startP);
        String gene = dna.substring(startP, endP);
        return gene;
    }
    public void testFindGeneSimple(){
        String dna = "AATG"
    }
}