public class Phone{
    String model;
    String color;
    int space;

    Phone(String mo,String co,int spa){
        String model=mo;
        String color=co;
        int space=spa;
    }

    void display(){
        System.out.println("model"+" "+"color"+space);
    }
        public static void main(String[]args){
        Phone p1=new Phone("Apple","spacegrey",16);
        System.out.println(p1.model+" "+p1.color+" "+p1.space);
        System.out.println(Phone p1);

    
    }
}



    