
/**
 * #### ## # ##### ## ###### ## ##### ## ########.
 * 
 * @###### ###### ########
 * @####### ##/##/##
 */
public class WeekDays
{
    private int weekDays[];

    /**
     * Constructor for objects of class WeekDays
     */
    public WeekDays()
    {
        weekDays = new int[7];
    }

    /**
     * method to get the number of elements in the weekDay array 
     */
    public int getWeekDay()
    {
        return weekDays[];
    }
}