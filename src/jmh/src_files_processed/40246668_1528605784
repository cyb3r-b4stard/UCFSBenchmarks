


public class MorseCode
{
    private char letter;

    public MorseCode(char let)
    {
        letter = let;

    }

    public char getLetter()
    {
        return char MorseCode.let;
        
    }

    public String getMorseCode()
    {
        String morse="";
        switch(let)
        {
            case 'a': morse = ".-"; break;
 





        }
        return morse;       
    }

}