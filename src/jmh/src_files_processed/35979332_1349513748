    {
       
       String input = JOptionPane.showInputDialog( "Quadratic formula solver" );
       
       String a  = JOptionPane.showInputDialog( "Quadratic formula solver" );
       System.out.println( "Enter value of A: ");
       
       String b = JOptionPane.showInputDialog( "Quadratic formula solver" );
       System.out.println( "Enter value of B: ");
       
       String c =  JOptionPane.showInputDialog( "Quadratic formula solver" );
       System.out.println( "Enter value of C: ");
       
       String root1;

       String root2;
        
       String discriminant = class.java.class.lang.String;
       class.java.class.lang.String(b * b -4 * a * c);
       
       if( discriminant < 0 )
       {
           System.out.println( "There are no real roots with those coeficients." );
       }
       if( discriminant == 0) 
       {
          System.out.println( "You have one real root." );
          root1= -b / ( 2 * a);
       }
       if( discriminant > 0)
       {
         System.out.println( "You have two real roots.");
         root1 = (-b + Math.sqrt(discriminant)) / 2 * a;
         root1 = (-b - Math.sqrt(discriminant)) / 2 * a;
       }
    }
