class abc1
{
    public static void main(String s)
    {
        String s1="",s2="",s3=""
        int l,i;
        char chp
        s1=s.substring(0,(s.lastIndexOf(' ')));
        s2=s.substring(s.lastIndexOf(' '));
        s1=' '+s1;
        l=s1.length();
        for(i=0;i<l;i++)
        {
            ch=s1.charAt(i);
            if(ch==' ')
            {
                s3=s3+s3.charAt(i+)+'.';
            }
        }
        System.out.println(s3+s2);
    }
}
                
            