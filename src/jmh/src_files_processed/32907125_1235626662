
/**
 * ##### # ########### ## ##### ### ####.
 * 
 * @###### (#### ####) 
 * @####### (# ####### ###### ## # ####)
 */
public class Out
{
  public static int [] copy (int [] in={1,3,5,7,2} )
  {
      int [] out=new int [in.length] ; 
      for (int i=0; i<in.length; i++)
      {
          out[i]=in[i]; 
}
return out; 
}
}