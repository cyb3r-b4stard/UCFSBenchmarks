import java.io.*;
import java.util.*;

public class Geomet
{
  

 public static void main(String[] args)
 {
 
  double sum;
  double num;
  double den;
  
  sum = 0;
  num = 3;
  den = 4;
    
    while (den<= 34);
   {
        sum = sum + num / den;
        den = den + num;
        num = num +3;
    }
    
    system.out.println(sum);

}}}

