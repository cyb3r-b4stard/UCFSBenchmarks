                p2.changeColor("braun_mario");
                p3.changeColor("braun_mario");
                p8.changeColor("braun_mario");
                p9.changeColor("braun_mario");
                p10.changeColor("braun_mario");
                p11.changeColor("braun_mario");        

                default: Methode();
            }
        }
    }
}