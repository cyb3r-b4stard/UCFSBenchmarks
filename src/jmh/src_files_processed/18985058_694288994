public class Element
{
 
  private Object eInhalt;
  private Element eNaechster;
  
  
  public Element()
  {
    eInhalt=null;
    eNaechster=null;
  }
   
  public Element(Object pInhalt)
  {
    eInhalt=pInhalt;
    eNaechster=null;
  }
  
  public void setInhalt(Object  pInhalt)
  {
    eInhalt=pInhalt;
  }
  
  public Object getInhalt()
  {
    return eInhalt;  
  }
  
  public void setNaechster(Element pNaechster)
  {
    eNaechster=pNaechster;  
  }
  
  public Element getNaechster()
  {
    return Naechster;  
  
}