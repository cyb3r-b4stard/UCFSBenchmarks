
/**
 * ##### # ########### ## ##### ###### ####.
 * 
 * @###### (#### ####) 
 * @####### (# ####### ###### ## # ####)
 */
public class member
{private long idNumber;
    private String fullName;
    private String emailAddress;
    private String telephoneNumber;
    private String dateOfJoin;
    private int eventsAttended;
    private boolean membershipPaid;
    public member(long id,String name,String email,String tele,String date,int events,boolean membership)
    {idNumber=id;
        fullName=name;
        emailAddress=email;
        telephoneNumber=tele;
        dateOfJoin=date;
        eventsAttended=events;
        membershipPaid=membership;}

    public void setidNumber(Long id)
    {idNumber=id;}

    public long getidNumber()
    {return idNumber;}

    public void setfullName(String name)
    {fullName=name;}

    public String getfullName()
    {return fullName;}

    public void setemailAddress(String email)
    {emailAddress=email;}

    public String getemailAddress()
    {return emailAddress;}

    public void setidtelephoneNumber(String tele)
    {telephoneNumber=tele;}

    public String gettelephoneNumber()
    {return telephoneNumber;}

    public void setiddateOfJoin(String date)
    {dateOfJoin=date;}

    public String getdateOfJoin()
    {return dateOfJoin;}
    
    public void setideventsAttended(int events)
    {eventsAttended=events;}

    public int geteventsAttended()
    {return eventsAttended;}
    
    public void setmembershipPaid(boolean membership)
    {membershipPaid=membership;}

    public boolean getmembershipPaid()
    {return membershipPaid;}
    
    public boolean isValidMember()
    {if (idNumber!=0){
        if (fullName!=null){
            if (membershipPaid=true){
                return true;}}
    
}