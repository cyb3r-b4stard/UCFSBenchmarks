import java.util.Scanner;

public class modifiedstudentaverage
{
    public static void main(String[] args)
   {
   Scanner stdIn = new Scanner(System.in);
   String first; 
   String last; 
      
   System.out.println(" Enter first name: " );
   first = stdIn.next ();
   System.out.println(" Enter your last name: " );
   last = stdIn.next ();
   System.out.println("  Hello  "  +  first  +  last  );
      
   double test1; 
   double test2; 
   double test3; 
   double test4; 
   double test5; 
       
   System.out.println(" Enter test1 : ");
   test1 = stdIn.nextDouble ();
   System.out.println(" Enter test2 : ");
   test2 = stdIn.nextDouble ();
   System.out.println(" Enter test3 : ");
   test3 = stdIn.nextDouble ();
   System.out.println(" Enter test4 : ");
   test4 = stdIn.nextDouble ();
   System.out.println(" Enter test5 : ");
   test5 = stdIn.nextDouble ();
      
   double average; 
   average = (test1 + test2 + test3 + test4 + test5)/5;
   
   
           
   System.out.println("Your initials are " + first.charAt (0) + " "  + last);
   System.out.print("Your average is : " + average);
       
     do
   {
       System.out.println("A");
       if (average <= 90)
       {

        }
        else
        {
            System.out.println("Your initials are " + first.charAt (0) + " "  + last);
    
    
}
}
}
}
