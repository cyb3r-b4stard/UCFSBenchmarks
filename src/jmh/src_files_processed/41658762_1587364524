class pellseries
{
    public void input()
        int a=1;
        int b=2;
        {
            for (int i=1;i<=15;i++)
        {
            
            System.out.println(a);
            System.out.println(b);
            int c=b*2+a;
            b=c;
            a=b;
            System.out.println(c);
        }
    }
}
    
                
