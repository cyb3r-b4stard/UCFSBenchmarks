import java.util.Random;

public class Zufallszahl
{
    private int[] zahl;
    public int z;
    
    public Zufallszahl()
    {
        zahl= new int[10];
    }
    public void erzeugen()
    {
        Random zufall= new Random();
        for (int i=0; i<100; i++)
        zahl[i]= zufall.nextInt(1000);
        z= ;
    }
}