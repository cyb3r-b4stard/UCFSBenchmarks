 
 
 
 
 
 

public class Estada{
    private Horario entrada;
    private Horario saida;
    private int duração;
    
   public Estada(int h,int m){
      entrada = new Horario(h,m);
   }
   
      
    public void setSaida(){
        if(saida<entrada)
        Horario salva;
        salva=entrada;
        entrada=saida;
        saida=salva;
    }
        
       
     
       
      
       
}
    }

    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    


