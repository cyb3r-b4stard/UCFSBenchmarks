public class AMO
{
    
    public static void main(String []args)
    {
        Scanner stdIn = new Scanner(System.in);
        String a=stdIn.nextline();
        int count=0:
        System.out.println(a.length());
        for(int i=0;i<=13;i++)
        { if (a.charAt(i)="s")
            count++;
        }
     
       
        
    }  
}