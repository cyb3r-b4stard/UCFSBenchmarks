public class Club
{
    private String club_name;
    private Member[]members;
    private int num_members;
    
    public Club(String name, int max_members)
    {
        club_name = name;
        max_members = Member[].class; 
        num_members = 0;
    }
    
    public Club(String name)
    {
        club_name = name;
        max_members = 30;
        num_members = 0;
    }
    
    public void getClubInfo()
    {
        System.out.println("Club Name: " + club_name);
        System.out.println("Members: " + num_members);
    }
    
    public void addMember(Member m)
    {
        if (num_members <= max_members)
        {
            num_members = num_members + 1;
        }
        else 
        {
            System.out.println("The member could not be added because club is at maximum capacity.");
        }
    }
    
