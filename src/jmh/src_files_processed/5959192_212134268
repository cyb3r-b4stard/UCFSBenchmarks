public class auto extends vehiculo
{
    private String codigo;
        
    public auto(String marcaAuto, String modelo ,int cantRue, int año)
    {
        setMarca (marcaAuto);
        setModelo (modelo);
        setCantRue (cantRue);
        setAño(año); 
        codigo = marcaAuto + modelo + "-" año;  
    }

    public String getMarcaAuto()
    {
        return getMarcaAuto();
    }
    
    public String getModelo()
    {
        return getModelo();
    }
        
    public int getCantRue()
    {
        return getCantRue();
    }
    
    public int getAño()
    {
        return getAño();
    }
    
    public String getCodigo()
    {
        return getCodigo();
    }
    
    public String getcodigo()
    {
        return codigo;
    }
    
}