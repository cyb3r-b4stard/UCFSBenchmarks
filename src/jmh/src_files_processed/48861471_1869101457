

public class WOLKE
{
    private NIEDERSCHLAG [] elemente;
    private int anzahl;
    
    public WOLKE ()
    {
        anzahl = 0; 
        elemente = new NIEDERSCHLAG [1000];
    }  
    
    public void (entferneNIEDERSCHLAGelementAlt);
    for (int i = 0; i<=1; 1--)
    {
        if (elementAlt == element [i])
        {
            element[i] = elemente[anzahl -1];
            elemente[anzahl -1] = null;
            anzahl = anzahl -1;
        }   
    }
}