import java.io.*;
class Record
{
String name[];
int rnk[];
Record()
{
name=String [50];
rnk=int[50];
}
void readValue()throws IOException
{
     BufferedReader br = new BufferedReader(new InputStreamReader(System.in));
     for(int i=0;i<50;i++)
     {
System.out.println("Enter"+(i+1)+"Name");
name[i]=br.readLine();
System.out.println("Enter"+(i+1)+"Rank");
rnk[i]=Integer.parseInt(br.readLine());
}
}
void display()
{
      for(int i=0;i<50;i++)
     {
         System.out.println("Name of "+(i+1)+" Person :"+name[i]);
         System.out.println("Rank of "+(i+1)+" Person :"rnk[i]);
         
        }
    }        
         