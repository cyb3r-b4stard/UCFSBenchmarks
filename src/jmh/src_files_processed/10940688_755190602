import java.util.*;
class code
{
public static void main()
{
int l,i,d;
char c;
Scanner obj=new Scanner(System.in);
System.out.println("enter string");
String s=obj.nextLine();
l=s.length();
for(i=0;i<l;i++)
{
 c=s.charAt(i);
 d=(int)c;
 System.out.println("character     ASCII");
 System.out.println("   "+c+"          "+d);         
}
}
}

