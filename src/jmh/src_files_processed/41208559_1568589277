import java.util.comparator;
public class Student {
    private String stMeno;
    private int stCislo;
    private int stVek;
    public Student(int stCislo, String stMeno, int stVek) {
      this.stCiislo = stCislo;
      this.stMeno = stMeno;
      this.stVek = stVek;
    }
    
    public String getStMeno() {
        return stMeno;
    }
    public void setStMeno(String stMeno) {
        this.stMeno = stMeno;
    }
    public int getStCislo() {
        return stCislo;
    }
    public void setStCislo(int stCislo) {
        this.stCislo = stCislo;
    }
    public int getStVek() {
       return stVek;
    }
    public void setStVek(int stVek) {
        this.stVek = st.Vek;
    }