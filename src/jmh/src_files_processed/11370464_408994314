
public class upg85
{
    public static void main(String[] args)
    {
        int talBas;
        int talExponent;
        
        System.out.println("Skriv in två tal, först basen, sedan exponenten.");
        talBas = Keyboard.readInt();
        talExponent = Keyboard.readInt();
        int summa = potenser(talBas, talExponent);
        System.out.println(summa);
    }
    static int potenser(int talBas, int talExponent)
    {
      int tal1 = talBas;
      int loop = 0;
      int summa = 1;
      
      while(loop > talExponent) 
      {
          loop++;
          talBas*tal1;
          
      } 
      return ;
    }
}