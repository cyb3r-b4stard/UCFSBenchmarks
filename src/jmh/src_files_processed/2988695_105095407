public class FehlerTeufel
{
  public static void main (String [] args){
    int a = 3 *4
    int b = a/2;
    int c = 1 / 3 * a + b%2;
    System.out.println (a =  + a);
    System.out.println ("b = " + b);
    System.out.println ("c = " );
}
}