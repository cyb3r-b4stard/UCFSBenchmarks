public class Cat
{
    private String name;
    private int yearBorn;
    private int weight;

    /**
     * default Constructor for objects Cat
     */
    public Cat()
    {
    }
    
    
    /**
     *  Constructor for class Cat
     */
     public Cat(String _name,int _yearBorn,int _weight)
     {
      setName(_name);
      setYearBorn(_yearBorn);
      setWeight(_weight);
        
        
    }
    
    public void setName(String _name)
    {
        if (name != null && name.length() > 0){
            name = _name;
        }else{
            ERROR IN INPUT;
        }
    }
    
   

}