
/**
 * ########### ### #### ### ###### ###.
 * 
 * ##### #####
 * ##.##.####
 */
public class DVD
{
    private String name;
    private String kat;
    private int pos;

    /**
     * Konstruktor f√ºr Objekte der Klasse DVD
     */
    public DVD(String name, String kat, int pos)
    {
        this.name=name;
        this.kat=kat;
        this.pos=pos;
    }
    
    public DVD () 
    {
        name= "hobbit";
        kat="fantasy";
        pos=1;
    }
    
    public boolean equals(DVD d)
    {
        if(this.getName().equals(d.getName()) &&
            this.getKat().equals(d.getKat()) &&
            this.getPos().equals(d.getPos())) && {
                return true;
            }
        return false;
    }
}