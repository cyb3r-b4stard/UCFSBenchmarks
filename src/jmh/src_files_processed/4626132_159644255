public class Execicio_01 {
private float numero1;
private float numero2;
private float resultado;
public Exercico_01 (float n1, float n2, float rt){
numero1 = n1;
numero2 = n2;
resultado = rt;
}
public void soma (){
resultado = numero1 + numero2
}
public void exibeDados () {
System.out.println ("A soma dos n√∫meros e igual a: " + resultado);
}
}
