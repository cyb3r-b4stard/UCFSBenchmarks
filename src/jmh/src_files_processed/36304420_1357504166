import java.util.Scanner;

public class BirthdayInfo
{
   public static void main(String[] args)
   {
       Scanner br = new Scanner (System.in);
       System.out.println("What month is your birthday?");
       String month = br.nextLine();
       System.out.println("What date of the month is your birthday on?");
       int date = br.nextInt();
       
       if ((month.equals("January"))||(month.equals("Febuary"))||(month.equals ("March")))
       {
           System.out.println("Your Birthday is in the 1st Quarter.");
        }
       if else (((month.equals("April"))||(month.equals("May"))||(month.equals ("June"))))
       {
          System.out.println("Your Birthday is in the 2nd Quarter."); 
        }
        if else 
        
       {
           System.out.println("Your Birthday is in the 3rd Quarter.");
        }
       
    }
   
   
}
