import java.util.*;
import java.io.*;

public class Test
{
    public static void main (String[] args) throws IOException
    {
        String series; 
        Scanner fileScan, titleScan; 
        ArrayList<String> test = new ArrayList<String>(); 
        int total = 0; 

        fileScan = new Scanner (new File("Series.txt")); 

        /**Reads the file line by line and prints the entire list
         * from the file
         * Adds the Strings from the file to an array list of type string
         * These values will be changed to clickable links at some point
         **/

        while (fileScan.hasNext()) 
        {
            series = fileScan.nextLine();

            test.add(series); 
        }

        for (int i=0; i<= test.size(); i++)
        {
            if (test(i).equals("Hunter x Hunter")
            {
                System.out.print("Good.");
            }
            else
            {
                System.out.println("what?");
            }
        }

    }
}