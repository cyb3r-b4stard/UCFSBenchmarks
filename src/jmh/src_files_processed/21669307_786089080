public class Schueler
{
    private String name;
    private int[] noten;
    private SchulKlasse klasse;
    
    public Schueler(String name)
    {
        setName(name);
        noten=new int[11];
        for(int i=0; i <= 11; i++)
        {
            noten[i]=1;
        }
        
    }
    
    public void setName(String name)
    {
        this.name=name;
    }
    
    public String getName()
    {
        return name;
    }
    
    public void printSchueler()
    {
        System.out.print(name);
        for(int i=0; i <= 11; i++)
        {
            System.out.print(noten[i]" ");
        }
    }
}