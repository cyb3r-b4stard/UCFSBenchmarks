import java.util.*;

/**
 * ##### # ########### ## ##### ############# ####.
 * 
 * @###### (#### ####) 
 * @####### (# ####### ###### ## # ####)
 */
public class ArrayProblems
{
    public int[] everyOther(int[] a) {
        int[] b;
        if (a.length%2==0) {b = new int [a.length/2];}
        else {b = new int [a.length/2+1];}
        int j = 0;
        for(int i=0;i<a.length;i+=2) {
            b[j++] = a[i];
        }
        return b;
    }
    
    
    public int countInversions(int[] a) {
        int count = 0;
        for (int i=0; i<a.length-1; i++) {
            for (int j=i+1;j<a.length;j++) { 
                if(a[j] < a[i]) {count++;}
            }
        }
        return count;            
    }
    
    public void squeezeLeft(int[] a) {
        int count = 0;
        for (int i=0;i<a.length;i++) {
            if (a[i]!=0) {count++;}
        }
        int[] b = new int [count];
        int j=0;
        for (int i=0;i<a.length;i++) {
           for (j=i;j<b.length;j++) {
                 if (a[i]!=0) {b[j]=a[i];}
           }
        }
        for (int i=0;i<a.length;i++) {
            if (i<=count) {a[i]=b[i];}
            else {a[i]==0;}
        }
    }
    
    public int[] runDecode(int[] a) {
        int[] b = new int[5];
        return b;
    }
    
}