
/**
 * ######### ###### ### ####### ## ########### ######## # ####### ## ## ##########.
 * 
 * @###### ##### ##### 
 * @####### ##-#-####
 */
public class MultTable
{
    public static void main (String[] args)
    {
        final int LIMIT = 12
        for (int num1 = 1; num1 <= LIMIT; num1++)
        {
            for (int num2 = 1; num2 <= LIMIT; num2++)
                System.out.print((num1 * num2) + "\t");
            System.out.println
        }
    }
}