public class Marble

{
    public int xPos;
    public int yPos;
    yPos + xPos = xyPos
    
    private int marble()
    {
        xPos = (int) ( Math.random()*300); 
        yPos = (int) ( Math.random()*300);
    }
    
    public int xyPos()
    {
        int get xPos;
        int get yPos;
    }
}