import javax.swing.JOptionPane;
public class Sueldo
{
   public static void main (String args[])
   {
       String nombredelempleado; 
       String horastrabajadas;
       String pagoporhora 
       
       int horastrabajdas;
       int pagoporhora;
       int mult;
       nombredelempleado=JOptionPane.showInputDialog
       ("Nombre Del Empleado:");
       horastrabajadas=JOptionPane.showInputDialog
       ("Horas Trabajdas:");
       pagoporhora=JOptionPane.showInputDialog
       ("Pago Por Hora:");
       nombredelempleado=Integer.parseInt(nombredelempleado);
       horastrabajadas=Integer.parseInt(horastrabajadas);
       pagoporhora=Integer.parseInt(pagoporhora);
       mult=horastrabajadas*pagoporhora;
       JOptionPane.showMessageDialog(null,"El sueldo de:"+ nombredelempleado + "es:" + mult "Resultados",JOptionPane.PLAIN_MESSAGE);
       System.exit(0);
       
      
}
}
