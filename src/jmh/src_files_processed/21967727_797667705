import java.util.StringTokenizer; 
package Rechenbaum;
public class Rechner {
    Knoten wurzel;
    public Rechner() {
        wurzel = new Knoten();
    }
    public void aufgabe(String aufgabe) {
        StringTokenizer st = new StringTokenizer(aufgabe);
        while (st.hasMoreTokens()) {
            wurzel.fuegeEin(st.nextToken());
        }
    }
}
   