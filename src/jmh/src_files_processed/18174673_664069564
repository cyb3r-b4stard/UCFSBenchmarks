
import java.util.Scanner;

public class Nota1
{
    public static void main( String[] args)
   
    {
     Scanner entrada = new Scanner( System.in);
     
     int numero1;
     
     System.out.println( "Ingrese una nota la nota entre 0 y 100")
     numero1 = entrada.nextInt();
     
     if ((  0 <= numero1 )&(numero1 < 50))
     System.out.println("El estudiante está reprobado");
   
     if ((  50 <= numero1 )&(numero1 < 65))
     System.out.println("El estudiante debe ir a apmliación");
     
     if ((  65 <= numero1 )&(numero1 < 100))
     System.out.println("El estudiante está aprobado");
   }
}