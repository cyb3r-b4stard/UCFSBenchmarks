 */
public class Library
{
    private string libraryName;
    private arrayList<book> holdings;

    /**
     * Constructor for objects of class Library
     */
    public Library(string name)
    {
        libraryName = name;
        holdings = new arrayList<book>();
    }

    /**
 
     */
    public int GetLibraryName()
    {
        System.out.println(libraryName);
    }
     public int Acquire(book newBook)
    {
        holdings.add(newBook);
    }
     public int discardByIndex(int index)
    {
        if(index<holdings.size && index>=0)
        {
            holdings.remove(newBook);
        }
        else
        {
            System.out.println("Unable to discard; index"+index+" invalid")
        }
    }
         public int getCount()
    {
        holdings.size();
    }
}