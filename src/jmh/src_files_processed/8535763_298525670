import java.util.Scanner;
class sw1
{
    public static void main()
    {
        Scanner a=new Scanner(System.in);
        int b,c,d;
        System.out.println("enter no.");
        b= a.nextInt();
        System.out.println("enter no.");
        c= a.nextInt();
        System.out.println("enter function no.");
        d= a.nextInt();
        switch(d)
        {
            Case1: System.out.println("add"+(b+c));
            break;
            Case2: System.out.println("subtract"+(b-c));
            break;
            Case3: System.out.println("multiply"+(b*c));
            break;
            Case4: System.out.println("divide"+(b/c));
            break;
        }
    }
}