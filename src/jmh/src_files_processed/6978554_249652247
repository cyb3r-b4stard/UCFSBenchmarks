import java.util.Scanner;
import java.lang.Math;
/**
 * ##### # ########### ## ##### ######### ####.
 * 
 * @###### (#### ####) 
 * @####### (# ####### ###### ## # ####)
 */
public static void main(String[] args)
    {
       int j;
       Scanner input = new Scanner(System.in);
    
       System.out.print("Enter your number: ");
       j = input.nextInt();
        
       for (int i = 1; i <= j; i++)
      { System.out.println(i);
      }
   }
}}