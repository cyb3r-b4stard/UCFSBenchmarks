public class Quiz4 {
    public static void indexLessThan(int[] a) {
        for (int i=0;i<a.length;i++) {
            if (a[i]<i) {
                a[i] = 0;
            }
        }
    }
    public static int[] multRow(int[][] a) {
        for (int i=0;i<a.length;i++) {
            for (int j=0;j<a.length;j++) {
                int[] product = new int[i];
            }
        }
        return product[];
    }
}