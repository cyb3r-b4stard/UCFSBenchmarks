import java.io.*;
import java.util.*;

public class test {
  public static void main (String [] args) throws Exception{
    
    Scanner scan = new Scanner (System.in);
    
    BufferedReader br = new BufferedReader (new FileReader("testing123.txt"));
    
    String line = null;
    int lineNum = 1;
    String question = null;
    String answer = null;
    String truefalse;
    
    lineNum++;
    
    do { 
      
      if (lineNum%2 == 0)
    line = question;
      question = br.readLine();
      if (lineNum %2 != 0)
    line = answer;
      answer = br.readLine();
      
      System.out.println(question);
      truefalse = scan.nextLine();
      
      
    } while  if (truefalse.equals("true") || truefalse.equals("false"));
      
      
    } while (question != null && answer != null); {
        
        
  br.close();
}

}
}