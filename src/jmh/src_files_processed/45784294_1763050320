
import java.io.*;
 {
  public class bufferedreader
{
  public static void main () throws Exception 
  {
   br= BufferedReader br= new BufferedReader (new InputStreamReader (System.in));

   double score1= 0, score2= 0, sacore3= 0, add= 0;

   System.out.print(" Please enter Score1:");
    score1= Double.parseDouble (br.readline());

  System.out.print("Please enter Score2:");
    score2=Double.parseDouble (br.readline());

 System.out.print("Please enter Score3:");
    score3=Double.parseDouble (br.readline());

add= score1 + score2 + score3;

System.out.println("\n\n\t The overall score is:" + 110);
System.out.println("\n\n\t The total score is:" + add);

 }

    }
}