public class q1
{
    public static double volumesphere (double d)
    {
      double result = Math.pow(d,3);
      result = result * Math.PI;
      result = result/6;
      
      return result;
    }
    public static double volumecylinder (double d, double h)
    {
      double result = Math.pow(d,2);
      result = result* Math.PI;
      result = result * h ; 
      result = result/4;
      
      return result;
    }
    public static void main (String [] args) 
    {
     double sdiameter GUI.getdouble "please Enter Sphere Diameter");
     double cheight GUI.getdouble("Please Enter Cylinder Height");
     double cdiameter GUI.getdouble "please Enter Cylinder Diameter");
     double vs = volumesphere(sdiameter);
     System.out.println(" sphere diameter and volume = " + Num.format (vs,3));
     double vc = volumecylinder (cdiameter,cheight);
     System.out.println ("Cylinder diameter, volume and height = " + Num.format (vc,3));
    }
}