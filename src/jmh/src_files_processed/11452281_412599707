
public class Queue
{
    
    private int nodes; 
    private Node tail = new Node;

    
    public Queue()
    {
        tail=null;
        nodes=0;
    }
    
      

    public boolean isEmpty()
    {
       if(tail == null) return true;
       else return false;
    }
    
    public void enqueue(Node node) 
    {
        if(!isEmpty()){
            Node help = tail;   
            node.setNextNode(tail.getNextNode());
            tail.setNextNode(node);
            nodes++;
        }
        else{
            tail.setNextNode(node);
            nodes++;
        }
    }
    
    public void dequeue()
    {
    }
    
    public void front()
    {
    }
    
   
}