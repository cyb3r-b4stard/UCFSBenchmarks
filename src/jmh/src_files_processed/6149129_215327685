
/**
 * ##### # ########### ## ##### ######### ####.
 * 
 * @###### (#### ####) 
 * @####### (# ####### ###### ## # ####)
 */
public class Automovil
{
    private String patente;
    private String marca;
    private String modelo;
    private int año;
    private Dueño propietario;

    /**
     * Constructor for objects of class Automovil
     */
    public Automovil()
    {
        patente = "RO-6666";
        marca="Audi";
        modelo="A8";
        año=2013;
        propietario=new Dueño():
    }
    /**
     * Accesadores
     */

    public String getPatente()
    {return this.patente;}
    public String getMarca()
    {return this.marca;}
    public String getModelo()
    {return this.modelo;}
    public int getAño()
    {return this.año;}
    public Dueño getPropietario()
    {return this.propietario;}
    
    /**
     * Mutadores
     */
    public void setPropietario (Dueño propietario)
    {this.propietario=propietario;}
    public void setPatente (String patente)
    {this.patente=patente;}
    public void setMarca (String marca)
    {this.marca=marca;}
    public void setModelo (String modelo)
    {this.modelo=modelo;}
    public void setAño (int año)
    {this.año=año;}
    
    public void imprimeAutomovil()
    {
       System.out.println("Patente: "+getPatente());
       System.out.println("Marca: "+getMarca());
       System.out.println("Modelo: "+getModelo());
       System.out.println("Año: "+getAño());
       getPropietario().imprimeDueño();
}
}