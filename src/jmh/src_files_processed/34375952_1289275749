
public class exemplul3
{
   public static void main(String [] args){
    
       int [][] m = new int m [5][4];
       int nr=0;  
     for(int i=0; i<m.length;i++){
        for(int j=0; j<m[i].length;j++){
          m[i][j]= nr++;
        }
     }
     
     
       for(int i:m){
         for(int j:i)
           System.out.print(i+" ");
         System.out.println();
        }
    
    
  }
}