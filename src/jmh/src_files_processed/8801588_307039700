import java.util.Scanner;
public class Eggs
{
    public static void main(String[] args)
    {
        Scanner laser = new Scanner(System.in);
        System.out.println("Enter the number of eggs you have ");
        Double eggs = laser.nextDouble();
        Double dozen = (eggs / 12);
        Double remainder = (eggs % 12);
        System.out.println("The number of dozen of eggs you have is " + dozen); 
        System.out.print("You also have " remainder "eggs remaining")
    }
}