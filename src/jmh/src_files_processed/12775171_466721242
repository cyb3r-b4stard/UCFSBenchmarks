import java.util.Scanner;
public class Drawer
{    
    public static void main(String[] args)
    {
        String[] items= new String[10];
        int i = -1;
        items[0] = "Pencil";
        items[1] = "Eraser";      
        items[2] = "Pen";
        items[3] = "Paper";  
        items[4] = "Bricks";  
        items[5] = "Steel Beam"; 
        items[6] = "My Mixtape"; 
        items[7] = "Body Cast"; 
        items[8] = "Stretcher";      
        items[9] = "Adrian Peterson";

        System.out.println("Item Menu");
        alpha(items);
        for (int x=0;x<items.length;x++) {
            System.out.println(items[x] );

        }
        add(items);
        alpha(items);

    }

    public static void alpha(String[] items) 
    {
        boolean fish = true;  
        String temp;

        while (fish) {
            fish = false;
            for (int j=0;j<items.length-1;j++ ) {
                if ( items[j].compareToIgnoreCase(items[j+1]) > 0 ) {       
                    temp = items[j];
                    items[j] = items[j+1];     
                    items[j+1] = temp;
                    fish = true;
                }
            }
        }
    }

    public static void add(String[] items) 
    {
        Scanner feds = new Scanner(System.in);

        for {
                        String smoove = feds.nextLine();
                        items[x] = items[x+1];
                        items[x] = smoove;


        }
    }
}
