public class Q23
{
    public static void main(int a, int b)
    {
        int square, cube, difference;
        if (a%2==0 && b%2==0)
        square=a*a+b*b;
        System.out.println("The sum of their squares=" +square);        
        else if (a%2!=0 && b%2!=0);
        cube=a*a*a+b*b*b;
        System.out.println("The sum of their cubes=" +cube);
        else 
        difference =a-b;
        System.out.println("The absolute difference="+difference);
    }
}