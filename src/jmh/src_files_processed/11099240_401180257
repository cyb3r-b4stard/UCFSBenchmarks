    public static boolean isADN (String s)
    { boolean a = false;
        int i = 0 ;
       
        
        for (i=0; i<s.length(); i++)
       
        { if (s.charAt(i) != 'a' && s.charAt(i) !='g' && s.charAt(i) !='c' && s.charAt(i) !='t')
        {
              
            i++; 
        }
        if (i==0)
          {
              a= true;
            }
            else
            {
                a=false;
            }
            
        }
       return a ;
    } 
   
    public static void testisADN()
    {
        
        String shortADN= "gatcctccatatacaacggtatctccacctcaggtttagatctcaacaacggaaccattgccgacatgagac";
    
        String longADN= "";
    
        String palindrome1="ttaggatt";
        
        String palindrome2="catggcactttgagcattttagccgattttacgagtttaacggtac"; 
        
        boolean a = isADN ( shortADN ); 
        if (a=false) 
        System.out.print ("la chaine n'est composÃ©e que de A,T,C et G");
        
        else 
        System.out.print ("la chaine n'est pas correcte");
    }
        
    public static int count (String s){
        int b=0;
        int i=0;
        
        for (i=0; i<s.length(); i++)
        {
            if (s.charAt(i) == 'a')
            b=(b+1);
            
            else 
            b=b+0;
           
        }
        return b;
    }
    
    
    public static void testcount()
    {
        
        String shortADN= "gatcctccatatacaacggtatctccacctcaggtttagatctcaacaacggaaccattgccgacatgagac";
    
        String longADN= "";
    
        String palindrome1="ttaggatt";
        
        String palindrome2="catggcactttgagcattttagccgattttacgagtttaacggtac"; 
        
        int b = count ( shortADN ); 
       System.out.print(b);
    
    }
    
    
    public static int distenceH (String s, String v){
    int i=0;
    int c=0;
        for (i=0; i<s.length(); i++)
        {
            if (s.charAt(i) == v.charAt(i))
              c=c+1;
              else
                c=c+0;
    
        }
    return
    }

    
    
   public static String PlusLongPalindrome (String s ){
        String rep = "";
        for (int i=0; i<s.length(); i++){
            for (int e=0; e<s.length(); e++){
                String tmp= s.substring(i, e);
                if(isPalindrome(tmp)){
                    if(tmp.length() > rep.length())
                        rep = tmp;
                }
            }
        }
    
        
        return rep;
    }
        
    public static boolean isPalindrome(String str){
    boolean d=false;
    {
       for (int i=0; i<str.length(); i++)
       {  if( 
           d = str.charAt(i) == str.charAt( str.length() -i));
           
        }
       
        
        
 }
    
   
}
