class Prime_or_Not
{
    public static void prime(int n)
    {
        System.out.println("The factors are:")
        int count=0;
        for(int i=1;i<=n;i++)
        if(n%i==0)
         {count++;
             System.out.println(i);}
             if(count==2)
             System.out.println("It is a prime number");
             else
             System.out.println("It is a composite number");
                 }
}