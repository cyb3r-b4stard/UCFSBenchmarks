
/**
 * #### #####.
 * @###### ####### ### 
 * @####### #.#
 */
public class Boat
{
    private int                   modelYear;
    private String                modelName;
    private double                horsepower;
    private String                manufacturer;
    private boolean               twinEngine;
    private static String         webPage = "http:
    private final static String   BODY_TYPE = "Powerboat";
    /**
     * No-arg constructor for objects of class Boat
     */
    public Boat(){
        {
          System.out.println("Default Constructor invoked");
        }
    }
    /**
     *@param int     modelYear    Must be 1973 or later
     *@param String  modelName    
     *@paran String  manufacturer
     *@param double  horsepower   Must be between 50 - 390
     *@param boolean twinEngine
     */
    public Boat(int theModelYear, String theModelName, String theManufacturer, double theHorsepower, boolean isTwinEngine)
    {
       if(theModelYear>=1973 && theModelYear<=2017){
         modelYear=theModelYear; 
       }
       else if(theModelYear<1973){
         System.out.println(theModelYear + " is too early, sorry");
        }
       else
       {
         System.out.println(theModelYear + " is too late, sorry");
        }
       
        if ((theModelName !=null)&&(theModelName.length()>0)){
         modelName = theModelName;
        }
       else{
        System.out.println("a model name was not provided,sorry");
        }
       
        manufacturer = theManufacturer;
       
       if(theHorsepower>=50 && theHorsepower<=390){
        horsepower = theHorsepower;
        }
       else if(theHorsepower<50){
        System.out.println(theHorsepower + " is too little, sorry");
        }
       else{
        System.out.println(theHorsepower + " is too much, sorry");
        }
        
        twinEngine = isTwinEngine;
    
        }
        
    /**
     * @param  theModelYear
     * @param  theModelName
     * @param  theManufacturer
     * @param  theHorsepower
     * @param  isTwinEngine
     * @param  theWebPage
     */ 
     public void setModelYear(int theModelYear){
      if(theModelYear>=1973 && theModelYear<=2017){
         modelYear=theModelYear; 
       }
       else if(theModelYear<1973){
         System.out.println(theModelYear + " is too early, sorry");
        }
       else
       {
         System.out.println(theModelYear + " is too late, sorry");
        }   
        }
     public void setModelName(String theModelName){
       if ((theModelName !=null)&&(theModelName.length()>0)){
         modelName = theModelName;
        }
       else{
        System.out.println("a model name was not provided,sorry");
        }
    }
     public void setManufacturer(String theManufacturer){ 
        manufacturer = theManufacturer;     
    }
     public void setHorsepower(double theHorsepower){
        if(theHorsepower>=50 && theHorsepower<=390){
        horsepower = theHorsepower;
        }
       else if(theHorsepower<50){
        System.out.println(theHorsepower + " is too little, sorry");
        }
       else{
        System.out.println(theHorsepower + " is too much, sorry");
        }
    }
    public void setTwinEngine(boolean isTwinEngine){
        twinEngine = isTwinEngine;
    }
    public static void setWebPage(String theWebPage, boolean twinEngine){
        if(twinEngine==true){
        webPage= theWebPage;
    }
       else{
        webPage="http:
    }
    }
    
    /**
     * @return theModelYear
     * @return theModelName
     * @return theHorsepower
     * @return theManufacturer
     * @return isTwinEngine
     * @return theWebPage
     */
    public int getModelYear(){
      return modelYear;
    }
    public String getModelName(){
      return modelName;
    }
    public double getHorsepower(){
      return horsepower;
    }
    public String getManufacturer(){
      return manufacturer;
    }
    public boolean getTwinEngine(){
      return twinEngine;
    }
    public static String getWebPage(twinEngine){
      if(twinEngine==true){
        webPage= theWebPage;
      }
       else{
        webPage="http:
      }
    }
    
    public void getBoatDetails(){
     if(twinEngine==true){
        System.out.println("This "+ BODY_TYPE + " is a "+ getModelYear() +" "+ getModelName() + " with "+ getHorsepower() + "hp"+ " and has Twin-engine. It is manufactured by "+ getManufacturer() + " and you can read more at "+ getWebPage());
    }
     else{
        System.out.println("This "+ BODY_TYPE + " is a "+ getModelYear() +" "+ getModelName() + " with "+ getHorsepower() + "hp"+ " and does not have a Twin-engine. It is manufactured by "+ getManufacturer() + " and you can read more at "+ getWebPage());
        }
    
}
}