

public class sum1
{
    public class sum1
    {
        public static void main(int n)
        {
            int n;s=0;
            for(i=1;i<=n;i=i+1)
            {
                s=s-(i*i);
            }
                elseif
            {
            s=s+(i*i);
        }
        System.out.println(s)
    }
}