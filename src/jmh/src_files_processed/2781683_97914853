  public void codigotest(){
  System.out.println(((int)(Math.random() * 9)) + "" + ((int)(Math.random() * 9)) + "" + ((int)(Math.random() * 9)) + "" + ((int)(Math.random() * 9)) + "" + ((int)(Math.random() * 9)) + ((int)(Math.random() * 9)));
  System.out.println(gerartest);
  }
  
  public void gerartest(){
  String codigo = "C";
  String[] letramin = new String[25];
  String[] letra = new String[25];
  
  letra[1] = "A";
  letra[2] = "B";
  letra[3] = "C";
  letra[4] = "D";
  letra[5] = "E";
  letra[6] = "F";
  letra[7] = "G";
  letra[8] = "H";
  letra[9] = "I";
  letra[10] = "J";
  letra[11] = "K";
  letra[12] = "L";
  letra[13] = "M";
  letra[14] = "N";
  letra[15] = "O";
  letra[16] = "P";
  letra[17] = "Q";
  letra[18] = "R";
  letra[19] = "S";
  letra[20] = "T";
  letra[21] = "U";
  letra[22] = "V";
  letra[23] = "X";
  letra[24] = "Z";
  
  letramin[1] = "a";
  letramin[2] = "b";
  letramin[3] = "c";
  letramin[4] = "d";
  letramin[5] = "e";
  letramin[6] = "f";
  letramin[7] = "g";
  letramin[8] = "h";
  letramin[9] = "i";
  letramin[10] = "j";
  letramin[11] = "k";
  letramin[12] = "l";
  letramin[13] = "m";
  letramin[14] = "n";
  letramin[15] = "o";
  letramin[16] = "p";
  letramin[17] = "q";
  letramin[18] = "r";
  letramin[19] = "s";
  letramin[20] = "t";
  letramin[21] = "u";
  letramin[22] = "v";
  letramin[23] = "x";
  letramin[24] = "z";
  
  for(int i = 0;i < 6;i++){
  int coin = ((int)(Math.random() * 1));
  
  if(coin == 0){
   int coin2 = coin = ((int)(Math.random() * 2));
   if(coin2 == 0){
   int num = (1 + (int)(Math.random() * 24));
   codigo = codigo + letramin[num];
   }else{
   int num = (1 + (int)(Math.random() * 24));
   codigo = codigo + letra[num];
   }
  }else{
   int num = ((int)(Math.random() * 10));
   codigo = codigo + num
  }
    
  }
  return codigo;
  }
