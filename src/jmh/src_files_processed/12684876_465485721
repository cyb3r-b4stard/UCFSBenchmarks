import static java.lang.System.*;

public class CoolNumbers
{
    public static boolean isCoolNum(int n)
    {
        return (n%60 == 1);
    }
    public static int countCoolNumbers( int stop)
    {   
        int count =0;
        for (int i = 6; i <= stop; i+= 60){
            count++;
        }
        
        {coun       return count;
    
}
