public class Oefeningen9punt4{
   public static double Discriminant ( double a,double b, double c) {
       ;double Discriminant = Math.sqrt(b*b-4*a*c);
       
        return Discriminant;}
    
   public static void Nulpunten(double a, double b, double c){
       double discri=Discriminant(a,b,c);
       
       if (discri >0) {
       double nulpunt1=(-b+Math.Sqrt(discri))/(2*a);
       double nulpunt2= (-b-Math.Sqrt(discri))/(2*a);Âµ
       System.out.printIn(nulpunt1 "en" nulpunt2)
    }
       else if (discri==0){
       System.out.printIn(-b/(2*a));
    }
       else {
       System.printIn("geen nulpunt");
    }
 
    }
}