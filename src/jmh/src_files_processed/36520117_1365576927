
/**
 * ##### # ########### ## ##### ########### ####.
 * 
 * @###### (#### ####) 
 * @####### (# ####### ###### ## # ####)
 */
public class GroceryItem
{
   private String name;
   private double cost;
   private int aisleNum;
   
   
   public GroceryItem(String iName, double iCost, int iAisle)
   {
       name= iName
       cost = iCost
       aisleNum = iAisle
    }
    
}