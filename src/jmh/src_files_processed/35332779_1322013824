



    public class sum3
    {
        public static void main(int n)
        {
            int n;s=0;
            for(i=2;i<=n;i=i+2)
            {
                s=s-(i*i);
            }
              else
            {
            s=s+(i*i);
        }
        System.out.println(s);
    }
}



    
    }
}