class Circle extends Shape{
     int r,x,y;
     
     Circle(){
         super(); 
         r=5;
         x=1;
         y=2;
         
        }
        
     @Override 
     public void drow() throws NumberFormatException{ 
           String x="3i+4";
           this();
           if (x.charAt(1)=='i'){
                throw new NumberFormatException("it's not ok man in Circle");
            }
           
         
        }
        
        
     
     public static void main (String[] args){
            Circle obiectulet = new Circle();
            obiectulet.drow();
         
        }
    
    
}