import java.util.*;
public class RandomShit
{
    public void getFormatted (String str) {
        ArrayList<Integer> whiteSpaceIndex = new ArrayList<Integer>();
        String formatted = "";
        boolean isSpace = false;

        for (int i = 0; i < str.length();) {
            char c = str.charAt(i);
            if (c == ' ' && !isSpace) {
                isSpace = true;
                whiteSpaceIndex.add(i++);
            } else if (isSpace && c != ' ') {
                isSpace = false;
            }
            i++;
        }
        String word_one = str.substring(whiteSpaceIndex.get(1), str.length());
        String word_two = str.substring(whiteSpaceIndex.get(0), whiteSpaceIndex.get(1)-1);
        System.out.println(word_one " " + word_two);
    }
}