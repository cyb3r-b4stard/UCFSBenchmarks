
/**
 * ##### # ########### ## ##### ##########_# ####.
 * 
 * @###### ##### ######
 * @####### ##/##/##
 */
public class Worksheet2_4
{
    public static void main()
    {
        for( int y = 1; y <= 10; y++)
        {
            System.out.print(y + " " );
        }
        System.out.println();
        for( int x = 1; x < 100; x++)
        {
            if(x % 7 == 0)
            {
                System.out.print(x + " ");
            }
        }
        System.out.println();
        for( int z = 7; z < 100; z += 7)
        {
            System.out.print(z + " ");
        }
        System.out.println();
        for( int o = 10; o >= 0; o--)
        {
            System.out.print( o + " ");
        }
        System.out.println();
        for( int p = 1; p > 13; p++)
        {
            System.out.print( p + " ");
            System.out.println( p * p);
    }
}