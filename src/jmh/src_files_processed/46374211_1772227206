public class CheckListDab
{
    public static void main(String [] args)
    {
       String path = "/Users/glassiter2017/Desktop/EHS/2016-2017/IntroCS/BlueJ 3.1.7/PictureLab-Final";
       
       Picture original = new Picture(FileChooser.pickAFile());
       
       PicturePlus pic1 = new PicturePlus(original);
       pic1.explore();
       pic1.maxBlue();
       pic1.explore();
       pic1.write(path+"/maxBlueLassiter.jpg");
       
       
       PicturePlus pic2 = new PicturePlus(original);
       pic2.explore();
       pic2.zeroGreen();
       pic2.explore();
       pic2.write(path+"/zeroGreenLassiter.jpg");
       
       
       PicturePlus pic3 = new PicturePlus(original);
       Picture pic33 = pic3.decreaseRed(70);
       pic33.explore();
       pic33.write(path+"/decreaseRedLassiter.jpg");
       
       PicturePlus pic4 = new PicturePlus(original);
       pic4.grayScale;
       pic4.explore();
       pic4.write(path+"/grayScaleLassiter.jpg");
    }      
}