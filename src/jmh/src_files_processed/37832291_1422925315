
public class ccl
{
    public void calc( double i,double n)
    {
        double sum=0;
        sum=i+n;
        System.out.println("the sum is"+sum)
    }
    public void calc(double s, double t)
    {
        double sub=0;
        sub=s-t;
        System.out.println(" the diff is"+sub);
    }
    public static void main(String args[])
    {
        calc ob=new calc();
        ob.calc(12,8);
        ob.clac(34,16);
    }
}
        