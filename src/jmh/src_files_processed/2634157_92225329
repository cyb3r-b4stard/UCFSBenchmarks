import java.util.*;

public class Exo2Zelf{

public static boolean verifierMul(int[] L, int n)
    {   
        boolean waarheid = true;
        Scanner scan = new Scanner(System.in);
        
        
        for(int i=0; i<L.length; i++)
        {
                        
           if(L[i]%n == 0)
           waarheid = true;
           else waarheid = false;
            
        }
        
        
        return waarheid;
       
    }
    


public static void main(String [] args){
    
int tab[] = {5,10,15,20}
int b = 5;

boolean geheim = verifierMul(tab,b)


}

}
