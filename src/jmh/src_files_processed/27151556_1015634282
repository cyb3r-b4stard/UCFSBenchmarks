class Dreieck{
    double a, b, c;
    double getA(){
        return a;
    }
    double getB(){
        return b;
    }
    double getC(){
        return c;
    }
    void setA(double d){
        a = d;
    }
    void setB(double d){
        b = d;
    }
    void setC(double d){
        c = d;
    }
    boolean istGueltigesDreieck(){
        if(a+b > c)
            if(a+c > b)
                if(b+c > a)
                    return true;        
        return false;
    }
    Dreieck(){
        a = 1;
        b = 1;
        c = 1;
        if(istGueltigesDreieck())
            System.out.println("Kein g端ltiges Dreieck");
    }
    Dreieck(double a){
        this.a = a;
        b = a;
        c = a;
        if(istGueltigesDreieck())
            System.out.println("Kein g端ltiges Dreieck");
    }
    Dreieck(double a, double b){
        this.a = a;
        this.b = b;
        c = b;
        if(istGueltigesDreieck())
            System.out.println("Kein g端ltiges Dreieck");
    }
    Dreieck(double a, double b, double c){
        this.a = a;
        this.b = b;
        this.c = c;
        if(istGueltigesDreieck())
            System.out.println("Kein g端ltiges Dreieck");
    }
    double umfang(){
        return a+b+c;
    }
    double flaeche(){
        double s = umfang() / 2;
        return Math.sqrt(s*(s-a)(s-b)(s-c));
    }
}