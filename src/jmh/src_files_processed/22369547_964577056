class VIDEOTHEK
{
    DVD dvd1;
    DVD dvd2;
    KUNDE kunde1;
    KUNDE kunde2;
    VIDEOTHEK()
    {
        dvd1=null;
        dvd2=null;
        kunde1=null;
        kunde2=null;
    }
    void DVDAnlegen(String titelNeu)
    {
        if(dvd1==null)
        {
            dvd1=new DVD(titelNeu);
        }
            else
        {
            if(dvd2==null)
            {
                dvd2= new DVD(titelNeu);
            }
        }
    }
    void KundeAnlegen(String nameNeu, int kundennummerNeu)
}
