 */
public class CarV5
{
   int myStartMiles, myEndMiles, myDistance;
   double myGallons;
   
   CarV5()
   {
   }
   
   CarV5(int sMiles, int eMiles, int dist, double gals)
   {
       myStartMiles = sMiles;
       myEndMiles = eMiles;
       myDistance = dist;
       myGallons = gals;
   }
   
   public int calcDistance()
   {
       return myEndMiles - myStartMiles;
   }
   
   public double calcMPG()
   {
       return myDistance/myGallons;
   }
   
   public static void main(String[] args)
   {
       int startMiles1, endMiles1, distance1;
       double gals1, mpg1;
       
       startMiles1 = 50000; endMiles1 = 50280;
       distance1 = 0; gals1 = 11.487; mpg1 = 0;
       carType1 = 01 Toyota Rav4;
       
       CarV5 car1 = new CarV5(carType1,endMiles1,startMiles1,gallonsUsed1,pricePerGallon1);
       
       distance1 = car1.calcDistance();
       mpg1 = car1.calcMPG();
       
       System.out.printf("%30s %n", "Gas Mileage Calculations");
       System.out.printf("%2s%5s%5s%5s%5s%5s %n", "Type of Car     ","Start Miles   ","End Miles   ","Distance    ","Gallons    ","Miles/Gal    ");  
       System.out.println("==============================================================================================");
       System.out.printf("%1s%5d%5d%5d%5.3f%5.3f %n", carType1   ,startMiles1,endMiles1,distance1,gals1,mpg1);
       
   }
}