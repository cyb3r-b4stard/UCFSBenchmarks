import java.io.*; 
public class primepaln
{
 int sum,c;
 boolean isPrime_num(int n)
 {
     for(i=0;i<n;i++)
     {
         if(n%i==0)
         c++;
        }    
        if(c==2)
        return true;
        else
        return false;
    }
    boolean isPaln(int n)
    {
        while(n!=0)
        {
            dig=n%10;
            sum=sum+dig;
            n=n/10;
        }
        if(sum==n)
        return true;
        else
        return false;
    }
    public void primepaln()throws IOException
    {
        BuffferedReader br= newBufferedReader(newInputStreamReader(System.in));
     System.out.println("Enter the lower limit:");
     int n=Integer.parseInt(br.readline());
     
     System.out.println("Enter the upper limit:");
     int m=Integer.parseInt(br.readline());
    }
     if(m>n||m<3000||n<3000);
     {
         else
         System.out.print("out of range");
        }
    
     for(int i=m;i<n;i++)
     if (isPrime_num(i)==true&&ispalin_num(i))
     {
         System.out.println("Prime palindron numbers are:");
         if(c==0)
         {
            {
                System.out.print(i);
            }
            else{
                System.out.print(""+i);
                c++;
            }
            System.out.print("Frequency of prime palindrome is"+c);
        }
    }
     public static void main(String args())throws IOException
        BufferedReader br= newBuffered(newInputStreamReader(Sytem.in));
