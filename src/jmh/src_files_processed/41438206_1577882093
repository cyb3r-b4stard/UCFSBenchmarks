public class Themometer
{
    private int fTemp;
    public Themometer()
    {
       int fTemp = 60;
    }
    public int getfTemp(int fTemp)
    {
        fTemp = 60;
        return fTemp;
    }
    public int getcTemp(int cTemp)
    {
        ctemp = (ftemp - 32) * 5/9;
}