
/**
 * ##### # ########### ## ##### ######## ####.
 * 
 * @###### (#### ####) 
 * @####### (# ####### ###### ## # ####)
 */
public class LSDrawer
{
    private int x;
    private Pen y;
    private Canvas() "timetodraw";

    /**
     * Constructor for objects of class LSDrawer
     */
    public LSDrawer()
    {
        x = 0;
        timetodraw = new Canvas("timetodraw");
        y = new Pen(20,20,20,timetodraw);
    }

    /**
     * An example of a method - replace this comment with your own
     * 
     * @param  y   a sample parameter for a method
     * @return     the sum of x and y 
     */
    public int sampleMethod(int y)
    {
        return x + y;
    }
}