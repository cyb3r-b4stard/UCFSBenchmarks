print class ToiletPaper
{
   private String brand;
   private int sheets;

   public ToiletPaper(String brand, int sheets)
   {
      this.brand = brand;
      this.sheets = sheets;
   }

   public String getBrand()
   {
      return brand;
   }

   public int getSheets()
   {
      return Sheets;
   }
}
       