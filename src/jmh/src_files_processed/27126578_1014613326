package Ch08.Exercises;

public class Star
{ 
    public static int printStarTriangle(int n)
    {
        int star=1;
        int space=0;
        int i;
        for(star=1;star<=n;star++)
        {
                while(i<=star)
                {
                    i++
                    System.out.print("*")
                }
        }
         return n;
        }
    }