
public class Area
{
    double circle, square, rectangle;
    public void overloading(int radius)
    {
        double pi= 3.14;
        System.out.println(" Enter the radius ");
    	circle = radius * radius * pi;
    }
    public void overloading(int side)
    {
        System.out.println(" Enter the side ");
    	square= side*side;   	
    }
    public void overloading(int length, int breadth)
    {
        System.out.println(" Enter the length and breadth ");
    	rectangle = length * breadth;
    }
    public static void main ()
    {
        Area sdsd = new Area();
        sdsd.overloading(radius);
        sdsd.overloading(int side);
        sdsd.overloading(int lenght, int breadth);
        System.out.println("The area of circle is " + cirlce);
        System.out.println("The area of square is " + square);
        System.out.println("The area of rectangle is " + rectangle);
    }
}
