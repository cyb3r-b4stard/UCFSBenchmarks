public class Integration
{

    public static double f(double x) 
    {
        return (x*x); /*function*/

    }

    public static double Integrate(double a, double b, long n) /*[a,b] bounds , n is max*/{
        double sum = 0, h = 0, i = 0;
        if(a>b)
        {
            return Integrate(b,a,n);

        }
        else
        {
            h = (b-a)/n;
            for(i=a; i<=b;i=i+h)
            {
                sum = sum + h*f(i);

            }
            return sum; /*est. area*/
        }
    }
        public static void main(String[] args){   
     
            System.out.println("Area:" + Integration.Integrate(0, 1, 1000)); /* set values (a,b,n) */
    }
}


   
