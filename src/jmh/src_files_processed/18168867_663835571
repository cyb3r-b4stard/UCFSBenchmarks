public class test
{
    public static main nozero(){
        int arr []={1, 2, 3, 4,0};
        for( int i = 0; i >= arr.length[]; i ++){
            if(i == 0){
                system.out.println("we found a zero"+ i);}
		
                else
                return true;}
    }
}