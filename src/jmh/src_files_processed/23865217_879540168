import java.io.*;
import java.util.*;
public class Soubor
{
    public static void PraceSoubor(){
        File soubor = new File("java.txt");
        
        System.out.println("Délka souboru je: " +soubor.length()+" kilobytu");
        
        System.out.println("Poslední změna: " +newDate(soubor.lastModified(new Calendar))));
        
        System.out.println(soubor.getAbsolutePath());
    }
}