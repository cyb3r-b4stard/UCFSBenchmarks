class last
{ public static void main()
    { int n=78812831;
        for(int dig=0;dig<=9;dig++)
        { int t=n;
            while(t>0)
            { int d=t%10;
                if(d==dig)
                {System.out.printf(%d);
                }
                t=t/10;
            }
        }
    }
}
                
            