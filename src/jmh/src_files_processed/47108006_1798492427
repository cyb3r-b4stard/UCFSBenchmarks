import java.util.*;
class menutraingle
{
   public static void main(String args[])throws IOException
   {
       int x=0,y=0,z=0,a,b;
       int c;
       InputStreamReader read=new InputstreamReader(system.in);
       BufferedReader in=new Bufferedreader(read);
       System.out.println("1.Equilateral traingle");
       System.out.println("2.Isosceles traingle");
       system.out.println("3.Scalene traingle");
       system.out.println("4.Right angle traingle");
       c=Integer.praseInt(in.readLine());
       switch(c)
         {
             case1:
             System.out.println("Enter the three sides");
             x=Integer.parseInt(in.readLine());
             y=Integer.parseInt(in.readLine());
             z=Integer.parseInt(in.readLine());
             if (x=y=z)
              System.out.println("It is an equilateral traingle");
             else
              System.out.println("It is not an equilateral traingle");
            
             case2:
             System.out.println("Enter the three sides");
             x=Integer.parseInt(in.readLine());
             y=Integer.parseInt(in.readLine());
             z=Integer.parseInt(in.readLine());
             if ((x==z)||(y==z)||(x==y))
              System.out.println("It is an isosceles traingle");
             else
              System.out.println("It is not an isosceles traingle");
            
              
             case3:
             System.out.println("Enter the three sides");
             x=Integer.parseInt(in.readLine());
             y=Integer.parseInt(in.readLine());
             z=Integer.parseInt(in.readLine());
             if (x==y)
              System.out.println("It is an scalene traingle");
             else
              System.out.println("It is not an scalene traingle");
            }
        }
    }
            
              
              
              
              
              
              
              
              
              
              
              
              
              
              
              
              
              
              
              
              
              
              
              
              
              
              
              
              
              
       