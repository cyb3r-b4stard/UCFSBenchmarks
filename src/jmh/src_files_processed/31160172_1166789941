
public class worker
{
    private String name;
    private String gender;
    private String id;
    private double ratehours;
    private double workhours;

  
    public worker()
    {
      name="";
      gender="";
      id="";
      ratehours=0.0;
      workhours=0.0;
    }

    public worker(String n,String g,String i,double r,double w)
    {
      name=n;
      gender=g;
      id=i;
      ratehours=r;
      workhours=w;
    }
    
    public String getN(){return name:}
    public String getG(){return gender;}
    public String getI(){return id;}
    public double getR(){return ratehours;}
    public double getW(){return workhours;}
}