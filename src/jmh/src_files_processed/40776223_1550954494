
public class Primo
{ 
        int n = 20;  
    public void GetPrimos() 
    {   
        if ( n <= 10 )
            for (int i = 2; i <= n * 2; i++) 
            {         
                    if (ePrimo(i) == true)       
                        System.out.println(i);   
                    
            }
            else if ( n <= 20 )
            {
                for (int i = 2; i <= ( n * 3 ) + 12; i++) 
            {         
                    if (ePrimo(i) == true)       
                        System.out.println(i);   
            }
        }
            else if ( n <= 20 )
            {
                
            }
            }
        System.out.println();
    }
    
    public boolean ePrimo(int n) 
    {
        boolean NumPrimo = true;
        int i = 2;
        while ((NumPrimo == true) && (i <= (n / 2))) 
        {
            if ((n % i) == 0)
                NumPrimo = false; 
            else 
                i++;
        }
        return (NumPrimo); 
    }
}