public class program5
{
    public static void main()
{ 
   int a = 20 ;
   float b = 30f ;
   double c = 50d ;
   System.out.println("the values to be added are" +a + "," + b + "and" + c) ;
   System.out.println("the sum of int, float and double is:" a + b + c) ;
        
    }
}

    