import java.util.Calendar;
import java.util.GregorianCalendar;
import java.util.Scanner;
public class Project_1
{
    public static void main(String[] args)
    {
        Calendar c = new GregorianCalendar();
        int month1 = c.get(Calendar.MONTH); 
        int day1 = c.get(Calendar.DAY_OF_MONTH);
        int year1 = c.get(Calendar.YEAR);
        int hour1 = c.get(Calendar.HOUR_OF_DAY);
        int minute1 = c.get(Calendar.MINUTE);
        int second1 = c.get(Calendar.SECOND);
        System.out.print(month1+" ");
        System.out.print(day1+" ");
        System.out.print(year1+" ");
        System.out.print(hour1+":");
        System.out.print(minute1+":");
        System.out.print(second1);
        
        int k1 = year1 % 7; 
        int j1 = (int)(year1 / 100.0); 
        int q1 = day1;
        int m1 = month1;
        int h1 = (q1 + (int)((26 * (m1 + 1)) / 10.0) + k1 + (int)(k1 / 4.0) 
                + (int)(j1 / 4.0) + (5 * j1)) % 7;
        
        if (h1 == 0) 
            System.out.println(" Saturday");
        else if (h1 == 1)
            System.out.println(" Sunday");
        else if (h1 == 2)
            System.out.println(" Monday");
        else if (h1 == 3)
            System.out.println(" Tuesday");
        else if (h1 == 4)
            System.out.println(" Wednesday");
        else if (h1 == 5)
            System.out.println(" Thursday");
        else
            System.out.println(" Friday");
            
        
        
        Scanner input = new Scanner(System.in);

        System.out.print("Enter year: ");
        int year = input.nextInt();

        System.out.print("Enter month: 1-12: ");
        int month = input.nextInt();

        System.out.print("Enter the day of the month: 1-31: ");
        int day = input.nextInt();
        
        if (month == 1 || month == 2) {
            month += 12;
            year--;
        }
        
        System.out.print(month+" ");
        System.out.print(day+" ");
        System.out.print(year);
        
        int k = year % 7; 
        int j = (int)(year / 100.0); 
        int q = day;
        int m = month;
        int h = (q + (int)((26 * (m + 1)) / 10.0) + k + (int)(k / 4.0) 
                + (int)(j / 4.0) + (5 * j)) % 7;
        
        if (h == 0) 
            System.out.println(" Saturday");
        else if (h == 1)
            System.out.println(" Sunday");
        else if (h == 2)
            System.out.println(" Monday");
        else if (h == 3)
            System.out.println(" Tuesday");
        else if (h == 4)
            System.out.println(" Wednesday");
        else if (h == 5)
            System.out.println(" Thursday");
        else
            System.out.println(" Friday");
        
        int Oseconds;
        int Eseconds;
        int Tseconds;
        Oseconds = (second1+(minute1*60)+(hour1*60*60)+(day1*24*60*60)+(month1*30*24*60*60)+(year1*12*30*24*60*60));
        Eseconds = (day*24*60*60)+(month*30*24*60*60)+(year*12*30*24*60*60));
        Tseconds = Math.sqrt(Math.pow((Oseconds - Eseconds)));
   }
}