import java.io.*;
public class Reading_Material
{
    private String Title,Publisher;
    private int date_published,current_page;
    
    public int get_page()
    {
        return current_page;
    }
    public void turn_page_forward()
    {
        current_page++;
    }
    public void turn_page_backwards()
    {
        current_page--;
    }
    public String get_Publisher()
    {
        return Publisher;
    }
    public int get_date_published()
    {
        return date_published;
    }
    public String get_Title()
    {
        return Title;
}