package OO.init;


/**
 * ####### ###### ###。
 * 
 * @##（####）
 * @##（###########）
 */
public class Robot2
{
    private int age = 10;
    private final int k = 2;
    private int power;
    int go(int xxx);
    {
    return xxx * k;
    }
}

    
    public Robot2(){
        age = 30;
    }
    public Robot2(int age)
    {
        this.age = age
        power = go(age);
    }

    