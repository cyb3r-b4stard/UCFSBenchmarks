Public class City {
    private string _cityName;
    private Point _cityCenter , _centralStation;
    private long _numOfResidents;
    private noOfNeighborhoods;
    final int ZERO=0 ,ONE=1;
    
public City (cityName,cityCenter,centralStation,numOfResidents,noOfNeighborhoods)
{
    _cityName=cityName;
    _cityCenter=cityCenter;
    _centralStation=centralStation;
    if (numOfResidents < ZERO)
        numOfResidents = ZERO;
    else
        _numOfResidents=numOfResidents;
    if (noOfNeighborhoods <= ZERO)
        noOfNeighborhoods = ONE;
    else 
        _noOfNeighborhoods=noOfNeighborhoods;
}
/*public string getCityName?()
{
   retu this._cityName; 
}




}
