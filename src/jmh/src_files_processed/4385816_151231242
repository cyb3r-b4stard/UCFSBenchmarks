public class Host {
    private String jmeno;
    private int penezenka;
    private int vypitaPiva;
    private int uctenka;
    private int platil;
    private int placeno;
    
    public Host(String jmeno_zakaznika, int penize){
        jmeno = jmeno_zakaznika;
        penezenka = penize;
    }
    
    public int setUctenka(int cena){
        cena = 29;
        uctenka 1*cena;
    }
}