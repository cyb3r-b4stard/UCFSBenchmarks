public class Persona
{
   private String nombre;
   private int edad;
   private int dni;
   private int peso;
   private double estatura;
   private char sexo;
   
   public String  persona (){
       this.nombre = ("");
       this.edad = 0;
       this.dni= 0;
       this.peso = 0;
       this.estatura = 0;
       this.sexo = "M";
   }
   
   public persona (String nombre, int edad, int dni, int peso,
   int estatura, char sexo){
    this.nombre = "Juan Francisco Navarrete Barboza"
    this.edad = 29;
    this.dni = 0;
    this.peso = 0;
    this.estatura = 0;
    this.sexo = "M";
    }
}