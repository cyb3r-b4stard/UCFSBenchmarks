      notes = new ArrayList<String>();
  }
  
  public void AjouterNotes (ArrayList<double>notes)
  {
      notes.add(note);
  }
  
  public int sizeNotes (String note)
  {
      notes.add(note);
  }
  
  public int numberOfNotes()
  {
      return notes.size();
  }
  
  public double Moyenne (ArrayList<double>notes)
  {
    double sum;
    int i;
    for (i = 0; i= notes.size(i, i++)
    {
      Sum = notes.get(i)
    }
    return sum/notes.size
  }
 
  public void Afficher ()
  {
      
  }
  
  public void Modifier (   note) 
   {
       int index = l.index()
