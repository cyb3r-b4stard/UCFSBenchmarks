public class Program9{  
  public static void main(String[] args) 
  {  Scanner sc=new Scanner(System.in);
      System.out.println(" enter 2 integer parameters, start with the lower integer");
        
                   int valueStart= sc.nextInt();
                   int valueEnd= sc.nextInt();
                    
                    int range = randomInRange(valueStart,valueEnd);
                    System.out.println(randomnum);
        }
      
 public static int randomInRange(int x, int y)
  {if(y>x)
     return 0;
      else{
      int randomnum= (Math.random()*(y-x+1)+x);
      return randomnum;
      
        }
    }
}