public class HelloPrinter 
{
   public static void main(String [] args) 
   {
       System.out.println("Hello World!");
       System.out.println("Answer is " +3);
       System.out.println("Answer is " +3 +2);
       System.out.println("Answer is " +(3+2));
       System.out.println(3+ 2+ "is Answer")
    }
}