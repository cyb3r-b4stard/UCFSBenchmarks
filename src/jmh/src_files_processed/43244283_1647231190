class Rectangle 
{
    double radius = 1;
    Rectangle();
    {}
    Circle(double newRadius) 
    {radius = new radius;
    }
    double getArea() {
        return radius * radius * Math.PI;
    } 
    double getPerimeter() { 
        return 2 * radius * Math.PI;
    }
    void setRadius(double new Radius) 
    {radius = new Radius
    }
}
    