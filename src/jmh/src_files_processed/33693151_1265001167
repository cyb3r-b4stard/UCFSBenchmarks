import java.util.Scanner;
public class if1
{
    int manth;
    void input()
    {
        Scanner sc=new Scanner(System.in);
        System.out.println("enter manth ");
       manth=sc.nextInt();
       if (manth==1||manth==3||manth==5||manth==7|| manth==9 || manth==11);
       System.out.println("manth is 31 days");
       else if (manth==4||manth==6||manth==8|| manth==10 || manth==12);
       System.out.println("manth is 30 days");
       else if (manth==2);
        System.out.println("manth is 28 days");
    }
       public static void main(String args[])
 {
    if1 b1=new if1();
     b1.input();
      b1.output();
}  
}