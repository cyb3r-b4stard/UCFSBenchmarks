import java.util.*;
public class neonn
{
    int neon(int p)
    {
        while (p>0)
        {
        d=p%10;
        s=s+d;
        p=p/10;
    }
    if (s==n)
    {
    System.out.println("It is neon no.  ");
}
    else 
    System.out.println("It is not neon no.  ");
    public static void main (String args[])
    {
        Scanner sc= new Scanner(System.in);
        int d,p,s=0;
        System.out.println("Enter the no. ");
        n=sc.nextInt();
        p=n*n;
