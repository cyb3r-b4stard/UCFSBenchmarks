import sacco.*;
public class Platform extends SaccoObject
{
    private int x, y, width, height;
    public Platform(int tmpx, int tmpy, int tmpwidth, int tmpheight)
    {
        x = tmpx;
        y = tmpy;
        width = tmpwidth;
        height = tmpheight;
    }
    public void paintSelf(PaintBrush brush)
    {
        brush.drawRectangle(x, y, width, height)
    }