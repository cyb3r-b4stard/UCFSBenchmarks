
/**
 * ##### # ########### ## ##### #### ####.
 * 
 * @###### (#### ####) 
 * @####### (# ####### ###### ## # ####)
 */
public class math
{
   private int a;
   private int b;
   public math(int pa,int pb)
   {
       a=pa;
       b=pb;
    }
    public int swap()
    {
    int c;
    c=a;
    a=b;
    b=c;
    return a,b;

}
public int sum()
{
    return a+b;
}
public int max()
{
    if(a>b)
    {
        return a;
    }
    else{
        return b;
    }}}
    
       