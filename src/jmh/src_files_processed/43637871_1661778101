import java.util.*;

public class CountLetters
{
    private String input;
    private String[][] mat;

    public CountLetters(String i)
    {
        input=i;
        mat=new String[20][20];
        randomize(input);
    }

    public void randomize(String i)
    {
        int length=i.length();
        int rand=0;
        
        for(int x=0;x<mat[].length();x++)
            for(int y=0;y<mat[x].length();y++)
            {
                rand=(int)Math.random()*length;
                mat[x][y]=i.substring(rand,rand+1);
            }   
    }
}