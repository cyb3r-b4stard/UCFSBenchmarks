
public class BRUCH {
    private int zaehler;
    private int nenner;
    private int dezimalzahl;
    
    public BRUCH (int z, int n){
        zaehler=z;
        nenner=n;
    }
    public void bruch_ausgeben (){
        System.out.println (zaehler);
        System.out.println ("------");
        System.out.println (nenner);

    }
    public void dezimalzahl_ausgeben (int z, int n){
        int dezimalzahl=z/n;
        return int dezimalzahl;
    }
}