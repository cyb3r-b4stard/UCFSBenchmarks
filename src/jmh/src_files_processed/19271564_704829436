 */
 public class Lamborghini 
 {
     private int        modelYear;
     private String     modelName;
     private double     horsepower;
     private String     manufacturerName;
     private boolean    isRearWheelDriveCar;
     private String     webPage;
     
     /**
      * Default constructor
      * @param lModelYear = 0
      * @param lModelName = null
      * @param lHorsepower = 0.0
      * @param lManufacturerName = null
      * @param lIsRearWheelDriveCar = false
      * @param lWebPage = null
      */
     public Lamborghini (){
        }
        
     /**
      * @param newModelYear The model year of the car, invaild if the year is not in range between 1963 and current year (2015) inclusive
      * @param newModelName The model name of the car, invalid if is the length of the name is less than 6 characters
      * @param newHorsepower The horsepower of the car, invalid if it's not in range between 300 and 600 inclusive
      * @param lManufacturerName The name of the manufacturer must be Lamborghini
      * @param newIsRearWheelDriveCar The layout of the car: if the car is rear-wheel drive
      * @param newWebPage The web page of Lamborghini class
      */
     public Lamborghini(int newModelYear, String newModelName, double newHorsepower, String newManufacturerName, boolean newIsRearWheelDriveCar, String newWebPage)
     {
         setModelYear(newModelYear);
         setModelName(newModelName);
         setHorsepower(newHorsepower);
         manufacturerName=lManufacturerName;
         setIsRearWheelDriveCar(newIsRearWheelDriveCar);
         setWebPage(newWebPage);
         
         
         
         
         if (lManufacturerName !=null && lManufacturerName == "Lamborghini")
         manufacturerName = lManufacturerName;
         
         
        }
        /**
         * @param newModelYear The model year of the car, invaild if the year is not in range between 1963 and current year (2015) inclusive
         */
        public void setModelYear(String newModelYear){
            if (newModelYear >= 1963 && newModelYear <=2015)
            modelYear=newModelYear;
        }
        else{ if (newModelYear <= 1963){
            System.out.print(newModelYear + "is too early, sorry");
        }
        else{ if (newModelYear >= 2015){
            System.out.print(newModelYear + "is too late, sorry");
        }
    
    
            
         
         
         
         
         