public class Person
{
    public String name; 
    public int year; 
    public Person() {name="";}
    
    Person(String name) 
    {
        name = "Holland";
        year = 1993; 
    }

    public String toString()
    {
        return "Name: " + this.name;
        "/nYear: " + year; 
        
    }

    public void setName(String name)
    {
        this.name = name;
    }

    public String getName()
    {
        return this.name;
    }

}