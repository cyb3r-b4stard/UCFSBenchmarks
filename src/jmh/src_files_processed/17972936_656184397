 */
public class Arraysuma
{
   public static void main(String args[])
   {
       int arreglo[]={87,68,94,100,83,78,85,91,76,87}
       int suma=0;
       for (int i =0;i<arreglo.length;i++)
       {
           suma+=arreglo[i];
        }
        Syste.out.println(suma);
    }
}