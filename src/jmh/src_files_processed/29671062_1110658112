public class Car
{
 
  public int modid,quantity;
  public String name;
  public double price;
  
  public class Car
  {
  public void  setCar(String n,int id,int q,double p)
  {
      name =n;
      modid=id;
      quantity=q;
      price=p;
    }
    
    public  Car(String n,int id,int q,double p)
  {
      setCar(n,id,q,p);
    }
    
    public int getmodid()
    {
        return modid;
    }

    public int setquantity()
    {
        return quantity;
    }
    
    public String getname()
    {
        return name;
    }
    
    public double getprice()
    {
        return price;
    }
    
    public int Discount()
    {
      if (price < 6000)
      return "5";
      else 
      if(price >6000 && price < 10000)
      return "10";
      else 
      if(price >10000 && price <15000)
      return 15;
      else
      return 20;
    }
    
    public String toString()
    {
    return ("modid"+modid+"\t"+"quantity"+quantity+"\t"+"name"+name+"\t"+"price"+price+"\t"+"Discount"+Discount);
}
}
    
    
    
    
    
    
    