
/**
 * ##### # ########### ## ##### ### ####.
 * 
 * @###### (#### ####) 
 * @####### (# ####### ###### ## # ####)
 */
public class Car
{
    private String car;
    private String make;
    private int year;
    private double accel;
    private int mileage;
    /**
     * Consturctor for car
     * @param String carN, String makeN, int yearT, double accelA, int mileageC
     * Variable names
     */
    public Car(String carN, String makeN, int yearT, double accelA, int mileageC)
    {
        car = carN;
        make = makeN;
        year = yearT;
        accel = accelA;
        mileage = mileageC;
    }
    
    /**
     * Overloaded constructor with this reference
     * @param String carN, String makeN, int yearT, double accelA
     * Variables relating to this reference
     */
    public Car(String carN, String makeN, int yearT, double accelA)
    {
        this(carN, makeN, yearT, accelA, 0);
    }

    pubilc double getTimeToAccelerate()
    {
        accel = (mileage / 4000) * .2
        
        return accel;
    }
}