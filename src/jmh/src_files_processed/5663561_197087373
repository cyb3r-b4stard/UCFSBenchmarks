class Rectangle
{
    int x;
    int y;
    int width;
    int height;
    int area
    
    Rectangle()
    {
        x = 0;
        y = 0;
        width = 10;
        height = 5;
    }
    
    void showArea()
    {
        area = (x * y *  width * height);
        
    }
    
    void sayHello()
    {
        System.out.println("The area is " + area);
    }
}