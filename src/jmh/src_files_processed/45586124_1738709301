public class vet1
{ public static int [] vet1 ( int [] a, int [] b)
  { int [] vet1 = new int [a.length];
    for (int i=0; i<vet1.length; i++)
    { if (a[i]%2==0 && b[i]%2!=0)
        vet1[i]=a[i];
        else if (a[i]%2!=0 && b[i]%2==0)
        vet1 [i]=b[i];
        else if (a[i]%2==0 && b[i]%2==0 && a[i]>b[i])
        vet1 [i] == a[i];
        else if (a[i]%2==0 && b[i]%2==0 && a[i]<b[i])
        vet1 [i] == b[i];
        else if (a[i]%2!=0 && b[i]%2!=0)
        vet1 [i] == 0;
    }
    return vet1;
}
}