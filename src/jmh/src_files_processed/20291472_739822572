class vowel
{
    public void find (char ch)
    {
        switch (ch)
        {
            case 'A':
            case 'a':
            case 'E':
            case 'e':
            case 'I':
            case 'i':
            case 'O':
            case 'o':
            case 'U':
            case 'u':
            System.out.println (ch+"is vowel");
            break;
            default:
            if (ch>='A'&&ch<='Z')||(ch>='a'&&ch<='z');
            System.out.println (ch+"is consonent");
        }
    }
}