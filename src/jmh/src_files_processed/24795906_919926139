public class Bankkonto
{
    int Kontonummer=123456;
    int Kontostand=500;
    String Name="Bastian Thielemann";

    void Einzahlen(int neuerK){
        Kontostand=neuerK+Kontostand;
    }

    void Auszahlen(int neuerK){
        Kontostand=Kontostand-neuerK; 
    }
    System.out.println("Kontostand");
}    

