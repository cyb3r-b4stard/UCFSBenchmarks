import java.util.ArrayList;
import java.util.Scanner;
import java.util.Collections;
public class Main{
    public static void main(String[] args){
     Main m=new Main();
     int num=0;
     Scanner sc=new Scanner(System.in);
     int c=0;
     ArrayList<ArrayList<Integer>> listalista = new ArrayList<>();
     while((num=sc.nextInt())!=0 &&c<=2000000){
        sc=new Scanner(System.in);
        String cad =  sc.nextLine();
        ArrayList<Integer> cade = m.getCadNum(cad);
        Collections.sort(cade);
        listalista.add(cade);
        sc=new Scanner(System.in);
        c+=1;
     }
     m.print(listalista);
    }
     private ArrayList<Integer> getCadNum(String cad){
     ArrayList<Integer> res=new ArrayList<>();
     int i=0;
     while(i<cad.length()){
        res.add((int)(cad.charAt(i)));
        i+=2;
     }
     return res;
    }
    private void print(ArrayList<ArrayList<Integer>> lista){
        for(int j=0;j<lista.size();j++){
          for(int i=0;i<lista.get(i).size();j++){
            System.out.print((char)((int)(lista.get(i).get(j))+ " ");
          }
          System.out.println();
        }
        System.out.println();
    }
}