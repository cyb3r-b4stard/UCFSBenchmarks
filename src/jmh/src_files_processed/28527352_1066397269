class DoingUse{
    public static void main (String args[]){
        Room DoingRoom = DoingRoom;
        DoingRoom.length = 7.23;
        DoingRoom.width = 7.2;
        DoingRoom.height = 8.2;
        
        System.out.println("Length ="DoingRoom.length)
        System.out.println("width = "DoingRoom.width)
        System.out.println("height = "DoingRoom.height)
    
    
    
    }
}