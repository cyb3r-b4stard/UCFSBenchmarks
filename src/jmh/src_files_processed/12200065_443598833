import java.util.*;
public class opos
{
    public static boolean m(int num,d )
    {
        if(num<10)
    {
        if( num ==d)
        {
            return true;

        }
    }
       
    m(num/10);
       
    if(m(num/10,d))
    {
        return(num==d);
    }
    
    return false;
    }
}