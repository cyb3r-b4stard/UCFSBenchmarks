public class test {
public static void main(String[] args) throws FileVideException {
            System.out.println("Test de constructeur \n");          
            FilePrioChainee test;
            test = new FilePrioChainee();
            System.out.println(test.taille());
            System.out.println("File vide? \n");
            System.out.println(test.estVide());
            
            System.out.println("Test d'enfilement \n"); 
            test.enfiler(new TachePrio (3,"a"));
            System.out.println(test.toString());
            test.enfiler(new TachePrio (2,"b"));
            System.out.println(test.toString());
            test.enfiler(new TachePrio (1,"c"));
            test.enfiler(new TachePrio (4,"d"));
            test.enfiler(new TachePrio (3,"e"));
            test.enfiler(new TachePrio (3,"f"));
            test.enfiler(new TachePrio (1,"g"));
            test.enfiler(new TachePrio (4,"h"));
            test.enfiler(new TachePrio (9,"i"));
            test.enfiler(new TachePrio (3,"j"));
            test.enfiler(new TachePrio (2,"k"));
            System.out.println(test.toString());    
            
           System.out.println("Test de defilement \n");
           Object defiler = null;
           defiler = test.defiler();
           System.out.println(defiler); 
           System.out.println(test.toString()); 
           System.out.println("Test de defilement avec priorite 3 \n");
           defiler = test.defiler(3);
           System.out.println(defiler); 
           System.out.println(test.toString());
           
           System.out.println("Test premier \n");
		   defiler = test.premier();
		   System.out.println(defiler);	

		   
		   System.out.println("Test premier prio 2\n");
		   defiler = test.premier(4);
		   System.out.println(defiler);	

		   System.out.println("Test premier prio 6 (null)\n");
		   defiler = test.premier(4);
		   System.out.println(defiler);	
		   System.out.println(test.toString());
           
           System.out.println("Test de defilement avec priorite 6 (null) \n");
		   defiler = test.defiler(6);
		   System.out.println(defiler);	
		   System.out.println(test.toString());	
		   
		   System.out.println("Priorite 4 existe?\n");
		   System.out.println(test.prioriteExiste(4));
		   
		   System.out.println("Taille priorite 6\n");
		   System.out.println(test.taille(6));
		   
                
