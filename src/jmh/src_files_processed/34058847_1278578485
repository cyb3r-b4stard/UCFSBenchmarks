public class Test {
    
    
   public void main () {
   
       double bh;
       double atA = 65;
   
       bh= 100-atA;
   if (atA > 100)
       System.out.println ("Die Eingabe ist zu hoch")
   else if (atA < 0)
       System.out.println ("Die Eingabe ist zu niedrig")
   else
       System.out.println ("In " +bh+ " Jahren sind Sie 100");
   
}
    
}