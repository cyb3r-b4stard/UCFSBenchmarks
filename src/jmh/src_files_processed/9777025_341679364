public class House
{
    import apcslib.*;

    public static void main(String args[])
   {
   SketchPad poster = new SketchPad(1080,720);
   DrawingTool Marker = new DrawingTool(poster);
   marker.forward(150);
   marker.turnRight(67);
   marker.turnLeft(90);
   marker.forward(60);
   marker.turnRight(80);

   }
}