public class arab {
public static void main (string []args) {
Scanner inpot = new Scanner (System.in) ;
int average =0 ;
int number =0 ;
int counter =0 ;
int total -0 ;

while (counter <10) {
number = inpot.nextlnt() ;
total = total + number ;
counter ++ ;
}
average = total/counter ;
System.out.println(average);
}
}