import java.util.ArrayList;

public class Aquarium
{
    private ArrayList <Fish> School;  
    private setColor;
    public Aquarium()
    {        
        School = new ArrayList<Fish>();
        for(int i = 1; i <= 20; i++)
        {   
            ArrayList<Fish> School = new ArrayList<Fish>(20);  
            School.add(new Fish());
        }        
    }
    
    public void main(String[] args)
    {
       for(int i = 1; i < School.size(); i++)
       {
           School.get(i).setColor = "blue";
       }
        
       School.get(8).setColor = "green";
    }
}