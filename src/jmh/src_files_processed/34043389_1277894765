

public class TELEFONO
{
   private int numero ;
   private String marca;
   private String modelo;
   private CLIENTE = CLIENTE
   public TELEFONO ( ){
       numero = 0;
       marca = "";
       modelo = "";
       CLIENTE = CLIENTE;
    }
    
    public CLIENTE (int numero, String marca , String modelo, CLIENTE CLIENTE){
     setNumero(numero);
     setMarca(marca);
     setModelo(modelo);
     setCLIENTE(CLIENTE);
    }
    
    
    
    
}