public class ABC
{   
public static void main(String args[]){
char c ='a';
for(int = 0; i<26; i++){
    System.out.println(c++);
}
System.out.println();
char c ='A';
for(int = 0; 1<26; i++){
    System.out.println(c++);
}
System.out.println();
char c ='0';
for(int = 0; i<26; i++){
    System.out.println(c++);
}
}
    
