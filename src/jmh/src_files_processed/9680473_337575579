
/**
 * ##### # ########### ## ##### ###### ####.
 * 
 * @###### (#### ####) 
 * @####### (# ####### ###### ## # ####)
 */
public class Animal
{
    private String name;
    private String species;
    private String dateOfBirth;
    private String arrived;
    private boolean isSick;
    private boolean isQuarantined;

    public Animal (String myName, String mySpecies, String myDateOfBirth, String myArrived, boolean myIsSick, boolean myIsQuarantined){
        name = myName;
        species = mySpecies;
        dateOfBirth = myDateOfBirth;
        arrived = myArrived;
        isSick = myIsSick;
        isQuarantined = myIsQuarantined;
    }

    public String getName(){
        return name;
    }

    public void String (String newName){
        name = newName;
    }

    public String getSpecies(){
        return mySpecies;
    }

    public void String (newSpecies){
        mySpecies = newSpecies;
    }

    public String getMyDateOfBirth (){
        return myDateOfBirth;
    }

    public void String (newMyDateOfBirth){
        myDateofBirth = newMyDateOfBirth;
    }

    public String getMyArrived(){
        return myArrived;
    }

    public void String (newMyArrived){
        myArrived = newMyArrived;
    }

}