import java.util.Scanner;
public class valid{
    public static boolean inputValid(String input, String validInputs){
        input = input.toLowerCase();
        String inputs[] = validInputs.split(",");
        System.out.println(inputs[]);
        if(inputs.equals(inputs[0])){
            return true;
        }
        else{
            return false;
        }
    }
}
        