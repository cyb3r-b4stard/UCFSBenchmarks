import java.awt.*;
import java.applet.*;
public class olympic extends Applet{
public void paint(Graphics g){
    {
        set Background"Color.Red";
        set Foreground"Color.pink";
        g.drawOval(100,100,50,50);
        g.drawOval(140,100,50,50);
        g.drawOval(180,100,50,50);
        g.drawOval(120,100,50,50);
        g.drawOval(160,100,50,50);
    }
}
}