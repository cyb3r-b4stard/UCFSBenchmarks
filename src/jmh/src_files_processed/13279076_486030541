import javax.swing.JOptionPane;
public class TestPoblacion{
    int habitantes,num;
    Poblacion pop=new Poblacion();
    habitantes=Integer.parseInt(JOptionPane.showInputDialog(null,"El numero de habitantes"," ",1));
    num=JOptionPane.showInputDialog(null,"Teclea a cuantos a√±os desea saber"," ",1);
    pop.setHabitantes(habitantes);
    pop.setCalcular(num);
    habitantes=pop.getHabitantes();
    JOptionPane.showMessageDialog(null,"Poblacion restante"+habitantes," ",1);
}