public class P3C
{
    private String firstName;
    private String lastName;
    private double[] grades = new double[5];
    public P3C()
    {
        firstName = "Alan";
        lastName = "Sanchez";
        grades[0] = 0.0;
        grades[1] = 0.0;
        grades[2] = 0.0;
        grades[3] = 0.0;
        grades[4] = 0.0;
    }
    
    public P3C(String primerNombre, String apellido, double english, double science, double math, double fineArts, double socialStudies)
    {
        firstName = primerNombre;
        lastName = apellido;
        grades[0] = english;
        grades[1] = math;
        grades[2] = science;
        grades[3] = fineArts;
        grades[4] = socialStudies;
    }
    
    public void setGPA(double e, double m, double s, double fa, double ss)
    {grades[0] = e; grades[1] = m; grades[2] = s; grades[3] = fa; grades[4] = ss;}
    
    public double calcGPA()
    {
        return (grades[0]+grades[1]+grades[2]+grades[3]+grades[4]/5);
    }
    
    public String toString()
    {
        return
    }
    
    
}