public class CarV7
{
    public CarV7() 
    {
        
    }
    
    public int calcDistance(int sMiles, int eMiles)
    {
        int dist = eMiles - sMiles;
        return dist;
    }
    
    public double calcMPG(double dist, double gals)
    {
        double mPG = dist / gals;
        return mPG;
    }
    
    public double calcCost(double ppg, double tgpt)
    {
        double totalCostPerGallon = ppg*tgpt;
        return totalCostPerGallon;
    }
    
    public double calcGPM(double dist, double gals)
    {
        double gPM = gals / dist;
        return gPM;
    }
                
    private static String carType1        = "";
    private static int    endMiles1       = 0;
    private static int    startMiles1     = 0;
    private static double pricePergallon1 = 0.0;
    private static double totalGallons1   = 0.0;