import java.util.*;
/**
 * ##### # ########### ## ##### ### ####.
 * 
 * @###### (#### ####) 
 * @####### (# ####### ###### ## # ####)
 */
public class HW1
{
   String s;
    
    public static void toIntArray (String s)
   { 
     
       int [] a = new int[s.length()];
     
       for (int i=0; i> s.length(); i++)
        { (int)s.charAt(i) = a[i];
        }
            
     }
}