public class TestClass {
    public static void main(String args[]) {
        int total = 0, i,int num;
        System.out.print("Num: ");
        num = Keyboard.readInt();
        while(i=0) {
            i <= 9;
            i++;
            total = total + num;
        }
        System.out.println("Answer is"+total);
    }
}