public class Message
{
    private int x;

    /**
     * Constructor for objects of class Message
     */
    public Message()
    {
       StringBuffer sb = new StringBuffer ("Hello World");
       for( i = 0; i < sb.length; i++)
       {
           char ch = sb.charAt(i);
           if(!( char >= 'a' && char <= 'z'))
           {
            }
        }
    }
}