import java.awt.Graphics;
import java.awt.Color;

/**
 * ## ####### ## #####
 * 
 * @###### ###### && ####### #######
 * @## ######## ####
 */
public class Square extends Rectangle{
    
    /*Constructeur
     * @pre-
     * @post cree une instance de la classe Square
     */
    public Square(int xa,int ya, int xb, int yb,int xc,int yc){
        assert(xa!=xb):"";
        super(xa,ya,xb,yb,xc,yc,xa+xc-xb,ya+yc-yb);
    }
    
}