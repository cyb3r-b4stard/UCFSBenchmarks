public class againstreverseTester
{private double myDouble;
    public againstreverseTester(double Double)
    {myDouble=Double;
     
}

public double setDouble(double Double)
{myDouble=Double;
 return myDouble;
}

public double calculate()
{myDouble=myDouble+1;
return myDouble;
}

public double e(n)

{ double e=Math.pow((1+1/n), n);
return e;
}
}