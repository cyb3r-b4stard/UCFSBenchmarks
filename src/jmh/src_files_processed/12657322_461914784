public class Employee extends Person
{
  final int  CURRENT_YEAR= 2014;
  private int hireYear;
  private String idNum;
  
  public Employee(int newHireYear, String newIdNum)
  {
      super(String name);
      setHiteDate(newHireYear);
      setIdNum(newIdNum);
      
      
    
    }
}