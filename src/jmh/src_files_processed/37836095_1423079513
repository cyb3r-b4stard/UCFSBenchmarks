/**
 * ########## #### ########## ##########.
 *
 * @###### ######## ##########
 * @####### ####-##-##
 */
public class DoeDeTest
{
    public DoeDeTest()
    {
        
    }
    
    /**
     * Controleert of twee letters in alfabetische volgorde staan.
     * @param   letter1 De eerste letter.
     * @param   letter2 De tweede letter.
     * @return  True indien letter1 alfabetische voor letter2 komt, anders false. True indien de letters gelijk zijn.
     */
    public boolean isAlfabetisch(char letter1, char letter2)
    {
        return letter1 <= letter2;
    }
    
    /**
     * Bepaald of een persoon met een gegeven leeftijd een tiener is.
     * @return  True indien de persoon een tiener is, anders false.
     */
    public boolean isEenTiener(int leeftijd)
    {
        return leeftijd >= 10 && leeftijd < 20;
    }
    
    public boolean isEenWinnaar(int aantalOgenOpDobbelsteen)
    {
        return aantalOgenOpDobbelsteen != 3 && aantalOgenOpDobbelsteen != 6;
    }
    
    public boolean isVIP(char lidnummer, int ingeschrevenVoor2000)
    {
        return lidnummer 'P'||'Y' && ingeschrevenVoor2000 < 2000
    }
}