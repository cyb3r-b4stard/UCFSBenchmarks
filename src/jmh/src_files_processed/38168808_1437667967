 */
public class test
{
  
    int[] primeNumbers = new int[5]; 
primeNumbers[0] = 2 ;
primeNumbers[1] = 3;
primeNumbers[2] = 5;
primeNumbers[3] = 7;
primeNumbers[4] = 11;

int product = 1;
for(int i=0; i<primeNumbers.length;i++){
product = product*primeNumbers[i];
}
System.out.println("The product:" + product);
    }
}