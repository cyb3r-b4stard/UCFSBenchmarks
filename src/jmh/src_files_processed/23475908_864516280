
/**
##
######
*/
public class Remote
{
    private int remote;
    private int pressButton;
    private int aSwitch;
    public Remote()
    {
        remote = 0;
    }
    public void pressButton()
    {
        pressButton = 1;
        remote = pressButton; 
        aSwitch = remote ;
    }
    public void attach(Switch aSwitch)
    {
        return attach(Switch aSwitch);
    }
}