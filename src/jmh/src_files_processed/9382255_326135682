public class Aluno{ 
  private String nome;
  private double p1,p2,p3,mt,tf;
  private int matricula;
  
  public Aluno(String n, int m){
      nome = n;
      matricula = m;
      p1 = 0;
      p2 = 0;
      p3 = mt = tf = 0;
    }
  public void setNome(String n){
      nome = n;
    }
  public String toString(){
     return "Nome do Aluno:" + nome + "\nMatricula do Aluno:"+ matricula + "\nP1:" + p1 + "\nP2" + p2 + "\nP3:" + p3 + "\nMÃ©dia dos Trabalhos:" + mt + "\nTrabalho Final:"+tf;  
     
         }
  public void setMatricula(int m){
      matricula = m;
    }
  public int getMatricula(){
      return matricula;
    }
    public void setp1(double p){
      p1 = p;  
  }
   public double getp1(){
    return  p1;  }
    public void setp2(double p){
      p2 = p;  
  }
  public double getp2(){
      return  p2  
    }
  public void setp3(double p){
      p3 = p;  
  }
  public double getp3(){
    return p3
  }
  
  
 
}