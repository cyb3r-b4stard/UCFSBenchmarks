        int chiffre1 = Integer.parseInt(input);
    input=JOptionPane.showInputDialog("Entrez le chiffre 2");
        int chiffre2 = Integer.parseInt(input);
    if(chiffre1<chiffre2)
        System.out.println("Le chiffre le plus petit est le chiffre 1");
    else
        System.out.println("Le chiffre le plus petit est le chiffre 2");
    }
