import java.util.Scanner;
/**
 * ##### # ########### ## ##### ###### ####.
 * 
 * @###### (#### ####) 
 * @####### (# ####### ###### ## # ####)
 */
public class Drills
{
    public static void main (String [] args){
        System.out.println("\f");  
        
        Scanner keyboard = new Scanner(System.in);
        int score;
        
        System.out.print("Enter a test score ");
        score = keyboard.nextInt();
               
        System.out.println("A) Conditional Execution");
        if(score>=65)
        {
            System.out.println("pass");
        }
        
        System.out.println("B) Alternative Execution");
        if(score>=65)
        {
            System.out.println("pass");
        }
        else
        {
            System.out.println("fail");
        }
        

        
        System.out.println("C) Chained Conditionals");
        if(score>=90)
        {
            System.out.println("A");
        }
        else if(score>=80)
        {
            System.out.println("B");
        }
        else if(score>=65)
        {
            System.out.println("C");
        }
        else if(score>=50)
        {
            System.out.println("D");
        }
        else
        {
            System.out.println("F");
        }
       
        
        
        
        System.out.println("D) Nested Conditionals");        
        
        System.out.print("   What is your age?");
        int age = keyboard.nextInt();
        
        if(age>12)
        {
            if(age<20)
            {
                System.out.println("Teenager");
            }
        }
        else
        {
            System.out.println("Not A Teenager");
        }

        
        payCheck( 40, 7.75);         
        
        payCheck( 20, 9.25);         
        
        payCheck( 50, 12.50);        
                                     
        payCheck( 45, 15.00);        

                                     
        
    }  
    public static void pay(double hours, double rate)
    {
        if(hours<=40)
        {
            return hours*rate;
        }
        else
        {
            return 
        }
    }
}