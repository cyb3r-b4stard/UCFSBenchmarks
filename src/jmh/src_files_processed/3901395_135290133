   
c class Test
{
public static void main(String[] args){
byte[] bajty={(byte)'b', (byte)'d', (byte)'e', (byte)'n', (byte)'v', (byte)'e', (byte)'s'};
String jmenoMesta= new String(bajty);
String velkaPismena = jmenoMesta.toUpperCase();
System.out.println("Porovnani pole "+ jmenoMesta.equals(velkaPismena));
System.out.println("Vypise posledni 4 pismena " + jmenoMesta.substring(jmenoMesta.length()-4));
String krava= "kravarka";
System.out.println(krava);
System.out.println("druha pozice: "+" " + krava.charAt(3));

}
}