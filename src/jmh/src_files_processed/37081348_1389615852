public class rainbow
{
    int ch;char rc;
public void method()
{
    switch(ch)
    {
        case 1:
        System.out.println("Enter the colours");
        if(rc="violet"||rc="indigo"||rc="blue"||rc="green"||rc="yellow"||rc="orange"||rc="red")
        System.out.println(rc+"is a rain bow colour");
        else
        System.out.println(rc+"is not a rainbow colour");
        break;
    }
}
}