import java.util.ArrayList;

/**
 * ################ ## ##########
 */
public class Collections
{
    /**
     * Berechnet den Minimalwert eines Arrays
     * 
     * @param array Array mit Zahlen
     * @return Minimalwert des Arrays oder 42, falls das Array leer ist
     */
    public static int min(final int[] array)
    {
        int min = Integer.VALUE_MAX;
        if(array == null || array.lengh = 0){
            for(int i : array){
                if( i < min){
                    min = i;
                }
                return min;
                            }
        }
        return 42;
   }
    
    /**
     * Berechnet den Durchschnittswert eines Arrays
     * 
     * @param array Array mit ganzen Zahlen
     * @return Durchschnittswert als Fließkommazahl oder 42, falls das Array leer ist
     */
    public static double average(final int[] array)
    {
        return 42.0;
    }
    
    /**
     * Verkettet alle Strings einer Liste
     * 
     * @param list Liste mit Strings
     * @return String, welcher alle Strings der Liste aneinandergehängt enthält
     */
    public static String concat(final ArrayList<String> list)
    {
        return "";
    }
    
    /**
     * Wandelt ein Array mit Zahlen in eine Zeichenkette um
     * 
     * @param Array mit Zahlen
     * @return mit Semikolon getrennte Stringdarstellung der Zahlen
     */
    public static String arrayToString(final double[] array)
    {
        return "";
    }
}