import comp102x.IO;

public class Lab02
{
    
    public static void multiply()
    {
        int x,y,calculated_result;
        IO.output("Enter integer, x: ");
        x = IO.inputInt( );
        IO.output("Enter integer, y: ");
        y = IO.inputInt( ); 
        calculated_result=x*y;        
        IO.output("Answer = " + calculated_result);
        
    }
    
    public static void calculateTriangleArea()
    {
        double width,height,calculated_area;
        IO.output("Enter width of triangle: ");
        width = IO.inputInt( );
        IO.output("Enter height of triangle: ");
        height = IO.inputInt( );        
        calculated_area=(width*height)/2;
        IO.output("The triangle area = " + calculated_area);
        
    }
    
    public static void solveQuadraticEquation()
    {
        int a,b,c;
        double first_x,second_x;
        IO.output("Enter a: ");
        a = IO.inputInt( );        
        IO.output("Enter b: ");
        b = IO.inputInt( );         
        IO.output("Enter c: ");
        c = IO.inputInt( );         
        
        first_x=(-b+math.sqrt(b^2-4*a*c))/(2*a)
    }
}