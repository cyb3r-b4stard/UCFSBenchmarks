import java.util.*;
public class Sandwich
{
    private ArrayList<String>  food = new ArrayList<String>();
    
    public Sandwich()
    {
        food.clear();
    }
    
    public void add(String item)
    {
        food.add(item);
    }
    
    public void eat()
    {
        food.clear();
    }
    
    public String toString()
    {
        String final = "[";
        for (int i = 0; i < food.size()-2; i++)
        {
            final += food(i);
            final += ", "
        }
        return final;
    }
}