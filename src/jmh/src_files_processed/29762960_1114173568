 * ##### # ########### ## ##### ###### ####.
 * 
 * @###### (#### ####) 
 * @####### (# ####### ###### ## # ####)
 */
public class Tester
{
    public static void main(String args[])
    {
        Garage myGarage = new Garage(2, 2);
        Address myAddress = new Address("Meade", "Appleton", "Wisconsin", "54911");
        House myHouse = new House(myAddress, myGarage, 1000000);
        House newHouse = copyHouse(myHouse);
        Garage testGarage = new Garage(5, 5);
        myHouse.setGarage(testGarage);
        printTestCopy(newHouse, myHouse);
    }
    
    public static void printTestCopy(House newHouse, House myHouse)
    {
        System.out.println(myHouse.getGarage());
        System.out,println(newHouse.getGarage().getSize() + newHouse.getGarage().getDoors());
    }
    
    public static void printTestG(Garage myGarage)
    {
        int size = myGarage.getSize();
        int doors = myGarage.getDoors();
        System.out.println(size);
        System.out.println(doors);
        myGarage.setSize(20);
        myGarage.setDoors(10);
        size = myGarage.getSize();
        doors = myGarage.getDoors();
        System.out.println(size);
        System.out.println(doors);
    }
    
    public static House copyHouse(House myHouse)
    {
        House newHouse;
        Garage newGarage = 
        new Garage(myHouse.getGarage().getSize(), myHouse.getGarage().getDoors());
        Address newAddress = 
        new Address(myHouse.getAddress().getStreet(), myHouse.getAddress().getCity(),
        myHouse.getAddress().getState(), myHouse.getAddress().getZip());
        double newPrice = myHouse.getPrice();
        newHouse = new House(newAddress, newGarage, newPrice);
        return newHouse;
    }
}