
/**
 * ##### # ########### ## ##### ### ####.
 * 
 * @###### (#### ####) 
 * @####### (# ####### ###### ## # ####)
 */


public class max
{
    public static int max(int x, int y, int z)
    {
   If (x>y, x>=z)
   {return x;}
If (y>=x, y>=z) {return y;}
If (z>=x, z>=y) {return z;}

        



}

}