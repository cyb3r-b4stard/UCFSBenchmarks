class NestedSeries6
{
    public void generate(int n )
    {
        float sum=0, term, sign=1;
        int x, j, i, fact ;
        
        for(i=0; i<=n ; i+=2)
        {
            fact=1;
            for(j=0; j<=n ; j+=2)
            fact *= j ;
            term = ((float)Math.pow(x,i)/fact))*sign;
            sum += term;
            
            System.out.println(i==1 ? term: "+"+term);
        }
    }
}

            