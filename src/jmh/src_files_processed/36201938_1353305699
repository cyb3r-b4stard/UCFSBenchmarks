public class Robot {
    public static void main (String args[] ) {
        
    }
    
    public static void speak() {
        
        System.out.println("greeting. I am your personal robot. My name is <name>");
        System.out.println("My serial # is : " + 23432343);
        
    }
    
    public static void dance() {
        
        System.out.println("Dancing.....Do you like my moves?");
        
    }
    
    public static void spin() {
        
        System.out.println("Watch me spin around " + 10 + " times.");
        
    }
    
    public static void cook() {
        
        System.out.println("Cooking your favorite foods!");
        
    }
    
    public static void beep() {
        
        System.out.println("Beep.")
        
    }
    
}    