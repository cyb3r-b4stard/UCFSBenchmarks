public class III_Aufg1a
{
    public static void main (String[] args)
    {
        Console cons = new Console();
        int zahl1, zahl2;
        zahl1 = cons.readInt("Bitte die erste Zahl ein: ");
        zahl2 = cons.readInt("und nun bitte die zweite Zahl eingeben: ");
        System.out.println("Der Buchstabe für die erste eingegebene Zahl entspricht: " + (char) zahl1 + " und für die zweite Zahl: " + (char) zahl2););
    }    
}