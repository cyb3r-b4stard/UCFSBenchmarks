
import java.util.*;
public class Q2
{
       int SID,N,GR,SQ;
       double IELTS;
       void Input()
       {
           Scanner sc=new Scanner(System.in);
       System.out.println("Enter the value sid:");
       SID=sc.nextInt();
       System.out.println("Enter the value n:");
       N=sc.nextInt();
       System.out.println("Enter the value gr:");
       GR=sc.nextInt();
       System.out.println("Enter the value ielts:");
       IELTS=sc.nextDouble();
    }
    void calculate()
    {
       if (gr>=2.5||ielts>=4.0)
       IELTS=Advance Diploma;
       else
      IELTS=No Advance Diploma;
      
    }
    void disblay()
    {
      System.out.println("Enter the value sid="+SID);
      System.out.println("Enter the value n="+N);
      System.out.println("Enter the value gr="+GR);
    }
     public static void main (String arge[])
    {
        Q2 s1 = new Q2();
        s1.Input();
