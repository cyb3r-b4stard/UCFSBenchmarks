import java.util.Scanner;
public class Hlavny {
    public static void main(String[]args) {
      Scanner klav=new Scanner(System.in);
      
      String aPriezvisko;
      int aRokNarodenia;
      int aSucasnyRok
      
      System.out.pritln("Zadajte priezvisko muzikanta: ");
      aPriezvisko=klav.nextLine();
      System.out.println("Zadajte rok narodenia: ");
      aRokNarodenia=klav.nextInt();
      
      Muzikant hrat=new Muzikant(aPriezvisko, aRokNarodenia);
      System.out.println(hrat);
      
    System.out.println("Zadajte sucasny rok: ");
    aSucasnyRok=klav.nextInt();
    
    hrat.ciMozeHrat(aSucasnyRok);
    if (hrat.ciMozeHrat(aSucasnyRok)==true)
    {
        System.out.println("Muzikant moze hrat");
    }
    else {System.out.println("Muzikant hrat nemoze");}
    }
}