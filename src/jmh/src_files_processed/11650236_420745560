
public class verifierMul
{
   public static boolean verifierMul(int[] L, int n);
{
     int taille = L.length;
     int i;
    for(i=0; i<taille; i++)
        {
	if (L[i]%n==0)
	{
		    System.out.println(" L[i] est un multiple de n");
	   return true;
      }
	
 else 
	  return false;


    }

  
    }
