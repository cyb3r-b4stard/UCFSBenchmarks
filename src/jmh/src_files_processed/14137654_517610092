public class A
{
    private int_x;
    
    public A(int x) {
    _x = x;
    }
    
    public int getX() {
    return _x;
    }
    
    public int doubleX() {
    return 2*getX();
    }
  
    public int tripelX() {
    return 3*_x;
    }
 
    public int subXhelperX() {
    return _x-1;
    }
    
    public int subX() {
    return subXhelper();
    }
    
}