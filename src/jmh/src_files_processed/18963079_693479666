

public class biblioteca
{
    public string void main 
    private string nombre; 
    private int edad;
    
    
    public string ();
    
    this.nombre= nombre;
    this.edad= edad;
    
    public setNombre (string Nombre)
    {
        this.nombre= nombre;
        
    }
    public getNombre(string nombre)
    {
        return.nombre = nombre;
    }
    
    public setEdad (String edad)
    {
        this.edad = edad;
    }
    public getEdad (String edad)
    {
        return.edad = edad;
        
    }
}