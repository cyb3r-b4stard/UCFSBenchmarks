public class TrianglePattern
{
    public static void main(String[] args)
    {
        int numberOfRows = 5;
        
        for ( int rows = 0; rows < numberOfRows; rows++)
        {
            for ( int columns = 0; columns < =rows; columns++)
            {
                System.out.print("[]");
            }
            System.out.println();
        }
    }
}
   