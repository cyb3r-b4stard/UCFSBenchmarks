 */

import gpdraw.*;
public class DrawHouse{
    private DrawingTool myPencil;
    private SketchPad myPaper;

    public DrawHouse(){
        myPaper = new SketchPad(500,500);  
        myPencil = new DrawingTool(myPaper);
    }   
    
       public void draw(){
          myPencil.turnRight(90);
           myPencil.forward(200);
           myPencil.turnLeft (90);
           myPencil.forward (100);
           myPencil.turnLeft(90);
           myPencil.forward (200);
           myPencil.turnLeft(90);
           myPencil.forward (100);
           myPencil.turn (180);
           myPencil.forward (100);
           
           
          myPencil.setDirection(45);
          myPencil.forward (150);
          myPencil.turnRight (45);
          myPencil.setDirection (311);
          myPencil.forward (145);
          
         myPencil.up();
         myPencil.move (0,0);
         myPencil.down();
         myPencil.setDirection (90);
         myPencil.turnRight (45);
         myPencil.forward(35)
         myPencil.turnLeft(45);
         myPencil.forward(30);
          
      
          
          
         
         
           
           
           
        
        
    }
}