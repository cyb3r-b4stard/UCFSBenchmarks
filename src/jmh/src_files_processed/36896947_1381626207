public Object head()
{
    if(isEmpty())
    {
        return  top.gibInhalt();
    }else {
      return null;
    }
}
public enqueue(Object pObject)
{
    Koffer neuer = new Koffer(pObject)
    neuer.setzeNachfolger(tail);
    tail = neuer;
}

public dequeue()
{
    top = top.gibNachfolger;
  }
}