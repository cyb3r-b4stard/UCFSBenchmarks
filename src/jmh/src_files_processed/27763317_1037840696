

public class Name
{
    private String vorname;
    private String nachname;
    
    public Name (String vorname,String nachname)
    {
       this.vorname=vorname;
       this.nachname=nachname;
    }
    
    public String getInitialen()
    {
        String v = vorname.substring(0,1);
        
        String n = nachname.substring(0,1);
        return v+n;
    }
    
    public String getAccount()
    {
        if(nachname<5)
        {
            String n = int.length();
        }
        String n = nachname.substring(0,5);
        String v = vorname.substring (0,4);
        return n+v;
    }
    
}

    
