import basis.*;
import java.math.*;

public class SchaltjahrRechner
{
    int x;
    boolean Schaltjahr;
    public SchaltjahrRechner(int x)
    {
        if (x%4==0)
        {
            Schaltjahr=false;
        }
        else{
            if (x%100==0)
            {
                x%400==0 ; Schaltjahr = true
            }
                else 
                {
                    Schaltjahr = false
                }
        }
        
    }

   
    
}