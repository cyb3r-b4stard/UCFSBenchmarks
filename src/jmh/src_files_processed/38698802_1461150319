public class P3H
{
    public static void main (String [] args)
    {
         System.out.println("N \t" +  "N Squared \t" + "Square Root of N \t" + "N Cubed \t"
         + "N to the 1/3");
        for (int N = 1; N<= 46; N = N + 5)
        
       {
          System.out.println(N +"\t" + Math.pow(N, 2)+ "\t" + Math.sqrt(N)+ "\t" + Math.pow(N,3)
          + "\t" Math.pow(N,1/3));
           
          

        
        }



    }




}