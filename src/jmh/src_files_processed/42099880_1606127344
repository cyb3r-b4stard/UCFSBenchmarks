import java.util.Scanner;
public class calculator { 
  public static void main(String[] args) {
    Scanner in = new Scanner(System.in);
    System.out.println("Wprowadź zmienną");
    int first = in.nextInt();
    System.out.println("Wprowadż znak");
    String answer = in.next();
    System.out.println("Wprowadż drugiej zmienną");
    int second = in.nextInt();
    in.close();
    System.out.println("odpowiedź" + ":" Calc.metodCalc(first,second,answer));
}
}
