public class Factorial
{
        private long factorial,numeroEntero;
        public Factorial(){
            this.factorial=0;
            this.numeroEntero=0;
        }
        public Factorial(long factorial){
        this.factorial=factorial;
        this.numeroEntero=numeroEntero;
    }
        
        
        public void setFactorial(long factorial){
                    this.factorial=factorial;
                }
        public void setNumeroEntero(long numeroEntero){
                    this.numeroEntero=numeroEntero;
                }
        public long getFactorial(){
                    for(long i=numeroEntero;i>=1;i--){
                        factorial=(i*=);
                        System.out.println(factorial);
                    }
                    return 0;
                }
}