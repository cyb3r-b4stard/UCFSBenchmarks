
/**
 * ##### # ########### ## ##### ####### ####.
 * 
 * @#####
 * @####### #
 */
public class Persona
{
    private String name;
    private double age;
    private String phone;
    /**
     * Constructor for objects of class Persona
     */
    public Persona()
    {
       
      setName("pepe") ;
      setAge (18);
      setPhone ("1234");
    }

    /**
     * Metodo Para Introducir nombre
     * 
     * @param  String para newName
     * 
     */
    public void  setName(String newName)
    {
        name = newName;
    }
    
    /**
     * Método para devolver nombre
     *
     * 
     * @return   el valor del atributo name
     */
    public String getName()
    {
        return name;
    }
    
    /**
     * Metodo Para Introducir edad
     * 
     * @param  double para edad
     * 
     */
    public void  setAge(double newAge)
    {
        age = newAge;
    }
    
     /**
     * Método para devolver edad
     *
     * 
     * @return   el valor del atributo edad
     */
    public double getAge()
    {
        return age;
    }
    
    /**
     * Metodo Para Introducir telefono
     * 
     * @param  String para newphone
     * 
     */
    public void  setPhone(String newPhone)
    {
        phone = newPhone;
    }
    
     /**
     * Método para devolver phone
     *
     * 
     * @return   el valor del atributo phone
     */
    public String getPhone()
    {
        return phone;
    }
    
    /**
     * Método para introducir peso
     * 
     * @param double newWeight
     */
    public void setWeight ( double newWeight)
    {weight = newWeight
    
    }