package BaldursGate;

import EspacioDeEstados.Estado;
import java.util.List;

/**
 * ##### # ########### ## ##### ########### ####.
 * 
 * @###### (#### ####) 
 * @####### (# ####### ###### ## # ####)
 */
public class BaldursGate extends Estado<BaldursGate>
{
    private int x;

    /**
     * Constructor for objects of class BaldursGate
     */
    public BaldursGate()
    {
        x = 0;
    }

    @Override
    public List<int> hijos()
    {
        List<int> lista = null;

        return lista;
    }

    @Override
    public int coste(Object e)
    {
       
        return 0;
    }
    
    
    @Override
    public boolean equals(Object e)
    {
       
        return e instanceof Jarras && this.jg == ((Jarras) e).jg && this.jp == ((Jarras)e).jp;
    }
    
    @Override
    public String toString()
    {
        return Integer.toString(this.jg) + " " + Integer.toString(this.jp);
    }
}