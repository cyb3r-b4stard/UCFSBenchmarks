import java.util.Scanner;
import java.io.IOException;
import java.io.File;
public class AnnualClimate1
{
    public static void main(String[]args) throws IOException
    {
        Scanner in = new Scanner (System.in);
       
        System.out.println("\f");
        
        System.out.println();
        System.out.println("                       Climate Data");
        System.out.println("             Location: West Palm Beach, Florida");
        System.out.println();
        System.out.println("       Month      Temperature (F)      Precipitaion (in.)");
        System.out.println("       **************************************************");  
        
        int months = 0;
        String [] monthsOfYear = new String[13];
        File fileName = new File ("Months.txt"); 
        Scanner inFile = new Scanner(fileName);
        while (inFile.hasNext())
        {
            monthsOfYear[months] = inFile.next();
            months++;
        }
        for(int m =0; m <= 11; m++)
        {
        }
        System.out.printf(%d%n, monthsOfYear[m]);
        inFile.close();
        
        int temperature2 = 0;
        String [ ] temperature = new String[13];
        File fileName1 = new File ("Temperature.txt");
        Scanner inFile1 = new Scanner(fileName1);
        
        while (inFile1.hasNext())
        {
            temperature[temperature2] = inFile1.next();
            temperature2++;
            
        }
        for(int t = 0; t <= 11; t++)
        {
        }
        inFile1.close();
        
        int precipitation2 = 0;
        String [ ] precipitation = new String[13];
        File fileName2 = new File ("Precipitation.txt");
        Scanner inFile2 =  new Scanner(fileName2);
        
       while(inFile2.hasNext())
       { 
           precipitation[precipitation2] = inFile2.next();
           precipitation2++;
        }
       for(int p = 0; p <= 11; p++)
       {
        }
       inFile2.close();
        
       

}
    
    
}