 */
public class include
{
     public boolean isAvailable(String haystack, String needle)
    {
        c=needle.length();
        b=haystack.length();
        a=0
        m=haystack.substring(a,c);
        if(a<=b)
        {
            while(needle.equalsIgnoreCase(m))
        }
    }
}