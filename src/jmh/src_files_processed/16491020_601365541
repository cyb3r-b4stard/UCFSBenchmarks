
import inout.Console;


public class CD_Sammlung
{
 private static final int lager = 10;
 private static String[][][] name = new String [lager][1][1];
 private static int anzahl = 0;
 private static int ende = 0;
 private static int anfang = 0;
 
    public static void main (String args[])
    {
     int element;
     char auswahl; 
     int nummer = 1;
     do
     {
      System.out.println
       ("Funktionen: \nEinf√ºgen = 1 \nEntfernen = 2 \nAnzeigen = 3 \nEnde = 9\n");
      auswahl = Console.readChar();
      if (auswahl == '9') break; 
      switch (auswahl) 
      {  
        case '1': System.out.print("Geben Sie eine CD ein"); 
                name[1][0][0] = {{1}, {Console.readString()}, {Console.readString()}};
                einfuegen(element);
                break; 
        case '2': System.out.print("Entferntes Element: "); 
                element = entfernen();
                System.out.println(element);
                break;
        case '3': System.out.println
                  ("Inhalt der Warteschlange:");
                System.out.print("Anfang ");
                for (int i = anfang; i < (anfang + anzahl); i++)
                {
                   int Inhalt = warteschlange[i % LAENGE];
                   System.out.print(Inhalt + " ");
                }
                System.out.println(" Ende");
                break;
         default: System.out.println
                       ("Auswahl wurde abgebrochen");   
       } 
      
      } while ( auswahl >= '1' && auswahl <='3');
   }
   public static void einfuegen(int element)
    { 
        if (anzahl == LAENGE)
            System.out.println("STOP!");
            
        else
        {
            warteschlange[ende % LAENGE] = element;
            anzahl++;
            ende = (ende + 1) % LAENGE;
        }
    }}
 