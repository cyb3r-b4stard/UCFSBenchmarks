 * @###### ####### ####
 * @####### ##.##.####
 */
public class TestEnum2{
enum TipoDeMadera(ROBLE,CAOBA,NOGAL,CEREZO,BOJ);
    public static void main(String[]Args){
        TipoDeMadera miTipoDeMadera=TipoDeMadera.CAOBA;
        System.out.println("Los posibles tipos de madera son: ");
        for(TiposDeMadera tmp:miTipoDeMadera.values()){
            System.out.println(tmp.toString()+ "\t");}
    }
}
