import java.util.*;
/**
 * ##### # ########### ## ##### ######### ####.
 * 
 * @###### (#### ####) 
 * @####### (# ####### ###### ## # ####)
 */
public class MensaTest
{
    private String name;
    private ArrayList<Geek> geeks;
    private ArrayList<Geek> clever;

    public MensaTest(String name){
        this.name = name;
        geeks = new ArrayList<Geek>();
    }
    
    public void add(Geek g){
        geeks.add(g);
    }
    
    public void remove(Geek g){
        geeks.remove(g);
    }
    
    public Geek mostGeeky(){
        Geek geeky = null;
        for (Geek g : geeks){
            if(geeky == null || geeky.getIq() < g.getIq()){
                geeky = g;
            }
        }
        return geeky;
    }
    
    public ArrayList<Geek> highestIQ(int threshold){
        clever = new ArrayList<Geek>();
        for (Geek g : geeks){
            if(g.getIq() => threshold){
                clever.add(g);
            }
        }
        return clever;
    }
    
    

}