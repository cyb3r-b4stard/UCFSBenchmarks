public class helloworld extends ibio
{
    public void  helloworlds ()
    {
        int age;
        output("Hello world");
        age = inputInt ("Please enter your age");
        if (age < 40) {
            output("You are young");}
        else if (age < 50)  {
            output ("you are the best age");}
        else {
            output ("you are really old!");}
            
            
    }   

   
    
    public void multiplication ()
    {
        int timestable;
        timestable = inputInt ("Please enter a number");
        for (int count = 1; count < 11; count = count + 1) {
            output (count + " * " + timestable + " = " + count * timestable);}}
           
     
            
        
    
    
        
    
    public void adventure()
    {
        int choice;
        output ("You wake up next to a candle and desk");
       
        output("You hear a creak");
        output("What do you want to do?");
        output("1, Leave the room 2, Take the candle 3, Explore the rest of the room");
        choice=inputInt();
        if (choice == 1)
        Hallway();
        else if (choice == 2)
        Candle();
        else if (choice == 3)
        Explore();}
        
   public void libary()
        
        
        
        {
