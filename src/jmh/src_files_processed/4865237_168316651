import java.util.Random;
/**
 * ##### # ########### ## ##### ######### ####.
 * 
 * @###### (#### ####) 
 * @####### (# ####### ###### ## # ####)
 */
public class Generator
{
   private Random random;
   
   public Generator(){
    random = new Random();
    }
    
    public int hazim(){
        for (int i=0;i++;i<4){
   return random.nextInt(10);
}
    }
}