public class MarioGame
{
    private in money;
    private int lives;

public MarioGame()
{
    money = 0;
    lives = 3;
}
public int getMoney()
{
    return money;
}
public int getLives()
{
    return = lives;
}
 public int addCoin(String c)
 {
     if (c == "Gold")
     {
         return = Money + 1000;
     }
     if (c == "Silver")
     {
         return = Money + 500;
     }
     if (c == "Copper")
     {
         return = Money + 1;
     }

     