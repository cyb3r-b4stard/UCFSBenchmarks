/**
 * #### ###### ## # ##### ######## ######_#.####
 * 
 * ###### ### ###### ###### ## ##### #### ### ######## ########## ## ### ###### ########.
 * 
 */

public class StringMachine{
   public int length (String s) 
   {                                                 
      int charCount;                                                               
      charCount = 0;                                                               
      while (!s.equals("")) 
      { 
        if (s.substring(charCount) != " ")
        {
            charCount = charCount + 1;                                                 
        }
        else
        {
            charCount += 1;
        }   
      } 
      return charCount
   }                                                                               
}
