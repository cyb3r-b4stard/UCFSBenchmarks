import java.util.*;
import java.io.*;


public class CartaoCliente
{
    private String Nome;
    private String codigo;
    private int pontos;
    private double preco;
    
    public CartaoCliente(){
        this.nome = "";
        this.codigo = "";
        this.pontos = 0;
        this.preco  = 0.0;
    }
    
    
    public CartaoCLiente(private String Nome, private String codigo,private int pontos,private double preco){
        this.nome = nome;
        this.codigo = codigo;
        this.pontos = pontos;
        this.preco  = preco;
    }
    
    public CartaoCliente(CartaoCliente c){
        this.nome = getNome;
        this.codigo = getCodigo;
        this.pontos = getPontos;
        this.preco  = getPreco;
    }
    
    
    
    
    
}