public class GameUtil
{
    priavte int zufallszahl;
        
    int [] randomize(int length, int valuerange) {
        for (int i, i < length, i++) {
            zufallszahl = (int)(Math.random() * valuerange); 
    int[i] = zufallszahl;
    return int[i];
}
}