 */
import java.util.Scanner;
public class FreeFallingObject{
    public static void final double g=-9.8m/s^2;{
        Scanner console = new Scanner(System.in);
        
        double D, sec, t;
        
        System.out.println("Free Falling Object");
        
        System.out.print("Enter time in minutes: ");
        t= console.nextDouble();
        
        sec= t*60;
        D = 1/2gt^2;
        
        System.out.print("An object that is dropped freely within " + t + " minutes " + " has a distance of " + D + " meters.");
        
        System.exit(0);
    }
}