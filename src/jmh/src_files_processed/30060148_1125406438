import java.io.*;
public class sujoy1
{
    public static void main(String[]args)throws IOException
    {
        InputStreamReader in=new InputStreamReader(System.in);
        BufferedReader out=new BufferedReader(in);
        double a;
        System.out.println("1.for Ground floore");
        System.out.println("2.for First floore");
        System.out.println("3.for Second floore");
        System.out.println("4.for Third floore");
        System.out.println("Enter your choice(1,2,3,4)");
        a=Double.parse.Double(out.readLine());
Switch(a)
        {
            case1:
            System.out.println("Child's wear");
            break;
            case2:
            System.out.println("Men's wear");
            break;
            case3:
            System.out.println("Women's wear");
            break;
            case4:
            System.out.println("Toy's store");
            break;
            default:
            System.out.println("Wrong choice");
        }
    }
}