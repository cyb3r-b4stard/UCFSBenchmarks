import gpdraw.*; 

public class Test 
{ 
    private DrawingTool myPencil; 
    private SketchPad myPaper; 
    
    public Test() {
        myPaper = new SketchPad (300, 300); 
        myPencil = new DrawingTool (myPaper);
    }
    
    public void draw(){
        myPencil.forward(100); 
        myPencil.turnLeft(0); 
        myPencil.forward(100);
        myPencil.turnLeft(0);
        myPencil.forward(100);
        myPencil.turnLeft(0);
        myPencil.forward(100);
        
    }
        
    
    public void(){
        myPencil.forward(100); 
        myPencil.turnLeft(0); 
        myPencil.forward(100);
        myPencil.turnLeft(0);
        myPencil.forward(100);
        myPencil.turnLeft(0);
        myPencil.forward(100);
        
    }
        
   
}