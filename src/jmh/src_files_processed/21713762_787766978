import sum.kern.*;
/**
*
*/
public class Dartscheibe 
{
  /* Attribute */
    private int xPos;
    private int punkte;
    private int yPos;
    private int radius;
      
    Bildschirm bildschi;
    Buntstift bunti;
    Maus MickyMaus;
  /* Methoden */
  /**  */
  public Dartscheibe()
  {
      bildschi = new Bildschirm();
      MickyMaus = new Maus();
      bunti = new Buntstift();
      xPos = 800;
      yPos = 200;
      
  }
  public void berechnePunktzahl(){
  }
  /**  */
  public boolean istTreffer(int  xPosPfeil,int  yPosPfeil){
  }
  /**  */
  public void spiel(){
      
  }
  /**  */
  public void bewegePfeil(){
  }
  /**  */

  /**  */
  public void zeichne(){
      bunti.bewegeBis(xPos,yPos);
      bunti.setzteFarbe(Farbe.ROT);
      bunti.runter;
      bunti.zeichneKreis(radius);
      bunti.zeichneKreis(radius-5);
      bunti.zeichneKreis(radius-10);
      bunti.hoch;
      
  }
}
