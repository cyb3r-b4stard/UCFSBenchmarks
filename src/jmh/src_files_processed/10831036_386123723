import java.util.Scanner;

public class Funcionário{
    public static void main (String args[]){
           Scanner ler=new Scanner(System.in);
           double VHT,QH,PC,TVH,RES;
           String NOME;
    
  
    System.out.println("Informe o nome");
    NOME=ler.nextString();
    
    System.out.println("Informe o valor de horas trabalhadas");
    VHT=ler.next();
    
    System.out.println("Informe o quantidades de horas trabalhadas");
    QH=ler.next();
    
    System.out.println("Informe o percentual de desconto");
    PC=ler.next();
    
      public double multipicar(){
            TVH=VHT*QH
        }
        public double multiplicar(){
            RES=TVH*PC
        }
        
        System.out.println(NOME+"possui salário bruton de"+ TVH +" e salário líquido de"+ RES)
    }
    
          
            }

    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
   
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    } 
} 
  
