/import java.util.Scanner;           

public class GettingInput {
    
    public static void main(String[] iLikeTurtles) {
        
        int x;
        Scanner readThing;
        readThing = new Scanner(System.in);
        
        System.out.println("Input an integer");
        x = readThing.nextInt();
        
        System.out.println("You Typed: " + x);
        
    }
    
} 
