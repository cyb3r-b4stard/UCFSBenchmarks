import java.util.*;
/**
 * ########## ### #### #### ## ### ###### #### ####.
 * 
 * @###### ###### #####, ##### #########. 
 * @####### ##.##.##
 */
public class Data
{
   private int _base;
   private int _height;
   private int _vol;
   
   public Data()
   {
       this._base=0;
       this._height=0;
       this _vol=0;
   }
   
   public Data(int b, int h)
   {
       this._base=b;
       this._height=h;
       this._vol=h*b;
   }
}