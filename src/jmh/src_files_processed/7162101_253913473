 */
public class HourDisplay extends NumberDisplay
{
    
    private boolean mode12hr;
    /**
     * Constructor for objects of class HourDisplay
     */
    public HourDisplay(int rollOverLimit, NumberDisplay next,boolean mode12,)
    {  super(int rollOverLimit, NumberDisplay next);
       mode12hr = true;
    }

    
}