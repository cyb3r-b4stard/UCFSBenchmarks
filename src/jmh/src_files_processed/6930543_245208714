public class ReadWriteFile
{
    public void readFile()
    {
        object obj = new Object();
    try
    {
     FileReader fileStream = new FileReader(getFileName());
     BufferedReader fileBuffer = new BufferedReader(fileStream);
     String inLine;
     String outLine = "";
     while((inLine = new fileBuffer.readLine()) != null)
     {
         inLine = fileBuffer.readLine();
         String delims = "[,]";
         String[] tokens = line.split(delims);
         if (tokens.length == 4)
         {
             Salaried sal = new Salaried();
             sal.setFirstName(tokens[0]);
             sal.setLastName(tokens[1]);
             sal.setSSN(tokens[2]);
             sal.setSalary(Double.parseDouble(tokens[3]));
             obj = (Salaried)sal;
            }
            else if (tokens.length == 5)
            {
             FstName;
             lastName;
             employeeID;
             hourlyWage;
             hoursWorked;
            }
            else if (tokens.length == 6)
            {
             firstName;
             lastName;
             employeeID;
             salary;
             bonus;
             executiveType;
            }
}
fileBuffer.close();
}
catch (IOException e)
{
System.out.println("Could not find file " + getFileName());
}
}
}