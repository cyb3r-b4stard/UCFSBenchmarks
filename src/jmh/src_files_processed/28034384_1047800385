import becker.robots.*;
public class Namensbot extends RobotSE
{
    private String bezeichnung;
    public Namensbot(String bezeichnung,City  aCity, int zeile, int spalte, Direction aDirection)
    {
        super(aCity,zeile,spalte, aDirection);
        this.bezeichnung=bezeichnung
    }
    public String getBezeichnung()
    {
        return bezeichnung;
    }
}