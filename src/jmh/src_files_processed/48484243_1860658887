
public class Rectangle
{
    private int length;
    private int width;
    private int perimeter;

    public void setLen(int Len)
    {
        int lenght = Len;
    }
    
    public void setWid(int Wid)
    {
       int width = Wid
    }

    public void perimeter()
    {
      int perimeter = 2*length + 2*width;
    }

    public void print()
    {
       System.out.println("The perimeter is :: " + perimeter);
    }
}