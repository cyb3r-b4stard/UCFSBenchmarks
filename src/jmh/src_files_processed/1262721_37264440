import java.util.*;
public class Exercice1
{
    public static void main()
    {  
        int nb;
        int paire;
        int i;
        
        Scanner saisieInt=new Scanner(System.in);
        System.out.println("Saisir un nombre");
        nb=saisieInt.nextInt();
        
        for(i=1;i<=nb;i++)
        {
            paire+=2
            System.out.println(paire);
        }
    }
}
