

public class Rating implements Comparable {
    private String movieid;
    private double value;
    
    public Rating(String id, double val)
    {
        movieid = id;
        value = val;
    }
    
    public String getMovieID()
    {
        return movieid;
    }
    
    public double getRatingValue()
    {
        return value;
    }
    
    public String toString()
    {
        return "Movie ID : " + movieid + " - " + value;
    }
    
    public int compareTo(Rating item)
    {
        if (value < item.getValue()) 
        {
            retrun -1;
        }
        if (vale > item.getValue())
        {
            return 1;
        }
        return 0;
    }

}