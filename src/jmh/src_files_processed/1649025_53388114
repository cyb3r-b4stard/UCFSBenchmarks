
/**
 * ##### # ########### ## ##### #####_############# ####.
 * 
 * @###### (#### ####) 
 * @####### (# ####### ###### ## # ####)
 */
public class multiDimentionnelt
{
    public static void main(String[]arg)
    {
        int[][] arrayTal = new int[2][3];
        arrayTal[0][0] = 10;
        arrayTal[0][1] = 12;
        arrayTal[0][2] = 43;
        
        arrayTal[1][0] = 20;
        arrayTal[1][1] = 45;
        arrayTal[1][2] = 56;
        int raekker = 2;
        int kolonner = 3;
        
        int i;
        int j;
        for (i=0; 1 < raekker; i++ ){
            for(j=0; j < kolonner; j++){
                System.out.print(arrayTal[i][j] + " ");
            }
            System.out.println(" ")
        }
}}
