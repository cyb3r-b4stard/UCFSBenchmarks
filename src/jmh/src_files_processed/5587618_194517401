class Counter
{
    long counter;
    public long getValue()
    {
        return counter;
    }

    public void reset()
    {
        counter = 0;
    }

    public void inc()
    {
        counter++;
    }

    public void dec()
    {
        counter--;
    }
    public void add5()
    {
        counter = counter+5;
    }

    public void add10()
    {
        counter = counter+10;
    }

    public void round()
    {
        int rounder = (counter >=0)? 5:-5;
        counter = (counter + rounder) / 10 * 10;
    }
    
    public void Yanes()
    {
        System.Out.Print "Edited by Yanes";
    }
}