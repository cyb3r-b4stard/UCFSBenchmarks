import java.util.*
public class array
{
    public static void main(String args[])
    {
        int i,sum=0;
        Scanner sc=new scanner(System.in)
        int ar[]=new int[10]);
        System.out.print("Enter 10 numbers in the array");
        for(i=0;i<10;i++)
    }
    ar[i]=sc.nextInt();
    sum=sum+ar[i];
}
System.out.print("The sum is"+sum);
}
}