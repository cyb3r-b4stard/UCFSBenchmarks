import java.util.Random;

public class ThrowingADie
{
    public static void main(String[] args)
    {
        Random generator = new Random(42);
        
        int requiredNumber = 6;
        int sides = 6;
        int count = 0;
        
        for(i = 1; i <= 100; i++)
        {
            int number = 1+generator.nextInt(6);
            if(number == requiredNumber)
            {
                count++;
                System.out.println(number);
            }
            else
            {
                System.out.println(number);
            }
        }
        System.out.println();
        System.out.println("The number of "requiredNumber"'s rolled was "count".";
    }
}