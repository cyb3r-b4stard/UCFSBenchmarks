
/**
 * ######## # ###### ###### ####### # ### #.
 * 
 * @######      ### ### 
 * @#######     #.# ####.#.##
 */
public class LuckyGuessGenerator
{
    int maximumNumber = 100;
    
    public int generateRandomNumber( maximumNumber)
    {
        return 1 + (int)(Math.random() * maximumNumber);
    }
}