import java.util.*;

public class q5
{
    public static List<Integer> longest(HashSet<List<Integer>> a)
    {
        if (a==null)
        {
            return List<Integer> b = emptyList();
        }
        else
        {

            int longest= a.get(0).size();

            for(int i=1; a.size();i++)
            {
                if(longest<a.get(i).size())
                {
                    longest = a.get(i).size();
                }
            }

        }

    }
}