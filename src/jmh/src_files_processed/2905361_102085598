import java.io.*;
class numbers
{
    int cube(int n)
    {
        int i;int s;
        {
          for(i=1;i<n;i++)
          s=Math.sqrt(s,3)
          return(s);
        }
        void main()throws IOException
        {
            int p,n;
            DataInputStream in=new DataInputStream(System.in);
            System.out.println("the number is");
            n=integer.parseInt(in.readLine());
            number ob=new number()
            p=ob.cube(n);
            System.out.println("the entered number is");
        }
    }
}
      
            
     
        
