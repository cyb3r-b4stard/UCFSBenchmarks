public class Payroll
{
    String employeeId, employeeFirst, employeeLast;
    private double grossPay, netPay;
    private final double STATE_TAX, FEDERAL_TAX, FICA_TAX;
    
    public Payroll()
    {
        employeeId="unknown";
        employeeFirst="unknown";
        employeeLast="unknown";
        STATE_TAX=.035;
        FEDERAL_TAX=.15;
        FICA_TAX=.065;
        grossPay=netPay=0;
    }
    
    public Payroll (double sTATE_TAX, double fEDERAL_TAX, double fICA_TAX, double grossPay)
    {
        STATE_TAX = sTATE_TAX;
        FEDERAL_TAX = fEDERAL_TAX;
        FICA_TAX=fICA_TAX;
        this.grossPay=grossPay;
        calcNetPay();
    }
    
    public setGrossPay()
    {
        
    }
    
    public getNetPay()
    {
        return netPay;
    }
    
    public getGrossPay()
    {
        return grossPay;
    }
    
    public toString()
    {
        return 
    }
    
    public void calcNetPay()
    {
        this.netPay=this.grossPay
    }
}