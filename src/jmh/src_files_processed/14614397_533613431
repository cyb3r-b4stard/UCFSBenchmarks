public class dicerole{
     int[] tally = {0,0,0,0,0,0};
     int dice
     public dicerole(){
     for (int i =0; i<36000;i++){
         dice = i+(int)(math.random()*6);
         
         tally[dice-1] = tally [dice-1] = 1;
        }
        for (int i=0; i<6; i++){
            system.out.println
            (talley[i]);
        }
    }
}
