import java.util.Scanner;
public class Binominalverteilung {
    public class main extends fakultaet  
    {
        int a = 0, b = 1, c = 0;
        Scanner scanner = new Scanner(System.in);
        System.out.print("Von welcher Zahl soll die FkaultÃ¤t bestimmt werden: ");
        a = scanner.nextInt();
        c = a;
        while(a > 0)
        {
        b = 1*a*b;
        a = a-1;
       }
       System.out.println(c + "! = " + b);
    }
    
    
}
