import java.util.*;
    class FactoranInteger {
        public static void main(String[] args) {
            Scanner in = new Scanner(System.in);
            System.out.println("What is your integer?");
            int a = in.nextInt();
            System.out.println(" -----");
        for(int i=2; i<=a; i++){
            while(a%i==0){
                System.out.println("| " + a" |");
                a = a/i;
        }
    }
}
}
            