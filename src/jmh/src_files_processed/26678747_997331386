
public class BMI
{

    private double groesse;
    private double gewicht;
    private double bmi;

    /**
     * Konstruktor f√ºr Objekte der Klasse BMI
     */
    public BMI(int groesseNeu, int gewichtNeu)
    {
        groesse = groesseNeu;
        gewicht = gewichtNeu;
        bmi=gewicht/(groesse*groesse);
    }
    
    if (bmi<=19)
    {System.out.println ("Undagevichd"+bmi)
        else 
          if (bmi<=24)
          {System.out.println ("midden in da midde"+bmi)
        else 
         if (bmi<=30)
          {System.out.println ("pumlig"+bmi)
        else 
         if (bmi<=40)
          {System.out.println ("richtig krass pumlig"+bmi)
        else 
        {System.out.println ("meeeeeega pumlig"+bmi)
     }