 */
public class Music
{
   private String tracktitle;
   private String date;
   private String length;
   private int rating;
   private String location;
   private double size;
   private String artist;
   private String artists;
   public Music(String tracktitle,String date,String length,int rating,String location,double size,String artists)
   {this.tracktitle=tracktitle;
       this.date=date;
       this.length=length;
       this.rating=rating;
       this.location=location;
       this.size=size;
      this.artists=artists;
    }
    
   public String Createtrack(){
       setTrackTitle();
       setDate();
      setRating();
       setLocation();
       setSize();
       setArtists();
   
    }
   public String setTrackTitle(String tracktitle)
   {this.tracktitle=tracktitle;
    }
    public String setDate(String date)
   {
       this.date=date;}
       public String setlength(String length)
   {
       this.length=length;}
       public int setRating(String rating)
   {
       this.rating=rating;}
       public String setLocation(String location)
   {
       this.location=location;}
       public String setSize(String size)
   {
       this.size=size;}
    public String setArtists(String artists)
   {   
      this.artists=artists;}
    
    public String getTrackTitle(){
    return tracktitle;
    }
       public String getDate(){
    return date;
    }
       public String getLength(){
    return length;
    }
           public int getRating(){
    return rating;
    }
           public String getLocation(){
    return location;
    }
           public double getSize(){
    return size;
    }
           public String getArtists(){
    return artists;
    }
  
}
}