import java.util.Scanner;
public class Game
{
    public static void main(String[]args)
    {
        Scanner in = new Scanner(System.in);
        System.out.println("Player 1 choose rock paper or scissor");
        String player1 = in.next();
        System.out.println("Player 2 choose rock paper or scissor");
        String player2 = in.next();
        System.out.println();
        String winner;

        if (player1.equals(player2) && player1.equals("rock") || player1.equals("paper") || player1.equals("scissor") ||)
        {           
            winner = "It is a tie";
        }
        else if (player1.equals("rock"))
        {
            if (player2.equals("scissor"))
            {
                winner = "Player 1 wins";
            }
            else
            {
                winner = "Player 2 wins";
            }
        }
        else if (player1.equals("scissor"))
        {
            if (player2.equals("paper"))
            {
                winner = "Player 1 wins";
            }
            else
            {
                winner = "Player 2 wins";
            }
        }
        else if (player1.equals("paper"))
        {
            if (player2.equals("rock"))
            {
                winner = "Player 1 wins";
            }
            else
            {
                winner = "Player 2 wins";
            }
        }
        else
        {
            winner = "Error";
        }
        System.out.println(winner);
    }
}