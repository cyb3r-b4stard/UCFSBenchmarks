import java.io.*;
import java.util.*;
class txtread
{
    public static void main()throws IOException
    {int pcode=0;String pname="";double uprice=0;int stock=0;String sent="";
        FileReader std=new FileReader("shop.txt");
        BufferedReader mat=new BufferedReader(std);
        System.out.println("pcode"+"\t"+"pname"+"\t"+"uprice"+ "\t"+"stock");
        while(sent=mat.readLine())!=null)
        {StringTokenizer str=new StringTokenizer(sent);
          System.out.println(sent);
          int p=str.countTokens();
          for (int i=0;i<p;i++)
          {
              System.out.println(str.nextToken());
        }
        mat.close();
    }
}
}