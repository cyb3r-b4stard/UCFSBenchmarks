package FiveYourOwnRobot;

import kareltherobot.*;
public class Tester1 
{

    public static void main(String [] args )
    {
        World.readWorld("steeple2.kwld"); 

        HurdlingRobot timmy = new HurdlingRobot (1,1,Directions.East,0);
        World.setVisible(true);

        while (!timmy.frontIsClear())
        {
            timmy.turnLeft();
            timmy.move();
            timmy       }

    }
}