public class functionwithnoargumentandreturntype
{
    public void add()
    {
        int a=30;
        int b=20;
        int c=a-b;
        System.out.println(c);
    }
    void main()
    {
        functionwithnoargumentandreturntype obj=new functionwithnoargumentandreturntype();
        obj.add();
    }
}
}