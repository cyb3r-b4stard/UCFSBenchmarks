
import java.io.*; 
import java.util.*; 
import java.text.*; 
public class prg122h 
{ 
    public static void main(String args[]) 
    {
        System.out.println("Sample Output:"); 
        System.out.println("Number" + "    " + "Square" + "    " + "Square Root" + "    " + "Cubes" + "    " + "Fourth Root"); 
        for (double number = 1; number<=20; number++)  
        { 
            
            double squared = Math.pow(number ,2.0); 
            double squareroot = Math.sqrt(number); 
            double cube = Math.pow(number, 3.0); 

            System.out.println(EasyFormat.format(number,4,1) + "    " +  EasyFormat.format(squared,6,1) + "        " + EasyFormat.format(squareroot,6,4)) + "     "  + EasyFormat.format(cube,4,1);  


        }
    }
}