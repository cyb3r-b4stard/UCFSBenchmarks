import java.awt.*;

public class VehicleFrame extends Frame
{
   public VehicleFrame()
   {
       super("Vehicles");
       setLayout(new FlowLayout());
       setSize(200,300);
       
       Choice VehicleList = new Choice();
       VehicleList.add("Sports Car");
       VehicleList.add("Van");
       add(VehicleList);
      
       TextArea a  = a new TextArea(20.20);
       add(a);
       setVisible(true);
      
    }
}