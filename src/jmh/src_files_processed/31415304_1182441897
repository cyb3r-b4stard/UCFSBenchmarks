
/**
 * ##### # ########### ## ##### ############### ####.
 * 
 * @###### (#### ####) 
 * @####### (# ####### ###### ## # ####)
 */
public class Array1DPractice
{
    public static int getToss()
    {
        return (int)(Math.random()*6)+1;
    }
    
    public static int[] getCubeTosses(int numTosses)
    {
        int[] cubeTosses;
        for (int i=0; i<numTosses; i++)
        {
            cubeTosses[i]=getToss();
        }
        return cubeTosses;
    }