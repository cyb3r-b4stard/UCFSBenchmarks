import java.util.*;
class numTeller
{
    public static Scanner reader = new Scanner (System.in);
    public static void main (String[] args)
    {
        int num1, num2, num3, count, digits, lastNum, numTemp;
        System.out.println("Enter in a number and I will tell you some things about it");
        num1 = reader.nextInt();
        count = 0;
        for (num1 = num1; num1 / 10 != 0; count++)
        {
            numTemp = num1;
            num3 = num1;
            numTemp /= 10;
            digits = numTemp + 1;
            num3 =% 10;
            lastNum = num3;
        }
    }
}