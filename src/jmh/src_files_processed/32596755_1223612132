
/**
 * #### ######## ########## ## ##### ## ## ####### ### #### ## ########### #########.
 * 
 * @###### (###### ####### ##########) 
 * @####### (####/##/##)
 */
public class fichaFarmaco
{
   private String tipoFarmaco;
   private String nombreProducto;
   private int codigoFarmaco;
   private int cantidadFarmaco;
   private boolean receta;
   private int dosisDiaria;
   private String nombreLaboratorio;
   private int precioFarmaco
   
   public fichaFarmaco(){
    }
   
   public fichaFarmaco (String tipoFarmaco, String nombreProducto, int codigoFarmaco, int cantidadFarmaco, boolean receta, int dosisDiaria, String nombreLaboratorio,int precioFarmaco) 
   {
       this.tipoFarmaco = tipoFarmaco;
       this.nombreProducto = nombreProducto;
       this.codigoFarmaco = codigoFarmaco;
       this.cantidadFarmaco = cantidadFarmaco;
       this.receta = receta;
       this.dosisDiaria = dosisDiaria;
       this.nombreLaboratorio = nombreLaboratorio;
       this.precioFarmaco = precioFarmaco;
   }
   
   public String getTipoFarmaco()
   {
    return tipoFarmaco;
   }
   
   public void setTipoFarmaco(String tipoFarmaco)
   {
     this.tipoFarmaco = tipoFarmaco;
   }
  
   public String getNombreProducto()
   {
       return nombreProducto;
   }
   
   public void setNombreProducto(String nombreProducto)
   {
     this.nombreProducto = nombreProducto;
   }

   public int getCodigoFarmaco()
   {
       return codigoFarmaco;
   }
    
    public void setCodigoFarmaco(int codigoFarmaco)
   {
     this.codigoFarmaco = codigoFarmaco;
   }
    
    public int getCantidadFarmaco()
   {
       return cantidadFarmaco;
   }
    
    public void setCantidadFarmaco(int cantidadFarmaco)
   {
     this.cantidadFarmaco = cantidadFarmaco;
   }
    
     public boolean getReceta()
   {
       return receta;
   } 
    
    public void setCantidadFarmaco(boolean receta)
   {
     this.receta = receta;
   }
   
      public int getDosisDiaria()
   {
       return dosisDiaria;
   } 
   
     public void setDosisDiaria(int dosisDiaria)
   {
     this.dosisDiaria = dosisDiaria;
   }
   
      public String getNombreLaboratorio()
   {
       return nombreLaboratorio;
   } 
   
     public void setNombreLaboratorio(String nombreLaboratorio)
   {
     this.nombreLaboratorio = nombreLaboratorio;
   }
   
      public int getPrecioFarmaco()
   {
       return precioFarmaco;
   } 
   
     public void setPrecioFarmaco(int precioFarmaco)
   {
     this.precioFarmaco = precioFarmaco;
   }
   
   
}


  
    
 