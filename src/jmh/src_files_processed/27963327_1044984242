
/**
 * ##### # ########### ## ##### ######### ####.
 * 
 * @###### (#### ####) 
 * @####### (# ####### ###### ## # ####)
 */
public enum CardValue
{
    CardSuit suit = CardSuit.heart ;
    
    System.out.print(suit);
    System.outprint(suit.getSuit());
    for(CardSuit suit : CardSuit.values())
    {
        system.out.print(suit + "\t" + suit.getSuit())
    }
    }
}