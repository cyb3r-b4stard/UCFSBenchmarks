
import java.util.Scanner;
public class Main
{
   public static void main(String args[]);
   Scanner in = new Scanner(System.in);
double dy,t,g;
dy=0;
g=0;
t=0;
System.out.println("Enter value for time:");
t=in.nextDouble();
dy=Math.sqrt(g*(t*t)/2);
System.out.println("Maximum Height:" + dy);
 }
}
   

