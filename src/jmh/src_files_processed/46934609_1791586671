
/**
 * ######## ##### ######### - ##### # ########### ## ### ##### ####
 * 
 * @###### (#### #### ####)
 * @####### (####### ###### ## #### ####)
 */
public abstract class Character implements ItemInfo
{
   public void setCurrentLocation(Place currentPlace);
   public Place getCurrentLocation()
   public abstract String getCharacterType();
}