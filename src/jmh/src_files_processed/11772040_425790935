
/**
 * ##### # ########### ## ##### ########### ####.
 * 
 * @###### (#### ####) 
 * @####### (# ####### ###### ## # ####)
 */
public class BankAccount
{
    double balance;
    String name;
public BankAccount(String n, double b) {
    balance = b
}

public  void deposit (double c) {
    this.balance = this.balance + c;
}

public void withdraw (double d){
    balance = balance - d;
}
}