

import java.util.*;

public class Pendulum
{
    public static void main(String[] args)
    {
        Scanner console = new Scanner(System.in);
        System.out.println("Pick a number>>>>");
        int a=console.nextInt();
        System.out.println("Pick another number>>>>");
        int b=console.nextInt();
        double result= Math.pow( a,b);
        System.out.println(result);
        System.out.println("Pick a [toCube] number>>>>");
        double toCube=console.nextDouble();
        result= Math.pow(toCube, 3);
        System.out.println(result);
        System.out.println("Pick a [base] number>>>");
        double base=console.nextDouble();
        System.out.println("Pick a [exponent] number>>>");
        double exponent=console.nextDouble();
        result= Math.pow(base,exponent);
        System.out.println(result);
        System.out.println("Pick a number with a decimal [round]>>>");
        double num=console.nextDouble();
        result= Math.round(num);
        System.out.println(result);
        System.out.println("Pick a number [square rooted] >>>>");
        double num2=console.nextDouble();
        result= Math.sqrt(num2);
        System.out.println(result);
        System.out.println("Pick a side [triangle]>>>>");
        double sideA=console.nextDouble();
        System.out.println("Pick another side>>>>");
        double sideB=console.nextDouble();
        sideA=Math.pow(sideA,2);
        sideB=Math.pow(sideB,2);
        result=Math.sqrt(sideB+sideA);
        System.out.println(result);
        System.out.println("Pick a value for c [close to zero]>>>>");
        double c=console.nextDouble();
        System.out.println("Pick a value for d>>>");
        double d=console.nextDouble();
        result= Math.min(c,d);
        System.out.println(result);
        System.out.println("Pick a value[surface area]>>>");
        double cylinderRadius=console.nextDouble();
        System.out.println("Pick a value>>>>");
        double cylinderHeight=console.nextDouble();
        double result1=Math.PI*cylinderRadius;
        double result2=Math.pow(cylinderRadius,2);
        double result3=Math.pow(cylinderHeight,2);
        System.out.println((result1)*(cylinderRadius+(Math.sqrt(Math.pow(result,2))+(Math.pow(result3,2)))));
        System.out.println("Pick a value[Sphere Radius]>>>>");
        double sphereRadius=console.nextDouble();
        double ft=4.0/3;
        ft=Math.PI*ft;
        double rpi=Math.pow(sphereRadius,3);
        double volume=rpi*ft;
        System.out.println(volume);
        System.out.println("Quadratic Formula Calculator");
        System.out.println("Pick a value for a");
        double aa=console.nextDouble();
        System.out.println("Pick a value for b");
        double bb=console.nextDouble();
        System.out.println("Pick a value for c");
        double cc=console.nextDouble();
        bb*=-1;
        double b2=Math.pow(bb,2);
        double numadd=(b+(Math.sqrt(b2-4*a*c)));
        double numsub=(b-(Math.sqrt(b2-4*a*c)))
        
    }
}