public class Anbieter
{
   
    private String name;           
    private int freiminuten ;               
    private int freisms ;           
    private float grundgebuehr ;                
    private char motor_starten;    
    private char motor_ausschalten;
    private int bremsen;           
    private float preis1;           
    private int hu_tag;
    private int hu_monat;
    private int hu_jahr;
    private int au1;
    private String farbe;
    
    
    public Anbieter(String n, int fm, int fs, float gb)
    {
        name = n ;
        freiminuten = fm ;
        freisms = fs ;
        grundgebuehr = gb ;
    }
    public int getAnbieter ()
    {
    return=name;
    }
    }