
/**
 * ####### #####
 * 
 * @###### (#### #####) 
 * @####### (####/#/##)
 */
public class Student
{
    private String studentName;
    private String studentID;
    private double testGrade;
    private double courseFees;
    private boolean isEligibleForADiscount;
    
    /**
     * Default constructor
     */
    public Student ()
    {
        studentName: "";
        studentID: "";
        testGrade: 0;
        courseFees: 0
        isEligibleForADiscount: false;
    }
}