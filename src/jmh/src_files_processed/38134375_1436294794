public class Kleinkind{
    String Vorname;
    String Nachname;
    int Alter;
    
    
public Kleinkind(){}
public Kleinkind(String Vor, String Nach,int c){Vorname=Vor;Nachname=Nach;Alter=c;}
public void AelterWerden(){Alter=Alter+1;}
public void NachnameAendern(){}   
public void Ausgabe(){System.out.print ("Vorname:  ";)}
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
}