public class Switch{
    public static void main(String[] args)
    {
       int month =2;
       switch(month)
       {
         case 1:System.out.println("January");
         break;
          case 1:System.out.println("February");
         break;
         case 1:System.out.println("March");
         break;
          case 1:System.out.println("April");
         break;
          case 1:System.out.println("Not Valid");
         break;
       }
       }
    }}
    