public class PointProj
{
   private int x,y;
   
   distance (int x,int y;)


    
     
     
     
    
    public double distance PointProj(int x,int y)
    {
    int dx =this.x-x;
   int dy = this y-y;
     return Math.sgrt(dx*dx+dy*dy);  
     
    }
}
     
}

    
    
        
    
