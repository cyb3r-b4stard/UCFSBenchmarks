import java.util.Scanner;
public class ASMD
{
    public static void main (String[] args){
        int first_digit;
        int second_digit;
        
        int[] numbers = new int[2];
        
        Scanner reader = new Scanner(System.in);
        System.out.print("Enter the first number: ");
        numbers[0] = reader.nextInt();
        System.out.print("Enter the second number: ");
        numbers[1] = reader.nextInt();
        
        int sum = numbers[0] + numbers[1];
        int difference = numbers[0] - numbers[1];
        int product = numbers[0] * numbers[1];
        float quotient = (float) numbers[0] / (float) numbers[1];
        
        int sum1 = numbers[1] + numbers[0];
        int difference1 = numbers[1] - numbers[0];
        int product1 = numbers[1] * numbers[0];
        float quotient1 = (float) numbers[1] - (float) numbers[0];
        
        System.out.println(numbers[0] "+" int numbers[1] "=" + sum);
        
        System.out.println("Difference: " + difference);
        System.out.println("Product: " + product);
        System.out.println("Quotient: " + quotient);
    }
}
