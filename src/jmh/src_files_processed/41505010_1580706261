
/**
 * ##### # ########### ## ##### ##### ####.
 * 
 * @###### ### ##### 
 * @####### #/##/####
 */
public class Photo{
    private String registration;
    private int speed;
    private boolean speeding;

    /**
     * Constructor for objects of class Photo
     */
    public Photo(String reg, int fast, boolean isSpeeding){
        registration = reg;
        speed = fast;
        speeding = isSpeeding;
    }
    
    public String getRegistration(){
        return registration;
    }
    
    public int getSpeed(){
        return speed;
    }
    
    public boolean isSpeeding(){
        return speeding;
    }
    
    public void getDetails(){
        if(speeding = true){
        String isSpeeding = "speeding";
        }else{
        String isSpeeding = ""
        }
        System.out.println(registration + " at " + speed + " mph " + isSpeeding);
    }
}