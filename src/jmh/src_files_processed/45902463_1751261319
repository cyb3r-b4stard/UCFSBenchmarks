
/**
 * ##### # ########### ## ##### ###### ####.
 * 
 * @###### (#### ####) 
 * @####### (# ####### ###### ## # ####)
 */
public class bubble
{
    public static void main(String args[])
    {
        
    int A[]={53,4,67,89,23};
    int i;
    bubble_sort(A,5)
    for(i=0;i<=3;i++)
        {
            System.out.println(A[i]);
        }
        
    
    }
    void bubble_sort(int A,int N)
    int i,round,temp;
    for(round=1;round<=N-1;round++)
        {
            for(i=0;i<=N-1-round;i++)
                {
                    if(A[i]>A[i+1])
                        {
                            temp=A[i];
                            A[i]=A[i+1];
                            A[i+1]=temp;
                        }
                }
        }
}