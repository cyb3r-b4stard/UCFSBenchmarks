import kareltherobot.*;
public class pinsetteer implements Directions
{
    public static void main(String[] args){
        UrRobot karel = new UrRobot(1, 5, North, 10); 
        karel.move();
        karel.putBeeper();
        karel.move();
        karel.turnLeft();
        karel.move();
        karel.putBeeper();
        karel.turnLeft();
        karel.turnLeft();
        karel.move();
        karel.move();
        karel.putBeeper();
        karel.turnLeft();
        karel.move();
        karel.turnLeft();
        karel.turnLeft();
        karel.turnLeft();
        karel.move();
        karel.putBeeper
        

    }
    
    static {
        World.reset(); 
        World.readWorld("pinsetter.kwld"); 
        World.setDelay(50);  
        World.setVisible(true);
    }
}

