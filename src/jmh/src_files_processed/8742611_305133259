public class Rechner
{
    @FunctionalInterface
    interface MathInteger
    {
        int berechne(int a, int b);
    }
    
    public static void main( String args[])
    {
        MathInteger addiere = (e, f) -> e + f;
        MathInteger mal = (c, d) -> c + d;
        
        System.out.println(addiere.berechne(30, 34);
        System.out.println(mal.berechne(4, 5);
        
        
    }
}