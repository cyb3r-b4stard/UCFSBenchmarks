
/**
 * ##### # ########### ## ##### ########## ####.
 * 
 * @###### (##### #####) 
 * @####### (##/##/####)
 */
public class Trabajador
{
    private String nombre;
    private String rut;
    private String direccion;
    private int telefono;
    private int hijos;
    private boolean titulado;
    private char sexo;
    private int sueldoHora;
    private String ingresoEmpresa;
    private String ISAPRE;
    private String AFP;
    
    public Trabajador()
    {
        nombre="null";
        rut="null";
        direccion="null";
        telefono=0;
        hijos=0;
        titulado=false;
        sexo=' ';
        sueldoHora=0;
        ingresoEmpresa="null";
        ISAPRE="null";
        AFP="null";
    }
    
    public Trabajador(String nombre_1, String rut_1, String direccion_1, int telefono_1, int hijos_1, boolean titulado_1, char sexo_1, int sueldoHora_1, String ingresoEmpresa_1, String ISAPRE_1, String AFP_1)
    {
        setNombre(nombre_1);
        setRut(rut_1)
        setDireccion(direccion_1)
        setTelefono(telefono_1)
        setHijos(hijos_1)
        setTitulado(titulado_1)
        setSexo(sexo_1)
        setSueldoHora(sueldoHora_1)
        setIngresoEmpresa(ingresoEmpresa_1)
        setISAPRE(ISAPRE_1)
        setAFP(AFP_1)   
    }
}