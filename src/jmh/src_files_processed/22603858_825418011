 public class Sequence
 {
     public static void main(String[] args);
     int a=5, b=2;
     double c=3.0;
     
     a+=b
     b++;
     c--
     c *= a
     System.out.println("a+b+c="+(a+b=c));
    
     
     

     
     
     
     
     
    }
     
 
    }end Sequence.java