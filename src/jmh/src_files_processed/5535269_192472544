
/**
 * ########### ### #### ### ###### #####.
 * 
 * @###### (### ####) 
 * @####### (#### ############## #### ### #####)
 */
public class Moebelstuek
{
    
    private String typ;
    private int hoehe;
    private int breite;
    private int tiefe;
    private int anzahl;
    private double preis;

    /**
     * Konstruktor für Objekte der Klasse Möbel
     */
    public Moebelstueck(String typ, int hoehe, int breite, int tiefe, int anzahl, double preis);
    {
        this.typ=typ;
        this.hoehe=hoehe;
        this.breite=breite;
        this.tiefe=tiefe;
        this.anzahl=anzahl;
        this.preis=preis;
    }

    /**
     * Ein Beispiel einer Methode - ersetzen Sie diesen Kommentar mit Ihrem eigenen
     * 
     * @param  y    ein Beispielparameter für eine Methode
     * @return        die Summe aus x und y
     */
    public String gettyp()
    {
        return typ;
    }
    
    public int gethoehe()
    {
        return hoehe;
    }
    
    public int getbreite()
    {
        return breite;
    }
    
    public int gettiefe()
    {
        return tiefe;
    }
    
    public int getanzahl()
    {
        return anzahl;
    }
    
    public double getpreis()
    {
        return preis;
    }
    
    
    
    public void ausgeben()
    {
        System.out.println("Typ:" + typ);
        System.out.println("Höhe:" + hoehe);
        System.out.println("Breite:"+ breite);
        System.out.println("Tiefe:" +tiefe);
        System.out.println("Anzahl:"+  anzahl);
        System.out.println("Preis:"+ preis);
    
    
    
    
    
    
    
    }
}
