public class Tetragono extends Sximata
{
    private int x;
   
    public Tetragono(int x)
    {
           super(x,x);
           this.x = x;
    }
    Tetragono()
    {
        super.Platos = this.x
    }
    public int Emvadon()
    {
        return x*x;
    }
    public int Perimetros()
    {
        return (4*x);
    }
    public void Print()
    {
        System.out.println("Emvadon"+Emvadon()+"Perimetros"+Perimetros());
    }
}

        
  