
public class Car 
{
    private String Make;
    private String Model;
    private int Mileage;
    
    public String getMake()
    {
        return Make;
    }
    public void setMake(String m1)
    {
        Make=m1;
    }
    public String getModel()
    {
        return Model;
    }
    public void setModel(String m2)
    {
        Model=m2;
    }
    public int getMileage()
    {
        return Mileage;
    }
    public void setMileage(int m3)
    {
        Mileage=m3;
    }
public Car(String m1, String  m2, int m3)
    {
        m1=Make;
        m2=Model;
        m3=Mileage;
         

    }
   public Car addMileag(int Mileage)
   {
       Mileage==0;
}
   }