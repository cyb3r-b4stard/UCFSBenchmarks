import java.util.ArrayList;

public class Avdelning
{

    private ArrayList<Anstalld> personallista;
    
    public Avdelning()
    {
        personallista = new ArrayList<>();
    }
    
    public void skapaAnstalld(String namn, int telefonNr)
    {
        new Anstalld(namn, telefonNr); 
    }
    
    public void laggTillAnstalld(String namn, int telefonNr)
    {
        personallista.add(skapaAnstalld(String namn,int telefonNr));
    }

}