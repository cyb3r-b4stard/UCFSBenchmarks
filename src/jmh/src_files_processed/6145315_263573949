import java.util.Scanner;
public class Principal {
    public static void main (String [] args){
        int valor;
        Scanner teclado = new Scanner (System.in);
        System.out.print ("\fDigite o valor: ");
        valor = teclado.nextInt ( );
        Calculadora c = new Calculadora (valor);
        System.out.println ("A soma dos valores da sequência de Fibonacci é: " c.somaDosValores ( ));
    }
}