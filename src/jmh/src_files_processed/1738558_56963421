import java.util.ArrayList;
import java.lang.String;

public class Combos_with_ArrayLists
{
    public static void main(String[] args)
    {
        
         System.out.println("Number 2b-> An array of objects:"); 
         ArrayList<String> arrList_objects = new ArrayList<String>();
         arrList_objects.add("came"); 
         arrList_objects.add("a"); 
         arrList_objects.add("m"); 
         arrList_objects.add("e");
         arrList_objects.add(0, "g");
         System.out.println(arrList_objects + " [Array Length: " + arrList_objects.size() + "]"); 
         AL_someObject(arrList_objects);
         System.out.println(arrList_objects);
    }
    
    public static void AL_someInt(ArrayList<Integer> arrList_ints)
    {
        arrList_ints= new ArrayList<int>();
        arrList_ints.set(0, 3); 
        System.out.println(arrList_objects);
    }
    
    public static void AL_someObject(ArrayList<String> arrList_objects)
    {
        arrList_objects = new ArrayList<String>();
        arrList_objects.remove("c"); 
        System.out.println(arrList_objects);
    }
    
    
    
    
}