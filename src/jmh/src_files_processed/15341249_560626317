public class compound
{
    
    public static void main(String args[])
    {
     int prin, time;
     double rate, amt, CI;
        rate = 13.0;
        time = 2;
        prin = 10000;
        amt= Math.round(prin*(Math.pow(1+rate/100,time),1);
        CI = amt-prin;
        System.out.println("the commpound interest="+CI);
        System.out.println("amt to be paid="+amt);
        
    }
    
}