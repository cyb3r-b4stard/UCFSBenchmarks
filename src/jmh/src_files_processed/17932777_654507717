
/**
 * ##### # ########### ## ##### ########## ####.
 * 
 * @###### (#### ####) 
 * @####### (# ####### ###### ## # ####)
 */
public class ANDCHECKER implements Checker
{
    
    String stuff="";
    String veggie="";
   public int AndChecker(String a,String b)
    {
       stuff=a;
       veggie=b;
     }
     
   public boolean accept(String word){
   if(stuff.contains(word) && veggie.contains(word){
    return true;
    }     
    return false;
        
    }
     
}
