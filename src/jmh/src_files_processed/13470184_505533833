public class Cat
{
 private String catName ;
 private int catAge;
 private double catWeight;
 private char catGender;
        /**
        * Constructor for objects of class Cat
        * Code is invoked each time a Cat object is instantiated with "new"
        */
          public Cat(String name, int age, double weight, char gender)
        {
            catName = name ;
            catAge = age ;
            catWeight = weight;
            catGender = gender;
        }
 
          /**
          * Cat Methods
          * Getters/Accessors:
          * 1) getName()
          * 2) getAge()
          * 3) getWeight()
          * 4) getGender()
          *
          * Putters/Mutators:
          * 1) putName(String name)
          * 2) putAge(int age)
          * 3) putWeight(double weight)
          * 4) putGender(char gender)
          */
           public String getName()
          {
             return catName;
           }

                                
   public int getAge()
    {    
      return catAge;
   }

 public double getWeight()
 { return catWeight;
 }
 
 public char getGender()
 {return catGender
 }

 public void putName(String name)
 {
  catName = name;
 }

 public void putAge(int age)
 {
  catAge = age;
 }

 public void putWeight(int weight)
 {
  catWeight = weight;
 }
 
 public void putGender (char gender)
 {
     catGender = gender;
    }
 
}