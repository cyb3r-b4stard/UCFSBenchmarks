public class TezinaClanovaEkipe
{
   double nizT[] = new double [20];
   int brojClanova = 0;
   static final int MIN_TEZINA = 40;
   TezinaClanovaEkipe(){
       for(int i = 0;i<nizT.length;i++){nizT[i]=-1;}
    }
   void dodajTezinu(int tezina){
      for(int i=0;i<nizT.length;i++)
           if(nizT[i]>-1 && tezina>MIN_TEZINA) nizT[i] = tezina;
      else System.out.println("Greska!");
    }
   void rangovi(){
       int laka=0;int srednja = 0;int teska =0;
       
       for(int i =0;i<nizT.length;i++){if (nizT[i]>MIN_TEZINA && nizT[i]<=65) }
    }
}