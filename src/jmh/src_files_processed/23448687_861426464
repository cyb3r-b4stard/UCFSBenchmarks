
import static java.lang.System.*;

public class ReverseNumber
{
   private int number;

    int num;
	public ReverseNumber(int num)
	{
	
	}

	public void setNumber(int num)
	{
	   num=num;	
	}

	public int getReverse()
	{
		int rev=0;
		while (num > 0)
	    {
	    rev= num%10;
	    num /=10;
	   }
		return rev;
	}

	public String toString()
	{ 
		return "Collin McConnell"+ num + " reversed is " = rev;
	}
}