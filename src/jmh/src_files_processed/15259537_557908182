public class Shop
{
    public static void main()
    {
        ShoppingCartAL tannersCart = new ShoppingCartAL("Tanner");

        Item canTuna = new Item("Bumblebee Tuna 4oz", 1.59);
        Item icedTea = new Item("Brisk Sweetened Iced Tea 1L", 2.49);
        Item canSeltzer = new Item("Big Y Seltzer 12oz", 0.89);
        Item bunchBroccoli = new Item("Organic Farms Broccoli 14oz", 4.89);
        Item ozAlmonds = new Item("Roasted Almonds 1oz", 1.29);

        Item smallLaysChips = new Item("Lays Original Potato Chips 4.5 oz", 1.19);
        Item largeLaysChips = new Item("Lays Original Potato Chips 12 oz", 2.89);

        Item frootLoops = new Item("Kellogg's Froot Loops Cereal 1lb", 1.89);
        Item holidayFrootLoops = new Item("Kellogg's Froot Loops Cereal 1lb", 2.09);

        tannersCart.addToCart(canTuna, 1);
        tannersCart.addToCart(canSeltzer, -3);
        tannersCart.addToCart(frootLoops, 2);
        tannersCart.addToCart(holidayFrootLoops, 1);
        
        System.out.println(tannersCart.toString())

        tannersCart.removeFromCart(canTuna, 3);
        tannersCart.removeFromCart(largeLaysChips, 1);
        tannersCart.removeFromCart(frootLoops, 2);
        tannersCart.removeFromCart(canTuna, -1);
        
        Systme.out.println(tannersCart.toString())
       
        tannersCart.updateQuantity(canTuna, -1);
        tannersCart.updateQuantity(canTuna, 0);
        tannersCart.updateQuantity(ozAlmonds, -1);
        tannersCart.updateQuantity(ozAlmonds, 4);
        
        System.out.println(tannersCart.toString())
    }
}
