public class Employee
{
    
   public void empInfo (String empID, String empName, int hoursWk, double ratePerHr)
   {
       employeeID = empID ;
       employeeName = empName ;
       hoursWork = hoursWk ;
       ratePerHour = ratePerHr ;
   }
   
   public String getempID();
   { return employeeID ; }
   
   
   public double calcSalary (); 
   
        







    