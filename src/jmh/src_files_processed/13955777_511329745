class Maximum{
static long max2(long value1, long value2){
    long m;
    if (value1>value2) {
        m=value1;
    }  else {
        m=value2;
    }
    return m;
}
static long max2test(long value1, long value2){
   max2(value1, value 2);
   System.out.println("Maximumberechnung:");
   System.out.println("Eingabe:"+ value1 +","+value2);
   System.out.print("Maximum:");
   System.out.println(max2);
   
}

}