import java.awt.KeyEventDispatcher;
import java.awt.KeyboardFocusManager;
import java.awt.event.KeyEvent;

class KeyListener{
    public KeyListener(){
        KeyboardFocusManager.getCurrentKeyboardFocusManager().addKeyEventDispatcher(new KeyEventDispatcher(){
        })
    }
    public boolean pressed(char c){ 
        return false;
    }
    public boolean released(char c){ 
        return false;
    }
    public boolean isPressedNow(char c){ 
        return false;
    }
        
    public char pressed(){ 
        return 0;
    }
    public char released(){ 
        return 0;
    }
    public String arePressedNow(){ 
        String s = "";
        return s;
    }
        
    public long pressedFor(char c){ 
        return 0;
    }
}