
public class IceCreamShoppe
{
    private double carton_radius;
    private double carton_height;
    private int cartons_used;
    Carton carton = new Carton(volume,carton_radius, carton_height);

    public IceCreamShoppe(){
        double carton_radius;
        double carton_height;
        int cartons_used;
    }
    public double getVolume(){
        return Carton

    public int used(){
        Scoop scoop = new Scoop(carton_radius);
        Carton carton = new Carton(volume,carton_radius, carton_height);
        cartons_used=1;
        if(!Carton.hasEnoughFor(scoop)){
            return cartons_used=cartons_used+1;
        }else{
            return cartons_used=cartons_used;
        }
    }
}
