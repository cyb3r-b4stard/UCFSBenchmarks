import java.io.*;
class pattern
{
    public void main()
    {
        int b=1,k=3
        for(int i=1;i<=5;i++)
        {
            
                for(int n=3;n>k;n++)
                System.out.print(i);
            for(int j=b;j<=5;j++)
            {
            System.out.print(j);
           
        }
         b=b+1;
         k=k-1;
        System.out.println();
    }}}
            