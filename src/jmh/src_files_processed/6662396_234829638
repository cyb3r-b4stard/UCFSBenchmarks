
/**
 * ##### # ########### ## ##### ########## ####.
 * 
 * @###### (###### #######) 
 * @####### (##.##.####)
 */
public class Musculines
{
    private String rut;
    private String nombre;
    private String apellido;
    private int edad;
    private boolean estado;
    private int valorMensualidad;
    private char sexo;
    private int cantidadHoras;

    /**
     * Constructor para la clase Musculines
     */
    public Musculines()
    {
        rut = "17383692-9";
        nombre = "Miguel";
        apellido = "Morales";
        edad = 24;
        estado = true;
        valorMensualidad = 10000;
        sexo = 'm';
        cantidadHoras = 10;

    }

    /**
     * Constructor con parametros para la clase Musculines
     */
    public Musculines(String rut,String nombre,String apellido,int edad,boolean estado,int valorMensualidad,char sexo,int cantidadHoras)
    {
        this.rut=rut;
        this.nombre=nombre;
        this.nombre=apellido;
        this.edad=edad;
        this.estado=estado;
        this.valorMensualidad=valorMensualidad;
        this.sexo=sexo;
        this.cantidadHoras=cantidadHoras;

    }

    /**
     * Metodo accesador para la clase Masculines
     * @return un valor string al atributo rut
     */
    public String getRut ()
    {
        return rut;
    }

    /**
     * Metodo accesador para la clase Masculines
     * @return un valor string al atributo nombre
     */
    public String getNombre ()
    {
        return nombre;
    }

    /**
     * Metodo accesador para la clase Masculines
     * @return un valor string al atributo apellido
     */
    public String getApellido ()
    {
        return apellido;
    }

    /**
     * Metodo accesador para la clase Masculines
     * @return un valor int al atributo edad
     */
    public int getEdad ()
    {
        return edad;
    }

    /**
     * Metodo accesador para la clase Masculines
     * @return un valor boolean al atributo estado
     */
    public boolean getEstado ()
    {
        return estado;
    }

    /**
     * Metodo accesador para la clase Masculines
     * @return un valor int al atributo valorMensualidad
     */
    public int getValorMensualidad ()
    {
        return valorMensualidad;
    }

    /**
     * Metodo accesador para la clase Masculines
     * @return un valor char al atributo sexo
     */
    public char getSexo ()
    {
        return sexo;
    }

    /**
     * Metodo accesador para la clase Masculines
     * @return un valor int al atributo cantidadHoras
     */
    public int getCantidadHoras ()
    {
        return cantidadHoras;
    }

    /**
     * Metodo mutador para la clase Masculines
     * @param da un nuevo valor al atributo rut
     */
    public void setRut (String rut)
    {
        this.rut=rut;
    }

    /**
     * Metodo mutador para la clase Masculines
     * @param da un nuevo valor al atributo
     */
    public void setNombre (String nombre )
    {
        this.nombre=nombre;
    }

    /**
     * Metodo mutador para la clase Masculines
     * @param da un nuevo valor al atributo apellido
     */
    public void setApellido (String apellido )
    {
        this.apellido=apellido;
    }

    /**
     * Metodo mutador para la clase Masculines
     * @param da un nuevo valor al atributo edad
     */
    public void setEdad (int edad)
    {
        this.edad=edad;
    }

    /**
     * Metodo mutador para la clase Masculines
     * @param da un nuevo valor al atributo
     */
    public void setEstado (boolean estado)
    {
        this.estado=estado;
    }

    /**
     * Metodo mutador para la clase Masculines
     * @param da un nuevo valor al atributo valorMensualidad
     */
    public void setValorMensualidad (int valorMensualidad)
    {
        this.valorMensualidad=valorMensualidad;
    }

    /**
     * Metodo mutador para la clase Masculines
     * @param da un nuevo valor al atributo sexo
     */
    public void setSexo (char sexo)
    {
        this.sexo=sexo;
    }

    /**
     * Metodo mutador para la clase Masculines
     * @param da un nuevo valor al atributo cantidadHoras
     */
    public void setCantidadHoras (int cantidadHoras)
    {
        this.cantidadHoras=cantidadHoras;
    }

    /**
     * Metodo Impresion
     */
    public void printMasculines()
    {
        System.out.println("Todos Los Datos De La Clase");
        System.out.println("Nombre : "+nombre);
        System.out.println("Apellido : "+apellido);
        System.out.println("Rut : "+rut);
        System.out.println("Edad : "+edad);
        System.out.println("Estado :"+estado);
        System.out.println("Valor Mensualidad :"+valorMensualidad);
        System.out.println("Sexo :"+sexo);
        System.out.println("Cantidad Horas Semanales :"+cantidadHoras);

    }

    /**
     * Metodo Customer
     */
    public void rebajarMensualidad(int porcentaje)
    {
        if (porcentaje>0)
        {
            this.valorMensualidad=valorMensualidad-porcentaje;
            System.out.println("Su Nueva Mensualidad Es :"+(valorMensualidad-porcentaje));
        }
    }
    /**
    * Metodo Para Obtener Datos
    */
    public void obtenerDatos()
        {
            System.out.println("Nombre : "+nombre+" "+ apellido+" "+cantidadHoras);
        }
 
    }
}