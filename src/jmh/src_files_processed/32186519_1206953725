public class test
{
    public test()
    {
        this.show();
    }

    double[] t = new double[5];
    double[] t2 = {
        1.0,-2.3,3.3};
    int[] tab = new int[25];

    public void show()
    {
        System.out.println("\n"+"----------"+"\n");
        for(int i=0; i<t.length;i++)
        {
            System.out.println(t[i]);
        }
        System.out.println("*-*-*-*");
        for(int i=0; i<t2.length;i++)
        {
            System.out.println(t2[i]);
        }
        System.out.println("*-*-*-*");

        t[t.length-1] = 98.76;
        System.out.println(t[t.length-1]);

        System.out.println("*-*-*-*");

        System.out.println("*-*-*-*");

        System.out.println("*-*-*-*");
        
        
        tab = {
            1,1,1,1,1,
            1,1,2,1,1,
            1,1,2,1,1,
            1,1,2,1,1,
            1,1,1,1,1
        };
    }
}