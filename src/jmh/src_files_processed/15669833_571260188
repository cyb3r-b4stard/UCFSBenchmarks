
public class hw
{
    public static void main(String args[]){
        int[][] hello = new int[3][5];
        int rowsum=0;
        for(int row=0;row<hello.length;row++)

        {
            for(int column=-;column<hello[0].length;column++){
                hello[row][column]=column+rowsum+1;
            }
            rowsum=rowsum+hello[0].length;
        }
    }
}
