import java.util.*;

public class onthi
{
    public void test1()
    {
         int i = 0 ;
        
        while (i < 5)
        {
            System.out.println("Hello");
            i++;
        }
        
        System.out.println ("Finished");
    }
   
    
    public void q1()
    {
        int start = 5;
        int end = 10;
        for ( int i = start; i <= end; i ++)
        {
            System.out.println(i);
        }

    }
    
    public void q1a(int start, int end)
    {
         for ( int i = start; i <= end; i ++)
        {
            System.out.println(i);
        }
    }
    
    public void q2()
    {
        ArrayList <Integer> nums;
        nums = new ArrayList <Integer>();
        nums.add (1);
        nums.add (3);
        nums.add (6);
        int total = 0;
        for (int x : nums)
        {
            total = total + x;
            
        }
         System.out.println(total);
    }
    
    public void q2a()
    {
        private int number []= {1,2,3,4,5}
                for (int x : number)
        {
            total = total + x;
            
        }
         System.out.println(total);
    }
}