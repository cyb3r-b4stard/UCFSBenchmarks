public class Crypto
    {
    public Crypto()
    {

    }
    public String encrypt(String sntnc)
    {
        encryptString = sntnc;
       for(int i = 0;i<sntnc.length();i++)
       {
          if(sntnc.charAt(i)!='v'|sntnc.charAt(i)!='m'|sntnc.charAt(i)!='b'|sntnc.charAt(i)!='g'|)
          {
              return sntnc.charAt(i);
            }
        }
       
        return encryptString;
    }
    public String encryptString;
    public String decryptString;
    }