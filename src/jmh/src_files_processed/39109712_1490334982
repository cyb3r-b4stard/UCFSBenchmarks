

import static java.lang.System.*;

public class OddOrEven
{
    public static boolean isOdd( )
    {
    if(string.length % 2 == 1)
    {
           return true;
    }
    else 
    {
        return false;
    }
    
    public static boolean isEven( )
    if(string.length % 2 == 0)
    {
        return true;
    }   
    else
    {
        return false;
    }   
    }