

public class exercice
{
   public static void main (String[] args)
   {
       int sec;
       int min;
       int heure;
       int n=9999;
       heure = n/3600;
       min = n%3600/60;
       sec = n%3600%60;
       System.out.println(n);
       System.out.println("h":heure);
       System.out.println(min);
       System.out.println(sec);
    }
}