public class RectangleDemo
{
    public static void main(String[] args)
    {
        Rectangle box = new Rectangle(20, 70, 40, 30);
        box.setColor(Color.BLUE);
        box.fill();
        Rectangle box2 = new Rectangle(20, 40, 40, 30);
        box.setColor(Color.RED);
        box.fill();
        Rectangle box3 = new Rectangle(20, 10, 40, 30);
        Color custom = (125, 125, 255);
        box.setColor(Color.custom);
        box.fill();
 
    }
}