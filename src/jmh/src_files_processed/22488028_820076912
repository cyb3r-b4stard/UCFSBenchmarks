

public class code1
{
    public static void main (String args[]) {
        
        char c, c1, c2, c3, c4;
        int i, i1, i2, i3, i4;
        c='H';
        c1='e';
        c2='l';
        c3='l';
        c4='o';
        i = (int)c; 
        i1 = (int)c1;
        i2 = (int)c2;
        i3 = (int)c3;
        i4 = (int)c4;
        System.out.println(i+" "+i1+" "+i2+" "+i3+" "+i4);
        c=',';
        i = (int) c; 
        System.out.println(i);
        c='p';
        c1='l';
        c2='e';
        c3='a';
        c4='s';
        c5='e';
        i = (int) c; 
        i1 = (int) c1;
        i2 = (int) c2;
        i3 = (int) c3;
        i4 = (int) c4;
        i5 = (int) c5;
        System.out.println(" "i+" "+i1+" "+i2+" "+i3+" "+i4+" "+i5);
        
        
        
    
 
    }
}