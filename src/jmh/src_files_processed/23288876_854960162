public class Triang {
    private int Px, Py, Qx, Qy, Rx, Ry;
    public Triang (){
        Px = 1;
        Py= 1;
        Qx = 4;
        Qy = 4;
        Rx = 5;
        Ry = 1;
    }
    public int incPx (int cord) {
        this.Px = this.Px + cord;
        return Px;
    }
    public double getPx () {
        return this.Px;
    }
    public int incPy (int cord) {
        this.Py = this.Py + cord;
        return Py;
    }
    public double getPy () {
        return Py;
    }
    public int incQx (int cord) {
        
        this.Qx = this.Qx + cord;
         
        return Qx;
    }
    public double getQx () {
        return Qx;
    }
    public int incQy (int cord) {
        this.Qy = this.Qy + cord;
        return Qy;
    }
    public double getQy () {
        return Qy;
    }
    public int incRx (int cord) {
        this.Rx = this.Rx + cord;
        return this.Rx;
    }
    public double getRx () {
        return this.Rx;
    }
    public int incRy (int cord) {
        this.Ry = this.Ry + cord;
        return this.Ry;
    }
    public double getRy () {
        return this.Ry;
    }
    public double sideLeght1 () {
        return sqrt (pow); Px) + (pow Qx);
    }
    }
        
        

