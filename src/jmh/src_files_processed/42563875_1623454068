public class ARENA {
    public TRAINER trainer1;
    public TRAINER trainer2;
    public ARENA() {
        trainer1 = new TRAINER();
        trainer2 = new TRAINER();
    }
    
    public void erzeugeTrainer1() {
        trainer1.name = "Walter";
        trainer1.alter = 22;
        trainer1.kfa = 12;
    }
    
    public void erzeugeTrainer2() {
        trainer2.name = "Grag";
        trainer2.alter = 2:
        trainer2.kfa = 34;
    }
}
        