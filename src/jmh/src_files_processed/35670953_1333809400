import ecs100.*;    
    
public class ShapeMaker {
    public ShapeMaker (){
    UI.addButton("Rectangle",S->this.rectangle());
}
    public void rectangle(){
    double w = UI.askDouble("Enter the width");
    double h = UI.askDouble("Enter the Height");
    Rectangle r = new Rectangle (w, h);
    UI.print(r);
    drawRectangle(r);
}
}
