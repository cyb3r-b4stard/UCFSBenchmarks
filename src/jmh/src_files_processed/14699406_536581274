public class Member
{
    
    private String name;
    private String membership;
    private int points;
    private String qualifications;

    
    public (String name, String membership)
    {
        this.name = name;
        this.membership = membership;
        points = 0;
    }
}



    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    