
/**
 * ##### # ########### ## ##### ############## ####.
 * 
 * ######## ###### ## ###### ####
 * ####
 */
public class DatePlanetaire
{
    /** tableau des planï¿½tes */
    private String[] planetes = {"terre","mars","jupiter","saturne","uranus","neptune","pluton"};
    private int quantieme; 
    private int annee; 
    private SystemeSolaire p=new SystemeSolaire();
    private int j,m,q;
    private long tu;
    
    public void DatePlanetaire(int j,int m,int a){
        int q=p.conv(j,m);
        set(q,a);
    }
    public void DatePlanetaire(java.lang.String planete,int q,int a){
        set(q,a);
    }
     
    public int annee(){
        return annee;
    }
    public int quantieme(){
        return quantieme;
    }
    public java.lang.String[] planete(){
        return planetes;
    }
    public void set(int q,int a){
        this.quantieme=q;
        this.annee=a;
    }
    public long tempsUniversel(){
        long tu=this.annee()*p.annee(String[].SystemeSolaire);
    }
 
}

