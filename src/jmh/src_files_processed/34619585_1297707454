package Arrays;
import java.util.*;
class Arrays1
{
    public static void main(String args[])
    {
        Scanner sc=new Scanner(System.in);
        System.out.println("Enter 10 numbers");
        int n[]=new int[10];
        int s=0;
        for(int i=1;i<10;i++)
        {
            n[i]=sc.nextLine();
            s=s+n[1];
        }
        int avg=s/10;
        System.out.println("the average ="+avg)
    }
}    