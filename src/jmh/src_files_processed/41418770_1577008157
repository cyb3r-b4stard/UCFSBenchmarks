public class Hello
{
  public static void main(String []args) {
int days;
   int k=1;
   printf("输入");
   scanf("%d",days);
   if(days!=0){
    for(int i=1;i<days;i++)
    {
    k=(k+1)*2;
    }
    printf("蟠");
}
}