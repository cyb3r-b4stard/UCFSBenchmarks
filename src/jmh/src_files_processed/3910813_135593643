mport javax.swing.JOptionPane;
public class MixedArray{
public static void main(String[] args){

int [] mixed = new int [10];
int max = 0;
int min = 100;
boolean flag = false;
int i=0;


for( i=0; i<10; i++){
mixed[i] = (int) (Math.random()*100)+1;
System.out.println(mixed [i]);

if(mixed[i] > max){
max = mixed[i];




}

if(mixed[i] < min){
min = mixed[i];


}



}

System.out.println("The maximum number is " + max);
System.out.println("The minimum number is " + min);

String s = JOptionPane.showInputDialog("Please enter a number"); 
int num = Integer.parseInt(s);


for(i=0; i<10; i++){
if(num == mixed [i]){
flag = true; 
JOptionPane.showMessageDialog(null,"Number found at position " + i); 
}
}

if(flag== false){
JOptionPane.showMessageDialog(null,"Sorry number not found" ); 

}






}

}