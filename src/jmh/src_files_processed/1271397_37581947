
/**
 * ######## ##### ###### # ###.
 *
 * @###### (##### ###)
 * @####### (## ###### ## ####### ## ### ####)
 */
public class void Echo {


  public static void main (String args[]) {
    for (int i = 0; i <= args.length - 1; i++)
      System.out.println("(" + i + ")" + args[i] + " ");
    if (args.length != 0) System.out.println();
  }
}

