
public class aman
{
   public static void rectangle (double l, double b)
   
    {
        double  area=l*b;
        System.out.println("length="+l+"breadth="+b+"then the area of the rectangle="+area);
        
    }
    
    public static void triangle (double b, double h)
      {
        double area=1.0/2*b*h;  
        System.out.println("base="+b+"height="+h+"then the area of the triangle="+area);
    }