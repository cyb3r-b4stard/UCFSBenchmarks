import java.util.Scanner;
public class Sum
{
      int x, y, z;
      System.out.println("Enter two integers to calculate their sum ");
      Scanner in = new Scanner(System.in);
      x = in.nextInt();
      y = in.nextInt();
      z = x + y;
      System.out.println("Great job! The sum of entered integers = "+z);
            
        

    }
