import comp102x.IO;

public class Lab02
{
    
    public static void multiply()
    {
        double operand1;
        double operand2;
        double mulresult;
        IO.outputln(" enter the value of operand1 :");
        operand1= IO.inputDouble();
        IO.outputln(" enter the value of operand2 :");
        operand2=IO.inputDouble();
        mulresult=operand1*operand2;
        IO.outputln("the multiplication of operand1 and operand2 is  " + mulresult);
        
        
        
        
    }
    
    public static void calculateTriangleArea()
    {
        double base;
        double height;
        double trianglearea;
        IO.outputln(" enter the value of base of triangle :");
        base= IO.inputDouble();
        IO.outputln(" enter the value of height of triangle :");
        height=IO.inputDouble();
        trianglearea=0.5*base*height;
        IO.outputln("the area of triangle with base " + base  " and height  " + height  "is  " + trianglearea);
        }
    
