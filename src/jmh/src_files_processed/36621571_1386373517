 */
public class DieCup{
    private Die d1;
    private Die d2;
    private int eyes;
    public DieCup(){
        d1=new Die();
        d2= new Die();
    }
    
    public void roll(){
        d1.roll();
        d2.roll();
        eyes=
    }
    
    public int getEyes(){
        return eyes;
    }
}