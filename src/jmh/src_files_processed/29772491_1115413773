import java.io.*;
class Triangles
{
    public static void main()throws IOException
    {
        int a,b,c;
        System.out.println("Enter three sides of the triangle");
        System.out.pritln("Enter side one");
        a=Integer.parceInt(.readLine());
        System.out.pritln("Enter side two");
        b=Integer.parceInt(in.readLine());
        System.out.pritln("Enter side three");
        c=Integer.parceInt(in.readLine());
        if(a+b>c&&b+c>a&&c+a>b)
        {
            System.out.println("Triangle is possible");
            if((a==b)&&(b==c)&&(c==a))
            System.out.println("Equaletral triangle");
            else if((a==b)||(b==c)||(c==a))
            System.out.println("Isosceles triangle");
            else if((a!=b)&&(b!=c)&&(c!=a))
            System.out.println("scalen triangle");
        }
        else
        System.out.println("Triangle not possible");
    }
}