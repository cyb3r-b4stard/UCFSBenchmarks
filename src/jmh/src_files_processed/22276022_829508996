enum Breed { GERMAN_SHEPHERD , PITBULL, BORDER_COLLIE;}


public class DogClient
{
    public static void main()
    {
        Dog d1 = new Dog ("Spot", 1, Breed.GERMAN_SHEPHERD, 123);
        Dog d2 = new Dog ("Soos", 2, Breed.PITBULL, 693);
        Dog d3 = new Dog ("Dash", 4, Breed.BORDER_COLLIE, 465);
        
        System.out.println(d1.convertToHumanYears());
        System.out.println(d2.convertToHumanYears());
        System.out.println(d3.convertToHumanYears());
        
        System.out.println(d1.convertToHumanYearsExact());
        System.out.println(d2.convertToHumanYearsExact());
        System.out.println(d3.convertToHumanYearsExact());
        
        System.out.println(d1.computeDogIDCheck());
        System.out.println(d2.computeDogIDCheck());
        System.out.println(d3.computeDogIDCheck());
        
        System.out.println(d1 .createDogTag());
        System.out.println(d2 .createDogTag());
        System.out.println(d3 .createDogTag());
        
        System.out.println(
    }
}
        