
import java.awt.*;
import javax.swing.*;
import java.awt.event.*;
public class ty_alien extends JApplet
{    
    public ActionListener list1;
    public ActionListener list2;
    public ActionListener list3;
    public ActionListener list4;
    public void init()
    {
       list1=new list();
       list2=new list();
       list3=new list();
       list4=new list();
        Panel mainbox=new Panel();
        Panel buttonpanel=new Panel();
        Panel imgpanel=new Panel();
        Button facehug=new Button("Fachehugger");
        Button chestburst=new Button("Chestburst");
        Button roar=new Button("Roar");
        Button acidblood=new Button("AcidBlood");
        facehug.add ActionListener (list1);
        chestburst.add(list2);
        roar.add(list3);
        acidblood.add(list4);
    }
}
public class list implements ActionListener
{   
    public void actionPerformed(ActionEvent event)
    {
        
    }
}