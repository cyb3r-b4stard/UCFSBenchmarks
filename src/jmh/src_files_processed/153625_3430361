
/**
 * ##### # ########### ## ##### ######### ####.
 * 
 * @###### (#### ####) 
 * @####### (# ####### ###### ## # ####)
 */
public class StringStr
{
    String str = "";
    boolean found=false;
    String ch="?";
    int position = 0;
    
    while (!found && position<str.length())
    {
        ch=str.substring(position, position+1);
        if(ch.equals(" "))
        {
            found=true;

        }
        else
        {
            position++;
        }
    }
}