
import java.util.*;
class program15
{
    public static void main(String args[])
    {
        Scanner sn=new Scanner(System.in);
        double c=0,s;
        System.out.println("Enter sales value");
        s=sn.nextDouble();
        if (s>=1&&s<=999)
        {
        c=0.01*s;
    }
    else if(s>=1000&&s<=9999)
    {
    0.5/0.01*s
}
else if(s>=100000)
{
c=0.1*s;
}
System.out.println("Commision:"+c);
}
}