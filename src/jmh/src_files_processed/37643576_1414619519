
/**
 * ##### # ########### ## ##### ###### ####.
 * 
 * @###### (#### ####) 
 * @####### (# ####### ###### ## # ####)
 */
public class Walrus
{
    private String walrusColor;
    private String walrusName;
    private int walrusAge;
    private String walrusGender;

    public Walrus(String color, String name, int age, String gender)
    {
        walrusColor = color;
        walrusName = name;
        walrusAge = age;
        walrusGender = gender;
    }
    
    public void setColor(String color)
    {
        walrusColor = color;
    }
    
    public void setName(String name)
    {
        walrusName = name;
    }
    
    public void setAge(int age)
    {
        walrusAge = age;
    }
    
    public void setGender(String gender)
    {
        walrusGender = gender;
    }
    
    public String getColor()
    {
        return color;
    }
    
    public String getName()
    {
        return name;
    }
    
    public int getAge()
}