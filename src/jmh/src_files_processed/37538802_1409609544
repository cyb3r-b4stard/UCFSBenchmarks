public class StringMethods
{
    public static String firstLast(String str)
    {
        if(str.length() <= 2)
        {
            System.out.println(str);
        }
        if(str.length() >= 3)
        {
            System.out.println((str.substring(0,1)) + (str.substring(str.length()-1)));
        }
    }
    return str;
}
