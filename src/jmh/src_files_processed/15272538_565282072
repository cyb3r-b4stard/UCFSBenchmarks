
public class Schaltjahre
{
 
    public Schaltjahre()
    {
    }
    
    public Boolean isSchaltjahr(Integer jahreszahl) {
        if ((schaltjahr % 4 = 0 && schaltjahr % 100 >= 1) || (schaltjahr % 400 = 0)) {
            return true;
        }
        else {
            return false;
        }
    }
    
}



