public class Coffee
{
    
   
   public static void main(String[] args) 
   {
       double [] sample = new double[100];  
       
       for( int index =0; index < sample.length; index++)
       {
           sample[index] = (Math.random()*100);
       }
       
       for( int index =0; index < sample.length; index++)
       {
           System.out.println( sample[index]);
       
      
    
    
    
    
    
    
    
    
    
    
    
   }
   
   
   
   
       
}    
 
          