class marks
{
    void main (int m,int p,int c,int b)
    {
        int t=m+p+c+b+;
        int pp=(t/400)*100;
        {
            if(pp>=80);
            System.out.println("EXCELLENT"+pp);
        }
        {
             if(pp>=50 and pp<80);
             System.out.println("GOOD"+pp);
        }
        {
            if(pp<50);
            System.out.println("FAIL"+pp);
        }
    }
}    
            
             