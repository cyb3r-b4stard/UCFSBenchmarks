public class Pictures
{

    /**
     * Constructor for objects of class Pictures
     */
    public Pictures()
    {
        table(12,12);
    }

    public void table(int a, int b)
    {
        int rows = a;
        int columns = b;

        for(int x = 1; rows != 0; rows--, x++)
        {
            System.out.printf("%5d", x);
            for()
            {
            }
        }
        System.out.println();
        for(int y = 1; columns != 0; columns--, y++)
        {
            System.out.println(y);
        }
        
    }
    
    public void math()
    {
        
    }
    
}