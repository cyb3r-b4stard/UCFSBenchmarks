public class Person
{
	public String fname;
	public String lname;
	private int age;
	private String address;
	
	public Person(String fn,String ln,int edad)
	{
	    fn=fname;
	    ln=lname;
	    age=edad;
	    address="BOHOL";
	}
	public Person(String fn,String ln,String adrs)
	{
	    fn=fname;
	    ln=lname;
	    age=1;
	    adrs=address;
	}
	public getString():String;
	
	public String toString()
	{
	    return("Person info:"+ lname +","+ fname +":"+ age +","+ address );	
	}
	
}