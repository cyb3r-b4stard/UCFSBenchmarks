import java.util.ArrayList;
/**
 * ##### # ########### ## ##### ############# ####.
 * 
 * @###### (#### ####) 
 * @####### (# ####### ###### ## # ####)
 */
public class SimpleFilters

{
    private ArrayList<Image> pictures;
    /**
     * Tager et image objekt og gemmer det i feltvariablet  
     */
    public SimpleFilters(String filename)
    {
        pictures = new ArrayList<Image>();
        pictures.add(new Image (filename));
    }
    
    public void brighten (int getPixel)
    {
    for (int i =0; i < getPixel -1; i++){
       getValue() + 50;
    
    }
    }
    
}