import java.util.*;
import java.io.*;
    
public class Vehiculo extendsJPane&    {
    private String marca;
        private String modelo;
        private int ruedas;
        private int cv;
        private int plazas;
        private String combustible;
        private int velocidad;
        
        
    public Vehiculo()
    {      
    }
    public Vehiculo(String marca, String modelo, int ruedas, int cv, int plazas, String combustible, int velocidad) {
        this.marca = marca;
            this.modelo = modelo;
            this.ruedas = ruedas;
            this.cv = cv;
            this.plazas = plazas;
            this.combustible = combustible;
            this.velocidad = velocidad;
        }

        public String getMarca() {
            return marca;
        }

        public void setMarca(String marca) {
            this.marca = marca;
        }

        public String getModelo() {
            return modelo;
        }

        public void setModelo(String modelo) {
            this.modelo = modelo;
        }

        public int getRuedas() {
            return ruedas;
        }

        public void setRuedas(int cv) {
            this.ruedas = cv;
        }

        public int getcv() {
            return cv;
        }

        public void setcv(int cv) {
            this.cv = cv;
        }

        public int getplazas() {
            return plazas;
        }

        public void setplazas(int plazas) {
            this.plazas = plazas;
        }

        public String getCombustible() {
            return combustible;
        }

        public void setCombustible(String combustible) {
            this.combustible = combustible;
        }

        public int getvelocidad() {
            return velocidad;
        }

        public void setvelocidad(int velocidad) {
            this.velocidad = velocidad;
        }
        
      }
		