package zad_nowe;
import java.util.*;

public class zad2
{
    public static void main ()
    {   Scanner in = new Scanner(System.in);
        drukuj("Podaj pierwszą część przedziału:" );
        int a = in.nextInt();
        drukuj("Podaj drugą część przedziału:" );
        int n = in.nextInt();
        int suma=0;  
        int a = i;
        for (i;i<n;i++)
        { if(i%6==0)
            {suma=suma+i;
            }

        }
        drukuj (""+suma);
    }

    public static void drukuj(String tekst)
    {
        System.out.println(tekst);
    }

}    