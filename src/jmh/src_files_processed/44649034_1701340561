{
    class Fare
    {
        public static void main(String args[])
        {
           int no;
           String t;
           String fare;
        {
           if( t.equalsignoreCase ("CHANDIGARH EXPRESS")||t.equalsignoreCase ("SHATABDI")||t.equalsignoreCase("JANSHATABDI"));
            {
                if (no<5);
                {
                    fare=200;
                    {
                        if (no<10);
                        {
                            fare=350;
                        }
                        
                                
                              else
                              {
                                  fare=450;
                                }
                                t.ignorecase MUMBAI EXPRESS||JAIPUR EXPRESS||RAJASTHAN EXPRESS;
                                {
                                    if no<5;
                                    {
                                        fare=100;
                                        {
                                            if no<10;
                                            {
                                                fare=200;
                                                {
                                                    else
                                                    {
                                                        fare=300;
                                                    }
                                                    return(fare);
                                                }
                                            }
                                        }

                                    
                                
           
           
    
            
        
        
        
            
            
            
            
                
    
    
    
    
    
    
    