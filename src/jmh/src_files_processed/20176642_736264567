import java.util.Scanner;

public class HollywoodCity
{
   private static Scanner keyboard = new Scanner(System.in);
   
   public static void main(String[] args) {
 
       System.out.print("How many houses in the street? ");
       int numberOfHouses = keyboard.nextInt();
       System.out.println("");
       
       int []houseNumbers = new int[numberOfHouses];
       
       for (int i = 0; i < numberOfHouses ; i++){
           System.out.print("What is the next house number?   : ");
           houseNumbers[i] = keyboard.nextInt();
        }
       System.out.println("");
       
       int [][] houseAges;
       houseAges = new int[numberOfHouses][];
       
       for (int house = 0; house < numberOfHouses; house++) {
           System.out.print("How many people live in house " + houseNumbers[house] + ": ");
           int numberOfPeople = keyboard.nextInt();
           
           houseAges[house] = new int[numberOfPeople];
           
           for (int people = 0; people < numberOfPeople; people++) {
               int person = people +1;
               System.out.print("What is the age of person " + person + " ? ");
               int age = keyboard.nextInt();
               
               houseAges[house][people] = age;
            }  
           System.out.println("");
        }
        
        
        for (int house = 0; house < numberOfHouses; house++) {
            for (int people : houseAges[house]) {
                System.out.print([house][people]);
            }
        }
       
    }
}