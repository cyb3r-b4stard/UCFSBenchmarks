 */
public class Dice
{
    private int sides;

    /**
     * Constructor for objects of class Dice
     */
    public Dice()
    {
        sides=6;
    }
    /**
     * 
     */
    public Dice(int k)
    {sides=k;}
    /**
     * 
     */
   public int   throw()
   {
   }

}