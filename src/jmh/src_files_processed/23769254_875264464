public class VotingMachine
{
    private double democrat;
    private double republican;
    public VotingMachine()
    {
        democrat = 0;
        republican = 0;
    }
    public double getDemocrat()
    {
        return democrat;
    }
    public double getRepublican()
    {
        return republican;
    }
    public void setDemocrat(double sd)
    {
       democrat = sd;
    }
    public void setRepublican(double sr)
    {
        republican = sr;
    }
    public double getDemocratCount()
    {
         double democratCount += 1;
        return democratCount;
    }
    public double getRepublicanCount()
    {
         double republicanCount += 1;
        return republicanCount;
    }
    public String toString()
    {
        String str = "The number of Democratic votes are:" + getDemocratCount();
        str += "\n" + "The number of Republican votes are:" + getRepublicanCount();
        return str;

}
}