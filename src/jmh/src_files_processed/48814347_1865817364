import java.io.*;
public class Nazivnik{
    public static void main(String[]args) throws IOException{
        BufferedReader citac=new BufferedReader ( new InputStreamReader(System.in));
        int a,b;
        System.out.println( " Unesi a ");
        a=Integer.parseInt(citac readLine());
        System.out.println ( " Unesi b " );
        b=Integer.parseInt(citac readLine());
        c=fact(b)/fact(a)*fact(b+a);}
        public static int fact (int i);
        int f=1,n=0;
        for (i=2;i<=n;i++)
        f=f*i;
        return f;
    }
        
        
        
        
        
        
    
            
             
            

            
            
