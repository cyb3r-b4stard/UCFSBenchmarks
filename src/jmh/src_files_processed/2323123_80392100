public class SmartVic extends Vic
{   
    public boolean hasJustOneOnStack()
    {   short CDs = 0;
        if (stackHasCD())
            takeCD();
            CDs++;
        else    
            return false;
        moveOn();
    }   
    

     {    moveOn();
          takeCD();
     }    
     
     public void movePut()
     {  moveOn();
        putCD();
     }    
     
     public void backTake()
     {  backUp();
        takeCD(); 
     }    

     public void backPut()  
     {    backUp();
          putCD();
     }    
}     