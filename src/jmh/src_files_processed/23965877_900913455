public class crapsTest
{
    public static void main( String [] args) { 
        Craps game = new Craps();
        System.out.println(game.getMessage());
        game.comeOut();
        System.out.println(game.getMessage());
        while (game.getPoint() !=-1){
            game.roll();
            
            System.out.println(game.getMessage());
        
    }
}