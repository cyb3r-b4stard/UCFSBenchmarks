import java.util.*;
public class Cone implements Shape{
    private double radius;
    private double height;
    private Cone (double a, double b){
        radius=a;
        height=b;
    }
    public double getSurfaceArea (){
        return (Math.sqrt(height*height*radius*radius)+radius)(3.1415*radius);
    }
    public double getVolume (){
        return 3.1415*radius*radius*(height/3);
    }
} 
