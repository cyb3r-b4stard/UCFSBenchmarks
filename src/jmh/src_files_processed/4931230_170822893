import java.io.*;
import java.util.Scanner;
public class Pineda
{
    public static void main (String[]args) 
    {
        int Math, Physics, English;
        double Average = 0.0;
        Scanner in = new Scanner(System.in);
        System.out.println("Enter a grade in Math:");
        Math = in.nextInt();
        System.out.println("Enter a grade in Physic:");
        Physics = in.nextInt();
        System.out.println("Enter a grade in English:");
        English = in.nextInt();
        Average = (Math + Physics + English)/3;
        System.out.println ("Average: " + Average );
         if ( Average >=75 );
         
           { System.out.println( Average + " is PASSED") ;}
             else 
             {System.out.println( Average + " is FAILED") ;}
         
         
 
    }
}