public class Vehicle
{
    double horsepower;
    double aerodynamics1;
    double weight;
    double acceleration;
  
 
    
    public Vehicle()
    {
        horsepower = 0;
        aerodynamics1 = 0; 
        weight = 0;
    }
    
    public double gethorsepower()
    {
        return horsepower;
    }
    
    public void sethorsepower(double x)
    {
        this.horsepower = x;
    }
    
    public double getaerodynamics1()
    {
        return aerodynamics1;
    }
    
    public void setaerodynamics1(double y)
    {
        this.aerodynamics1 = y;
    }
    
    public double getweight()
    {
        return weight;
    }
    
    public void setweight(double z)
    {
        this.weight = z;
    }
    
    public double acceleration(double aerodynamics)
    {
        acceleration = (100/horsepower)*aerodynamics*weight/100;
        return acceleration;
    }
    
    public double gettopspeed()
    {
        return double topspeed;
    }
    
    public void settopspeed(double x)
    {
        this.topspeed = x;
    }
   
}