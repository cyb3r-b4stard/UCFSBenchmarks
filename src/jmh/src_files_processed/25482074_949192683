
public class number4
{
    public static void main(String [] args)
    {
        int a = 3, b = 10, c = 7;
        double w = 12.9, y = 3.2;
        
        System.out.println(" a. a + b * c =" + (a + b * c) );
        System.out.println(" b. a -b - c =" + (a- b- c) );
        System.out.println(" c. a/b = " +(a/b) );
        System.out.println(" d. b/a = "(b/a) );
        
    }
}
