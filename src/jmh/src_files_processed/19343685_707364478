
/**
 * ##### # ########### ## ##### ############## ####.
 * 
 * @###### (#### ####) 
 * @####### (# ####### ###### ## # ####)
 */
public class RegistroPuesto
{
  private String tipoPuesto;
  private int sueldo;
  private String fechaIngreso;
  private String estadoCivil;
  private boolean experiencia;
  
  public RegistroPuesto ()
  {
   tipoPuesto = "Bodeguero";
   sueldo = 276000;
   fechaIngreso = "16/05/2013";
   estadoCivil = "Separado";
   experiencia = true;
  }
  
  public RegistroPuesto (String tipoPuesto, int sueldo, String fechaIngreso, String estadoCivil, boolean experiencia)
  {
   this.tipoPuesto = tipoPuesto;
   this.sueldo = sueldo;
   this.fechaIngreso = fechaIngreso;
   this.estadoCivil = estadoCivil;
   this.experiencia = experiencia;
  }
  
  public String getTipoPuesto ()
  {
   return tipoPuesto;
  }
  public int getSueldo ()
  {
   return sueldo;
  }
  public String getFechaIngreso ()
  {
   return fechaIngreso;
  }
  public String getEstadoCivil ()
  {
   return estadoCivil;
  }
  public boolean getExperiencia ()
  {
   return experiencia;
  }
  public void setTipoPuesto (String tipoPuesto)
  {
   this.tipoPuesto = tipoPuesto;
  }
  public void setSueldo (int sueldo)
  {
   this.sueldo = sueldo;
  }
  public void setFechaIngreso (String fechaIngreso)
  {
   this.fechaIngreso = fechaIngreso;
  }
  public void setEstadoCivil (String estadoCivil)
  {
   this.estadoCivil = estadoCivil;
  }
  public boolean setExperiencia (boolean experiencia)
  {
   this.experiencia = experiencia;
  }
  
  adsf;
}