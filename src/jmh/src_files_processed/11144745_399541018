 
public class TicketMachine


{ private String city;
  private int price;
  private int balance;
  private int due;
  private int takings;
  
  public TicketMachine(String city, int price)
    { 
        this.city=city;
        this.price=price;
        this.balance=0;
        this.due=0;
        this.takings=0; 
    
    }
    
    public String getCity()
    {
        return this.city;
    }
    
    public int getPrice()
    {
        return this.price;
    }
    
    public int getTakings()
    {
        return this.takings;}
        
    public void choseTicket (String ticketType)
    {

   if (ticketType =="SINGLE") 
    
    {
        this.due = price;
        
    }
    
     else if(ticketType == "RETURN")
    
    {
        this.due = price*2;
    }
}
public int insertMoney(int moneyEntered)
{
    return this.balance = balance + moneyEntered;
}
public String buyTicket()
{
    if(due >= due)
    {if(balance >=due)
        {
            double dueInPounds = due / 100.0;
            
            System.out.printIn("################");
            System.out.printIn("#"+city+"Subway Ticket")
        }
        
        

    

    
    