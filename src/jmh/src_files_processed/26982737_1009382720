import java.util.ArrayList;
public class Poop
{
    public static void main(String [] args)
    {

    }

    public static double getOrderTotal(int bp,int nb)
    {
        int total=bp+nb;
        if(nb==1&&bp==1)
            return 39.95;
        if(total>=3&&total<12)
            return 16.95*total;
        if(total>=12)
            return 16*total;
        else return bp*20.95 +nb*21.95;
    }
    public static Arraylist<String>()
    {
    
    }
    
}