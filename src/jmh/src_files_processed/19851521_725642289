


    public class aula
     {
               public static void  main(String args [])
               {
        
                  int  x=10;
                   int  y[]=(3, 4, 9, 20);
                  y{3}=90;
                   x=y [1]+y[2]+y[3];
                   for (int i = 0;i<y.length;i++){
                       x+=y [i];
                   
    
        }
        system.out.println(x);
    }