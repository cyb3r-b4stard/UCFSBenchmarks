
import java.util.*;
public class examPREP
{
    public static void main(String[] args)
    {
      Scanner s = new Scanner(System.in);
        
        int[][] student = new int[20][3];
        
        for (int i=0; i<20; i++){
            for (int j=0; j<3; j++){

                System.out.print("Enter Student score: ");
                int student[i][j]= Integer.parseInt(s.nextLine());
            }
        }
        
    }
}