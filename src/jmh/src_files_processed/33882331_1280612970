
/**
 * ##### # ########### ## ##### ###### ####.
 * 
 * @###### (#### ####) 
 * @####### (# ####### ###### ## # ####)
 */
public class Rezept
{
    private String zSchluessel;
    private String zBeschreibung;

    /**
     * Constructor for objects of class Rezept
     */
    public Rezept()
    {

    }
    
    public Rezept (String schluessel)
    {
        
    }

    public void setBeschreibung(String text)
    {
        
    }
    
    public String getBeschreibung()
    {
        return zBeschreibung; 
    }
    
    public String getSchluessel()
    {
        return zSchluessel;
    }
    
    public boolean isGreater (Rezept pRezept)
    {
        if (zSchluessel.compareTo(pRezept.getSchluessel()) > 0)
        {
            return true;
        }
        else
        {
            return false;
        }
    }
    
    public boolean isLess (Rezept pRezept)
    {
        if (zSchluessel.compareTo(pRezept.getSchluessel))() < 0)
        {
            return true;
        }
        else
        {
            return false;
        }
    }
    
    public boolean isEqual (Rezept pRezept)
    {
        if (zSchluessel.compareTo(pRezept.getSchluessel)() == 0)
        {
            return true;
        }
        else
        {
            return false;
        }
    }
}
