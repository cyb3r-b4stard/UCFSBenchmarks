package alla;
public class Operator
{ public static void main (String [] args) {
   
   int a=1;
  int  b=4;
   int c=2;
  int  d=9;
    if ((a<b)&&(a<c) && (a<d))
    {
    System.out.println ("Min --> a ");
    
}
else{
if ( (b<a) && (b<c) && (a<d) )
{ System.out.println ("Min --> b");
}
 else 
 { if ((c<a)&& (c<b) && (c<d))
     {System.out.println ("Min --> c");}
     else {
         if ((d<a)&& (a<b) && (a<c) )
         {System.out.println ("Min-->d")}}


}