import static java.lang.System.*;
import java.util.Scanner;
public class DigAdd
{
    private int numD;
    int count;
    int sum;
    public DigAdd()
    {
        numD=0;
    }
    public DigAdd(int num)
    {
        numD=num;
    }
    public void setNumber(int num)
    {
        numD=num;
    }
    public int getDigits()
    {
        for(int count=0;count<5; count++)
        {
            numD/=10;
        }
        return
    }
    public int getSum()
    {
        for(int sum=0;numD>0;)
        {
            sum=numD%10+sum;
            numD/=10;
        }
        return 
    }
}