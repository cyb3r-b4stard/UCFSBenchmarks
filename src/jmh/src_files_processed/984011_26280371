import java.io.*;
import java.util.*;
public class Gpas{
    public static void main(String args[]){
        System.out.print("Input GPS's here:  \n");
        Scanner input1=new Scanner(System.in);
        double first=input1.nextDouble();
        Scanner input2=new Scanner(System.in);
        double second=input2.nextDouble();
        Scanner input3=new Scanner(System.in);
        double third=input3.nextDouble();
        Scanner input4=new Scanner(System.in);
        double fourth=input4.nextDouble();
        if (first<2.0) {
            if (first<0) System.out.println("Did you honestly get a "+first+"?"
            System.out.println("You got "+first+"? Great job genious <_<");
        else System.out.println(first+"?  Not too shabby");
        if (second<2.0) System.out.println("You got "+second+"? Great job genious <_<");
        else System.out.println(second+"?  Not too shabby");
        if (third<2.0) System.out.println("You got "+third+"? Great job genious <_<");
        else System.out.println(third+"?  Not too shabby");
        if (fourth<2.0) System.out.println("You got "+fourth+"? Great job genious <_<");
        else System.out.println(fourth+"?  Not too shabby");
    }
}
       
        
        