import java.util.Scanner;
import java.io.File;

public class Book
{
    private String bookString;
    private String bookTitle;
    private String bookAuthor;
    
    public Book(String fileName, String title, String author)
    {
        readBook(fileName);
        bookTitle = title;
        bookAuthor = author;
    }
    public boolean containsTitle(title);
}