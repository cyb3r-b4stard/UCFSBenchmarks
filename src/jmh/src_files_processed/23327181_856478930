
/**
 * ##### # ########### ## ##### ######## ####.
 * 
 * @###### (#### ####) 
 * @####### (# ####### ###### ## # ####)
 */
public class practice
{
   void main()
   int i,j;
   int k=0;
   for (i=5;i>=1;i--)
   {
       for(j=1;j<=i;j++)
       {
           k++;
           System.out.println(k+ " ");
        }
        System.out.println();
    }
}

   