
public enum Monat
{
    JAENNER(1,"January", "Jaenner"),
    FEBRUAR(2, "February","Jaenner"),
    MAERZ(3, "March","Jaenner"),
    APRIL(4, "April","Jaenner"),
    MAI(5, "May","Jaenner"),
    JUNI(6, "June","Jaenner"),
    JULI(7, "July","Jaenner"),
    AUGUST(8, "August","Jaenner"),
    SEPTEMBER(9, "September","Jaenner"),
    OKTOBER(10, "October","Jaenner"),
    NOVEMBER(11, "November","Jaenner"),
    DEZEMBER(12, "December","Jaenner");
    
    
    private int mnum;
    private String monatsnameEnglisch;
    private String monatsnameDeutsch;
    
    private Monat(int mnum, String monatsnameEnglisch,monatsnameDeutsch)
    {
        this.monatsnameEnglisch=monatsnameEnglisch;
        this.monatsnameDeutsch=monatsnameDeutsch;
        this.mnum=mnum;
    }
    
    
}