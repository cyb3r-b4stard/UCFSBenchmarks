/**
 * 
 */
class Time1 {
    private int _hour ;int _minute;int _second;
     /**
     * The test method if the time is real , if not the time will "00:00:00"
     */
    public Time1(int h,int m,int s){
        if(h<0 || h>23)
            h=0;
        if(m<0 || m>59)
            m=0;
        if(s<0 || s>59)
            s=0;
        _hour=h; _minute=m; _second=s;
    }
    /** The new second constructor
     * 
     */
    public Time1(Time1 other){
    }
    /** =======================
     * 
     */
    public int getHour(){
        return _hour;
    }
    public int getMinute(){
        return _minute;
    }
    public int getSecond(){
        return _second;
    }
    public void setHour(int num){
        if (num>=0 && num<=23)
        _hour=num;
         }
    public void setMinute(int num){
        if (num>=0 && num<=59)
        _minute=num;
    }
    public void setSecond(int num){
        if (num>=0 && num<=59)
        _second=num;
    }
    public String toString(){
        if(_hour<10)
        String res="";
        if(_Hour<10)
            res=res+"0";
        res=res+_hour+":";
        return res;    

    }
    
}
    
   
    
    
    
        