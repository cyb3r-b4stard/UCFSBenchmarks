import java.io.* ;
class xyz
{
    public void input() throws IOException
    {
      BufferedReader br = new BufferedReader(new InputStreamReader(System.in)) ;
      FileWriter fw = new FileWriter("name.txt");
      BufferedWriter bw = new BufferedWriter(fw);
      PrintWriter pw = new PrintWriter(pw) ;
      System.out.println("Enter name , rollno, marks of 5 subjects") ;
      for(int i = 1 ; i<=3 ; i++)
      {
          int rollno = Integer.parseInt(br.readLine ());
          String nm = br.readLine () ; 
        int m1 = Integer.parseInt(br.readLine ());
        int m2 = Integer.parseInt(br.readLine ());
        int m3 = Integer.parseInt(br.readLine ());
        int m4 = Integer.parseInt(br.readLine ());
        int m5 = Integer.parseInt(br.readLine ());
        pw.println(rollno) ;
        pw.println(nm) ;
        pw.println(m1) ;
        pw.println(m2) ;
        pw.println(m3) ;
        pw.println(m4) ;
        pw.println(m5) ;
    }
    pw.close() ;
    }
    public void input() throws IOException
    {
        FileReader fr = new FileReader("name.txt") ;
        BufferedReader brf = new BufferedReader (fr) ;
        String txt ;
        int rno ;
        int num ;
        int num[] = new int[5] ;
        while((rno=Integer.parseInt(brf.readLine()))!=-1&&(txt=brf.readLine())!=""&&(num=Integer.parseInt(brf.readLine()))!=-1) 
        {
         System.out.println(rno) ;
         System.out.println(txt) ;
         for(int i=1 ; i<=5 ; i++)
         {
         System.out.println(num[]=num)
        }
        brf.close();
        }
    }
}