
import cha.*;
import java.awt.*;
import javax.swing.*;
import java.util.*;

import static java.awt.Color.*;
import static javax.swing.SwingConstants.*;

public class Neighborhood 
extends CHApplet 
{
    public void init() {
        House house1;
        house1 = new House();
        add(house1);        
        
        House house2;
        house2 = new House();
        add(house2);        
        house2.setLocation(50, 220);
        
        House house3;
        house3 = new House();
        add(house3);
        house3.setBounds(325, 210, 50, 40);
        
        house2.addBird();
        
        CHLabel label;
        label = new CHLabel();
        add(label);
        label.setText"Evening in the Neighborhood by Brandon Doherty";
        label.setBounds(300,400,50,250);
        
    }
    
    
}