public class PersonTester
{
    public static void main (String[] args){
        Person p1 = new Person();
        Person p2 = new Person(25, 75, "male", false);
        
        System.out.println(p1.toString());
        
        System.out.println("\n"p2.toString());
        
        p2.setAge(26);
        
        System.out.println("\n"p2.toString());
    }
}