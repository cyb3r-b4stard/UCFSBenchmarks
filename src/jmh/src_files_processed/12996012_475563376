class Huisdier{
    String Naam, Soort;
    int Leeftijd;
    Eigenaar eigenaar;
    
    Huisdier (String n){
        Naam=n;
        Soort="?";
        Leeftijd=0;
        Eigenaar=null;
    }
    
    void kenEigenaarToe(Eigenaar e){
        eigenaar=e;
    }
    
    void kenLeeftijdToe(Leeftijd l){
        Leeftijd=l;
    }
    
    void kenSoortToe(Soort s){
        Soort=s;
    }
    
    void toonNaam(){
        System.out.println("naam = " + naam);
    }
    
    void toonEigenaarsGegevens(){
        if (eigenaar==null){
            System.out.println("geen eigenaar");
        }
        else if{
            eigenaar.toonEigenaarGegevens();
        }
    }
}