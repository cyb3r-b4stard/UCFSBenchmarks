public class Quadratic
{
    public static void main(String[] args)
    {
        System.out.print("Please enter a,b, and c for a quadratic equation. ");
        double a1 = Double.parseDouble(args[0]);
        double b1 = Double.parseDouble(args[1]);
        double c1 = Doulbe.parseDouble(args[2]);
     
        System.out.println("One root of your equation is: " + quad(a1, b1, c1));
    }
    public static double quad(double a2, double b2, double c2)
    {
        double root = (-b2 + Math.sqrt((b2*b2) - (4*a2*c2))/ (2*a2);
        return root;
    }