import java.util.Date;
public class Account 
{	
public int id = 0; 	
public double balance = 0; 	
public double annualInterestRate = 0; 	
public Date dateCreated;	
public Account() 
{	
dateCreated = new Date();	
}

public Account(int newID, double newBalance, double interestRate) 
{	
dateCreated = new Date();	
id = newID;	
balance = newBalance;
annualInterestRate = interestRate;	
}

public int getID() 
{	
return id;	
}

public void setID(int newID) 
{	
id = newID;	
}

public double getBalance() 
{	
return balance;	
}

public void setBalance(double newBalance) 
{	
balance = newBalance;	
}

public double getAnnualInterestRate()
{	
return annualInterestRate;	
}

public void setAnnualInterestRate(double newAnnualInterestRate) 
{	
annualInterestRate = newAnnualInterestRate;	
}

public Date getDateCreated() 
{	
return dateCreated;	
}

public double getMonthlyInterestRate() 
{	
return annualInterestRate/12;	
}

public void withdraw(double withdrawAmount)
{	
balance -= withdrawAmount;	
}

public void deposit(double depositAmount) 
{	
balance += depositAmount;	
}
public void deposit(int depositAmount) 
{	
balance += depositAmount;	
}
}

public class Savings extends account{
public Savings(int newID, double newBalance, double interestRate)
{
super(newID, newBalance, interestRate);
}
public void withdraw(double withdrawAmount)
{	
balance = super.getbalance
balance -= withdrawAmount;
if(balance<0)
System.out.println("Insufficient funds to withdraw!"); 
}
public String toString()
{
return "Account ID: "+id+"\nBalance: "+balance+"\nAnnual Interest rate: "+annualInterestRate+"\nDate of the Account Creation:"+dateCreated;
}
}

public class Checking extends account{
final double overDraft = 5000;
public Checking(int newID, double newBalance, double interestRate)
{
super(newID, newBalance, interestRate);
}
public void withdraw(double withdrawAmount) 
{
balance = super.getbalanace
balance +=overDraft;
balance -= withdrawAmount;
if(balance<0)
System.out.println("Insufficient funds to withdraw!");

}
public String toString()
{
return "Account ID: "+id+"\nBalance: "+balance+"\nAnnual Interest rate: "+annualInterestRate+"\nDate of the Account Creation:"+dateCreated;
}
}
