
/**
 * ##### # ########### ## ##### ###### ####.
 * 
 * @###### (#### ####) 
 * @####### (# ####### ###### ## # ####)
 */
public class APLine
{
    private int ayy, bee, cee;
    private double slope;
  public APLine (int a, int b, int c)
  {
      ayy = a;
      bee = b;
      cee = c;
}
public double getSlope()
{
    slope = ( ayy / bee ) * -1;
    return slope;
}
public boolean isOnline(int x, int y)
{
    if ((ayy * x) + (bee * y) + cee) = 0
    return true;
}
}