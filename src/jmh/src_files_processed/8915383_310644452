public class Colegio
{
   
  
   private String nombre;
   private String ciudad;
   private String comuna;
   private int cantEstudiantes;
   
   public Colegio(){}
   
   public Colegio(String nombre,String ciudad,String comuna,int cantEstudiantes)
   {
  this.nombre = nombre;
  this.ciudad = ciudad;
  this.comuna = comuna;
  this.cantEstudiantes = cantEstudiantes;
  
 
  matricularEstudiantes();
  
   
  

   public void ImprimirDatos()
   {
    
    
   System.out.println("Datos de estiudiantes");
   
   System.out.println("Nombre: "+ nombre);
   System.out.println("ciudad: "+ ciudad);
   System.out.println("Comuna: "+ comuna);
   System.out.println("cantidad de Estudiantes: "+ cantEstudiantes);
   }
   
   public void matricularEstudiantes()
   {
  
   System.out.println("Cantidad de estudiantes fueron"+cantEstudiantes);
   
       
   }
   
   public void pagarProfesores()
   {
       System.out.println("El monto total asciende a 5.000-000 ");
   }
   
}
