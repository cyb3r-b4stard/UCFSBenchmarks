 */
public class BinarySub1
{
     static String x ;

    /**
     * Constructor for objects of class BinarySub1
     */
    public BinarySub1()
    {
     
    }

    public static void main(String[] args){
        x = "111";
        int xlength=x.length();
        char[] y =new char[xlength()]
        for(int i=0;i<xlength;i++){
            y[i]=x.charAt(i);
        }
        
        for(int i=x.length()-1;i>=0;i--){
           if(x[i]=='1'){
               x[i]
            }
    }
}
    
}