

public class me
{
    public static void main()
    {
        int k=5;char n= 'a'

        for(int j=1;j<=5;j++)
        {
            for(int i=j;i<=k;i++)
            {
                System.out.print((char)n);
                
            }
            System.out.print("\n");
            n++;
            k--;
        }
    }
}