 */
public class Seal
{
    private String name;

    public Seal(String newName)

    {

        name = newName;

    }

    public String getName()
    
    {

        return name;

    }

    public void roar()

    {

        System.out.println(&quot;ROOOOOOAAAAAAAAAAAAAAR!!!!!!!!!&quot;);

    }
}