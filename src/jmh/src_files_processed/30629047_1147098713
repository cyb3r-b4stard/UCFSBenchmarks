public class DayClient
{
    public static void main(String args[])
    {
        Day d1 = new Day(12,14,"Monday",true);
        String s="";
        if(d1.getIsAM())
        s = "AM";
        else
        s="PM";
        System.out.println(d1.getName()+": " + d1.getHour()+ ":" +d1.getMinute();
        Day d2 = new Day(11,59,"Tuesday",false);
        if(d2.getIsAM())
        s="AM";
        else
        s="PM";
        System.out.println(d2.getName()+": " + d2.getHour()+ ":" +d2.getMinute();
    }
}
            