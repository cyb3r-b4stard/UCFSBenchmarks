

public class Cliente
{
    private String nombre;
    private int rut;
    private String direccion;
    private String comuna;
    
    
    public Cliente(){
        this.nombre="luis salcedo";
        this.rut=18219164-7;
        this.direccion="pasaje las orquideas #0865";
        this.comuna="la granja";
    }
    
    public Cliente(String nombre,int rut, String direccion, String comuna){
        this.nombre=nombre;
        this.rut=rut;
        this.direccion=direccion;
        this.comuna=comuna;
    }
    
    public String getNombre(){
        return this.nombre;
    }
    
    public int getRut(){
       return this.rut;
    }
    
    public String getDireccion(){
        return this.direccion;
    }
    
    public String getComuna(){
        return this.comuna;
    }
     
    public void setNombre(){
        this.nombre=nombre;
   
    }
    
    public void setRut(){
        this.rut=rut;
    }
    
    public void setDireccion(){
        this.direccion=direccion;
    }
    
    public void setComuna(){
        this.comuna=comuna
    }
}