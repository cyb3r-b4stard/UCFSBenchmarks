public class Lanzador
{
    public static void main(String[] args )
    {
       
       Student student;
       student = new Student();
       student.getNombre();
       student.getApellidos();
       student.getDate();
       student.getTlf();
       student.getDir();
       student.getFamilia();
       Student.setPadron(boolean student.padron);
       student.mostrarInfoAlumno();
       
    }
    
    
}