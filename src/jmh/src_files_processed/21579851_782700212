public class Tester
{
 public static void main(String[args])
 {
     String s1 = "Allan Alda";
     String s2 = "John Wayne";
     String s3 = "Gregory Peck";
    
     String 1 = s1.substring(2,6);
     String 2 = s2.substring(2,6);
     string 3 = s3.substring(2,8);
     
     System.out.length(1);
     System.out.length(2);
     System.out.length(3);
     
     
 }

 
}