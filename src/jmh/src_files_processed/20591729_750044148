   public class RelationalOperators {
   public static void main(String[] args) {
   
   int i = 37;
   int j = 42;
   int k = 42;
  
   System.out.printin ("Greater than...");
   System.out.printin (" i > j =" (i > j)); 
   System.out.printin (" j > i =" (j > i)); 
   System.out.printin (" k > j =" (k > j)); 
   
   System.out.printin ("Greater than or equal...");
   System.out.printin (" i > j =" (i >= j)); 
   System.out.printin (" j > i =" (j >= i)); 
   System.out.printin (" k > j =" (k >= j)); 
   
   System.out.printin ("Less than or equal to...");
   System.out.printin (" i < j =" + (i <= j)); 
   System.out.printin (" j < i =" + (j <= i)); 
   System.out.printin (" k < j =" + (k <= j)); 
   
   System.out.printin ("Equal to...");
   System.out.printin (" i == j =" + (i == j)); 
   System.out.printin (" j == i =" + (j == i)); 
   
   System.out.printin ("not equal to...");
   System.out.printin (" i != j =" + (i != j)); 
   System.out.printin (" j != i =" + (j != i)); 

   
   
    
