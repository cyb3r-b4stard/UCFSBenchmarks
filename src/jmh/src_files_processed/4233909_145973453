public class Course
{
    private String courseId;
    private String courseName;
    private String teacher;
    private String students[];
    private int student_count;
    
    public Course ()
    {
        courseId = "";
        courseName = "";
        teacher = "";
        students [] = new String[50];
        student_count = 0;
    }
    
    public Course (String _courseId, String _courseName, String _teacher, String _students, int _student_count)
    {
        courseId = _courseId;
        courseName = _courseName;
        teacher = _teacher;
        students = _students;
        student_count = _student_count;
    }
    
    public void setCourseId (String _setcourseId)
    {
        courseId = _setcourseId;
    }
    public void setCourseName (String _courseName)
    {
        courseName = _courseName;
    }
    public void setTeacher (String _teacher)
    {
        teacher = _teacher;
    }
    public void register (String _register)
    {
        students [student_count] = _register;
        student_count++;
    }
    
    public String getCourseId ()
    {
        return courseId;
    }
    public String getCourseName ()
    {
        return courseName;
    }
    
    
    public void printTeacher ()
    {
        System.out.println ("Teacher : "+teacher);
    }
    public void printStudents ()
    {
        System.out.println ("Students : "+students );
    }
}
    
    
    
    