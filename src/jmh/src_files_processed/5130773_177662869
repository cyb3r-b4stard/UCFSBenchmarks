import java.io.*;

public class stu
{
    public String[50] nm;
    int tot;
public void ac()throws IOException
{
    BufferedReader br=new BufferedReader(new InputStreamReader(System.in));
    System.out.println("enter straingth of the stu");
    tot=Integer.parseInt(br.readLine());
    
    for(int i=0;i<=tot;i++)
    {
        System.out.println("enter roll no "+(i+1));
        String g=br.readLine();
        nm[i]=g;
    }
    
}
public void dis()throws IOException
{
    for(int k=0;k<=tot;k++)
    System.out.println(k+nm[k]);
}

public static void main(String args[])throws IOException
{
    stu s=new stu();
    s.ac();
    s.dis();
}
}