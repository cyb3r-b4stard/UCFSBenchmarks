public class Haupt
{public static void main(string args[]){
    int [] unsortiert = {4,1,8,-3,5,7};
    insertion Sorter; S=new InsertionSortier();
    S.Sort(unsortiert);
    unsortiert = S.sortiert ();
    for (int i=0; i < unsortiert.lenght;i++){
        System.out.println(unsortiert[i]++ " ");
    }
}
}

