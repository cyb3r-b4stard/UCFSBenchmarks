public class Triangle
{
    private int angle1, angle2, angle3;
    angle1 =0;
    angle2 =0;
    
    public Triangle (int angle1, int angle2)
    {
        this.angle1 = angle1;
        this.angle2 = angle2;
    }
    
    public void setAngle1 (int newAngle1)
    {
        this.angle1 = newAngle1;
    }
    public void setAngle2(int angle2)
    {
        this.angle2 = angle2;
    }
    public void setAngle3 (int angle3)
    {
        this.angle1-this.angle2=angle3;
    }
    public void getAngle3()
    {
        return this.angle3;
    }
}