import java.util.ArrayList;
import java.util.Iterator;


class BookCase{
    
    private String[] titles;
    public BookCase(){
        
        titles = new String[4];
        titles[0] = "objects first";
        titles[1] = "save the pixel";
        titles[2] = "the catcher in the rye";
        titles[3] = "to kill a mockingbird";   
    }
    
    public String getLongestTitle() {
        String longestTitle = null;
        
        for(int i = 0; i <= 3 ; i ++)
           {
            if (titles[0].length() > longestTitle[i].length(){
                longestTitle = titles; 
            }
        }
        return longestTitle; 
    }
    
    public boolean doesBookcaseContainString(String substring){
       Iterator<String> it = titles.iterator();
       String currentTitle = null;
      
       while(it.hasNext()){
          currentTitle = it.next();
          if(currentTitle.contains(substring)){
              return true;
          }
       }
       return false;
    }
}





