
/**
 * @###### (###### ######) 
 * @####### (##/##/####)
 */
public class Products
{
    private int prijs;
    private int verzendKost;
    private int typeNummer;
    private String merk;
    private enum categorie{};
    
    private Products result;
    

    /**
     * Constructor van Product
     * @param Geef in het type String: de naam, het merk, en de cathegorie op
     * @param Geef in het type int: de prijs dat het product moet kosten 
     * @param Geef in het type int: het type nummer, waar ze die op kunne zoeken
     * @param Geef in het type int: verzendkost
       
       */
    
    public  Products(String naam ,  int prijs,  int verzending , int typeNummer ,String merk, catehorie){
       
        this.prijs = prijs;
        verzendKost = verzending;
        this.typeNummer = typeNummer;
        this.merk = merk;
        this.cathegorie = cathegorie; 
       
    }
      
    
    
    /** Verander de prijs van het product
     * @param geef in het type int : de nieuwe prijs op 
     */
      public void setPrijsVerandering (int newPrice){
        prijs = newPrice;
      }

    
    /**
     *  Vraag de cathegorie van het product op
     */
    public String getCategorie(){
        return  categorie;
    }
    /**
     *  Vraag de prijs van het product op
     */
    public int getPrijs() {
        return  prijs;
    }
    /**
     *  Vraag het merk van het product op
     */
    public String getMerk() {
        return  merk;
    }
    /**
     *  Vraag het typenummer  van het product op
     */
    public int getType() {
        return  typeNummer;
    }
}
    