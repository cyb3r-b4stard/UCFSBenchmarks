import java.util.ArrayList;
import java.util.Random;
public class ArraysUnit
{
    public static void main (String []args)
    {
          ArrayList<Integer> myArray = new ArrayList<Integer>();
          fill(myArray);
    }
    public static void fill(ArrayList<Integer> myArray)
    {
        Random generator = new Random();
        for(int x=0;x<10;x++)
        {
            myArray.add(generator.nextInt(101));
            System.out.print(myArray.get(x) + " ");
        }
    }
    public static void listDelEasy(ArrayList<Integer> myArray)
    {
        int x =0;
        while(x<10)
        {
            if (myArray.get(x)%3==0||(myArray.get(x)==10))
            {
                myArray.remove(x)
                myArray.add(5);
            }
            
        }
            
    }
}
