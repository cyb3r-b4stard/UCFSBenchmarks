import java.util.Scanner;
public class Month{
    public static void main (String [] args){
    int month;
    System.out.println("Enter a number from 1-12;");
    Scanner in = new Scanner (System.in);
    month=in.nextInt();

    if(month==1){
        System.out.println("1 is January. Happy New Year!");
    }
    else if(month==2){
        System.out.println("2 is February. Happy LOVE Month!");
    }
    else if(month==2){
        System.out.println("3 is March. Graduation Day!");
    }
    else if(month==2){
        System.out.println("4 is April. Happy FOOLS Day!");
    }
    else if(month==2){
        System.out.println("5 is May. Friend's Birthday !");
    }
    else if(month==2){
        System.out.println("6 is June. First Day of School!");
    }
    else if(month==2){
        System.out.println("7 is July. Nicolle's Birthday!");
    }
    else if(month==2){
        System.out.println("8 is August. Buwan ng Wika!");
    }
    else if(month==2){
        System.out.println("9 is September. Mother Ignacia BirthMonth!");
    }
    else if(month==2){
        System.out.println("10 is October. Happy Octoberfest!");
    }
    else if(month==2){
        System.out.println("11 is November. Happy All Saints Day!");
    }
    else if(month==2){
        System.out.println("12 is December. Happy Christmas Day!");
    }
}