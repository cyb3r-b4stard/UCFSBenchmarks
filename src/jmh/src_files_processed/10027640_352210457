public class Automobile
{
    public int miles;
    public int mpg;
    {
        mpg=m/g
    }
    public int gallons;
    {
        gallons = 0;
    }
    public void fillUp(int f)
    {
        mpg=f+mpg;
    }
    public void takeTrip(int t)
    {
        mpg=mpg-t;
    }
    public void reportFuel(double r)
    {
        r = mpg;
    }
}