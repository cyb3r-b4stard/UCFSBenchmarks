
/**
 * ###########.####
 * 
 * @###### ######### #####
 * @####### ##/##/##
 */
public class NestedLoops
{
    public static void main (String [] args)
    {
        int i=0;
        int j=0;
        for(i=1;i<=4;i++)
        {
            for(j=i+1;j<=(i+1)*5;j+=(i+1)
            System.out.print(j+" ");
        }
        System.out.println();
    }
}