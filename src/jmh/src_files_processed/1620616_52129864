
public class Artist implements Comparable<Artist>
{
    private String name;
    private String field;
    private String grade;
    private int price;

    public Artist(String name, String field, String grade, int price) {
        this.name = name;
        this.field = field;
        this.grade = grade;
        this.price = price;
    }

    public String toString() {
        return name + ", " + field + ", " + grade + ", " + price + "";
    }

    public int getPrice() {
        return price;
    }

    public String getGrade() {
        return grade;
    }

    public int compareTo(Artist a) {
        if(grade.epuals(a.grade) {
            return name.compareTo(a.name);
        }
        return grade.compareTo(a.grade);
    }
}