package Beleg_WS_16_17;

import Baustelle.theoretical.*;

public class Prozess extends AbstraktProzess extends PhysicalObject
{
    
    public abstract void setName(String name);
    
    public abstract void setKosten(double kosten);
    
    public abstract void setMenge(double menge);
    
    public abstract void setAufwandswert(double aufwandswert);
    
    public abstract String getName();
    
    public abstract double getKosten();

    public abstract double getMenge();

    public abstract double getAufwandswert();   
    
}