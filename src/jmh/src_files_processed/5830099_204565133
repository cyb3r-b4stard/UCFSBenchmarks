public class Taxi
{
    private Pris pris;    
    
    public Taxi()
    {       
    }    

    public int newPris(int price, int km)
   
    {
        pris = new Pris(price, km);
        kvittering(pris);
        return pris.turPris();
    }    
    
    public int getPris()
      {  return pris.turPris();
        }
                
    public String kvittering(Pris p)
      p.printInfo();
       
}