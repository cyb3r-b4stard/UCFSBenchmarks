
public class BandBooster
{
    private String name;
    private int boxesSold;
    public BandBooster(String name)
    {
        boxesSold = 0
        name = "";
    }
    
    public String getName()
    {
        return name;
    }
    
    public void updateSales(int extraSold)
    {
        boxesSold += extraSold
    }
    
    public String toString()
    {
        return name + ": " + boxesSold + "boxes";
        