import java.io.*;
import java.util.*;
public class Tester
{
public static void main(String args[])
{
    Scanner input1 = new Scanner (System.in);
    Scanner input2 = new Scanner (System.in);
    System.out.println("Enter C, T, or S");
    String type = input1.next();
    System.out.println("Enter how long vehicle has been in lot(in minutes)");
    Double time = input2.nextDouble();
    if (type.equalsIgnoreCase("C"));
    {
        if (time <= 120)
            System.out.println("The car was in the lot for " + time + " minutes. The total is ");
        else if ((time > 120) && (time <= 300))
            System.out.println("The car was in the lot for " + time + " minutes. Your total is " + .5 * (Math.ceil(time/60) - 2));
        else
            System.out.println("The car was in the lot for " + time + " minutes. Your total is " + .25 * (Math.ceil(time/60) - 5));
        }
    if(type.equalsIgnoreCase("T"));
    {
         if (time <= 60)
            System.out.println("The car was in the lot for " + time + " minutes. The total is ");
        else if ((time > 60) && (time <= 180))
            System.out.println("The car was in the lot for " + time + " minutes. Your total is " + 1 * (Math.ceil(time/60) - 1));
        else
            System.out.println("The car was in the lot for " + time + " minutes. Your total is " + .75 * (Math.ceil(time/60) - 5));
    }
        else
    {
        System.out.println("free :)");
    }  
}
}