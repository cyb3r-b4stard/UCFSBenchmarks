package myform;


import javax.swing.*;
import java.awt.*;



public class SchoolChooser extend JPanel

 private School[] schools;
 private Object callback;
{
   
  public SchoolChooser (){
      
       super();
       
   
       
       
         private Map<Schools,Schools> set 
         
    }
        
        
        public static School add(School, schools) {
        School retValue = allSchools.get(schools);
        if (retValue==null)
            allSchools.put(name, new School(schools));
        return retValue;
    }
    
    
    private School(Schools school) {
        this.name = name;
        set = new TreeMap<String,school>();
        
    }
    
    public static Collection<School> getCollection() {
        return allSchools.values();
    }
        public static Collection<School> getCollection() {
        return allSchools.values();
        
        
    }
    
      public String getName() { return school; }
        
      
    }
    public Collection<Schools> getSchools() { return set.values(); }
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        public interface Collection<E> extends Iterable<E> {