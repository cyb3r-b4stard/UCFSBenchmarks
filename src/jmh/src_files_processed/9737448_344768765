
/**
 * ##### # ########### ## ##### ### ####.
 * 
 * @###### (#### ####) 
 * @####### (# ####### ###### ## # ####)
 */
public class Car
{
    private int yearBuilt;
    private boolean newCar;
    private double carValue;
    private String make;
    public Car ()
    {
        yearBuilt = 0;
        newCar = false;
        carValue = 0;
        make = "unknown";
    }
    
    public Car(int yearBuilt, boolean newCar, double carValue, String make)
    {
        Car.yearBuilt = yearBuilt;
        Car.newCar = newCar;
        Car.carValue = carValue;
        Car.make = make;
    }