import java.io.*;
public class petnaesti{
public static void main (String[]args) throws IOException{
int i,j, m, n;
BufferedReader c=new BufferedReader(new InputStreamReader(System.in));
System.out.println(" m");
m=Integer.parseInt(c.readLine());
System.out.println(" n");
n=Integer.parseInt(c.readLine());
int x[][]=new int[m][n];
int y[][]=new int [n][m];
for (i=0;i<m;i++){
for (j=0;j<n;j++){
System.out.println("ucitaj element za a matricu");
x[i][j]=Integer.parseInt(c.readLine()); }}
System.out.println();
System.out.println();
System.out.println();
System.out.println();
for (i=0;i<m;i++){
for (j=0;j<n;j++){
System.out.print(x[i][j]+" ");}System.out.println();}
System.out.println();
System.out.println();
for (i=0;i<m;i++){
for (j=0;j<n;j++){
y[j][i]=x[i][j];}}
for (i=0;i<n;i++){
for (j=0;j<m;j++){
System.out.print(y[i][j]+" ");System.out.println();
System.out.println(); }}
