import java.util.*;
public class grades 
{
    public static void main(String[] args) 
    {
        Scanner scan = new Scanner(System.in);
        System.out.println("Type in your letter grade");
        String grade = scan.nextLine.();
        char letter = grade.charA + (0);
        switch (grade) {
            case 0: System.out.print("The grade isn't valid");
                   
                    break;
            case 1: System.out.print("Excellent");
                    grade = 90;
                    break;   
            case 2: System.out.print("Very good");
                    grade = 80;
                    break;
            case 3: System.out.print("Decent");
                    grade = 70;
                    break;
            case 4: System.out.print("Do better");
                    grade = 60;
                    break;
            case 5: System.out.print("Seriously");
                    grade = 50;
                    break;
            
                  
            default: System.out.print("Probabaly an F");
        }
        
    }
}