
/**
 * ##### # ########### ## ##### ######### ####.
 * 
 * @###### (#####, ######, ####) 
 * @####### (#-#-##)
 */
public class Algorithm
{
    public static void main(String[]args){
        final int value = 100;
        int[]random = new int(value);
        
        for(int i = 0; i < 100; i++){
            i = (int)Math.random()*10;
            random[i] += 1;
        }
        for(int i = 0; i(random.length); i++){
            system.out.println(i + "=" + random[i]);
        }
    }
}