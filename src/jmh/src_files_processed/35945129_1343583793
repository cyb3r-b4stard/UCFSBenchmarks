public class Week5Q3
{
    /**
     * Constructor for objects of class Week5Q3
     */
    public Week5Q3()
    {
    }
    
    public boolean checkStringLength(newString string)
    {
        boolean check = true;
        if (string.length(newString)=4) check = true;
        else check = false;
        return check;
    }
    
    public boolean isCharcterNumeric(newChar char)
    {
        boolean check = true;
        if (newChar - "0" >= 0 && newChar - "0" <=9 )  check = true;
        else check = false;
        return check;
    }
    }