public class InteresectingRectanglesStudent
{
    public static void main(String[] args)
    {
        int x = 100;  
        int y = 75;   
        int width = 80;
        int height = 125;

        int x1 = 130;  
        int y1 = 99;
        int width1 = 141;
        int height1 = 166;

        Rectangle box1 = new Rectangle(x,y,width,height);
        Rectangle box2 = new Rectangle(x1,y1,width1,height1);

        box1.translate(20,30);  
        box2.translate(15,42);

        box1.draw();
        box2.draw();

        Rectangle box3 = new Rectangle (box2.getX(), box2.getY(), box1.getWidth() - (box2.getX()- box1.getX()), box1.getHeight() -(box2.getY()-box1.getY())); );
        box3.draw();
        box3.setColor(Color.GREEN);
        box3.fill();

        /* add your code to create intersecting rectangles here.
         * with rectangles below here...
         *
         */

    }
}