
/**
 * ##### # ########### ## ##### ##### ####.
 * 
 * @###### (#### ####) 
 * @####### (# ####### ###### ## # ####)
 */
public class Shape
{

private int area;
}

private class Circle extends Shape
{

private int r=3;
private int area= pi*r*r;
System.out.println(area);

}

private class Rectangle extends Shape
{

private int l = 5;
private int b = 3;
private int area = l*b;
System.out.println(area);

}

Class Square extends Shape
{

private int a=5
area = a*a
System.out.println(area);

}

}