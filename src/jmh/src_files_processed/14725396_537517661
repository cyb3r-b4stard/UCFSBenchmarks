import javax.swing.JOptionPane;
public class FtoCelcius
{
    public static void main(String[] args)
        {
           double F = 100;
           double C;
           C = (F - 32) * 5 / 9;
           C = (int)(C * 100) / 100.0
           JOptionPane.showMessageDialog(null, F + " Farenheit is " + C +" Celcius");
        
        }
}