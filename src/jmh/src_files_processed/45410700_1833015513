public class pyramid33
{
    public static void main()
    {
        int i,j;
        char c;
        for(i=1;i<=5;i++)
        {
            c='A';
            for(j=1;j<=i;j++)
            {
                if(i%2==0)
                {
                System.out.print(j);
            }
            else
            {
                System.out.print(c++2);
                
            }
        }
            System.out.println();
     }
    }
}