
public class TenthProblem
{
   
    private long int x;

    public TenthProblem()
    {
     
        x = 2;
    }

   
    public void sumOfPrime()
    {
        int t;
      for(int i=3;i<2000000;i+=2){
          t=0;
          for(int j=2;j<i;j++){
              
              if(i%j==0){
                break;
                t=1;
            }
        }
            if(t==0){
                x+=i;
            
            }
        }
        System.out.println(x);
    }
  }