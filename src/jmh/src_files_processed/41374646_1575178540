import apcslib.*;
import java.awt.*;
import chn.util.*;
class PineHouse
{
    private int x;
    private int y;
    private int R;
    private int G;
    private int B;
    private int x1;
    private int y1;
    private int x2;
    private int y2;
    private int x3;
    private int y3;
    private int height;
    private int width;
    private int penW;
    private static DrawingTool pen = new DrawingTool(new SketchPad(500,500));
    
    PineHouse(int myX, int myY, int myR1, int myG1, int myB1, int myPenW, int myHeight, int myWidth)
    {
        x = myX;
        y = myY;
        R = myR1;
        G = myG1;
        B = myB1;
        penW = myPenW;
        height = myHeight;
        width = myWidth;
    }
    
    PineHouse(int myX1, int myY1, int myX2, int myY2, int myX3, int myY3, int myR1, int myG1, int myB1)
    {
    }
    
    public void rect()
    {
        pen.up();
        pen.move(x,y);
        pen.down();
        pen.setColor(new Color(R,G,B));
        pen.setDirection(90);
        pen.setWidth(penW);
        pen.move(height);
        pen.setDirection(0);
        pen.move(width);
        pen.setDirection(270);
        pen.move(height);
        pen.move(x,y);
    }
    
}

public class zackaryBrown
{
    public static void main (String[] args)
    {        
        PineHouse Rect1 = new PineHouse(-100,0,139,69,19,2,80,200);
        rect.Rect1;
    }
}