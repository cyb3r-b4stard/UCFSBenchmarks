        for(int i=1;i<11;i++){
            System.out.println(n+" * "+(i)+" = ");
            System.out.println(" ");
            int ans=ni.nextInt();
            int ans= ni.nextI
            int res= n*(i);
            if(ans==res){
                continue;
            }
            else{
