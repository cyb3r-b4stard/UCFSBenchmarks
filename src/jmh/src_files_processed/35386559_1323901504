public class whileee1{

    public static void main(String args[]){
    
       double para = 100;
       double faiz = 0.01;
       int yil = 0;
       while (para < 1000000){
          yil++;
          para = para + para*faiz
        
        }
        
    
        System.out.println(yil);
    
    
    
    
    } 
        
        
        
        
        
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
   



}