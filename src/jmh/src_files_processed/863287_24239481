public class Numeros
{

    public Numeros()
    {

    }

    public void quadrado()
    {
        int num = 15;
        while (num <= 200) {
            int quadrado = num * num;
            String q = " O valor do quadrado de :" + num + " é igual a : " + quadrado; 
            num = num + 1;
            System.out.print (q);
        }        
    }

    public void somaPares ()
    {
        double n = 2;
        double soma = 0;
        while (n <= 500) {
            soma = soma + n;
            n = n + 2;
        }
        String s = " A soma dos pares é : " + soma;
        System.out.print (s);
    }

    public int div (int n1, int n2)
    {
        double div = 1;
        int r = 0;
        if (n1 >= 1 && n1 <= 9) {
            while (div <= n2) {
                if (div % n1 == 0) {
                    r = r + 1;
                } 
                div = div + 1;
            }
        }
        return r;
    }

    public long elevado(int n1, int n2) 
    {
        long s = n1;
        int b = 1;
        if (n1 >= 2 && n1 <= 9) {
            if (n2 > 0 && n2 <= 30) {
                while (b < n2) {
                    s = s * n1;
                    b = b + 1;
                }
            }
        }
        return s;
    }
    
    public void temperatura ()
    {
        double c = 0;        
        while (t <= 30) {
            double f = c * 9 / 5 + 32;
            String a = c + "º em Celsius equevalem a " + f + "º em fahrenheit ";
            c = c + 0,5;
        }
        System.out.printLN (a);
    }
}