public class Consulta
{
    private String nombrePaciente;
    private String rut;
    private int edad;
    private float altura;
    private float peso;
    private char sexo;
    private boolean fuma;
    private boolean presionAlta;
    private int valorConsulta;
    private double copago;
    private int costoExamenes;
    private int costoMedicamentos;
    private int masaCorporal;
    private int descuento;
 

    /**
     * Constructor sin parametros para la clase Consulta
     */
    public Consulta()
    {
       
    }

    /**
     * Constructor con parametros para la clase Consulta
     */
    public Consulta(String nombrePaciente, String rut, int edad, float altura, float peso, 
    char sexo, boolean fuma, boolean presionAlta, int valorConsulta, double copago, int costoExamenes, int costoMedicamentos, int masaCorporal, int descuento)
    {
       this.nombrePaciente=nombrePaciente;
       this.rut=rut;
       this.edad=edad;
       this.altura=altura;
       this.peso=peso;
       this.sexo=sexo;
       this.fuma=fuma;
       this.presionAlta=presionAlta;
       this.valorConsulta=valorConsulta;
       this.copago=copago;
       this.costoExamenes=costoExamenes;
       this.costoMedicamentos=costoMedicamentos;
       this.masaCorporal=masaCorporal;
       this.descuento=descuento;
    }
    
    
    public String getNombrePaciente()
    {   
        return nombrePaciente;
    }
    public String getRut ()
    {
        return rut;
    }
    public int getEdad()
    {
        return edad;
    }
    public float getAltura()
    {
        return altura;
    }
    public float getPeso()
    {
    return peso;
}   
public char getSexo()
{
    return sexo;
}
public boolean getFuma()
{
    return fuma;
}
