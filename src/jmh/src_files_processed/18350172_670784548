{
    this.nombrePaciente=nombrePaciente; 
}
public void setRut (String rut)
{
   this.rut=rut;
}
 public void setEdad (int edad)
    {
        this.edad=edad;
    }
public void setAltura(float altura)
    {
         this.altura=altura;
    }
