{
    return presionAlta;
}
public int getvalorConsulta()
{   
    return valorConsulta;
}
public double getCopago()
{
    return copago;
}
public int getcostoExamenes()
{
    return costoExamenes;
}
public int getcostoMedicamentos()
{
    return costoMedicamentos;
}
public int masaCorporal()
{
    return masaCorporal;
}
public int descuento()
{
    return descuento;
}


