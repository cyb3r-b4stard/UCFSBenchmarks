    {
    this.peso=peso;
}  
public void setSexo(char sexo)
{
    this.sexo=sexo;
}
public void isFuma(boolean fuma)
{
    this.fuma=fuma;
}
public void isPresionAlta(boolean presionAlta)
{
    this.presionAlta=presionAlta;
}
public void setValorConsulta(int valorConsulta)
{   
    this.valorConsulta=valorConsulta;
}
public void setCopago(double copago)
{
    this.copago=copago;
}
public void SetCostoExamenes(int CostoExamenes)
{
    this.CostoExamenes=CostoExamenes;
}
public void setCostoMedicamentos(int CostoMedicamentos)
{
    this.setCostoMedicamentos=setCostoMedicamentos;
}

