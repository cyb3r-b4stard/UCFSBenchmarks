 * @######### (#### ####) 
 * @#.## (# ####### ###### ## # ####)
 */
public class Book
{
    private int numPages;
    private int currentpage;

    /**
     * Constructor for objects of class ThreeEleven
     */
    public Book(int x)
    {
       currentpage=1;
        numPages=x;
    }

  public void nextpage()
  if (currentpage< numPages)
  currentPage++;
  
 
}


    
    
    
    
    
    
    
    
    
    
    
    
    

