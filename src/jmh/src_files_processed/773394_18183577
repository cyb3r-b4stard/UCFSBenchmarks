      System.out.ptintln("Answer is " + (3 + 2));  
      System.out.ptintln(3 + 2 + " is Answer");      
      
      System.out.ptintln("int x = 3");
      System.out.ptintln("double d = 7.56");
      System.out.ptintln("String str = ")"Goodbye");   
      
      
      
   

