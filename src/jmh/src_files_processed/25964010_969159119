public abstract class Vehicul{
    String serie;
    Tip tip;
    
    public Tip setTip(Tip tip) throws NotValidTypeException{
        switch(tip){
            try{
                case DIESEL:
                    System.out.println("DIESEL");
                break;
                
                case BENZINA:
                    System.out.println("BENZINA");
                break;
                
                case NONE:
                        System.out.println("NONE");
                break;
                
                default:
                    System.out.println("Nu exista acest tip!");
                break;
            }catch(NotValidTypeException e){
                System.out.println("Nu exista acest tip!");
            }
        }
        return tip;
    }
}