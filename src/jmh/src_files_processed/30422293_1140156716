
/**
 * ##### # ########### ## ##### ########### ####.
 * 
 * @###### (#### ####) 
 * @####### (# ####### ###### ## # ####)
 */
public class temperature
{
    double c, f;
    public void cal_Temp(double cc)
    {
        c=cc;
        f=(9*c/5)+32;
        System.out.println("temperatue="+f);
    }
    public static void main (String args[])
    {
        Temperature t1=new Temperature();
        t1 cal_Temp (90.0);
    }
}

