   JButton ok = new JButton ("OK");
   JCheckBox checkit = new JCheckBox("WÃ¤hle aus: ", true);
    
    public PasswortGrafisch()
    {
      super("Gitteranordnung");
      setSize(260, 260);
      setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
