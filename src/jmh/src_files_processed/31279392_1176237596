public class Proto
{
    private int max;
    private Object mesa1[];
    private Object mesa2[];
    private Object mesa3[];
    private Cola personas[];
    private Pila platosNormales=new Pila(4);
    private Pila platoOndo=new Pila(4);
    private Pila platoLargo=new Pila(4);
    private Lista cucharas=new Lista(5);
    private Lista tenedores=new Lista(5);
    private Lista ollaMole=new Lista(10);
    private Lista ollaPicadillo=new Lista(10);
    private Lista ollaArroz=new Lista(10);
    private Cola comidaSeca1=new Cola(5);
    private Cola comidaSeca2=new Cola(5);
    public Proto(){
     this.max=max;
    
     mesa1=new Object[5];
     mesa2=new Object[3];
     mesa3=new Object[2];
     personas=new Cola[max];
    }
    public Object platosNormales(){
        mesa1[0]=new Pila[4];
        return mesa1[0];
    }
    public Object platosOndos(){
        mesa1[1]=new Pila[4];
        return mesa1[1];
    }
    public Object PlatosLargos(){
        mesa1[2]=new Pila[4];
        return mesa1[2];
    }
    public Object cucharas(){
        mesa1[3]=new Lista[5];
        return mesa1[3];
    }
    public Object tenedores(){
        mesa1[4]=new Lista[5];
        return mesa1[4];
    }
    public Object mesadeOllas(){
        mesa2[0]=new Lista[10];
        mesa2[1]=new Lista[10];
        mesa2[2]=new Lista[10];
        return mesa2;
    }
    public Object comidasSecas(){
        mesa3[0]=new Lista[5];
        mesa3[1]=new Lista[5];
        return mesa3;
    }
    public Object coladePersonas(){
        personas=new Cola[5];
        return personas;
    }
    
    public Object agregarplatosNormales(Object o){
        for (int i = 0 ; i <4 ; i++){
            platos.pop();
      mesa1[0]=platosNormales
    }return mesa1[0];
    }
    public Object agregarplatosOndos(Object o){
        for (int i = 0 ; i <4 ; i++){
      mesa1[1]=platoOndo.push(o);
    }return mesa1[1];
    }
    public Object agregarplatosLargos(Object o){
        for (int i = 0 ; i <4 ; i++){
      mesa1[2]=platoLargo.push(o);
    }return mesa1[2];
    }
    public Object agregarcucharas(Object o){
        for (int i = 0 ; i <5 ; i++){
      mesa1[3]=cucharas.agregarFin(o);
    }return mesa1[3];
    }
    public Object agregartenedores(Object o){
        for (int i = 0 ; i <5 ; i++){
      mesa1[4]=tenedores.agregarFin(o);
    }return mesa1[4];
    }
    public Object agregarcomidaOllaMole(Object o){
       for (int i = 0 ; i <5 ; i++){
      mesa2[0]=ollaMole.agregarFin(o);
    }return mesa2[0];
    }
    public Object agregarcomidaOllaPicadillo(Object o){
       for (int i = 0 ; i <5 ; i++){
      mesa2[0]=ollaPicadillo.agregarFin(o);
    }return mesa2[1];
    }
    public Object agregarcomidaOllaArroz(Object o){
       for (int i = 0 ; i <5 ; i++){
      mesa2[0]=ollaArroz.agregarFin(o);
    }return mesa2[2];
    }
    public Object agregarcomidaSeca1(Object o){
       for (int i = 0 ; i <5 ; i++){
      mesa3[0]=comidaSeca1.push(o);
    }return mesa3;
    }
     public Object agregarcomidaSeca2(Object o){
       for (int i = 0 ; i <5 ; i++){
      mesa3[1]=comidaSeca2.push(o);
    }return mesa3;
    }
    public Object popPlatosNormales(){
       mesa1[0]=platosNormales.pop();
       return mesa1[0];
    }
    public Object popPlatosOndos(){
       mesa1[1]=platoOndo.pop();
       return mesa1[1];
    }
    public Object popPlatosLargos(){
       mesa1[2]=platoLargo.pop();
       return mesa1[2];
    }
    public Object popCucharas(){
       mesa1[3]=cucharas.eliminarFin();
       return mesa1[3];
    }
    public Object poptenedores(){
       mesa1[4]=tenedores.eliminarFin();
       return mesa1[4];
    }
    public Object popollaMole(){
       mesa2[0]=ollaMole.eliminarFin();
       return mesa2[0];
    }
    public Object popollaPicadillo(){
       mesa2[1]=ollaPicadillo.eliminarFin();
       return mesa2[1];
    }
    public Object popollaArroz(){
       mesa2[2]=ollaArroz.eliminarFin();
       return mesa2[2];
    }
    public Object popcomidaSeca1(){
       mesa3[0]=comidaSeca1.pop();
       return mesa3;
    }
     public Object popcomidaSeca2(){
       mesa3[1]=comidaSeca2.pop();
       return mesa3;
    }
}

