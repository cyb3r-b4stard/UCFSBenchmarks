class libro
{
   int cod, nro_stock, precio;
   String tit, editorial;
    
    libro(int cod, int nro_stock, int precio)
    {
        cod=c;
        nro_stock=n;
        precio=p;
    }
    
    int getCod()
    {
        return(cod);
    }
    
    int getNro_stock()
    {
        return(nro_stock);
    }
    
    int getPrecio()
    {
        return(precio);
    }
    
    String getTit()
    {
        return(tit);
    }
    
    String getEditorial()
    {
        return(editorial);
    }
    
    void setCod(int c)
    {
        cod=c;
    }
    
    void setNro_stock(int n)
    {
        nro_stock=n;
    }
    
    void setPrecio(int p)
    {
        precio=p;
    }
    
    void setTit(String t)
    {
        tit=t;
    }
    
    void setEditorial(String e)
    {
        editorial=e;
    }