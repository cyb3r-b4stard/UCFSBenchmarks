package box1;




public class student
{
    private String ner;
    private int nas;
    private double golch;
    private String code;
    student[] = new int[4];
    public student[](String ne, int na, float g, String c){
        
        ner = ne;
        nas = na;
        golch = g;
        code = c;   
    }
    public void hevle(){
        System.out.println(ner+" "+nas+" "+golch+" "+code);
    }
    void oorchil(){
        golch = 4.8;
    }
    }