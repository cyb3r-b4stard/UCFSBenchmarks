public class Strings
{
    public static void main (String [] args)
    {
        String quote = "Solutions are not the answer.";
        String answer1 = quote.substring(8, 11); 
        String answer2 = quote.substring(19);
        String answer3 = quote.substring(quote.length() - 3);
        int answer4 = quote.indexOf("s");
        int answer5 = quote.indexOf("s".toUpperCase());
        int answer6 = quote.indexOf("s", 14);
        int answer7 = quote.indexOf("N");-1
        String answer8 = quote.substring(0, 3).toUpperCase().toLowerCase();
        System.out.println(answer1+answer2+answer3+answer4+answer5+answer6+answer7+answer8);
    }
}