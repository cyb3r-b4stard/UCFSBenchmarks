import java.util.*;
public class Shapes
{
    private LinkedList<Shape> shapes = new LinkedList<Shape>();
    public Shapes()
    {
    }
    
    public void get(Shape shape)
    {
        shapes.add(shape);
    }
    
    public void set()
    {
        for(Shape shape : shapes)
        {
            shape.setPerimeter();
            shape.setArea();
            shape.show()
        }
    }
}