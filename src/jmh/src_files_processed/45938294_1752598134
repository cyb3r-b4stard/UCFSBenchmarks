
import java.io.*;
class volume
{
public static int cuboid(int a,int bb,int c)
{
return(a*bb*c);
}
public static void main(String args[])throws IOException
{
int l,b,h,vol;
DataInputStream in=new DataInputStream(System.in);
System.out.println("enter the values of l,b,h");
l=Integer.parseInt(in.read Line());
b=Integer.parseInt(in.read Line());
h=Integer.parseInt(in.read Line());
vol=cuboid(l,b,h);
System.out.println("the volumeof a cuboid is"+vol);
}
}