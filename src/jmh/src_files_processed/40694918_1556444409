import java.io.*;
import java.util.*;
public class Weird_Wages {
    public static void main(String[] args) {
        System.out.print('\u000C');
        Scanner sc = null;
        File file = new File("Prog213a.dat");
        String weeklyHours = "";
        Scanner hoursScanner = new Scanner(weeklyHours);
        int dailyHours = 0;
        int totalHours = 0;
        double totalWages = 0.0;
        int dayCounter = 0;
        int weekCounter = 1;
        try {
            sc = new Scanner(file);
        } catch (FileNotFoundException e) {
            e.printStackTrace();
        }
        while (sc.hasNextLine()) {
            weeklyHours = sc.nextLine();
            dayCounter = 1;
            totalHours = 0;
            while (hoursScanner.hasNext()) {
                dailyHours = hoursScanner.nextInt();
                totalHours += dailyHours;
                totalWages += (dailyHours * 30
                dayCounter++;
            }
            weekCounter++;
        }
    }
}