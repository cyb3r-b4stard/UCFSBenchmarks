public class Squares
{
    public static void main(String [] args)
   {    
        final int COLS = 12;
        final int ROWS = 18;
        
        for (int row=0; row<ROW; row++)
        {
            for (int row=0; row<ROW; row++)
            {
                System.out.print("*");
            }
            System.out.println("");

