public class Player
{
    string fabricante;
    string nome;
    string[] formatos = new string [5];

    /**
     * Constructor for objects of class Player
     */
    public Player(string nome, string fabricante)
    {
       this.nome = nome;
       this.fabricante = fabricante;
    }

    void addFormato(string formato){
    int index = 0;
    do{
    
    index ++;
    }while (formatos [index ] !+ null);
    
}
boolean temFormato(string formato){
boolean achou = false;
for(int i =0; i<formatos.lenght; i++){
      if(formatos [i].equals(formato)){
        achou = false;
        }
}
return achou;
