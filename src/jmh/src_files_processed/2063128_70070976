public class Fraction
{
   int numerator;
   int denominator;
   
   public Fraction (int top, int bot)
   {
       numerator = top;
       denominator = bot;
   }
   
    public static int gcf (int x, int y)
    {
        if (y == 0)
        {
            return x;
        }
        
        else if (x >= y && y > 0)
        {
            return gcf (y, x%y);
        }
        
        else
        {
            return gcf (y, x);
        }
    }

   
   public String toString()
   {
       return numerator + "/" + denominator;
   }
   
   public int getNumerator()
   {
       return numerator;
   }
    
   public int getDenominator()
   {
       return denominator;
   }
   
   public int changeNumerator(int x)
   {
       numerator = x;
       return numerator;
   }
   
   public int changeDenominator(int x)
   {
       denominator = x;
       return denominator;
   }
   
   public void simplify()
   {
       int simpNum = gcf(numerator, denominator);
       
       numerator = numerator/simpNum;
       denominator = denominator/simpNum;
   }
   
   public Fraction multiply(Fraction b)
   {
       numerator = getNumerator() * getNumerator();
       denominator = getDenominator() * getDenominator();
       String multFraction = new String();
       multFraction = 
   }
}