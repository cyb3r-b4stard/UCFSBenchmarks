
public class CIA {
    public static String removeSecrets(String Text) {
        int open = Text.indexOf ("<");
        int close = Text.indexOf (">");
        int a = Text.substring(0,open-1)
        int b = Text.substring(close+1, open-1)
        String tag = Text.substring(open, close);
        String openingTag = (open + tag + close);
        
        String textCoded = "";
        while(Text.indexOf(open) > -1){
            if (a) && (b) > -1
            textCoded = a + b;
            
        }    
        
            return Textcoded;
        
            
    }
}