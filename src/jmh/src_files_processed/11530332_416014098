import javax.swing.*;
import java.awt.*;


/**
 * ##### #### ####### ######
 */
class PanelDeReloj extends JPanel
{
    
    public PanelDeReloj()
    {
        dibujarLinea(int a);                
    }
    
    
    public void dibujarLinea(Graphics g)
    {
        int x = (int)(Math.random()*255);
        Color c1 = new Color(x,x,x);
        g.setColor(c1);
        g.drawLine(1,1,50,50);
        
    }


}