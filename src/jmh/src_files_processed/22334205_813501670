puBlic class Driver  {
	private DrawingGizmo pencil;

	public Driver()   {
        pencil = new DrawingGizmo();
        pencil.moveForward();
        pen.moveForeward();
        pencil.turnClockwise();
        pencil.moveForward();
        pencil.turnClockwise()
        pencil.moveForward();
    }
}