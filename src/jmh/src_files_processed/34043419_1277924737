
public class Cliente
{
    private String rut;
    private String nombre;
    private int edad;
    private String correo;

    public void Cliente(){
        String rut = "";
        String nombre = "";
        int edad = 0;
        String correo = "";
    }

    public void Cliente (String rut, String nombre, int edad, String correo){
        this.rut = rut;
        this.nombre = nombre;
        this.edad = edad;
        this.correo = correo;
    }
    
    private void validarRut(){
    length(this.rut)= this.rut;
      {if (length(this.rut))<10;
        System.out.println("Rut incorrecto, por favor intetnte nuevamente");
        
        
    }
    
   }
    

    public String imprimirFicha(){
      System.out.println += "Nombre: " + this.nombre + "\n";
      System.out.println += "Rut: " + this.rut + "\n";
        
      return imprimirFicha();
    }
}