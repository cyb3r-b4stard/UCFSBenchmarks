    static void demandeEtAfficheDate() {
        int jour;
        String mois;
        int annee ;
        jour = input.nextInt();
        mois = input.next();
        annee = input.nextInt();
        output.println("Saisissez le jour, le mois et l'année (int, String, int) :");
        output.println("Nous sommes le : " + jour + " " + mois + " " + annee);

    }

    static void demandeEtAfficheDate2() {
        int jour = input.nextInt();
