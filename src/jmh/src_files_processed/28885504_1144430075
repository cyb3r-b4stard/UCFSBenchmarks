    static void produit() {
        double a = input.nextDouble();;
        int b = input.nextInt();
        double p = a * b;
        output.print("a * b = ");
        output.println(p);
    }

    static void debugTest() {
        int i;
        double d = 1.23;
        String s = "Hello";
        i = random.nextInt(100);
        d = random.nextDouble();
        output.print("Saisissez un mot : ");
        s = input.next();
        output.println(i + " " + d + " " + s);
    }

    static void variables() {
        double i = 5.5;
        double j;
        j = i;
        j = 2.0;
        j = i * j;
    }

    static void operations() {
        int a = 5;
        int b = 3;
        int c;
        c = a / a;
        c = a / b;
        c = b / a;
        c = b / a * 10;
        c = a / b * 10;
        c = a / (b * 10);
    }

    static void chaines() {
        String s;
        s = "Hello";
        output.print("Tapez un mot : ");
        s = input.next();
        s = "La saisie est " + s;
        s = "Le nombre est " + 1;
        s = "Le nombre est " + 1 + 2;
        s = "Le nombre est " + (1 + 2);
    }

    static void echange() {
        String bonjour = "Bonjour";
        String bonsoir = "Bonsoir";
        String temp;
        temp = bonjour;
        bonjour = bonsoir;
        bonsoir = temp;
    }

    static void erreurs1() {
        double d
        Int i;
        d = random.nextDouble;
