import javax.swing.*;
import java.util.*;
 
public class iteration 
{
    public static void main(String[] args) 
    {
        
        int num;
        num = Integer.parseInt(JOptionPane.showInputDialog("Enter as integer number : "));
                        
            System.out.println("fib(n) :"+loop(num));
     
    }
            
    public static void loop(int n)
    {
    {
        for(int i= 0 ; i<=n; i++)
        {
            if(n== 1 || n== 2)
            return 1;
            else
            return loop(n-1)+loop(n-2);
        }
        return n;
    }