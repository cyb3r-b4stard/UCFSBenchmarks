import java.util.Random; 
 
 public class GameBooth { 
private double cost; 
private String firstPrize, consolationPrize; 
private int consPrize;
private int prizes;
/** 
 * constructor 
 
* pre: none 
* post: A GameBooth object created. 
* The cost and prizes are set. 
*/ 
public GameBooth(double charge, String pi, String p2) 
{ 
cost = charge; 
firstPrize = p1; 
consolationPrize = p2; 
consPrize = 0;
prizes = 0;
} 
 
/** 
* Game is played and prize * pre: none 
* post: Player had three tries. Player successful all three times received the 
first prize. A consolation prize has been awarded otherwise 
*/ 
public String start() 
   {
    int toss; 
    int successes = 0; 
    Random rand = new Random 


        for (int i = 0; i < 3; i++) {
            int toss = rand.nextInt(2) ;
            if (toss == 1) 
              successes += 1; 
  
            }

        /* award prize */ 
        if (successes == 3) 
        {
            prizes++;
           return(firstPrize); 
        }
        else {
            consPrize++;
            return (consolationPrize) ;
        }
         
    }
 
/** 
* Returns the cost to play the game 
* pre: none 
* post: Cost of the game has been returned
*/ 
    public double getCost()
    {
        return cost
    }
    /*
     * outputs prizes
     * pre none
     * post prizes output
     */
    public String prizeNumbers()
    {
        String str = "number of first prizes: " + prizes + "\n number of consolation prizes: " + consPrize;
        return str;
    }