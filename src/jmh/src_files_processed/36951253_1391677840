
public class Member{
    private String name;
    private String phoneNr;
    private String email;
    private Dog dog;
public Member(String newName, String newPhoneNr, String newEmail, Dog newDog){
    name = newName;
    phoneNr = newPhoneNr;
    email = newEmail;
    dog = newDog;
}
public boolean isFeeDue(int thisYear){
    boolean due = false;
    if(dog !=null){
        due = dog.isFeeDue(thisYear);
    }
        return due;
}

public void printInfo(){
    if(dog != null){
    System.out.println ("Name: " + name);
    System.out.println ("PhoneNr: " + phoneNr);
    System.out.println ("Email: " + email);
    dog.printInformation ();
}
else{
    System.out.println ("Name: " + name);
    System.out.println ("PhoneNr: " + phoneNr);
    System.out.println ("Email: " + email);  
    System.out.println ("No dog: ");
}
}
public void addDog(){
    Dog.add(dog);
}
public void ArrayList<dog> getDogs(){
   return dogs;
}
}

