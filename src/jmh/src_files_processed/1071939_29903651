import java.util.Scanner;

public class Playa
{
    private Scanner input;
    private int age = 0;
    private String married;
    private int dates = 0;
    private boolean marStat = false;
    
    public Playa()
    {
        input = new Scanner(System.in);
    }
    
    public void input()
    {
        System.out.println("Are you married? Yes/No");
        String married = input.nextLine();
        if (married.equals("Yes"))
            marStat = true;
        else if (married.equals ("No"))
            marStat = false;
        else
            (System.out.println("Your input data is incorrect."))
            return;
        System.out.println("Enter you age.");
        int age = input.nextInt();
        System.out.println("How many dates do you go on per week?");
        int dates = input.nextInt();        
        if (marStat == true || age < 18)
        {
            System.out.println("You shold not date.");           
        }

        else if ((age >=18 || age<= 30) && dates < 4)
        {
             System.out.println("You should date more.");
        }
        else if (age >30  && dates < 2)
        {
             System.out.println("You should date more.");
        }
        else if ((age >=18 || age<= 30) && dates > 4)
        {
             System.out.println("You date too much.");
        }
        else if (age >30 && dates > 2)
        {
             System.out.println("You date too much.");
        }
     }
}