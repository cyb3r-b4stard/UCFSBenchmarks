import java.util.Scanner;
import java.util.Random;

class SquareBoard {
    public static void squareBoard () {
        int n;
        int i;
        
        System.out.println("Enter your desired number. It will produce n x n square board");
        
        Scanner in = new Scanner (System.in);
        n = in.nextInt();
        
        for (i=0; i<n; i++){
            System.out.print(" #");
            if (i%n == 0){
                System.out.print(" #");
                System.out.println("");                
        }
    }
}
            