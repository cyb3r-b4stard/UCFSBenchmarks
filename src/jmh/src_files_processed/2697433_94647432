package uebungen.ue01.Turmrechner;
import java.lang.Math.*;
import toolbox.*;

public class Turmrechner
{ 
   public static void main()
   {
    
      int h;
      h=Tastatur.readInt("Höhe des Turms="); 
      double e=Math.pow(2,h)-1;
      System.out.println("Sie benötigen" +" " e + "Züge!");
      
    
    
    
    
    
   } 
}