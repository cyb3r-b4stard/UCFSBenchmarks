class justforfun
{
    void justforfun()
    {
        String justforfun;
        int length=justforfun.length;
        int a=1
        for(int i=0; i<length;i++)
        {
            if(charAt(i)==" ")
            {
                a=a+1;
            }
        }
        System.out.print("The no. of words are: "+a);
    }
}
                
