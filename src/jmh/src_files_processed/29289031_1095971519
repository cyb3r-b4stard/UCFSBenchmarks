
/**
 * ##### # ########### ## ##### ########## ####.
 * 
 * @###### (#### ####) 
 * @####### (# ####### ###### ## # ####)
 */
public class PostPreFix
{
    class PostPreFix{
        public static void main (String args[]) {
            
            int n = 12;
            int m = ++n;
            System.out.println("Prefix ++n : m = " + m);
            System.out.println();
            
            n=12;
    }
}