
public class Rechteck{
    public double l;
    public double b;
   public double f;

    public Rechteck(double laenge, double breite){
        laenge = l;
        breite = b;
        f = b*l;
    }
    public void Flaecheberechnen (){
        System.out.println("Die Fläche beträgt "+f+" FE.");
    }
    public boolean istQuadrat(){
        l==b = true;
    }
}