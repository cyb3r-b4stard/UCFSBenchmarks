public class NumberLoop
{
    private int x;
    private int y;
    
    System.out.println("Enter a starting number: ");
    x = keyboard.next();
    System.out.println("Now enter an ending number: ");
    y = keyboard.next();
    
        do {  
            System.out.println(x);
            x = x + 1;
        
        } while (x < y);
