

class Main
{
    public static void main( String args[] )
    {

        System.out.print( "#Enter amount : " );
        int amount = BIO.getInt(); 
        int startamount = amount;
        if( amount >= 0 && amount >500)
        {

        int a;
        int b;
        int c;
        int d;
        int e;
        int f;
        int g;
        int h;
        int twopound;
        int onepound;
        int fifthyp;
        int twentyp;
        int tenp;
        int fivep;
        int twop;
        int onep;
        twopound = 0;
        onepound = 0;
        fifthyp = 0;
        twentyp = 0;
        tenp = 0;
        fivep = 0;
        twop = 0;
        onep = 0;
        a = 200;
        b = 100;
        c = 50;
        d = 20;
        e = 10;
        f = 5;
        g = 2;
        h = 1;
                     
        int sum = 0;
        
        while(amount != 0){
            twopound = amount / a;
            amount = amount % a;
            onepound = amount / b;
            amount = amount % b;
            fifthyp = amount / c;
            amount = amount % c;
            twentyp = amount / d;
            amount = amount % d;
            tenp = amount / e;
            amount = amount % e;
            fivep = amount / f;
            amount = amount % f;
            twop = amount / g;
            amount = amount% g;
            onep = amount / h;
            amount = amount % h;
            sum = twopound + onepound + fifthyp + twentyp + tenp + fivep + twop + onep;

        }
        System.out.println("amount of coins");
        System.out.printf("%dp %d coins %d * twopounds %d * onepound %d * 50p %d * 20p %d * 10p %d * 5p %d *2p %d * 10", startamount,sum, twopound, onepound, fifthyp, twentyp, tenp, fivep,
        twop, onep);
    }
    else System.out.print("invalid amount");
}
}
    

}