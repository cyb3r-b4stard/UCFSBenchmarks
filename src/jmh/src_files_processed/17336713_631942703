    double d=9.5;

    System.out.println(i+=3);
    System.out.println(d==d);
    System.out.println(!(d<2*i||j>0));
    System.out.println(s+=1);
    System.out.println("s"+"1"+1);
    System.out.println(1==1||(++==1));
}