public class kjhhjjj
{
 {
   private String color;
   private int model;
   private double price;
   
   
   public void setCardetails(String color, int model, double price)
   
   {
       this.color= color;
       this.model= model;
       this.price= price;
       
  
}
public void display()
{
    System.out.println(this.color + this.model + " " + this.price);
}
}

    
