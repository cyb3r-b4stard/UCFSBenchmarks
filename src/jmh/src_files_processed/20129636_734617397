public class Umrechner
{
    public double getBetragInCent(double euro)
    {
        return euro*100;
    }
    
    public double getBetraginEuro(int cent)
    {
        return euro/100;
    }
    
    public double getBetraginEuro(double gbp)
    {
        return euro*1,2;
    }
}