

import java.util.LinkedList;
public class Squadra
{ 
    private LinkedList<Calciatori> lc;
    private String nome;
    private int annodifondazione;
    private Campionato link;
    public Squadra ( String a, int b)
    {
        nome=a;
        annodifondazione=b;
        lc=new LinkedList <Calciatore> ();
    }
    
    public String getNome() 
    {return nome;}
    
    public int getAnnodifondazione()
    {return annodifondazione;}
    
    public void setNome (String a)
    {nome=a;}
    
    public void setAnnodifondazione (int b)
    {annodifondazione=b;}
    public Campionato getLink()
    {return link;}
    public void setLink (Campionato a )
    {link =a;}
    public LinkedList<Calciatore> getLc ()
    {return lc;}
    public boolean Aggiungi Calciatore(Calciatore a )
    {return lc.add(a);}
    public boolean Rimuovi Calciatore (Calciatore  a)
    {return lc.remove(a)}
}