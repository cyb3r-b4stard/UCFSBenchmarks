import java.util.*;
public class Factorials
{
    Scanner in = new Scanner(System.in);
    int fact(int  n)
    {
        if(n==1)
        {
            return 1;
            else
            return(n*fact(n-1));
        }
    }
    
    public static void main()
    {
        System.out.print("Enter a number: ");
        int n = in.nextInt();
    }
}
    