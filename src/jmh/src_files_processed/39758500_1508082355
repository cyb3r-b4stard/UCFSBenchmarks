
import java.util.Scanner;
import java.io.*;
public class Election
{
    public static void main(String[] args) throws DoesNotExistException
    {
       Scanner input = new Scanner(new File("election.txt"));
       int penguin = 0;
       int unicorn = 0;
       while (hasNext())
       {
           input.next();
           int vote1 = input.nextInt();
           int vote2 = input.nextInt();
           if ( vote1 > vote2)
           {
               unicorn += input.nextInt();
               
            }
            else if (vote2 > vote1)
            {
                penguin += input.nextInt();
            }
            
        }
        System.out.println("Unicorns: " + unicorn + " votes")
        System.out.println("Penguins: " + penguin + " votes")
        
    }
}