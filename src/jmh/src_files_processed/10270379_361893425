

public class fabonacci
{
    void main(int m,int n)
    {
        int a,b=0,c=0;
        if (m>n)
        {
            System.out.println("wrong input");
        }
        else if (m<n)
        {
            for (a=m;a<n;a++)
            {
                if (a%2=0)
                {
                    c=c+a;
                }
                if (a%2!=0)
                {
                    b=b+a;
                }
            }
        }
        System.out.println(c+b);
    }
}