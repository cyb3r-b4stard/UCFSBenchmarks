import java.io.*;
class sp7
{
    public static void main(String args[]) throws IOException
    {
int p=0;
int i,j,x=0,in=0;
String g[] = new String [20];
String s;


public void E()
{
	T();
	ED();
	x=1;
}
void ED()
{
	if(g[p]=="+")
	{
		p++;

	E();
	}
	else
	{
	return;
	}

}
void T()
{
	F();
	TD();
}
void TD()
{
	if(g[p]=="*")
	{
		p++;
		T();
	}
	else
	{
		return;
	}
}
void F()
{
	if(g[p]=="(")
	{
	p++;
	E();
	if(g[p]==")")
	{
	p++;
	}
	else
	{
	System.out.println("error");
	System.exit(0);
	}
	}
	else if(g[p]=='i')
	{
	p++;
	}
	else
	{
	System.out.println("error");
	System.exit(0);
	}
}








	System.out.println("grammar is :\nE->T+E|T \nT->F*T|F\n F(E)|i\n");
	System.out.println("enter input string:");
	 BufferedReader br=new BufferedReader( new InputStreamReader (System.in));
          
          s=br.readLine();
          for(int i=0;i<s.length();++i)
          {
               if(s.charAt(i)==' ')
               {
                    p[in++]=s.substring(j,i);
                }
                j=i+1;
            }
	
	 E();
	if(x==1 && p==strlen(g))
	System.out.println("accepted");
	else
	System.out.println("not");

	
}
}