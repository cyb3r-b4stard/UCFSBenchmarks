import ecs100.*;   
import java.io.*;
import java.util.Scanner; 
    
public class dice {
        public void differencesFileInput(){
         String filename=UIFileChooser.open("Choose file");
         File f = new File(filename);
         try{
            Scanner s= new Scanner(f);

            int total=0;
            int input;
            int[]counts=(0.0.0.0.0.0);
            while(s.hasNextInt()){
                input=s.nextInt();
                total++;
                counts[input-1]++;
                
            }
        }catch(IoException e){
            UI.println("Couldn't open file for reading");
        }
      
            UI.println(counts[0]);
            UI.println(counts[1]);
             UI.println(counts[2]);
            UI.println(counts[3]);
             UI.println(counts[4]);
            UI.println(counts[5]);
                
   
      

}