import kareltherobot.*;
import java.awt.Color;

public class World implements Directions
{
    public static void main(String [] args)
    {
        UrRobot Jerry= new UrRobot(3,4,West,0);
        BetterRobot John = new BetterRobot(1,1, North, 0, Color.BLACK);
            Jerry.move();
            Jerry.move();
            Jerry.turnLeft();
            Jerry.turnLeft();
            Jerry.turnLeft();
            Jerry.putBeeper();
            Jerry.move();
            Jerry.move();
            Jerry.move();
            Jerry.move();
            Jerry.move();
            Jerry.move();
            Jerry.move();
            
            John.move(4);
            John.putBeeper(8)
            
    }
}