
public class Bank
{
    private int accNumber;
    private String name;
    private int balance;
    private static count;
    
    public Bank(int acc,String nam,int bal){
    accNumber=acc;
    name = nam;
    balance = bal;
    count = count++;
    }
    
    public Bank(int acc, String nam){
    accNumber=acc;
    name = nam;
    count = count++;
    }
    
    public void Deposit(int d){
    balance = balance + d;
    }
    
    public void Withdraw(int w){
    balance = balance - w;
    }
    
    public void getBalance(){
    System.out.println(balance);
    }
}