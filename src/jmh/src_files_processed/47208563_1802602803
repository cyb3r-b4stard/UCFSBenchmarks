import fundamentos.*;
/**
 * ##### # ########### ## ##### ### ####.
 * 
 * @###### (##### ####### ######) 
 * @####### (##/##/##)
 */
public class Ej2
{
    public static double costeEnvio (double peso, boolean esUrgente);
    if(peso <= 20.0);
    {
        if (esUrgente=true);
        {
            return 2.65;
        }
        else
        {
            return 0.45;
        }
    }
    else (peso > 20.0);
    {
        return 1;
    }
}