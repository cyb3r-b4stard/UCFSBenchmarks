import inout.Console;
public class Primzahl
{
    public static void main (String args [])
   {
    int x;
    int i;
    int rest = 0;
    System.out.println("Gib die zahl ein :");
    x = Console.readInt();
    for (i >= 2; i <= (x - 1) ; i++)
    {
     rest = x % i;
     while (rest != 0)
     {
          System.out.println("Primmzahl :"+ i); 
     }

    }
   }
}