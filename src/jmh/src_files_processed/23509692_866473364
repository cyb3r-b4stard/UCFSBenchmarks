public class APStudent
{
    private String myName;
    private String myGrade;
    private int highestSAT;
    public APStudent(String input)
    {
        myName = input;
        myGrade = "Freshman";
        highestSAT = 0;
    }
    public String getName()
    {
        return myName;
    }
    public String getGrade()
    {
        return myGrade;
    }
    public int highestSAT()
    {
        return highestSAT;
    }
    public void printStudentData()
    {
        if (myGrade.equals("Alumni"))
        {
            System.out.println(myName + " is an " + myGrade);
        }
        else if (highestSAT > 0)
        {
            System.out.println("Student " + myName + " is a " + myGrade + " with best SAT score of " + highestSAT);
        }
        else
        {
            System.out.println("Student " + myName + " is a " + myGrade + " and has not taken the SAT");
        }
    }
    public void graduate()
    {
        if (myGrade.equals("Freshman"))
        {
            myGrade = "Sophomore";
        }
        else if (myGrade.equals("Sophomore"))
        {
            myGrade = "Junior";
        }
        else if (myGrade.equals("Junior"))
        {
            myGrade = "Senior";
        }
        else if (myGrade.equals("Senior"))
        {
            myGrade = "Alumni";
        }
        else
        {
        }
    }
    public void newSATScore(int inputscore)
    {
        if (inputscore > highestSAT)
        {
            highestSAT = inputscore;
        }
        else
        {
        }
    }
    public String name()
    {
        return "Liam"
    }
}
        