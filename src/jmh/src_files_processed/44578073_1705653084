import apcslib.*;
public class Rectangle
{
    private static DrawingTool pencil = new DrawingTool (new SketchPad (500, 500, 10) );
    
    private double myX, myY, myWidth, myHeight;
    
    private double myDirection;
    
     
     Rectangle ()
     {
      myX = 0;
      myY = 0;
      myWidth = 0;
      myHeight = 0;
      myDirection = 0;
     }
        
     Rectangle (Rectangle rect)
     {
      myX = rect.getXpos();
      myY = rect. get.Ypos();
      myWidth = rect.getWidth();
      myHeight = rect.getHeight();
      myDirection = rect.getDirection();
     }
     
     Rectangle (double x, double y, double width, double height)
     {
      myX = x;
      myY = y;
      myWidth = width;
      myHeight = height;
     }
        
    public void setXPos (double x)
    {
        myX = x;
    }
    public void setYPos (double y)
    {
        myY = y;
    }
    
    pubic static void main (Strings [] args)
    {
     pencil.up();
     pencil.move (-100, 100);
     pencil.down();
     pencil.turnRight(135);
     pencil.forward (width);
     pencil.turnRight(90);
     pencil.forward (height);
     pencil.turnRight(90);
     pencil.forward (width);
     pencil.turnRight (90);
     pencil.forward (height);
     
     pencil.turnRight (90);
     pencil.forward (100);
     pencil.turnRight (90);
     pencil.forward (height);
     
     pencil.turnLeft (90);
     pencil.forward (100);
     pencil.turnLeft (90);
     pencil.forward (height);
     
     pencil.turnRight(90);
     pencil.forward (100);
     pencil.turnRight (90);
     pencil.forward (100);
     pencil.turnRight (90);
     pencil.forward (width);
     pencil.turnLeft (90);
     pencil.forward (100);
     pencil.turnLeft(90);
     pencil.forward (width);
     
     
     
     double areaTotal = width * height;
     double perimeterTotal = (2 * width) + (2 * height);
     System.out.println ("Area of square: " + areaTotal);
     System.out.println ("Perimeter of square: " + perimeterTotal);
     
     double areaVertRect = m * height;
     System.out.println ("Area of vericle rectanlges : " + areaVertRect);
     double perimeterVertRect = (2 * 100) + (2 * height);
     System.out.println ("Perimeters of verticle rectangle : " + perimeterVertRect);
     
     double areaHorRect = m * width;
     System.out.println ("Area of horizontal rectangles : " + areaHorRect);
     double perimeterHorRect = (2 * 100) + (2 * width);
     System.out.println ("Perimeters of horizontal rectangle : " + perimeterHorRect);
     
    }
}