class Shirt
{
    static int F1( int a,int b,int c,int d,int e, int f, int g, int n)
    {
        if(a==1&&b==2&&c==3&&d==4&&e==5&&f==6&&g==7)
        {
            return n;
            
        }

        else
        {
            return F1(d,a,b,c,e,f,g,n+1);
            return F1(b,c,d,a,e,f,g,n+1);
            return F1(a,b,c,e,f,g,d,n+1);
            return F1(a,b,c,g,d,e,f,n+1);
        }
    }
    static void main(int a,int b,int c,int d,int e, int f, int g, int n )
    {
        int x=F1(a,b,c,d,e,f,g,n););
    }
}
