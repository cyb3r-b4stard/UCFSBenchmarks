public class Chips extends Nahrungsmittel
{
    public Chips(double calorien, double eigengewicht)
    {
      calorien = 2200.3; 
      eigengewicht = 200;
      gewicht=eigengewicht;
      cal = calorien;
    }
    public void anzeigen()
    {
        return double cal;
        return double gewicht;
    }
}
   