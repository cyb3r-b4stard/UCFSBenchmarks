import java.util.Scanner; 

public class Week5_Car
{
    
    public static void main (String[]args){
        
        Scanner scannerObject = new Scanner(System.in); 
        
        System.out.println("Enter the make and model of the car");
        String model = scannerObject.nextLine();
        System.out.println("Enter the colour of the car");
        String colour = scannerObject.nextLine();
        System.out.println("The car is a " + model "and the colour of the car is " + colour);
        
        
        
        
        
    }
}