public class Palco
{

    private int codigo;
    private String nome;
    private Banda [] banda;
    private int quantidadeDeBandas;

    public Palco(int cod, int nom, int qtdBandas, int tamanho){
        codigo=cod;
        nome=nom;
        quantidadeDeBandas=qtdBandas
        banda new Banda[tamanho];

    }

    
    
    
    
    
    
    
    
    
}