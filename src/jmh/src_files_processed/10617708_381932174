import java.util.Random;
public class P1D
{
    public static void main (String [] args)
    {
        int s;
        Random x = new Random();
        int two;
        int num;
        for(int r =1; r<=10; r++)
        {
            System.out.println();
            for (int c = 1; c<=1; c++)
            {
                two = r*r;
                s=x.nextInt(two)+1;
                s>r && s<two
                System.out.print(r+"\t"+ two +"\t"+ s);
            }
        }
    }
}