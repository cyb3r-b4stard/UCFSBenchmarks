import java.io.*;
class library
{
    public static void main(String args[]) throws IOException{
BufferedReader br=new BufferedReader(new InputStreamReader (System.in));
System.out.println("Please Type The 6 Digit Book Number");
int bn=integer.parseInt(br.readLine());
System.out.println("Please Type Phone No.");
int phno=Integer.parseInt(br.readLine());
System.out.println("Please type your name");
String name=br.readline();
System.out.println("No. of days returned Late");
int days=Integer.parseInt(br.readLine());

double amount=0.0;
if(days<5&&days>0)
amount=days*2;
else if(days<10&&days>5)
amount=days*3;
else if(days<20&&days>10)
amount=days*4;
else if (days<30&&days>20)
amount=days*5;
else if (days<100&&days>30)
amount=days*10;
else
System.out.println("INVALID");


System.out.println("Book NO\t\tPhone No\t\t Name\t\t No.of days\t\t Fine");
System.out.println(bn+"t\t"+phno+"t\t"+days+"\t\t"+amount;
}
}