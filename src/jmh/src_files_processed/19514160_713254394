


public class Book
{
    private String _bookName;
    private String _authorName;
    private int _numPage;
    private boolean _available;
    private String _friendName;
    
    public String book()
    {
        _friendName=_friendName;
       
        
    }
    
    public boolean book (_available)
    {
        _available = _available;
            return true;
            
    }
}
