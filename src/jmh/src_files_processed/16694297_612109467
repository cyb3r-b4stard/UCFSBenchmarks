import java.io.*;
class revstring
{
 public static void main()throws IOException
{
DataInputStream in = new DataInputStream(System.in);
String rj;
int l,i,v;
char A;
System.out.println("enter str");
rj=in.readLine();
l=rj.length();
for(i=0;i<=l;i++)
{
A=rj.charAt(i);
           
if(A=='A' ||A=='a'||A=='E'||A=='e'||A=='I'||A=='i'||A=='O'||A=='o'||A=='U'||A=='u')
{
v=v+1;
}
}
} 
}
}
              
              
              
              
              
              
              
              
              
              