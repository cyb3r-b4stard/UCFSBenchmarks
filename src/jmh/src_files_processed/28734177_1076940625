    private void UfoErscheint() {

        Rumpf= new Rechteck();
        Rumpf.farbeAendern("ufograu");
        Rumpf.groesseAendern(90,20);
        Rumpf.xSetzen(900);
        Rumpf.ySetzen(50);
        Rumpf.sichtbarMachen();

        Cockpit= new Rechteck();
        Cockpit.farbeAendern("ufogelb");
        Cockpit.groesseAendern(40,15);
        Cockpit.xSetzen(925);
        Cockpit.ySetzen(37);
        Cockpit.sichtbarMachen();

        Beamer= new Rechteck();
        Beamer.farbeAendern("hellgrau");
        Beamer.groesseAendern(40,10);
        Beamer.xSetzen(925);
        Beamer.ySetzen(67);
        Beamer.sichtbarMachen();

        Strahl= new Rechteck();
        Strahl.farbeAendern("ufohellgelb");
        Strahl.groesseAendern(30,490);
        Strahl.xSetzen(930);
        Strahl.ySetzen(74);

    }

    private void RehLäuftWeg() {
        for(int i=0; i<55; i++) {
            Bauch.nachLinksBewegen();
            Hals.nachLinksBewegen();
            Bein1.nachLinksBewegen();
            Bein2.nachLinksBewegen();
            Kopf.nachLinksBewegen();
            Nase.nachLinksBewegen();
            Augen.nachLinksBewegen();
            Ohren.nachLinksBewegen();
        }
    }

    private void UfoEntführtReh() {
        for(int i=0; i<105;i++) {
            if(Cockpit.xPosition == Bauch.xPosition) {
                Strahl.sichtbarMachen();
                Bauch.nachObenBewegen();
                Hals.nachObenBewegen();
                Bein1.nachObenBewegen();
                Bein2.nachObenBewegen();
                Kopf.nachObenBewegen();
                Nase.nachObenBewegen();
                Augen.nachObenBewegen();
                Ohren.nachObenBewegen();}
            else {
                Cockpit.nachLinksBewegen(); 
                Rumpf.nachLinksBewegen();
                Beamer.nachLinksBewegen();
                Strahl.nachLinksBewegen();

            }
        }
        Bauch.unsichtbarMachen();
        Hals.unsichtbarMachen();
        Bein1.unsichtbarMachen();
        Bein2.unsichtbarMachen();
        Kopf.unsichtbarMachen();
        Nase.unsichtbarMachen();
        Augen.unsichtbarMachen();
        Ohren.unsichtbarMachen();
        Strahl.unsichtbarMachen();

    }

    private void UfoFliegtWeg() {
        for(int i=0; i<50; i++) {
            Cockpit.nachLinksBewegen(); 
            Rumpf.nachLinksBewegen();
            Beamer.unsichtbarMachen();
            Strahl.unsichtbarMachen();
        }
        Cockpit.unsichtbarMachen(); 
        Rumpf.unsichtbarMachen();
        Beamer.nachLinksBewegen();
        Strahl.nachLinksBewegen();

    }

    public void DasRehUndDasUfo() {
        UfoErscheint();
        RehLäuftWeg();
        UfoEntführtReh();
        UfoFliegtWeg();
    }

