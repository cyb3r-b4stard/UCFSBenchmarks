        Stern19.farbeAendern("gelb");
        Stern19.groesseAendern(3,2);
        Stern19.xSetzen(120);
        Stern19.ySetzen(310);
        Stern19.sichtbarMachen();

        Stern20= new Dreieck ();
        Stern20.farbeAendern("gelb");
        Stern20.groesseAendern(5,3);
        Stern20.xSetzen(430);
        Stern20.ySetzen(250);
        Stern20.sichtbarMachen();

        Stern21= new Dreieck ();
        Stern21.farbeAendern("gelb");
        Stern21.groesseAendern(5,2);
        Stern21.xSetzen(800);
        Stern21.ySetzen(300);
        Stern21.sichtbarMachen();

        Stern22= new Dreieck ();
        Stern22.farbeAendern("gelb");
        Stern22.groesseAendern(4,3);
        Stern22.xSetzen(450);
        Stern22.ySetzen(120);
        Stern22.sichtbarMachen();

        Mond= new Kreis();
        Mond.farbeAendern("weiss");
        Mond.groesseAendern(70);
        Mond.xSetzen(925);
        Mond.ySetzen(500);
        Mond.sichtbarMachen();
        Mond.sichtbarMachen();
        for(int i=0; i<380; i++) {
            Mond.nachObenBewegen();
            Wiese.sichtbarMachen();
        }
        
        status = "Nacht";
    }
    else { 
        System.out.print("Die Sonne muss untergegangen sein!");
    }

    public void Sonnenaufgang() {
        if(status=="Nacht") {
            
            for(int l=0; l<380; l++) {
                Mond.nachUntenBewegen();
            }

            Hintergrund.farbeAendern("dblau");   
            Sonne.sichtbarMachen();
