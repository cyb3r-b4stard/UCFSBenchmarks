public class pal
{
    public static void main(int n)
    { 
        int m,ld,s=0;
        m=n;
        do
        {
            ld=n%10;
            s=s*10+ld;
            n=n/10;
        }
        while(n!=0);
        if(s==m))
        System.out.println("the no. is palindrome="+m);
        else
        System.out.println("the no. is not palindrome"=+m);
    }
}