 public class Musculines
 {
     private int rut;
     private string nombre;
     private string apellido;
     private double edad;
     private boolean estado;
     private int valor;
     private char sexo;
     private int horas;
     
    /**
      * Constructor, donde se le da algun valor a los atributos
      */
     
     public Musculines(int rut,string nombre,string apellido, double edad,boolean estado,int valor, char sexo, int horas)
    
     this.rut=rut;
     this.nombre=nombre;
     this.apellido=apellido;
     this.edad=edad;
     this.estado=estado;
     this.valor=valor;
     this.sexo=sexo;
     this.horas=horas;
     
    
     
    /** Metodo accesador para cada atributo
      * @return devuelve un valor int al atributo rut
      */
     public int getRut()
     {
         return rut;
        }
    /**Metodo accesador para cada atributo
      * @return devuelve un valor int al atributo nombre
      */
     public string getNombre()
     {
         return nombre;
        }
     /**Metodo accesador para cada atributo
      * @return devuelve un valor string al atributo apellido
      */
     public int getApellido()
     { 
         return apellido;
        }
     /**Metodo accesador para cada atributo
     * @return devuelve un valor double al atributo edad
     */
     public double getEdad()
        {
            return edad;
        }
     /**Metodo accesador para cada atributo
      * @return devuelve un valor boolean al atributo estado
      */
     public boolean getEstado()
     {
         return estado;
        }
     /**Metodo accesador para cada atributo
      * @return devuelve un valor int al atributo valor
      */  
     public int getValor()
     {
         return valor;
        }
     /**Metodo accesador para cada atributo 
      * @return devuelve un valor char al atributo sexo
      */
     public char getSexo()
     {
         return sexo;
        }
     /**Metodo accesador para cada atributo
      * @return devuelve un valor int al atributo horas
      */
     public int getHoras()
     {
         return horas;
        }
     /** Metodo mutador para cada atributo
      * @param un nuevo valor para rut
      */   
     public void setRut(int rut)
     {
       this.rut=rut;
    }
    /**Metodo mutador para cada atributo
     * @param un nuevo valor para nombre
     */
    public void setNombre(string nombre)
    {
        this.nombre=nombre;
    }
    /**Metodo mutador para cada atributo
     * @param un nuevo valor para apellido
     */
    public void setApellido(string apellido)
    {
        this.apellido=apellido;
    }
    /**Metodo mutador para cada atributo
     * @param un nuevo valor para edad
     */
    public void setEdad(double edad)
    {
        this.edad=edad;
    }
    /**Metodo mutador para cada atributo
     * @param un nuevo valor para estado
     */
    public void setEstado(boolean estado)
    {
        this.estado=estado;
    }
    /**Metodo mutador para cada atributo
     * @param da un nuevo valor para atributo valor
     */
    public void setValor(int valor)
    {
        this.valor=valor;
    }
    /**Metodo mutador para cada atributo
     * @param da un nuevo valor para sexo
     */
    public void setSexo(char sexo)
    {
        this.sexo=sexo;
    }
    /**Metodo mutador para cada atributo
     * @param da un valor para horas
     */
    public void setHoras(int horas)
    {
        this.horas=horas;
    }
    
    /**Metodo impresion
     * 
     */
    {
    System.out.println("Su rut es:"+rut)
    System.out.println("Su nombre es:"+nombre)
    System.out.println("Su apellido es:"+apellido)
    System.out.println("Su edad es:"+edad)
    System.out.println("Su estado es:"+estado)
    System.out.println("Su valor es:"+valor)
    System.out.println("Su sexo es:"+sexo)
    System.out.println("Sus horas es:"+horas)
    }
    
    /**metodo customer
     * 
     */
    {
    public void InsertDesc(int desc)
    valortotal=(valor*horas)-desc/100
    }
 

    
    
    
    
        
        
     
     
     
     
     
    
     
     
     
     
     
     
     
     
     
     
     
     
     
     
     
     
     
     