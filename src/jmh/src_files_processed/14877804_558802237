class prime
{
    public static void main(String[]args)
    {
        int i,n;
        System.out.println("enter value in n");
        n=Integer.parseInt(args[0]);
        for(i=2;i<n;i+);
        {
            if(n%i==0)
break;
}
if(n==1)
System.out.println(n+"is a prime no");
else
System.out.println(n+"is not a prime no");
}
}
        
        