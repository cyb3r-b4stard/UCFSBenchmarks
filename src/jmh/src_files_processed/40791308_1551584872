import java.util.Scanner;

class Rownanie{
    private double a;
    private double b;
    private double c;
    private double x;
    
    public Rownanie(double a1, double b1, double c1){
        a=a1;
        b=b1;
        c=c1;
    }
   
    
    public void pobierzDane(double x1){
        x=x1;
    }
    
    public void wyswietlDane(){
        System.out.print("a=");
        System.out.println(a);
        System.out.print("b=");
        System.out.println(b);
        System.out.print("c=");
        System.out.println(c);
    }
    
    public void oblicz(){
        double x11=(-c/b);
        double d=(b*b-4*a*c);
        double x12=(-b-d)/2*a;
       
        
       if(a==0 && b==0 && c==0)
        System.out.println("Rozwiazaniem rownania jest kazda liczba rzeczywista");
       
       else if(a==0 && b==0 && c!=0)
        System.out.println("Sprzecznosc - brak rozwiazan");
        
       else if(a==0 && b!=0)
        System.out.print("Funkcja jest liniowa i posiada jedno rozwiazanie: ");
        System.out.println(x11);
       
       else if(a!=0 && d<0)
        System.out.println("Delta ujemna - brak rozwiazan w zakresie licz rzeczywistych");
        
       else if(a!=0 && d>=0)
       double x12=(-b-d)/2*a;
        
       
       if(d<0)
       System.out.println("brak rozwiazan w zakresie licz rzeczywistych");
       
       
    }

   public static void main(String args[]){
       Scanner sc = new Scanner(System.in);
       int i = sc.nextInt();
 
       Rownanie rownanie=new Rownanie(2,5,2);
       rownanie.pobierzDane(i);
       rownanie.wyswietlDane();
       
    }
        
    
    
}
 
      