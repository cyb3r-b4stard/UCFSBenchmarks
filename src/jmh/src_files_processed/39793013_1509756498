public class Exersises
{
  public static int isPythaTriple (int a, int b, int c)  
    {boolean bl;
      
     if (a*a+b*b == c*c)
         {bl;}
         else
         {bl = false;}}
    
    
    
    
        
  public static int determineMaximum (int a, int b, int c)
  {
      
      if (a<c & a<c)
      {return c;}
         
      else if (a<b & c<b)
      {return b;}
          
      else 
          {return a;} }
   
  public static void main (String[]args)
  { System.out.print("The biggest number is " + isPythaTriple (3,6,15));}}
   
      
  
