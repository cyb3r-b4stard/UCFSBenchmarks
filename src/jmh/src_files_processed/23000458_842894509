
public class QuizMethods
{
public static double square(double nbr) {
  double answer = nbr * nbr;
  return answer;
}

public static int round(double nbr ) {
   nbr += 0.5;
   int rounded = (int) nbr;
   return rounded;
}


public static int subtract(int nbr1, int nbr2) {
  
  return nbr1 - nbr2;
}

public static int larger(int nbr1, int nbr2) {
    if (nbr2 > nbr1) 
         return nbr2;
    return nbr1;
}

public static void main(String[ ] args) {
   java.util.Scanner boom = new java.util.Scanner(System.in);
   System.out.print("enter two integers-> ");
   int a = boom.nextInt( );
   int b = boom.nextInt( );
   int result = larger(a, b);
   System.out.println("The result of larger(" + a + ", " + b + ") is " + result);
   
}

}
}