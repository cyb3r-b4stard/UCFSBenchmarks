/**
 * ######## ##### ###### - ##### # ########### ## ### ##### ####
 * 
 * @######: 
 * ####: 
 */
public class auto extends EQUIPO

{
    private String puertas;
    private String maletero;
    private String manual;
    private String cinturondeseguridad;
    private String parabrisas;
    public auto (String puertas, String maletero,String manual,String cinturondeseguridad,String parabrisas,String marca, String modelo, String año, String version){
        super(String marca, String modelo, String año, String version);
        this.puertas=puertas;
        this.maletero=maletero;
        this.manual=manual;
        this.cinturondeseguridad=cinturondeseguridad;
        this.parabrisas=parabrisas;
    }
    
     public get puertas(){
        return puertas;
    }
    
    public setPuertas(String puertas){
        this.puertas=puertas;
    }
    
    public get maletero(){
        return maletero;
    }
    
    public setMaletero(String maletero){
        this.maletero=maletero;
    }
    
    public get manual(){
        return manual;
    }
    
    public setManual(String manual){
        this.manual=manual;
    }
    
    public get cinturondeseguridad(){
        return cinturondeseguridad;
    }
    
    public setcinturondeseguridad(String cinturondeseguridad){
        this.cinturondeseguridad=cinturondeseguridad;
    }
    
    public get parabrisas(){
        return parabrisas;
    }
    
    public setparabrisas(String parabrisas){
        this.parabrisas=parabrisas;
    }
}