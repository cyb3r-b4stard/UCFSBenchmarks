public class practice
{
   public static final int control=10
    {
        for (int line=1; line<=7-line;line++)
        {
           for (int rdot=7;rdot<=7;rdot--)
           {
               System.out.print(".");
            }
            System.out.print(line);
        for(int ldot=1; ldot<=line-1;ldot++)
        {
        System.out.print(".");
    }
    System.out.println();
}
}
}
        