 import java.util.*;
public class Shi_thead
{
    public static void main (String[] args){
     int n;
     n = console.nextInt();
     int result = 1;
     for (int i = 1; i <= n; i++) {
       result = result * i;
     }
     System.out.println("The factorial of " + n " is " + result);
   }
}
  