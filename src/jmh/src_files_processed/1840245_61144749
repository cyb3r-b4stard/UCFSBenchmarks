
import java.util.Scanner;
public class PasajeBoleto {
public static void main(String [] args) {
    Scanner teclado=new Scanner (System.in) ;
    k=0.118046
    dolar=13.200;
    float x;
    float pasaje;
    
    System.out.printl("Escribe el total de kilometros que se realizara el vuelo");
    x=teclado.nextFloat ();
    
    pasaje=k*x(dolar);
    
    if(x>800) {
    System.out.println("se le a descontado un 30% "+x);
}
else
System.out.println("su pasaje es"+x);
}
}
