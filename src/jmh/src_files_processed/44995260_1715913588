import java.io.*;
public class reverse
{
    public static void main () throws IOException
    {
        DataInputStream in= new Data Input(System.in);
        system.out.println ("Enter a Number");
        int n= integer.parseint(readline);
        int rev=0,rem;
        while(n!=0)
        {
            rem=n%10;
            rev=rev*10+rem;
            n=n/10;
        }
        system.out.println ("reverse"=+rev);
    }
}

    