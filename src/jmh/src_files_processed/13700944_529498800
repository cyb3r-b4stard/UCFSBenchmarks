class day
{
 public static void main (int n)
 {
  switch (n)
  {
   Case1:System.out.println("Monday");
         break;
   Case2:System.out.println("Tuesday");
         break;
   Case3:System.out.println("Wednesday");      
         break;
   Case4:System.out.println("Thursday");
         break; 
   default:system.out.println("Invalid Input"); 
  }  
 }  
}