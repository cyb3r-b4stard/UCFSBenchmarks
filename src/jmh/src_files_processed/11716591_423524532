public class CigarParty
{
    public static void main (String args[])
    {
    int cigars = 10;
    boolean weekend;
    weekend = true;
    call goodparty();
    }
    
    public static void goodParty(int cigars, boolean weekend)   
    {
       if (cigars >=40 && cigars <= 60)
       
       System.out.println("good party");
       
       if (cigars >=40 && weekend == true)
       System.out.println("good party");
       
       else
       
       System.out.println("bad party");
    }
}