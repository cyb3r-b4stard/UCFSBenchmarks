public class superComputadora{
    private int identificador;
    private double precio;
    private trabajador trabajador;
    private memoriaRam memoriaRam;
    private procesador procesador;
    public superComputadora (int identificador, double precio, trabajador trabajador, memoriaRam memoriaRam, procesador porcesador){
        this.identificador = identificador;
        this.precio = precio;
        this.trabajador = trabajador;
        this.memoriaRam = memoriaRam;
        this.procesador = procesador;
        
    }
    public int getIdentificador (){
        return identificador;
    }
    
    public double getPrecio (){
        return precio;
    }
    
    public trabajador getTrabajador (){
        return trabajador;
    }
    
    public memoriaRam getMemoriaRam (){
        return memoriaRam;
    }
    
    public procesador getProcesador (){
        return procesador;
    }
    public void setIdentificador (int identificador){
        this.identificador = identificador;
    }
    
    public void setPrecio (double precio){
        this.precio = precio;
        
    }
    
    public void setTrabajador (trabajador trabajador){
        this.trabajador = trabajador;
    }
    
    public void setMemoriaRam (memoriaRam memoriaRam){
        this.memoriaRam = memoriaRam;
    }
    
    public void setProcesador (procesador procesador){
        this.procesador = procesador;
    }
    
    public void descuento (){
        if (trabajador == 'E')
        precio = precio * 0.15
        else
        
    }
    
    public void imprimir (){
        System.out.println ("identificador" + identificador);
        System.out.println ("precio" + precio);
        System.out.println ("trabajador" + trabajador);
        System.out.println ("memoria ram" + memoriaRam);
        System.out.println ("procesador" + procesador);
    }
    
    

}