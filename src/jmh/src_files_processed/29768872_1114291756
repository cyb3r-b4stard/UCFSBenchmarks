public class Rectangles
{
    private double length;
    private double width;
    public Rectangles ()
    {
        length = 2;
    }
    
    public Rectangles ()
    {
        width = 1;
    }
    
    public Rectangles (double length1)
    {
        length = length1;
    }
    
    public Rectangles (double width1)
    {
        width = width1;
    }
        width
    
    
public void setlength(double newLength)
    {
        length = newLength;
    }
    
    public void setWidth(double newWidth)
    {
        width = newWidth;
    }
    
    public double area()
    {
        double rectangleArea;
        rectangleArea = 2*length*2*width;
        return(rectangleArea);
    }

}