

public class Methoden extends Prototyp
{
    public void machSchon1()
    {
        int a = Integer.parseInt(textField1.getText());
        int b = Integer.parseInt(textField2.getText());
        int Fläche = a*b;
        textField3.setText(Integer.toString(Fläche));
        int Umfang = 2*(a+b);
        textField4.setText(Integer.toString(Umfang)); 
        int Diagonale = 1/2*(a^2+b^2)
    }
}