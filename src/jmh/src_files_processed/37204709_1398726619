
public class wew
{
    public int dice(){
        double randomn = Math.random();
        randomn = randomn * 6;
        randomn = randomn + 1;
        int wew = (int) randomn;
        return wew;
}

int kul = dice();
int nice = dice();
System.out.print("kul" + kul);
System.out.println("nice" + nice);
} 