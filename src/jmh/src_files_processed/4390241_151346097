public class PoleDoubleCisel
{
    private double [] pole;
    
    private int pocetPrvkov = pocetPrvkov;
    private int pocetAktPrvkov = pocetAktPrvkov;
    public PoleDoubleCisel( int pocetPrvkov) 
    {  
        this.pocetPrvkov = pocetPrvkov;
        pole = new double [pocetPrvkov];
        pocetAktPrvkov = 0 ;
    
    
    }
    
    
    
    