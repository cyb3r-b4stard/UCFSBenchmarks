public class BMI
{
    double masse;
    double grosse;
    BMI(double masse1, double grosse1)
    {
        double masse = double masse1;
        double grosse = double grosse1;
    }
        
    
    double BMIErrechnen ()
        {
            return masse1/grosse1*grosse1;
        }
        
    void BMIBerechnen(double BMINEU)
    {
    if (BMINEU <=19)
    {
        System.out.println("Untergewicht");
    }
    else
    {
        if (BMINEU <=24)
        {
            System.out.println("Normalgewicht");
        }
        else
        {
            if (BMINEU <=30)
            {
                System.out.println("Ãœbergewicht");
            }
            else
            {
                if (BMINEU <=40)
                {
                    System.out.println("Adipositas");
                }
                else
                {
                    System.out.println("schwere Adipositas");
                }
            }
        }
    }
}
}
            