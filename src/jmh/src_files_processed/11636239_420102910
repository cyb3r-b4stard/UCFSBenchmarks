import becker.robots.*;

public class neu
{
        public static void fahre()
        {
            City leoben = new City();
            Robot rob = new Robot(leoben,1,2,Direction.SOUTH);
            
            new Wall(leoben,2,3,Direction.EAST);
            new Wall(leoben,3,3,Direction.EAST);
            new Wall(leoben,4,3,Direction.EAST);
            
            new Thing(leoben,3,2)
            
            rob.move();
            rob.move();
            rob.turnLeft();
            rob.move();
            
            
        }
    }
        

