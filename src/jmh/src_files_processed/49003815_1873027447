import java.util.*;
public class q1
{
    public static main()
    {
        int a, b, c, d, e;
        Scanner sc= new Scanner(System.in);
        System.out.prinltn("Enter the numbers.");
        a=Int.nextInt();
        b=a;
        while(c=1; c<0; c!=0)
        {
            d= a%10;
            e=d*a;
        }
