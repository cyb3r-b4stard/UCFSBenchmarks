

public class Parralleogram extends Quadrilateral implements Comparable
{
    private double hieght;
    private double width; 
   public Parralleogram()
   {
       super();
       hieght = 0.0;
       width=0.0;
       
       
    }
    public Parralleogram(String name, double hieght, double width)
    {
        super(name);
        this.hieght = hieght;
       this.width = width;
      
        
    }
    public double getHieght()
    {
        return hieght;
    }
    public double getWidth()
    {
        return width;
    }
    public void changeHieght(double hieght)
    { this.hieght = hieght;
    }
    public void changeWidth( double width)
    {
        this.width= width;
    }
   public double area()
   {
       return hieght*width;
}
public int compareTo(Object other)
{
    
     Parralleogram object2 = (Parralleogram) other;
     return ((int)(object2.area()-area()));
    
}
public boolean equals(Object object)
{
    if (!(super.equals(object)))
    {
        return false;
    }
    
    
    Parralleogram object2 = (Parralleogram) object;
    double ELIPSON = 1E14;
    if(!(object2.getWidth()- getWidth()<ELIPSON) || !(object2.getHieght()-getHieght()<ELIPSON))
    {
        return false;
    }
    return true;
    

}
public String toString()
{
    return super.toString() + "width:" + width + "hieght:" + hieght + "Area:" area;
}
}