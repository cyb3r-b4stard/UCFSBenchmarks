
import static java.lang.System.*;

public class StarsAndStripes
{
   public void StarsAndStripes()
   {
      out.println("StarsAndStripes");
      printTwoBlankLines();
   }

   public void printTwentyStars()
   {
	   for(int i=1;i<20;i++)
	   {
		   out.print("*");
	   }
	   out.print("\n");
   }

   public void printTwentyDashes()
   {
	   for(int i=1;i<20;i++)
	   {
		   out.printl"-");
	   }
	   out.print("\n");
   }

   public void printTwoBlankLines()
   {
	   out.println("\n");
   }
   
   public void printASmallBox()
   {
	   printTwentyDashes();
	   printTwentyStars();
	   printTwentyDashes();
	   printTwentyStars();
	   printTwentyDashes();
	   printTwentyStars();
	   printTwentyDashes();
   }
 
   public void printABigBox()
   { 
	  printASmallBox();
	  printASmallBox();
   }   
}