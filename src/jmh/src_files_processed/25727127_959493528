import java.util.Scanner;
public class Fun
{
   public static void main(String [] args)
   {
        System.out.println("Select a menu");
        System.out.println("=============");
        System.out.println("[A] Profile");
        System.out.println("[B] GotQuestions?");
        System.out.println("[C] The Matchmaker");
        System.out.println("[D] Feelin' Lucky?");
        Scanner in = new Scanner(System.in);
        String choice = in.next();       
        if (choice.equalsIgnoreCase("a") )
            Profile();        
        else if (choice.equalsIgnoreCase("b"))
            GotQuestions();
        else if (choice.equalsIgnoreCase("c"))
            TheMathcmaker();
        else if (choice.equalsIgnoreCase("d") )   
            FeelinLucky feelinLucky = new FeelinLucky();
        else
            System.out.println("You are supposed to choose either a, b, c, or d.");
    }
}