
/**
 * ##### # ########### ## ##### ######### ####.
 * 
 * @###### (#### ####) 
 * @####### (# ####### ###### ## # ####)
 */
public class Rectangle
{
    private double witdth;
    private double length;
    
    public void overloadedconstructure(double width, double length)
    {
        width = width;
        length = length;
    }
    public void overloadedconstructure(double width, double length)
    {
        width = 3.0;
        length = 5.2;
    }
    
    public double Getwidth(double newwidth)
    {
       return width = newwidth;
    }
    
    public double Getlength(double newlength)
    {
        return length = newlength;
    }
    
    public double setwidth(double newwidth)
    {
         return width = newwidth;
    }
    
    public double setlength(double newlength)
    {
         return length = newlength;
    }
    
    public double calculateArea()
    {
        return double area = length*width;
    }
    
    public double calculatePerimeter()
    {
        return double perimeter = 2*length+2*width;
    }
    
    public static void print()
    {
        System.out.print("");
    }
    
}