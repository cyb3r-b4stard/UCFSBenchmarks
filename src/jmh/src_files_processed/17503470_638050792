import java.util.HashMap;
import java.util.ArrayList;
import java.util.Set;
import java.util.Iterator;

class NobelPrizeWinners
{
    private HashMap<Integer, String[]> nobelPrizeWinners;
    
    public NobelPrizeWinners()
    {
        nobelPrizeWinners = new HashMap<Integer, String[]>();
        
        String[] names = new String[2];
        names[0] = new String("Nelson Mandela");
        names[1] = new String("Frederik Willem de Klerk");
        nobelPrizeWinners.put(new Integer(1993), names);
        
        names = new String[3];
        names[0] = new String("Yasser Arafat");
        names[1] = new String("Shimon Peres");
        names[2] = new String("Yitzhak Rabin");
        nobelPrizeWinners.put(new Integer(1994), names);
        
        names = new String[2];
        names[0] = new String("Joseph Rotblat");
        names[1] = new String("Pugwash Conferences on Science and World Affairs");
        nobelPrizeWinners.put(new Integer(1995), names);
        
        names = new String[2];
        names[0] = new String("Carlos Filipe Ximenes Belo");
        names[1] = new String("Jose Ramos-Horta");
        nobelPrizeWinners.put(new Integer(1996), names);
        
        names = new String[2];
        names[0] = new String("International Campaign to Ban Landmines (ICBL)");
        names[1] = new String("Jody Williams");
        nobelPrizeWinners.put(new Integer(1997), names);
        
        names = new String[2];
        names[0] = new String("John Hume");
        names[1] = new String("David Trimble");
        nobelPrizeWinners.put(new Integer(1998), names);
        
        names = new String[1];
        names[0] = new String("Medecins Sans Frontieres");
        nobelPrizeWinners.put(new Integer(1999), names);
        
        names = new String[1];
        names[0] = new String("Kim Dae-jung");
        nobelPrizeWinners.put(new Integer(2000), names);
        
        names = new String[2];
        names[0] = new String("United Nations(U.N.)");
        names[1] = new String("Kofi Annan");
        nobelPrizeWinners.put(new Integer(2001), names);
        
        names = new String[1];
        names[0] = new String("Jimmy Carter");
        nobelPrizeWinners.put(new Integer(2002), names);
        
        names = new String[1];
        names[0] = new String("Shirin Ebadi");
        nobelPrizeWinners.put(new Integer(2003), names);
        
        names = new String[1];
        names[0] = new String("Wangari Muta Maathai");
        nobelPrizeWinners.put(new Integer(2004), names);
        
        names = new String[2];
        names[0] = new String("International Atomic Energy Agency (IAEA)");
        names[1] = new String("Mohamed ElBaradei");
        nobelPrizeWinners.put(new Integer(2005), names);
        
        names = new String[2];
        names[0] = new String("Muhammad Yunus");
        names[1] = new String("Grameen Bank");
        nobelPrizeWinners.put(new Integer(2006), names);
        
        names = new String[2];
        names[0] = new String("Intergovernmental Panel on Climate Change (IPCC)");
        names[1] = new String("Albert Arnold (Al) Gore Jr.");
        nobelPrizeWinners.put(new Integer(2007), names);
        
        names = new String[1];
        names[0] = new String("Martti Ahtisaari");
        nobelPrizeWinners.put(new Integer(2008), names);
        
        names = new String[1];
        names[0] = new String("Barack H. Obama");
        nobelPrizeWinners.put(new Integer(2009), names);
    }
    
    public int getNumberOfWinners()
    {
        int counter = 0;
        for (int i = 0; i < String.length; i++){
            if(String[i] != null{
                counter ++;
            }
        }
        return counter;
        
    }
}