
/**
 * ########## # ###### #######
 * 
 * @###### (########) 
 * @####### (####.##.##)
 */
public class Country
{
  private String countryName;
  private double surfaceArea;
  private double totalPopulation;
  private double birthRate;
  private double deathRate;
  private double fertilityRate;
  private double ruralPopulation;
  private double telephoneLines;
  private double mobilePhone;
  public Country(String theCountry, double theSurface, double theTotal,
    double theBirth, double theDeath, double theFertility, double theRural,
        double theTelephone, double theMobile)
  {
     countryName = theCountry;
     surfaceArea = theSurface;
     totalPopulation = theTotal;
     birthRate = theBirth;
     deathRate = theDeath;
     fertilityRate = theFertility;
     ruralPopulation = theRural;
     telephoneLines = theTelephone;
     mobilePhone = theMobile;
  }
  
  /**
   * 
   */
  public String toString()
  {
      return countryName + " (Population: " + totalPopulation ")";
    }
}