import KuF.*;
import javax.swing.JFrame;
/**
 * ###### ### ######## ### #########
 * 
 * ###### ####
 * ####### #.#
 */
public class schnittstelle extends JFrame
{
    public schnittstelle()
    {
        load datei = new load(this,".i");
        String line = datei.next();
        
        String[] parts = line.split("!");
        
        if(parts.length==0){ return;}
        
        if(parts[0].equals("display"))
        {
        if(parts.length!=7){ return;}
        String[] funktion_part = parts[6].split(":");
        if(funktion_part.length==0){ return;}

        polynom funktion = new polynom(tool.toDouble(funktion_part));
        
        new ausgabe_newton(funktion,parts[1],parts[2],parts[3],parts[4],parts[5])
        }
    }
}