public class Lab5
{
    private int x;
        public static void posNeg()
    {
        int x = (2+3) * 46 - 53 + 47 * (2 + 3) - 100;
        if (x > 0)
            {
                System.out.print("positive");
            }
        else
            {
                System.out.print("not positive");
            }
    }
    public static void teenager(int age)
    {
        if (age > 12 && age <20)
            System.out.println("You are a teenager.");
            else
            System.out.println("You are not a teenager.");
    }
    public static void showGrade(int mark)
    {
        if (mark > 84)
        {
            System.out.println("Your grade is HD");
        }
            else if (mark > 75 && mark < 85)
        {
            System.out.println("Your grade is D");
        }
            else if (mark > 65 && mark < 75)
        {
            System.out.println("Your grade is C");
        }
            else if (mark > 49 && mark < 65)
        {
            System.out.println("Your grade is P");
        }
            else
        {
            System.out.println("Your grade is Z");
        }
    }
    public static int smallest( int a, int b, int c)
    {
        if (a < b && a < c)
        {
            return a;
        }
            else if (b < a && b < c)
        {
            return b;
        }
            else if (c < a && c < b)
        {
            return c;
        }
    }
  