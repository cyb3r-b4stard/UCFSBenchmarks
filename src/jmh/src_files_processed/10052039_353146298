
/**
 * ##### # ########### ## ##### ######## ####.
 * 
 * @###### (###### ##) 
 * @####### (#/##/##)
 */
public class Computer
{
    private String manufacturer;
    private int hardDriveStorage;
    private int mainMemory;
    
    /**
     * Constructor for creating a Computer object with
     * default values for the fields.
     */
    public Computer(String initManufacturer, int inithardDriveStorage, int initmainMemory) {
        manufacturer = initManufacturer;
        hardDriveStorage = inithardDriveStorage;
        mainMemory = initmainMemory;
    }
    
    /**
     * Constructor for objects of class Account
     * getMainMemory
     */
    public int getMainMemory(){
        return mainMemory/2014;
        
    }
    
        
   

  
    }
}