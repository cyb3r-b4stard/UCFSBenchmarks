
import java.awt.geom*;
import javax.swing.*;

/**
 * ##### ################ - ##### # ########### ## ### ##### ####
 * 
 * @###### (#### ####) 
 * @####### (# ####### ######)
 */
public class GraphicsPractice extends JApplet
{
   public void paint(Graphics g)
   {
       Graphics2D g2 = (Graphics2D) g;
       Rectangle r1 = new Rectangle(5, 10, 20, 30);
       g2.draw(r1);
       
   
       Ellipse2D.Double ellipse = new Ellipse2D.Double(30, 40, 50, 50);
       g2.draw(ellipse);
    }  
}