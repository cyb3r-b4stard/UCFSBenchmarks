import java.util.*;

public class World
{
   private int CharRow = 0;
   private int CharCol = 0;
   private char[][] WorldSize;
   
   
   public World(int Height, int Width)
       {
        int HeightCounter = 0; 
        WorldSize = new char[Height][Width];
       
    } 
     
   public void FillWorld(
         do
         {
             int WidthCounter = 0;
             do
             {
                         
              WorldSize[HeightCounter][WidthCounter] = 45;
            
                  
              WidthCounter++;
            
            }while(WidthCounter != Width);  
            
            HeightCounter++;
         }while(HeightCounter != Height);
    
    
    
    
   
   
  
       
       

}