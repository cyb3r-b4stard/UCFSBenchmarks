import java.util.Scanner;

public class CheckDigitThingAlgorithim {
    public static void main(String[] args) {
        static Scanner scan = new Scanner(System.in);
        int Num1 = sc.nextInt();
        int Num2 = sc.nextInt();
        int Num3 = sc.nextInt();
        int Num1X = Num1*4;
        int Num2X = Num2*3;
        int Num3X = Num3*2;
        int Addition = Num1X + Num2X + Num3X
        int LastDigit = Addition % 11
        System.out.println(Lastdigit)
    }
}
