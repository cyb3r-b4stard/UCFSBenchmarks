{
    public static void main(String args[])
     {
        char letter = 'A'; 
        int number = 75; 
        System.out.println("letter = " + letter); 
        System.out.println("its ASCII value = "); 
        System.out.println((int)letter);    
        System.out.println("ASCII value 75 = "); 
        System.out.println((char)number);   
    }
         
