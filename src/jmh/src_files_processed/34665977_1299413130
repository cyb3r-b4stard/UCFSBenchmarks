  /**
  *# ######### ###### #### ###### ### #### ##### ########### ##### ## ######.
  *## ### ###### ## # ### ###### ######.
  *@##### # - ### ###### ## ### ######.
  *@##### # - ###### #, #####'# ###### ### ######.
  */
  public static int domino (int n, int m) 
  {
    if ((n<=0) || (m<=0)) 
    {
      return 0;
    }
    else if (n==1 || n==2)
    {
      return n;
    }
    else
    {
      return domino(n-1,m) + domino(n-2,m);
    }
  }