
import java.util.Scanner;

public class Histogram
{
     

    public static int times(int[] array)
    {
        int[] counter= new int [10]; 

        for(int x=0; x<10; x++)
        {
            for(int y=0; y<array.length; y++)
            {
                if (x== array [y])
                {
                    counter[] x++; 
                }

            }
        }
        int times= counter; 
        return counter;
        
        
        

    }

    
}