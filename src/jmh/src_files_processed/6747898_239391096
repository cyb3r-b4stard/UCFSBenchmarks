
     public class Espaco_Festa extends Espaco
     {
         private  double ServicoBar;
         public Espaco_Festa( double Servicobar , double ValorDiario);
        }
             set( Servicobar,ValorDiario);
             setServicoBar(Servicobar);
            }
            public void setServicoBar(double Servicobar){
                this.ServicoBar=ServicoBar;
            }
            public double getServicoBar(){
                return this.ServicoBar;
            }
            public double retornarDados(){
                return this.retornarDados;
                
              
            }
        }
            
         