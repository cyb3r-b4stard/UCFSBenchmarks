import java util Scanner;
public class Ksiazka extends Publikacja 
{ String uczelnia;
  public Ksiazka(String tytul, String autor, int rok, String uczelnia)
  { 
       super(tytul,autor,rok); 
!
       this.uczelnia= new String(uczelnia); 
  }
  
  String dajUczelnie()
  {
      return uczelnia;
  }
  
  public String toString()
  { 
      return super.toString()+ uczelnia;
  }
}
