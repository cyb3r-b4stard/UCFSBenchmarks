class dowhileloop { 
public static void main (String[] args) {
    
    int TopNumber = 15; 
    int BottomNumber = 1;
    
    do 
    
    {
        System.out.print ("Old McDonald Had a Farm"); 
        BottomNumber++;
        
    { 
    
    While (BottomNumber < TopNumber); 
}
}
]
