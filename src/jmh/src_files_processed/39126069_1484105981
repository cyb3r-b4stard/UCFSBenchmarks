package Ch06.Exercises;
public class Roots
{
    private double a;
    private double b;
    private double c;
    private double x1;
    private double x2;
    public Roots()
    {}
    public double calcx1(double a, double b, double c)
    {
        double d = (b*b - 4*a*c);
        x1 = a-2*a + Math.sqrt(d);
        return x1
    }
    public double calcx2(double a, double b, double c)
    {
        x2 = a + Math.sqrt(d);
        return x2
    }
}