
/**
 * ##### # ########### ## ##### ###### ####.
 * 
 * @###### (#### ####) 
 * @####### (# ####### ###### ## # ####)
 */
public class Digits
{
    public static void main(String [] args)
    {
        int x=1;
        int y = 20;
        while(x < 20)
        {
            System.out.println(x + "/t" + x * x +  "/t" + x * x * x);
            x= x + 2;
        }
        while(y < 21) &&; y >= 0)
    {
        System.out.println(y);
        y= y - 2;
    }
}
}