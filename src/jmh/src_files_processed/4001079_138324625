import java.util.Scanner
/**
 * ##### # ########### ## ##### ###### ####.
 * 
 * @###### ####### #### 
 * @####### ### ##, ####
 */
public class random
{
    public static void roll(){
        double r= Math.random();
        double s= r*6;  
        double t= Math.floor(s); 
        double u= t+1;
        int w= (int) u;
        int v= (int)Math.floor(Math.random()*6)+1;
        system.out.println("You rolled a"+w+"and"+v);
    }
}