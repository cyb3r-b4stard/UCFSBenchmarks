
/**
 * ######## ### ########### ### ############ ## ###### ## ######
 * 
 * ### ########
 * ##/##/####
 */
public class Animal
{
    private String animalName = ;
    private double animalAge = ;
    private double animalWeight = ;
    
    private static double totalCount ;
    
    public Animal(String name, double age, double weight)
    {
        animalName = name ;
        animalAge = age ;
        animalWeight = weight ;
    }
    
    
    public String getName()
    {
        return animalName;
    }
    
    public void setName(String newVal)
    {
        animalName = newVal ;
    }
    
    public double getAge()
    {
        return animalAge;
    }
    
    public void setAge(double newVal1)
    {
        animalAge = newVal1 ;
    }
    
    public double getWeight()
    {
        return animalWeight;
    }
    
    public void setWeight(double newVal2)
    {
        animalWeight = newVal2 ;
    }
    
    public static double getCount()
    {
        return totalCount ;
    }
    
    public static void setCount(double newVal3)
    {
        totalCount = newVal3 ;
    }
}



