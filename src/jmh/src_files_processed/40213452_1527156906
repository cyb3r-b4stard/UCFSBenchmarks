import java.util.Scanner;
import java.io.*;
import java.lang.Math;

public class log
{
    public static void main()throws IOException
    {
        Scanner sc=new Scanner(System.in);
        System.out.println("Enter a number");
        int a=sc.nextInt();
        
        System.out.println("the lognumber is:"+Math.log(a);
        
        
            
        
        
        
        
       }
}