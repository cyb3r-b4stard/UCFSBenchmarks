public class ShoutBack
{
    private string Hey;

    /**
     * Constructor for objects of class ShoutBack
     */
    public ShoutBack()
    {
        Hey = "Hey!";
    }

    /**
     * An example of a method - replace this comment with your own
     * 
     * @param  y   a sample parameter for a method
     * @return     the sum of x and y 
     */
    public string Shout(string Hey)
    {
        if(this.Hey == Hey){
            return string this.Hey;
        }
       
    }
}