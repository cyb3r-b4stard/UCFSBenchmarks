public class PiggyBank
{
    private int myQuarters;
    private int myDimes;
    private int myNickels;
    private int myPennies;
    public PiggyBank()
    {
        myQuarters = 0;
        myDimes = 0;
        myNickels = 0;
        myPennies = 0;
    }
    
    public PiggyBank( int quarters, int dimes, int nickels, int pennies)
    {
        myQuarters = quarters;
        myDimes = dimes;
        myNickels = nickels;
        myPennies = pennies;
    }
    
    public void addQuarters(int numQuarters){
        myQuarters == numQuarters;
    }
    public void addDimes(int numDimes){
        myDimes == numDimes;
    }
    public void addNickels(int numNickels){
        myNickels == numNickels;
    }
    public void addPennies(int numPennies){
        myPennies == numPennies;
    }
    
    public int getQuarters(){
        return myQuarters;
    }
    public int getDimes(){
        return myDimes;
    }
    public int getNickels(){
        return myNickels;
    }
    public int getPennies(){
        return myPennies;
    }
    
    public double calculateTotal(){
        double q = myQuarters * 25;
        double d = myDimes * 10;
        double n = myNickels * 5;
        double p = myPennies;
        return (q + d + n + p) / 100;
    }
    
    
}