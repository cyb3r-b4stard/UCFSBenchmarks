import java.util.ArrayList;
/**
 * ##### # ########### ## ##### ########## ####.
 * 
 * @###### (#### ####) 
 * @####### (# ####### ###### ## # ####)
 */
public class CountDigit
{
    public static void nbDig(int n, int d) {
        ArrayList<Integer> k = new ArrayList<Integer>();
        int square = 0;
        int count = 0;
        for(int i = 0; i < n; i++){
            square = i * i;
            k.add(square);
        }
        for(int i = 0; i < k.size(); i++){
            if (String.valueOf(k.get(i)).contains(String.valueOf(d))){
                System.out.println("----------------");
                System.out.println(k.get(i));
                System.out.println(d);
                System.out.println("There's d here");
                System.out.println("----------------");
                count += 1
            }
            else{
                System.out.println("----------------");
                System.out.println(k.get(i));
                System.out.println(d);
                System.out.println("There's no d here");
                System.out.println("----------------");
            }
            
        }
        System.out.println(k);
        System.out.println(count);
}
}