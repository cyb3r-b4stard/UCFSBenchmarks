
/**
 * #### ## #### ####### #### #### #####.
 * 
 * 
 */
public class MathDemo
{
    public static void main (String[] args)
    {
        int tomCash = 100;
        int billCash = 200;
        int nancyCash = 150;
        int tracyCash = 175;
        int tonyCash = 160;
        
        int maleCashlarger = Math.max(tomCash, billCash , tonyCash);
        int maleCashsmaller = Math.min(tomCash, billCash , tonyCash);
        int femaleCashlarger = Math.max(nancyCash, tracyCash);
        int femaleCashsmaller = Math.min(nancyCash, tracyCash);
        
        System.out.println("Amount of Cash Tom has = " + tomCash);
        System.out.println("Amount of Cash Bill has = " + billCash);
        System.out.println(" Amount of Cash Nancy has = " + nancyCash);
        System.out.println("Amont of Cash Tracy has = " + tracyCash);
        System.out.println("Amount of Cash Tony has = " + tonyCash);
        System.out.println("The larger of the cash for the men (" + tomCash + ", " + billCash + ", " tonyCash + ") = " + maleCashlarger);
        System.out.println("The smaller of the cash for the men (" + tomCash + ", " + billCash + ", " tonyCash + ") = " + maleCashsmaller);
        System.out.println(" The larger of the cash for the women (" + nancyCash + ", " + tracyCash + ") = " + femaleCashlarger);
        System.out.println(" The smaller of the cash for the women (" + nancyCash + ", " + tracyCash + ") = " + femaleCashsmaller);
    }
}
         
        