public class Car
{
    private String car;
    private double milesPerGallon;
    private int tankLevel;
    private double addGas;
    private double drive;
    
    public Car(double milesPerGallon)
    {
        this.milesPerGallon = milesPerGallon;
    }
    
    public Car()
    {
        tankLevel = 0;
    }
    
    public void addGas(double amount)
    {
        this.addGas = addGas;
    }
    
    public void drive(double amount)
    {
        this.drive = drive;
    }
    
    public void getGasInTank(double tankLevel)
    {
        return double tankLevel;
    }
}