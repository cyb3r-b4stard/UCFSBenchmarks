
/**
 * ########### ### #### ### ###### ##########.
 * 
 * @###### (### ####) 
 * @####### (#### ############## #### ### #####)
 */
public class freierfall
{
    private Körper K;
    private double g;

    /**
     * Konstruktor für Objekte der Klasse freierfall
     */
    public freierfall(Körper derfallendeKörper)
    {
        K = derfallendeKörper;
    }
    
    
    public int Erdanziehung(double g)
    {
        F=g*m
    }

    public double gibWert(double m)
    {
       return g*m;
    }

    /**
     * ein Körper fällt im freien Fall 
     * EA -9,81
     */
    public void freierfall()
    {
        Körper K = new Körper(1,10,0);
        Erdanziehung F= new Erdanziehung (-9.81);
        System.out.println(K.gibOrt());
        for(int i=0;i<100;i++){
            K.schritt(0.1,F.gibWert (K.gibMasse()));
            System.out.println(K.gibOrt());
        }
    }

}