public class Car
{
    String type;
    int year;
    double price;
    double mpg;
    public Car(String ty , int ye , double pri , double mp)
    {
        type = ty;
        year = ye;
        price = pri;
        mpg = mp;
    }
    boolean greatGasMileage()
    {
       if(mpg >= 36)
       {
           return true;
        }
        return false;
    }
    String getInfo()
    {
        return( year + " " + type + " ," + mpg + " ," + price)
    }
}