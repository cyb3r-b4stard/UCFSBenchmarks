public class Lesson14
{
    public static void main(String args[])
    {
        System.out.printn("Decimal Binary Octal Hex Character");
        int a =0;
        for(a = 65; a<= 90; a++)
        {
            char ch = (char)a;
            System.out.println(a+" "+Integar.toBinary(a)+" "+Integar.toOctalString(a)+" "+Integar.toHexString(a)+"  "+ch+" ");

    
    
  
    }

}
