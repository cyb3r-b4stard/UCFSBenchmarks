import java.util.*

public class Kandidaat
{
    private String gsmNummer;
    private char[] antwoorden;
    private int geboortejaar;
    private char geslacht;
    private double schiftingsvraag;
    
    public Kandidaat(String gsmNummer, int geboortejaar, char geslacht)
    {
        this.gsmNummer = gsmNummer;
        this.geboortejaar = geboortejaar;
        this.geslacht = geslacht;
        schiftingsvraag = -1;
        antwoorden = new char[10];
    
    }

    public String getGsmNummer()
    {
        return gsmNummer;
    }
    
    public void setGsmNummer(String gsmNummer)
    {
        this.gsmNummer= gsmNummer;
    }
    
