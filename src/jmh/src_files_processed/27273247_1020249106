{
    public static void main()
    {
        int a,b,c;
        a=20;
        b=20;
        c=a+b;
        System.out.println("The addition of 2 number is: " +c);
        c=a-b;
        System.out.println("The subtraction of 2 number is: " +c);
        c=a*b;
        System.out.println("The multiplication of 2 number is: " +c);
        c=a/b;
        System.out.println("The division of 2 number is: " +c);
        c=a%b;
        System.out.println("The modulus of 2 number is: " +c);
    } 
}   