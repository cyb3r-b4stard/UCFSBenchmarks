
import java.util.*;  

public class HeartRatez
{
    private String Fnamee;
    private String Lnamee;
    private int Bmonthh;
    private int Bdayy;
    private int Byearr;
    
    public Heartratez()
    {
        Fnamee="";
        Lnamee="";
        Bmonthh=0;
        Bdayy=0;
        Byearr=0;
    }
    public void setFname(String fname)
    {
        Fnamee = fname;
    }
    
    public void setLname(String lname)
    {
        Lnamee = lname;
    }
    
    public void setBmonth (int bmonth)
    {
     Bmonthh = bmonth;
    }
    
    public void setBday(int bday)
    {
        Bdayy = bday;
    }
    
    public void setByear(int byear)
    {
        Byearr = byear;
    }
    
    public String getFname()
    {
        return Fname();
    }
    
    public String getLname()
    {
        return Lname();
    }
    

    public  int getBmonth ()
    {
        return Bmonthh;
    }
    
    public int getBday ()
    {
        return Bdayy;
    }
    
    public int getByear()
    {
        return Byearr;
    }
    
    public String toString()
    {
        String str;
        str= 
                "Full Name: " + fname + " " + lname + "\n" +
                "Birthday is: " + bmonth + "/" + bday + "/" + byear + "\n" +
                "It is now: " + currentMonth + currentDay + currentYear + Time + "\n" +
                "You are " + age + " years old \n" +
                "Max Heartrate is: " + mheartrate + "\n"
                "Your target Heartrate is: " + theartrate;
        return str;
    }
    
    }
