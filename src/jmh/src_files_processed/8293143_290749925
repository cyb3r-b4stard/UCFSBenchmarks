public class Dibujar
{
     private int _medida;
    private String _simbolo;
    private String _espacio;

    public Dibujar()
    {
        
    }
    
    public String drawLine(String _simbolo,int _medida){
        
        for (int i=0; i>_medida; i++){ System.out.println("\n"+_simbolo;
   
        }
    
    } 

}