import java.util.*;
public class friends
{
    public static void main(String args[])
    {
        String n[] = new String [10] ;
        Scanner sc = new  Scanner(System.in);
        for( i =0;i<10;i++;)
        {
            System.out.println("enter the name of ur friends");
            n[i] = sc.nextLine();
        }
        System.out.prinln("enter the name to be searched");
        String c = sc.nextLine();
        for(i=0;i<10;i++)
        {
            if( c.equalsIgnorecase(n[i])
            k = 1;
            pos = i;
            break;
        }
        if (k == 1)
        System.out.println( " search succesful " + " position of index " + pos );
        else 
        System.out.println("search unsuccessful");
    }
}

