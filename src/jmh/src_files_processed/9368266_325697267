public class Tester
{
    public static void main(String args[])
    {
        double d1 = (3*Math.PI)*Math.sin(187) + Math.abs(Math.cos(122));
        double d2 = Math.pow(14.72, 3.801) + Math.log(72);
        double toRad1 = Math.toRad(int)(187);
        double toRad2 = Math.toRad(int)(122);
        System.out.println("d1 = " + d1);
        System.out.println("d2 = " + d2);
    }
}