class IntegerDivision
{
  public static void main ( String[] args )
  {
      long April = 12;
      long May = 14;
      long June = 8;
      long Sum= April +May +June;
    System.out.println("The April Rainfall is: " + April );
    System.out.println("The May Rainfall is: " + (May) );
    System.out.println("The June Rainfall is: " + (June) );
     System.out.println("The Average Rainfall is: " + (Sum/3 );
 }
}