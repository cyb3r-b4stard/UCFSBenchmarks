class Pattern1
{
    public static void main()
    {
        int i,j;
        String a="*",b="#";
        
        for(i=1;i<=5;i++)
        {
            for (j=1; j<i;j+)
            System.out.print(a);
            if(j%2==0)
            System.out.println(b);
        }
            System.out.println;
            
        }
    }
}