 public static void readLineFromFile(String filePath)
 {
    try {
        BufferedReader br = new BufferedReader(new InputStreamReader(new FileInputStream(filePath), "Cp1252"));         

        String line;
        while ((line = br.readLine()) != null) {
        }
        br.close();

    } catch (IOException e) {
        e.printStackTrace();
    }     
 }

    
