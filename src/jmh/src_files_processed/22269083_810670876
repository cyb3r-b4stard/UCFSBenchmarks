public class bankrekening
{
    private String naam;
    private int nummer;
    private double saldo;
    
    public Bankrekening ( String naam,
                          int nummer,
                          double saldo )
    {
        this.naam = naam;
        this.nummer = nummer;
        this.saldo;
    }
}