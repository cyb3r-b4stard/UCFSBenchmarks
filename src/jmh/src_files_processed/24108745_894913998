        ShipIDs ShipID = new String;
        
       double userSpeed, userHealth;
       int shipID;
       int userShipID;
       int weaponID;
       int userWeaponID;
      
      double armor;
      double health;
      double shield;
      double speed;
       
      Random generator = new Random();
      shipID = generator.nextInt(10);
      weaponID = generator.nextInt(20);
     
      
      switch(shipID) {
          case 0: shipID = 1;
          break;
          case 1: shipID = 2;
          break;
          case 2: shipID = 3;
          break;
          case 3: shipID = 4;
          break;
          case 4: shipID = 5;
          break;
          case 5: shipID = 6;
          break;
          case 6: shipID = 7;
          break;
          case 7: shipID = 8;
          break;
          case 8: shipID = 9;
          break;
          case 9: shipID = 10;
          break;
          default: shipID = 0;
          
        }
        if(shipID == 1){
                 System.out.println("Enemy Hawk spotted!");}
        if(shipID == 2){
                 System.out.println("Enemy Vulture spotted!");}
        if(shipID == 3){
                 System.out.println("Enemy Eagle spotted!");}
        if(shipID == 4){
                 System.out.println("Enemy Seagull spotted!");}
        if(shipID == 5){
                System.out.println("Enemy Owl spotted!");}
        if(shipID == 6){
                System.out.println("Enemy Sparrow spotted!");}
        if(shipID == 7){
         System.out.println("Enemy Raven spotted!");}
