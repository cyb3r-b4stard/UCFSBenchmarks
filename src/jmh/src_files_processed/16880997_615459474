public class Vuelo
{
  private String Linea;
  private int Hora;
  private String Despege;
  private String Aterrizaje;
  private int Precio;
  private String Aciento;
  private String Clase;
  private String Escala;
 
public Reloj ()
  {
   Linea = lan
   Hora = 1630
   Despege = santiago, chile
   Aterrizaje = madrid, espa√±a
   Precio = 49990
   Aciento = l43
   Clase = economica
   Escala = lima, peru
}
public Reloj (String Linea, int Hora, string Despege, string Aterrizaje, int Precio, string Aciento, string Clase)
{ this.Linea = Linea
  this.Hora = Hora
  this.Despege = Despege
  this.Aterrizaje = Aterrizaje
  this.Precio = Precio
  this.Aciento = Aciento
  this.Clase = Clase
  this.Escala = Escala
}
