import java.util.Arrays;
import java.util.Scanner;
public class Ikasleak
{
    public static void main (String []args)
    {
      Scanner sarrera = new Scanner(System.in);
      
      int[] ikasleak = new int[10];
      int bata=0;
      int max=0;
      int min=0;
      int indizea = 0;
      int k;
      
      for(k=0; k < ikasleak.length ; k++)
      {
          System.out.printf("Sartu %d ikasleren nota: ",k+1);
          ikasleak[k] = sarrera.nextInt();
      }
      
      
       for (indizea = 0 ; indizea < ikasleak.length ; indizea++)
       {
            bata=bata+ikasleak[indizea];
            if(ikasleak[indizea]>ikasleak[indizea+1])
            {    
                max=ikasleak[indizea];
            }
            else{
                max=ikasleak[indizea+1];
            }
            min=;
       }
        System.out.printf("Batazbestekoa: %d",bata/indizea);
    }
}