import java.util.Scanner;
    class nested_7
{public static void main()
{int i,n,d,f=1,og,sum=0;
    Scanner sc=new Scanner (System.in);
    System.out.print("enter number of iterations");
    n=sc.nextInt();
    og=n;
    while(n>0)
    { d=n%10;
        f=1;
        for(i=1;i<=d;i++)
    {f=f*i;}
    sum=sum+f;
      n=n/10;}
      if (sum==og)
      System.out.println("krishnamurthy number");
    }}} 