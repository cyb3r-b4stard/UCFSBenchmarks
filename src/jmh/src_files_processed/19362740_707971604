public class Habitacion {

   private Paciente paciente;
   private int numero;
   private char tipoHab;
   private String fechaIngreso;
   private int CantDias;

    public Habitacion() {
    }

    public Habitacion(Paciente paciente, int numero, char tipoHab, String fechaIngreso, int CantDias) {
        this.paciente = paciente;
        this.numero = numero;
        this.tipoHab = tipoHab;
        this.fechaIngreso = fechaIngreso;
        this.CantDias = CantDias;
    }

    public void setPaciente(Paciente paciente) {
        this.paciente = paciente;
    }

    public void setNumero(int numero) {
        this.numero = numero;
    }

    public void setTipoHab(char tipoHab) {
        this.tipoHab = tipoHab;
    }

    public void setFechaIngreso(String fechaIngreso) {
        this.fechaIngreso = fechaIngreso;
    }

    public void setCantDias(int CantDias) {
        this.CantDias = CantDias;
    }

    public Paciente getPaciente() {
        return paciente;
    }

    public int getNumero() {
        return numero;
    }

    public char getTipoHab() {
        return tipoHab;
    }

    public String getFechaIngreso() {
        return fechaIngreso;
    }

    public int getCantDias() {
        return CantDias;
    }
   public void imprimirFichaPaciente()
   {
       System.out.println("__________________________________");
       System.out,println("       CLINICA SANITA             ");
       System.out,println("__________________________________");
       System.out,println(" "+getNumero()+" "+paciengetRut());
   
    
}