import java.io
/**
 * ########### ### #### ### ###### ####.
 * 
 * @###### (### ####) 
 * @####### (#### ############## #### ### #####)
 */
public class Blob
{
    private String name;

    private String vorname;
    
    public Blob()
    {
        name = vorname;
    }

    public String name(String vorname)
    {
        if (vorname.String.endsWith("obin"))
        {
            return true;
        }
        else
        {
            return false;
        }
    }
}