
/**
 * ##### # ########### ## ##### ######## ####.
 * 
 * @###### (#### ####) 
 * @####### (# ####### ###### ## # ####)
 */
public class SwordMan extends Soldier
{

    /**
     * Constructor for objects of class SwordMan
     */
    public SwordMan()
    {
        health = super().setMaxHealth(15);
        damage = super().setAttackDamage(5);
        maxCompass = super().setMaxCompassDistance(1);
        maxDiagonal = super().setMaxCompassDistance(0);
        
    }

    
}