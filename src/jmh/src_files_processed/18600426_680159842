
/**
 * ####### # ######### ## ###### ##### ####.
 * 
 * @###### (### ####) 
 * @####### (###### ## ###### ## ####)
 */
public class Livro
{
    private double valorUnit;
    private String titulo;
    private String autor;

    /**
     * COnstrutor para objetos da classe Livro
     */
    public String getTitulo(){
    {
        return titulo; 
    } 

   public String getAutor()
    { 
      return autor
    }

    public double getValorUnit(){
        return valorUnit;
    }
}