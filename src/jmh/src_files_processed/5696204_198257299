 */
public class FensterTellerrad extends JFrame
{
    
    /**
     * Konstruktor f√ºr Objekte der Klasse FensterTellerrad
     */
    public FensterTellerrad()
    {
        setSize(300,500);
        SetTitle("Tellerrad 1");
       
    }
    public static void main(String[]args)
    {
        FensterTellerrad = new FensterTellerrad();
        fensterTellerrad.setDefault(CloseOperation(JFrame,EXIT_ON_CLOSE);
        fensterTellerrad.setVisble(true);

}