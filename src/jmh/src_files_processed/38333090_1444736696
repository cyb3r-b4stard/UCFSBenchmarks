public class random1
{
    public static int randInt(int min, int max) {

        Random rand = new Random();

        int randomNum = rand.nextInt((max - min) + 1) + min;

        return randomNum;
    }
    
    pubblic static void main()
    {
        System.out.println("test");
    }
}