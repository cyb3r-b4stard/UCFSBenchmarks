public class TwoOutputs
{
    public static void main( String[] args )
    {
       int ctr = 1;
       int evens = 0; 
       String nums = "";
       while( ctr <= 10 )
       {
           nums += ctr + " ";
           if( ctr % 2 == 0)
           {
              evens++;
       } 
       ctr++;
       } 


   
   
  
   System.out.println( "There are " evens + " even numbers in that number set" );
     }
} 


