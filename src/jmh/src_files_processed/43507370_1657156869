 */
public class Examen
{
    public static int x;

    /**
     * Constructor for objects of class Examen
     */
    public Examen()
    {

    }

    public static void boeh()
    {
        System.out.println("BOEH!");
    }

    public static String getBoeh()
    {
        return "BOEH!";
    }

    public static int verdubbel(int n)
    {
        return n*2;
    }

    public static void boehh()
    {
        System.out.println("boeh");
    }

    public static void isDrie(int x)
    {
        if(x==3)
        {
            System.out.println("x is drie *dap*");
        }
        else
        {
            System.out.println("x is" +x);
        }
    }
    public static int i;
    public static void isKleinerDanTien()
    {
        for(i=0;i<10;i++)
        {
            System.out.println("i is " +i);  
        }
    }
    public static int j;
    public static void isGroterDanNul(int x)
    {
        for(j=x;j>0;j--)
        {
            System.out.println("j is " +j);
        }
    }

    public static void discriminant(int a, int b, int c)
    {
        System.out.println( b*b-4*a*c);
    }

    public static void nulpunten(int a, int b, int c)
    {
        x=b*b-4*a*c;
        if(x >0)
        {
            System.out.println(((-b-Math.sqrt(x))/2*a)); and ((-b-Math.sqrt(x))/2*a);)
        }
    }
}