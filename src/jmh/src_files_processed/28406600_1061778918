import javax.swing.JOptionPane;
public class jinair
{
    public static void main (String[] args)
    {
        String skt = "";
        while(skt.equals("y"))
        {
            String five = JOptionPane.showInputDialog("Type in a five word sentence. Don't try anything lower or this program will not work lol");
            String first = "d";
            String attack = JOptionPane.showInputDialog("Type in \"y\" to restart program, type in anything else to quit.");
            skt = attack;
        }
    }
    for(int i = 0; i < five.length() + 1; i++)
    {
        if(! five.charAt(i).equals(" "))
        {
            System.out.print(five.charAt(i);
        }
        else
        {
            System.out.println(five.charAt(i));
        }
    }
}