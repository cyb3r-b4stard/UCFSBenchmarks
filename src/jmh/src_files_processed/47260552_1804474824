

/**
 * ##### # ########### ## ##### ###### ####.
 * 
 * @###### (#### ####) 
 * @####### (# ####### ###### ## # ####)
 */
public class Matrix
{
    int [][] matrix;
    int rijen;
    int kolommen;
   

public Matrix(int rijen, int kolommen){
   
   matrix = new int[rijen][kolommen];
   
 
}



public int getAantalRijen(){
    return matrix.length;
    

}

public int getAantalKolommen(){
    return matrix[].length

}
}