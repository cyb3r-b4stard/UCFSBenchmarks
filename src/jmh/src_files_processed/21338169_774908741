
public class Test
{
  public static void main (String args[])
  {
     
      
      
      int m=5, n=6;
      int k=0;
      k= factorial(int m);
      System.out.println("Factorial of"  + m +  " is " + k);
         
     public int factorial (int m);
      {
      int f=1;
      int i;
      for (i=1;i<=m; i++)
      {
          f=f*i;
        }
        return (f);
    }
        
      