
import java.util.Arrays;
import static java.lang.System.*; 

public class WordSort
{
	private String[] wordRay;

	public WordSort(String line)
	{
	   setList(line);
	}

	public void setList(String line)
	{
		wordRay = line.split(" ");
	}

	public void sort()
	{
	    wordRay[].sort();
	}

	public String toString( )
	{
		String output="";
		return output+"\n\n";
	}
}