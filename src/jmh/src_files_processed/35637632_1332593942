class lcm{
public static void main(String[] args){
int LCM(int a,int b){
	if(a%b==0)
		return a;
	if(b%a==0)
		return b;	
	int ab=a*b;
	while(a!=b){
		if(a>b){
			a=a-b;
		}
		 
		else{
		 b=b-a;
		}
	}
	return (ab)/a;
}
	int t,i,n,j;
	int lcm;
	t=sc.nextInt(t);
	for(i=0;i<t;i++){
		n=sc.nextInt();
		int fo[n];
		for(j=0;j<n;j++){
			fo[j]=sc.nextInt();
			if(j==0)
				lcm=fo[0];
		 lcm=LCM(lcm,fo[j]);	
		}
		
		
		System.out.println(lcm);
	}



}

}