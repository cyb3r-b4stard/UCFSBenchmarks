class studentreport
{
public static void main(int suman,int kalam,int sanskriti,int ankita)
{
{
int Suman=50;
int kalam=70;
int Sanskriti=40;
int Ankita=50;
int sum;
int sum; sum=0;
float avg; avg=0;
sum=(Suman+kalam+sanskriti+Ankita);
avg=sum/4;
System.out.println("Total no. of these students="+sum);
System.out.println("the average="+avg);
}
}    