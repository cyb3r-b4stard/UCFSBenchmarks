public class City{
    private String name; 
    private double north; 
    private double west; 
public Citycreate ( String name, double latitude, double longitude) { 
    this.name = name; 
    this.north = latitude; 
    this.west = longitude; 
    return this.name, this.north, this.west; 
}
public void display() { 
    System.out.print(this.name+"\t"+this.north+"\t"+this.west);
}}