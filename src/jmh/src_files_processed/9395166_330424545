import java.util.Random;

public class Die{
    private int eyes;
    private Random r;
        
    /**
     * constructor
     */
    public Die()
    {
        r= new Random();
    }
    
    /**
     * obtain a new number of eyes for this die
     */
    public void roll(){
        r.nextInt(6) +1;
    }
    
    /**
     * = the number of eyes of this die
     */
    public int getEyes(){
        return 0;
    }
}