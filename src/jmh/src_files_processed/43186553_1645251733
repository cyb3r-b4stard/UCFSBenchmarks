public class Car
{
    private double milesDriven;
    private double gasInTank;
    
    public void addGas(double amount)
    {
        gasInTank += amount;
    }
    
    public void drive(double distance)
    {
        milesDriven += distance;
    }
    public double getMilesDriven()
    public double getGasInTank()
    
}