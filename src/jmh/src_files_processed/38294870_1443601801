public class ISP
{
    private char pkg;
    private double hoursUsed;
    /**
     * 
     */
    public ISP(char pkg, double hoursUsed)
    {

    }

    public char getPkg()
    {
        return pkg;

    }

    public double getHoursUsed()
    {
        return hoursUsed;

    }

    public void setPkg(char newPkg)
    {
        pkg = newPkg;
    }

    public void setHoursUsed(double newHoursUsed)
    {
        hoursUsed = newHoursUsed;
    }

    /**
     * 
     */
    public double calculateCharges(char pkg, double hoursUsed)
    { double monthlyPay;
        double additionalHours;

        if(pkg == 'A')
        {
            monthlyPay = 9.95;
            hoursUsed = 10;
            additionalHours = 2.00;

        }
        else if (pkg == 'B')
        {
            monthlyPay = 14.95;
            hoursUsed = 20;
            additionalHours = 1.00;
        }
        else if (pkg == 'C')
        {
            monthlyPay = 19.95;
        }
        return 0;

    }

