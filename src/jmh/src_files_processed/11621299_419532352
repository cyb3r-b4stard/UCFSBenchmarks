

public class TestProject3
{
    public static void main(String[] args)
    {
        
        String[] list = new String["Springfield", "Hartford", "Canon"]; 
        int n = 3; 
        System.out.println("Unsorted list: \n" + list); 
        System.out.println(); 
        System.out.println("Sorted list: \n" + Sorting.bubbleSortRecursive(list, n)); 
        
