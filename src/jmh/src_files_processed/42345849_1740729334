            System.out.println ("Enter your grade.");
            grades = SC.nextDouble();
            percentage = grades/divisor;
            if (count == divisor){
                System.out.println("Your end percentage is " +percentage+" %");
            }
            if (percentage >=93 & percentage <=100 & count == divisor) {
                System.out.println ("Your final grade is A");

            }
            if (percentage >=90 & percentage <=92 & count == divisor) {
                System.out.println ("Your final grade is A-");

            }
