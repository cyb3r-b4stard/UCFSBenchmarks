import java.util.Scanner;

class Parking

 {
     
     public static void main (String[]args)
     
    { final int HOUR_2_CHARGE = 10;
      final int HOUR_3_CHARGE =  3;
      final int  HOUR_MIDNIGHT_C = 20;
      int enterMin, enterHour;
      int exitMin, exitHour;
      int parkingDurationMin, parkingDurationHour;
      System.out.println ( "Enter entrance time: ");
      Scanner scan = new Scanner (System.in);
      enterHour = scan.nextInt();
      enterMin = scan.nextInt();
     System.out.println ("Enter exit time: ");
     exitHour = scan.nextInt();
     exitMin = scan.nextInt();
     parkingDurationHour = ((exitHour - enterHour) - 1);
    
     if (enterMin <= exitMin)
          {    parkingDurationHour = parkingDurationHour + 1 ;
              parkingDurationMin = (exitMin - enterMin);}
            
              
         
    
     
     System.out.println(parkingDurationHour);, (parkingDurationMin;
      
      
      
      
          
     
    }
}
      
      