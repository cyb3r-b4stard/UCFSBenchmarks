
import java.util.*;

	public class PracticeProblems
	{
		public static void main(String[] args)
		{
			Scanner console = new Scanner(System.in);
			
			System.out.print("Enter a whole number without a decimal >>> ");
			int num1=console.nextInt();
			System.out.println("Ten more than that is "+(num1+10));
			console.nextLine();
			System.out.println();
			
			System.out.print("Enter your name >>> ");
			String name1=console.nextLine();
			System.out.println("Hello, "+name1+"!");
			System.out.println();
			
			System.out.print("Enter any number >>> ");
			double something=console.nextDouble();
			System.out.println("Your number squared is >>> "+(something*something));
			console.nextLine();
			System.out.println();
			
			
			System.out.print("Enter your name and age >>> ");
			String name2=console.next();
			int age=console.nextInt();
			System.out.println(name2+" is "+age+" years old");
			System.out.println();
			
			System.out.print("Enter any number to represent a radius of a circle >>> ");
			double radius=console.nextDouble();
			System.out.println("The area of your circle is >>> "+(3.14159*(radius*radius)));
			System.out.println();
			
			System.out.print("Enter the number of seconds of free fall >>> ");
			double time=console.nextDouble();
			console.nextLine();
			System.out.println("Distance traveled (in feet) >>> "+(32.174*time));
			System.out.println();
			
			System.out.print("Who is your favorite person >>> ");
			string name3=console.nextLine
			System.out.println(name3+name3+name3);
		}
	}
