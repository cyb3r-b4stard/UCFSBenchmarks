import javax.swing.*;
public class average {
    public static void main (Sting[]args) {
        float a,b,c,d,e,f,g,ave;
        String name="";
        
        name = Float.parsefloat(JOptionPane.ShowInputDialog("Enter Student Name"));
        a = Float.parsefloat(JOptionPane.ShowInputDialog("English Grade"));
        b =Float.parsefloat(JOptionPane.ShowInputDialog("Math Grade"));
        c =Float.parsefloat(JOptionPane.ShowInputDialog("Science Grade"));
        d =Float.parsefloat(JOptionPane.ShowInputDialog("Filipino Grade"));
        e =Float.parsefloat(JOptionPane.ShowInputDialog("Makabayan Grade"));
        f =Float.parsefloat(JOptionPane.ShowInputDialog("HELE Grade"));
        g =Float.parsefloat(JOptionPane.ShowInputDialog("Computer Grade"));
        ave = (a+b+c+d+e+f+g)/7;
        
        (if ave 75>=)
        System.out.println("Congratulations " + name + ", your grade is " + ave + ", you passed!");
        else
        System.out.println(name + " your grade is " + ave + ", you failed, please study harder"); 
        
        
    
        
    }
}
