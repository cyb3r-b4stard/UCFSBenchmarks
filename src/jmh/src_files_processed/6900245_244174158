public class test
{
    public static void main(String[] args){
        for(int n = 0; n < 10; n+=1)
        {
            int xgridnew = (int)((0.5 * n * 5) + 10);
            
            System.out.println(xgridnew);
            
            xgridnew = 10
        }
    }
}