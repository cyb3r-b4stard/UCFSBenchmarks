    public static void main(String[] args)
    {
        int x = 10, y =2 , p =0, q-0, e=0, f=0;
        int[] n = {12,23,34};
        p = add(x,y,n);
        System.out.println ("Value of P = " + p + "Value of n[y] = " + n[y]);
        p = subtract(x,y,n);
        System.out.println ("Value of P = " + p + "Value of n[y] = " + n[y]);
        p = multiply(x,y,n);
        System.out.println ("Value of P = " + p + "Value of n[y] = " + n[y]);
        p = divide(x,y,n);
        System.out.println ("Value of P = " + p + "Value of n[y] = " + n[y]);
    }
        static int add(int m, int b, int c[])
        {
            m = b+m;
            return m;
        }
        static int subtract(int m, int b, int c[])
        {
            m = b-m;
            return m;
        }
        static int multiply(int m, int b, int c[])
        {
            m = b*m;
            return m;
        }
        static int divide(int m, int b, int c[])
        {
            m = b/m;
            return m;
        }
    }
    