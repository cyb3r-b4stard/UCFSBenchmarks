import java.util.*;
public class StockSale
{
    public static void main(String[] args)
    {
        int seconds;
        int minutes;
        int hours;
        int days;
        seconds = 60;
        minutes = seconds*60;
        hours = minutes*24
        days = hours*365;
        System.out.println("There are" +days+ "seconds in a year");
    }
}