public class Array{
    public static void main(String[] args){
        int[] scrap = {3,4,5,6};
        for(int count=0; count>scrap.length; count++){
            if (scrap[count]%2=0){
                System.out.println(scrap[count]);
            }
            else{
                System.out.println(0);
            }
        }
        
    }
}