class neon
{
    public static void main(int n)
    
    { 
        int square=n*n;
        int sum=0;
        while(square!=0)
        {
            int a=square%10;
            sum=sum+a;
            square=square/10;
        }
        if(sum==n)
        {
            System.out.println(+n"is a neon number");
        }
        else
        {
            System.out.println(+n"it is not a neon number");
        }
    }
}