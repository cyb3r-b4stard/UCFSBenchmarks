import java.util.*;
public class PAIR
{
   public static int pair (int val)
   {
       int x;
       Scanner scan = new Scanner(System.in);
       System.out.println("Veuillez entrer un nombre entier");
       int i = scan.nextInt();
       for (x=2; x<i; x %2=0)
       {
           
           System.out.print( x );
        }
       
       
    }

}