public class Konto
{
    private int Kontonummer;
    private double Zinssatz;
    private double Kontostand;
    
    public Konto( int Kontonummer, double zins, double Kontostand)
    {
        Kontonummer = 0;
        Zinssatz = zins;
        Kontostand = 10;
    }
    
    public void set_Kontonummer(int kontonummer)
    {
      if ((Kontonummer>999999 && Kontonummer<1000000)) 
      { 
         System.out.println(Kontostand);
      
        }
       
    }
    
    public void berechne_Zinsen(double zins)
    { zins = Kontostand*Zinssatz/100;
    }
    
    public void set_Zinssatz(double zins)
    {
     Zinssatz= zins*100/Zinssatz-100; 
    }
    
    public void einzahlen(double Geld)
    {
     Kontostand =  Kontostand+ Geld;
    }
    public void auszahlen(double Geld)
    {
        Kontostand = Kontostand - Geld
    }
    
    public void berechne_Konto_neu(double zins, double kontostand)
    {
        Kontostand = Kontostand + zins;
    }
}
    