 */
public class NumberProcessor
{
    private InputReader reader;
    private int num1;
    private int num2;

    /**
     * Constructor for objects of class NumberProcessor
     */
    public NumberProcessor()
    {
        reader = new InputReader();
    }

    public void sum()
    {
        int num;
        int end;
        int total;
       
        num = 0;
        end = 5;
        total = 0;
        
        num = reader.GetInt("Type in a positive integer please.")
        
        while (end<5)
        {
            if (num<=0)
            {
                sum += num;
                end++;
            }
        }
        
    }
}