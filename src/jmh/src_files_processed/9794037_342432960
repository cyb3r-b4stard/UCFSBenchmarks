
public class Gpa{

    private String myName;
    private String myId;
    private int totalPoints;
    private int myNumGrades;
    private double myGpa;
    public void aName(String aName){
        aname = myName;
    }

    public void Id(String Id){
        Id = myId;
    }
    
    public void tPoints(int tPoints){
        tPoints = totalPoints;
    }
    
    public void grades(int grades){
        grades = myNumGrades;
    }
    
    public double gpa(double gpa){
        gpa = myGpa;
    }
    
    public String (String getName){
        return myName:
    }
       
