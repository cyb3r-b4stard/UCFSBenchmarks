

public class CarTester
{
    
    public static void main()
    {
        Car myMini=new Car(22);
       
        myMini.addGas(15); 
        myMini.drive(134); 
        myMini.addGas(11); 
        myMini.drive(250); 
        double GasLeft = myMini.getGasLeft();
        
        System.out.println("Gas left=" +myMini.getGasLeft());
        System.out.println(GasLeft)
        
    }
        
    
        
}