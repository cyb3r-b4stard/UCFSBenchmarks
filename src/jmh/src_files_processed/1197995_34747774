import java.util.Scanner;

/**
 * #### ##### #### ##### ##########
 * ## ##### ###### ### ########
 * ##### ##### ##########.
 */
public class WhilePractice
{
    /**
    * This is a method that uses a while statement, an int x
    * and prints 10 asterics horizontally.
    */
    public void printTen()
    {
        int x = 0;
        while (x < 10)
        {
            System.out.print("*");
            x++;
        }
    }
    public int printN(int y)
    {
        int y = 0;
        
        Scanner scanner;
        scanner = new Scanner(System.in);
   
        System.out.print("Enter an int 1 - 5: ");
        y = scanner.nextInt();
        while (y == 1)
        {
        System.out.print("/n" + "*");
        }
        while (y == 2)
        {
        System.out.print("/n" + "**");
        }
                while (y == 3)
        {
        System.out.print("/n" + "***");
        }
                while (y == 4)
        {
        System.out.print("/n" "****");
        }   
        while (y == 5)
        {
        System.out.print("/n" + "*****");
        }
    }
}
     