
public class Auto extends Gestione
{
    public Auto(String new_modello,String new_colore,int new_immatricolazione,int new_posti,int new_chilometri,int new_numeroporte)
    {
        super (String new_modello,String new_colore,int new_immatricolazione,int new_posti,int new_chilometri);
        numeroporte = new_numeroporte;
    }
  
}