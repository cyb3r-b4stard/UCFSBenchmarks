package uebungen.ue03;

import toolbox.*;

public class logikschaltung
{
    boolean a;
    boolean  b;
    boolean  c;
    boolean  d;
    boolean  k;
    boolean  m;
    boolean  o;
    boolean  y;
    k=(a&b);
    m=(c|d);
    n=(k&m);
    o=(n^m);
    y=(!o);
    a=Tastatur.readBoolean("sind die folgende werte Wahr oder Falsch",'w','f');
    b=Tastatur.readBoolean("sind die folgende werte Wahr oder Falsch",'w','f');
    c=Tastatur.readBoolean("sind die folgende werte Wahr oder Falsch",'w','f');
    d=Tastatur.readBoolean("sind die folgende werte Wahr oder Falsch",'w','f');
    
}