package Stockroom;
import javax.swing.JOptionPane; 
import java.util.*;

public class Sales
{
    public static void main(String[] args){
        Scanner reader = new Scanner(System.in);
        StockItem prod2 = new StockItem("eggs", 175,10);
        System.out.println("How many eggs do you want?");
        int eggs = reader.nextInt();
        System.out.println("You have asked to buy " + eggs + " eggs");
        System.out.println("The new stock level is:" + prod2.getStockLevel());
    }  
       if (prod2.getStockLevel() - reader.nextInt()  <=0);
       System.out.println("There is insufficient stock");
       }  
        }
        
                              
    }


   


   