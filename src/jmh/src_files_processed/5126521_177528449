public class Student extends Person
{
    
    private static int studentId; 
    private int level;
    
    public Student(String fName, String lName, int gLevel)
 Person(String fName, String lName, int gLevel)
 {
        Person(fName,lName);
        
        if ((gLevel<0)||(gLevel>12))
            level = 0;
        
        else
            gLevel = level;
            
       
        
        return level;
        
    }
    
   public String toString()
   {
       return fullName+"\n  Grade Level: "+level+"\n   ID #: "+studentId;
       
   }
}
