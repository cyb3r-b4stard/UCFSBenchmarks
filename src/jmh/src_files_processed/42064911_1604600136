import java.util.Scanner;
public class CelToFah
{
    Scanner in = new Scanner(System.in);
    
    System.out.println("Convert Celcius to Fahrenheit: ");
    double cel = in.nextDouble();
    
    System.out.println(Conversion(cel));
    
    double Conversion(c) {
        return (c + 32.0) / (5.0 / 9);
    }
}