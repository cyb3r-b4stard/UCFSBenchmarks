public class bla
{
   public boolean decreasing(int[] arr)
   {
     for(int i = 0; i<arr.length-1;i++)
     {
       if(arr[i]>arr[i+1])
       {
         return false;  
        }

        }
     return true;   
    }
   public static void main()
   {
     int [] arr = (0,3,4,6,2,4);
     
    }
}