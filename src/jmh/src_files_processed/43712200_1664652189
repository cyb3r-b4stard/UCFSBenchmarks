public class Middle
{
    public static void main(String[]args)
    {
        new double[] doubleArgs = double[3];
       
        for (int i = 0; i<=2;i++)
        {
            doubleArgs[i] = Double.paseDouble(args[i]);
        }
        
        sort(doubleArgs);
    }
    
    public static double sort(double[] doubleArgs)
    {
        boolean sorted = false;
        while (!sorted)
        {
            sorted = true;
            for (int i = 0; i<=2;i++)
            {
                if (doubleArgs[i] > doubleArgs[i+1])
                {
                    int temp = doubleArgs[i+1];
                    doubleArgs[i+1] = doubleArgs[i];
                    doubleargs[i] = tmp;
                    sorted = false;
                }
            }
        }
        
        
        
        
    }

}