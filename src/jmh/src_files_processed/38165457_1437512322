public class Method
{
    public static void main(String[]args)
    {
       sayHello();
       sayHelloTo(Stu);
    }
    public static void sayHello()
    {
    System.out.println("Hello World");
    }
    public static void sayHelloTo(String name)
    {
        System.out.println("Hello, "+name)
    }
}