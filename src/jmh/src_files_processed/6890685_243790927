/**
 * # ##### #### ########## # ##### ## ### ##-#####.
 * 
 * @###### (##### #####) 
 * @####### (#-##-##)
 */
public class Point
{
    private double x;
    private double y;

    public Point (double x_par , double y_par)
    {
        x = x_par;
        y = y_par;
    }
    
    public String toString()
    {
        return "( " + x + " , " + y + " ) "; 
    }
    
    public double distance ( double point )
    {
        double dist;
        Math.sqrt(((x2 - x1)(x2 - x1)) + ((y2 - y1)(y2 - y1)));
        return dist;
    }
}