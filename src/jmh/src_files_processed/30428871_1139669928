public class Ordenamientos
{
public static void main(String[] args){
int aux; 
boolean cambios = false;

while(true){
    for(int i = 1; i < array.length; i++){i
        if (array[i] < array[i-1]{
            aux = array[i];
            array[i] = array[i-1];
            array[i-1] = aux;
        }
    }
}
}
}