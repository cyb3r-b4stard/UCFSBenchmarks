import jewl.*;
public class Notenordner
{
    Frame Meinfenster1, MeinFenster2;
    IntegerField English, Deutsch, Info, Mathe, Kunst, Paeda, Chemie, Geschichte, ProjektK, Reli, Sport;
    TextField English2, Deutsch2, Info2, Mathe2, Kunst2, Paeda2, Chemie2, Geschichte2, ProjektK2, Reli2, Sport2;
    Button OrdnenButton;
    public Notenordner()
    {
        Meinfenster1 = new Frame(700, 200, "Noten-Ordner", 'Q');
        Label label = new Label(Meinfenster1, 5, 10, -10, 15, "Geben Sie ihre Noten ein:");
        Label english = new Label(Meinfenster1, 5, 30, -10, 20, "English: ");
        Label info = new Label(Meinfenster1, 100, 30, -10, 20, "Informatik: ");
        Label deutsch = new Label(Meinfenster1) 
        English = new IntegerField(Meinfenster1, 50, 30, 20, 20);
        CheckBox besteN = new CheckBox(Meinfenster1, 200, 100, -20, 25, "Beste u. schlechteste Note anzeigen");
    }
}