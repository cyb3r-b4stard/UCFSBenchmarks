
import java.util.Scanner;
public class Arrays {
    public static void main (int[] arrayToDisplay)
    {
        Scanner keyboard = new Scanner(System.in);
       int[] number;
       number = new int[5];
       displayArray(number);
       
       for (int i = 0; i < arrayToDisplay.length; i++)
        {
            System.out.print("Please enter a number: ");
            number[i]= Integer.parseInt(keyboard.nextLine());
            System.out.print(arrayToDisplay[i] +  "");
        }
        displayArray(number);
        min = max;
        
        for (int i = 0; number.length; i++)
        {
            if (number[i] >max) max = number[i];
            if (number[i] < min) min= number[i];
        }
        
        System.out.println(" the largest number entered is : " + max);
        System.out.println(" the Smallest number entered is : " + min);
    }
    
    
    public static void displayArray(int[] arrayToDisplay)
    {
        for (int i = 0; i < arrayTDisplay.length;i++)

        {
            System.out.print(arrayToDisplay[i] + "");
            
        }
            
            
