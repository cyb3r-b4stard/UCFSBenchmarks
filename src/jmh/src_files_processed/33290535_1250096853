
/**
 * ##### # ########### ## ##### ##### ####.
 * 
 * @###### (#### ####) 
 * @####### (# ####### ###### ## # ####)
 */
public class Grafo
{
   private Arista [] grafo;
   private int num ;
   public Grafo(){
       this.num=1000;
       grafo=new Arista[num];
       for(int i=0;i<=num;i++){
           grafo[i]=(0,0,0);
        }
    }
    
       
}
   