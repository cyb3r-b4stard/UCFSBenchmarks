import java.io.*;
public class ordinamento
    { public static void main (String args [])
        { BufferedReader input = new BufferedReader (new InputStreamReader (System.in));
            int numeri [] = new int [6];
           try{
             for (int i=0; i <numeri.length ; i++) 
             {
               System.out.println ("Inserire elemento di posizione" +i+ "del vettore");
               String line = input.readLine();
               numeri [i] = Integerparseint (line);
              }
             for (int i=0; i<=numeri.length-1; i++)
             {
                for (int j=i+1; j<numeri.length; j++)
                {
                    if (numeri[i] > numeri[j]) 
                    int scambio = numeri[i];
                    numeri [i]= numeri [j];
                    numeri [j]= scambio;
                }
              }
              for (int i=0; i<numeri.length; i++)
                {
                  System.out.println("Elemento di posizione"+i+":"+NUMERI [i]);
                 }
                }catch (IOException ex)
            {   System.err.println ("si Ã¨ verificato un errore");
                ex.printStackTrace();
            }
          } 
        }
        
    
     

