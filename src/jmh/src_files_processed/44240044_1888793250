import java.util.ArrayList;
public class AnalogDigitalDriver
    {
     public static void main (String[] args)
     {
      BWTV bwtv = new BWTV(false);
      HDTV hdtv = new HDTV(false);
     
      ArrayList<TV> inventory = new ArrayList<TV>();
      inventory.add(bwtv);
      inventory.add(hdtv);
      boolean tvON;
      for(TV tv : inventory)
        {
         System.out.println(tv.tvType());
         System.out.println(tv.getistvOn());
            
        }
      for(int index = 0; index < inventory.size(); index++)
        {
         inventory.get(index).getistvOn;
            
        }
     }
}