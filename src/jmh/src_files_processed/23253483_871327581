import java.util.*;


public class Elemental
{
    private String aries;
    private String taurus;
    private String gemini;
    private String cancer;
    private String leo;
    private String virgo;
    private String libra;
    private String scorpio;
    private String sagittarius;
    private String capricorn; 
    private String aquarius;
    private String pisces;
    private String Zodiac;
    private String March;
    private String April;
    
    public void Zodiac()
    {
        aries = March;
        taurus = April;
        gemini = "May";
        cancer = "June";
        leo = "July";
        virgo = "August";
        libra = "September";
        scorpio = "Ocotober";
        sagittarius = "November";
        capricorn = "December";
        aquarius = "January";
        pisces = "February";
        March = "aries"
        April = "taurus"
        
        if (aries == March)
        {
            System.out.println("Your zodiac sign is the April");
        }
        else if (April == taurus){
            System.out.println("Your zodiac sign is the Taurus");
        }
        else
        {
            System.out.println("You are nothing, please retype the month of your birth");
        }
    }
    
    public void Print() 
    {
        Scanner keyboard = new Scanner(System.in);
        System.out.println("What month is your birthday?");
        String month = keyboard.nextLine();
        Zodiac ();
        System.out.println();
        keyboard.close();
    }
}