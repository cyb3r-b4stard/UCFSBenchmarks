import java.io.*;
class Doit
{
    public int arr[];
    public int size ;
    public int top;
    
    public Doit(int s)
    {
        size=s;
        arr=new int[size];
        top=-1;
    }
    
    public void push(int n)
    {
        if(top==size-1)
        {
            System.out.println("OVERFLOW");
        }
        else
        {
            arr[++top]=n;
        }
    }
    
    public int pop()
    {
        if(top==-1)
        {
            return 999;
        }
        else
        {
            return arr[top--];
            
        }
    }
    
    public void display()
    {
        int i=0;
        for(i=top;i>=0;i++)
        {
            System.out.println(arr[i]);
        }
    }
    
    public static void main(String args[])throws IOException
    {
        BufferedReader br=new BufferedReader(new InputStreamReader(System.in));
        System.out.println("ENTER THE SIZE OF THE ARRAY:");
        int n=Integer.parseInt(br.readLine());
        Doit ob=new Doit(n);
        int nn=0;
        do
        {
            System.out.println("ENTER 1 FOR INSERT" + "\n" + "ENTER 2 FOR DELETE" + "\n" + "ENTER 3 FOR DISPLAY" + "\n" + "ENTER 4 FOR EXIT");
            nn=Integer.parseInt(br.readLine());
            switch(nn)
            {
                case 1 : System.out.println("ENTER THE VALUE TO BE INSERTED:");
                         int ins=Integer.parseInt(br.readLine());
                         ob.push(ins);
                         break;
                case 2 : int ret=ob.pop();
                         if(ret==999)
                         {
                           System.out.println("STACK IS EMPTY");
                         }
                         else
                         {
                           System.out.println("DELETED NUMBER = " + ret);
                         }
                         break;
                case 3 : ob.display();
                         break;
                case 4 : break;
                default : System.out.println("WRONG CHOICE");
            }while(nn!=4);
        }}}
    
                                
                                
                                
                                
    