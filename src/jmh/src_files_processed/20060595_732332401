import java.util.ArrayList;
import java.util.Iterator;
import java.util.Calendar;

public class BOBCampagne
{
    public String campagneNaam;
    public ArrayList<AlcoholTest> testen;
    public BOBCampagne(String naam)
    {
        campagneNaam = naam;
        testen = new ArrayList<AlcoholTest>();
    }
    public int geefAantalTesten()
    {
        return testen.size()
    }
    public void addTest(Test test)
    {
        testen.add(test);
    }
}