public class Bombero
{
    String  Nombre;
    String Apellidos;
    int edad;
    boolean casado;
    especialista boolean;

    /**
     * Constructor for objects of class Bombero
     */
    public Bombero(String nombreBombero, String apellidosBombero, int edadBombero, boolean estadoCivil, boolean bomberoEspecialista )
    {
        Nombre=nombreBombero;
        Apellidos=apellidosBombero;
        edad=edadBombero;
        casado=estadoCivil;
        especialista=bomberoEspecialista;  
    }

    /**
     * An example of a method - replace this comment with your own
     * 
     * @param  y   a sample parameter for a method
     * @return     the sum of x and y 
     */
    public int sampleMethod(int y)
    {
        return x + y;
    }
}