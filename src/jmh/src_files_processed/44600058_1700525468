
public class Grünland
{
private String flurname; 
private double flaeche; 
private double guellefaesser; 
private double fassvolumen;
private double ngehalt; 
private double geschwindigkeit; 
private double anzahl; 
private double arbeitsbreite; 
private double streumenge; 

public Grünland ()
{
}

public Grünland (String fn, double f, double gf, double fv, double ng, double g, double a, double ab, double sm)
{
    flurname = fn;
    flaeche = f;
    guellefaesser = gf;
    fassvolumen = fv;
    ngehalt = ng;
    geschwindigkeit = g;
    anzahl = a;
    arbeitsbreite = ab;
    streumenge = sm;
   
    
}    

public void flurname_eingeben (String fn)
{
    flurname = fn;
}

public void flaeche_eingeben (double f)
{
    if (f < 0) {
        System.out.println ("Negative Fläche nicht möglich");
    } else {    
    flaeche = f;
}
} 

public void guellefaesser_eingeben (double gf)
{
    if (gf < 0) {
        System.out.println ("Negtive Fassanzahl nicht möglich");
    } else {     
    guellefaesser = gf;
}
}

public void fassvolumen_eingeben (double fv)
{
   if (fv < 0) {
       System.out.println ("Negativer Füllstand nicht möglich");
    } else {
    fassvolumen = fv;
}    
}

public void ngehalt_eingeben (double ng)
{
    if (ng < 0) {
        System.out.println ("Negativer N-Gehalt nicht möglich");
    
    } else if (ng >= 3.5 && ng < 3.8) {
        System.out.println ("Sie fahren Milchviehgülle");
    
    } else if  (ng == 3.8) {
        System.out.println ("Sie fahren Mastbullengülle");
        
    } else if (ng >= 3.3 && ng <= 3.9) {
        System.out.println ("Sie fahren Schweinegülle");
    } else {
        ngehalt = ng;
    }    
}

public void geschwindigkeit_eingeben (double g)
{
    if (g < 0) {
        System.out.println ("Negative Geschwindigkeit nicht möglich");
    } else {    
       geschwindigkeit = g;
}
}

public void anzahl_eingeben (double a)
{
    if (a < 0) {
        System.out.println ("Negative Anzahl nicht möglich");
    } else {    
    anzahl = a;    
}
}

public void arbeitsbreite_eingeben (double ab)
{
    if (ab < 0) {
        System.out.println ("Negative Arbeitsbreite nicht möglich");
    } else {
    arbeitsbreite = ab;
}
}
    
public void streumenge_eingeben (double sm)
{
    if (sm < 0) {
        System.out.println ("Negative Streumenge nicht möglich");
    } else {
    streumenge = sm;
}
}       

public String flurname_ausgeben ()
{
    return flurname;
}

public double flaeche_ausgeben ()
{
    return flaeche;
}

public double guellefaesser_ausgeben ()
{
    return guellefaesser;
}    

public double ngehalt_ausgeben ()
{
    return ngehalt;
}


public double anzahl_ausgeben ()
{
    return anzahl;
}    

    


public double ausbringmenge ()
{
    return ((streumenge * 600) / (arbeitsbreite * geschwindigkeit));
    
}
public doubel jahresngehalt ()
{
    return ( ausbringmenge 
}