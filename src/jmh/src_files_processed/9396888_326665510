import java.util.Scanner;

public class operator
{ public static void main(String[]args)
    {
        System.out.println ("Input first population:");
        Scanner scan= new Scanner (System.in);                                                               
        int pop1 = scan.nextInt();
        System.out.println("Input second population:");
        int pop2 = scan.nextInt();
        int pop = pop2-pop1/pop1*100;
        System.out.println("The PGR is=%:"((+pop2-pop1/pop1*100));             
   }
}