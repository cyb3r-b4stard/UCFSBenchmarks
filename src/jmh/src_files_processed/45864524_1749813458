public class SimpleLotto {
    public static void main(String[] args) {
        int [] lottoNums = new int [3];
        int index = 0;
        while ( index < lottoNums.length){
            lottoNums [index] = (int(Math.random()*50)+1;
            index++;
        }
        for(int number: lottoNums){
            System.out.print (number + " ");
        }
    }
}