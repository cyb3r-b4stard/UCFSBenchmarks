 */
public class JoliDessin extends Shape {

    public int xMin;
    public int yMin; 
    public int xMax;
    public int yMax;
    
    /**
     * Constructor for objects of class JoliDessin
     */
    public JoliDessin()
    { 
        
        xMin = 0;
        yMin = 0; 
        xMax = 500;
        yMax = 500;
    }
    
    public void keur_keur(Graphics g) {
    for ( int i = 500 ; i> 20 ; i = i-10 )
    {
        Rectangle test = new Ractangle (0,0, 500- i , 500-i);
        g.setColor( Color.PINK);
        test.draw(g);
        
    
    
    }
   }
    }