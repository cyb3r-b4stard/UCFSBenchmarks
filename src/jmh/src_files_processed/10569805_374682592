public class FizzBuzz
{
  public void fizz(int numberToCountTill)
     {int i = 0;
      
      while(i < nmberToCountTill) 
      {
       i+++;
       System.out.println(i);
      }
  }
  
  public static void main(String [] args)
  {
    fizz();
      
  }
 }
}