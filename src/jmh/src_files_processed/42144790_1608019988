 */
public class One_Hundred_Numbers
{
    public static void main(String[] args)
    {
     
   int [1][1001] A; 
   A= new int [10][10]
    for(row= 0; row <5; row ++) 
    { for (row= 0; col < 7; col ++)
     {system.out.printf( "%7d", A{row]{col] );
        }
    system.out.println();
}