 */
public class SnowfallDemo
{
    public static void main(String[]args)
    {
        double ws;
        double gso;
        double hp;
        double bur;
        double ral;
        double dur;
        double triadavg;
        Scanner keyboard = new Scanner(System.in);
        Snowfall snow = new Snowfall();
    
      System.out.println("Please enter snowfall for amount Winston Salem");
      ws=keyboard.nextInt();
      ws.setws(ws);
      
      System.out.println("Please enter snowfall amount for Greensboro");
      gso=keyboard.nextInt();
      gso.setgso.(gso);
      
      System.out.println("Please enter snowfall amount for High Point"):
      hp=keyboard.nextInt();
      hp.sethp.(hp);
      
      System.out.println("Please enter snowfall amount for Burlington");
      bur=keyboard.nextInt();
      bur.setbur.(bur);
      
      System.out.println("Please enter snowfall amount for Raleigh");
      ral=keyboard.nextInt();
      ral.setral.(ral);
      
      System.out.println("Please enter the snowfall amount for Durham");
      dur=keyboard.nextInt();
      dur.setdur.(dur);
      
      System.out.println("Winston Salem :    "+ws);
      System.out.println("Greensboro :    "+gso);
      System.out.println("High Point :    "+hp);
      System.out.println("Burlington :     "+bur);
      System.out.println("Raleigh :    "+ral);
      System.out.println("Durham :     "+dur);
      System.out.println("triadavg :   "+triadavg);
      
      
   

    
    }
}