

public class arr_1
{
   void main()
   {
       BufferedReader br=new BufferedReader(new InputStreamReader(System.in);
       int sal[];
       sal=new int[7];
       for( int i=1; i<=7; i++)
       {
           
         System.out.println("Enter next salary");
         int p=br.readLine(System.in);
         sal[i]=p;
        }
        for(i=1; i<=7; i++);
        {
            if(sal[i]>50000)
            {
                System.out.println("this person has a salary more than 50000"+ sal[i]);
            }
        }
    }
                
}