
/**
 * ##### # ########### ## ##### ############# ####.
 * 
 * @###### 
 * @##.##.####
 */
public class Versandkosten
{
   private double menge;
   private double preis;
   private double warenwert;
   private double nettobetrag;
   private double bruttobetrag;
   private double rabattsatz;
   private double versandkosten;
   private double umsatzsteuerbetrag;
   
   
   public Versandkosten (double pmenge, double ppreis){
    menge= pmenge;
    preis= ppreis;
    }
    
   public void setmenge ( double pmenge){
    menge= pmenge; 
    }
   public void setpreis ( double ppreis){
    preis = ppreis;
    }
    
   public double getwarenwert (){
    return warenwert;
    }
   public double getnettobetrag (){
    return nettobetrag;
    }
   public double getbruttobetrag (){
    return bruttobetrag
     }
}