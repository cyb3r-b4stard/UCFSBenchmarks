public class Card
{
    private String value; 
    private String suit;
    
    public Card(String x)
    {
        String v = x.substring(0,1);
        String s =  x.substring(1,2);
    }
    public String getSuit()
    {
        if(suit.equals("C"))
            return "Clubs";
        else if(suit.equals("D"))
            return "Diamonds";
        else if(suit.equals("H"))
            return "Hearts";
        else return "Spades";
    }
    public String getValue()
    {
        if(value.equals("A"))
            return "Ace";
        else if(value.equals("2"))
            return "Two";
        else if (value.equals("3"))
            return "Three";
        else if(value.equals("4"))
            return "Four";
        else if(value.equals("5"))
            return "Five";
        else if(value.equals("6"))
            return "Six";
        else if(value.equals("7"))
            return "Seven";
        else if(value.equals("8"))
            return "Eight";
        else if(value.equals("9"))
            return "Nine";
        else if(value.equals("T"))
            return "Ten";
        else if(value.equals("J"))
            return "Jack";
        else if(value.equals("Q"))
            return "Queen";
        else 
            return "King";
      }
    public String toString()
    {
        String retVal = value+" of "+suit;
        return retVal;
    }
    public int getRank()
    {
        if(getValue().equals("Two")
            return 2;
        if(getValue().equals("Three")
            return 3;

    }
}