public class TraversingStrings
{
    public static boolean isACap(String str)
    {
        String up= str.toUpperCase();
        return str.equals(up);

    }

    public static boolean isAllInts(String str)
    {
        int leg= str.length();
        String one=str.substring(0,leg);    
        if(one.compareTo("0")>=0 && one.compareTo("9")<=0)
        {
            return true;
        }
        else
        {
            return false;
        }
    }

    public static boolean containsACapital(String str)
    {
        int leg= str.length();
        String one=str.substring(0,leg);
        String up= one.toUpperCase();
       for(int i=one;i<leg;i++)
       {
        String sub=str.substring(0,1);  
        if(
                    return true;

        else
        {
            return false;
        } 

    }
}
}