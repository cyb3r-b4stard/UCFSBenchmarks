import java.io.FileWriter;
import java.io.IOException;
public class ThreadLogSync implements Runnable
{
    int jumlah, i, j;
    public ThreadLogSync(FileWriter log, String nama, int x)
    {
        logtThread = log;
        namaThread = nama;
        jumlah=x;
    }
    
    public void run() 
    {
try
         {
            
            synchronized(logThread) {
                for(i= k; i<=jumlah; i++){
                    for(j=2; j<i; j++){
                        if(i%j==0){
                            break;
                        }
                    }
                    if(i==j){
                        logThread.write(i+" ");
                        k=i;
                        break;
                    }
                }
                logThread.write("\n");
                
                
            }
        }
        
/**
 * Write a description of class fhxgfx here.
