public class Participant
{
    public String name;
    public String IC;
    public int number_phone;
    
    public Participant(){
        name="";
        IC="";
        number_phone=0;
    }
    public Participant(String name,String IC,int number_phone)
    {
        this.name=name;
        this.IC=IC;
        this.number_phone=number_phone;
        
    }
    
   public void setName(String name)
    {
       this.name=name;
    }
   public void setIC(String IC)
    { 
       this.IC=IC;
    }
    public void setNumber_phone(int number_phone)
    {
        this.number_phone=number_phone;
    }
    
    public String getName(){
        return name;
    }
    public String getIC(){
        return IC;
    }
    public int getNumber_phone(){
    return number_phone;
    }
    
    public String toString(){
        return "Name :" + getName() + "\n   No IC: " + getIC() + "\n   Phone Number: " + getNumber_phone() "\n");
    }
}