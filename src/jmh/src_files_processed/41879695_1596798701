import java.io.File;
import java.io.PrintWriter;
import java.io.FileNotFoundException;
import java.util.Random;
import java.util.Scanner;
import java.util.ArrayList;
public class Statistika {
    public static final int A = 2;
    public static final int B = 8;
    public static final int ROZSAH = 1000;
    public static void generujSubor(String subor) throws FileNotFoundException{
        int a = Statistika.A;
        int b = Statistika.B;
        File file = new File(subor);
        PrintWriter printWriter = new PrintWriter(file);
        int nahodneCislo;
        for(int i = 0;i< Statistika.ROZSAH;i++){
            nahodneCislo = (int)Math.round(Math.random()) * b;
            printWriter.println(a + (nahodneCislo*b));
        }
        printWriter.close();
    }
    public static void spracujSubor(String subor) throws FileNotFoundException{
        ArrayList<Integer> cisla = new ArrayList<Integer>();
        File file = new File(subor);
        Scanner skener = new Scanner(file);
        while(skener.hasNext()) {
            cisla.add(skener.nextInt());
        }
        int min = cisla.get(0);
        int max = cisla.get(0);
        int sucet = 0;
        int pocet = 0;
        for(int cislo:cisla){
            if(min>cislo){
                min = cislo;
            }
            if(max<cislo){
                max = cislo;
            }
            sucet += cislo;
            pocet++;
        }
        skener.close();
        System.out.println("Max " + max);
        System.out.println("Min " + min);
        System.out.println("Avg " + (sucet/(double)pocet));
    }
    public static void spracujVstup(){
        File file = new File(subor);
        Scanner sc = new Scanner(System.in);
        PrintWriter printWriter = new PrintWriter(file);
        System.out.println("");
        sc.nextInt();
        while(){
            printWriter.println();
        }
        printWriter.close();
    }
}