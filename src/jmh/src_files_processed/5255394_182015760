import java.awt.*;
import java.awt.event.*;
import javax.swing.JOptionPane; 
public class AdventureGame extends Frame
{
     public AdventureGame(){
        super ("Simple Button");
        setSize(100,100;
        Button b = Button("a button");
        add(b);
        setVisible(true);
        }     
    public static void main(String[] args){
        new AdventureGame();
     }

  
}