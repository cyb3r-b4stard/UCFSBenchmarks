public class MyResizableIntArrayList
{
  MyResisableIntArray[]List(int Array[], data[])
  {
      int Array[];
      int data[]=Array[];
    }
  public void set(int index, int elem)
  {
      int INDEX=index;
      int ELEMENTS=elem;
    }
   
       
   public void add(int elem)
   { 
      int ELEMENTS=elem;
      ELEMENTS=ELEMENTS+1;
      
    }
    public void add(int index,int elem)
    {
        int INDEX=index;
        INDEX=2*INDEX; 
        int ELEMENTS=elem;
        ELEMENTS=ELEMENTS+1;
    }
   public void clear()
   {
       int ELEMENTS;
       ELEMENTS=0;
    }
  public void remove(int index)
  {
      int INDEX=index;
      INDEX=INDEX/2;
    }
}
 

     