import java.util.Scanner;
public class integer {
    public static void main(String [] args) {
        double first;
        double exponent;
        Scanner scan = new Scanner(System.in);
        System.out.println("What number would you like to begin with?");
        first=scan.nextDouble();
        System.out.println("And by what number would you like to increase exponentially by?");
        exponent=scan.nextDouble();
        while (first<=100000000) {
            first=first^exponent;
            System.out.println(first);}
        if (first>100000000) {
            System.out.println("It has been computed up to 100000000"); }
            