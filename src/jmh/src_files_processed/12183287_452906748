import java.util.*;
public class Parking
{
    public static void Parking (String args[])
    {
                        int final allNight = 20;
                        int final forHouer = 10;
         Scanner park = new Scanner (System.in); 
                System.out.println("Enter entrance time:");
                int enterParkH;
                enterParkH = park.nextInt();
                int enterParkM;
                enterParkM = park.nextInt();
                        System.out.println("Enter exit time:");
                        int outParkH;
                        outParkH = park.nextInt();
                        int outParkM;
                        outParkM = park.nextInt();
    
        
        if ((enterParkH/23)+(outParkH/23)+(enterParkM/59)+(outParkM/59)!=0)
        System.out.println("Error Ileegal Data");
        else
        if(outParkH*59+outParkM<enterParkH*59+enterParkM)
        System.out.println("Exit time must precede enter time");
        else
        System.out.println ("Total charge is "+enterParkM+" NIS");
    }
}
        
                        
                        
                        
                        
                        
                