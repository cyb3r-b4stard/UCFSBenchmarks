
/**
 * ##### # ########### ## ##### # ####.
 * 
 * @###### (#### ####) 
 * @####### (# ####### ###### ## # ####)
 */
public class t
{
    public static void main (String args [])
    {
        int list[5];
        int max = list.length-1;
        for (int k = 0; k < max; k++ )
            if (list[k] < list[k+1] )
            {
                int temp = list[k];
                list[k] = list[k+1];
                list[k+1] = temp;
            }
    }
}