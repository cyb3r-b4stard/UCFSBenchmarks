
import javax.swing.JOptionPane;
public class StudentExam 
{
    public static void main(String[] args)
    { 
        int n = Integer.parseInt(JOptionPane.showInputDialog("Enter number of students: "));
        Student s[] = new Student[n]; 
        
        int j;
        for (j = 0; j < s.length; j++)
        {
            String type = JOptionPane.showInputDialog("Enter type:");
            String name = JOptionPane.showInputDialog("Enter name");
            float test1 = Float.parseFloat(JOptionPane.showInputDialog("Enter test 1"));
            float test2 = Float.parseFloat(JOptionPane.showInputDialog("Enter test 2"));
            char sType = type.toUpperCase().charAt(0);
            
            if (sType == 'U')
            { 
                String pp = JOptionPane.showInputDialog("Enter Practical Placement");
                s[j] = new Undergraduate(type, name, test1, test2, pp);
            }
            else if (sType == 'G')
            { 
                String pf = JOptionPane.showInputDialog("Enter Project Field");
                s[j] = new Graduate(type, name, test1, test2, pf);
            }

        }
        
        String output = "";
        for (int k=0; k < s.length; k++)
        { 
            if (s[k] instanceof Undergraduate)
            output += s[k].toString() + "\n";
        }
        
        Graduate gd;
        for (int k=0; k < s.length; k++)
        { 
            if (s[k] instanceof Graduate)
            {
                gd=(Graduate)s[k];
                if (gd.getProject().equalsIgnoreCase("Networking");
                output += s[k].toString() + "\n";
            }
        }

        Undergraduate ug;
        for (int k=0; k < s.length; k++)
        { 
            if (s[k] instanceof Undergraduate)
            { 
                ug=(Undergraduate)s[k];
                if (ug.getPracticalPlace().equalsIgnoreCase("Kuala Lumpur")
                output += s[k].toString() + "\n";
            }
        }
    }
}