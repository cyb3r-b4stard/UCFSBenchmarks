
/**
 * #####-#
 * ######## ########
 * #/#/####
 */
public class Ski
{
    private String name;
    private Ski s1;
    private Ski s2;
    private int addCustReview;
    private int totalRating;
    private int timesReviewed;
    
    public Ski(String inName, Ski in1, Ski in2)
    {
        name = inName;
        s1 = in1;
        s2 = in2;
    }
    
    public String getName()
    {
        return name;
        
     }
    
     
    public void addCustReview(int InReviewVal)
    {
        timesReviewed++;
        totalRating = totalRating + InReviewVal;
    }
    
    