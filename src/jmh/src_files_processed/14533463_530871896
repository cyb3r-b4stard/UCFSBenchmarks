
public class stringNode
{
   private StringNode next;
   private String info;
   
   StringNode (String info){
       this.info= info;
    }
    
   StringNode (String info,StringNode next ){
       this.info= info;
       this.StringNode= next;
    }
     
    StringNode getnext(){
       return next;
    }
    
    String getinfo (){
       return info;
    }
    
    void StringNode setnext(StringNode next){
          this.StringNode= next;
        }
        
    void String setinfo (String info){     
       this.info= info;
    }
    
    String toString (){
        return " " + this.info ;
    }
    
    
    
   
   
   
}