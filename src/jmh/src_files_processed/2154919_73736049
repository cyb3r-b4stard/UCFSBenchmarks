
/**
 * ##### # ########### ## ##### #### ####.
 * 
 * @###### (#### ####) 
 * @####### (# ####### ###### ## # ####)
 */
public class Game extends Item
{
    private String platform;

    /**
     * Constructor for objects of class Game
     */
    public Game(int pKey, String pName, String pPlatform, double pPrice)
    {
        super(pKey, pName, pPrice)
        platform = pPlatform
    }

    public String getPlatform()
    {
        return platform;
    }
}