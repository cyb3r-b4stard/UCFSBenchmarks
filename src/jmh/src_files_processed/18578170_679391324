import java.util.ArrayList;
/**
 * ########### ### #### ### ###### ################.
 * 
 * @###### (### ####) 
 * @####### (#### ############## #### ### #####)
 */
public class EnergieVersorger
{
   private ArrayList<Verbraucher>kunden;
   
    public EnergieVersorger()
    {
        kunden=new Verbraucher();
        kunden=new ArrayList<Verbraucher>;
    }
    
    public void erfasseKunde(String Stadt,double Energie)
    {
        kunden.add(new Verbraucher);
    }
}