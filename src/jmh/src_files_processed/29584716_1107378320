public class Instruction
{
    public static void main(String args[])
    {
        String a = "The Dukes of Hazzard";
        String b = "hot fudge sundae";
        
        
        int j = a.indexOf(102, 15);
        System.out.println(j);
        
        int k = a.lastIndexOf('z');
        System.out.println(k);
        
        char c = b.charAt(10);
        System.out.println(c);
        
        String myString = a.replace("Dukes", "Nerds");
        System.out.println(myString);
        
        String fat = "       This is an obese string   /n  /t  ");
        String skinny = fat.trim();
        System.out.println(fat);
        System.out.println(skinny + "X");
    }
}