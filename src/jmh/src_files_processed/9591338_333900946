
/**
 * ######### ## #### ### "####" #####
 * 
 * @###### #####_###### 
 * @####### ##/##/####
 */
public class Book
{
    private String name;
    private int num;
    private double cost;
    
    public Book(String inName, int inNum, double inCost)
    {
        name = inName;
        num = inNum;
        cost = inCost;
    }
    
    public String getName ()
    {
        return name;
    }
    
    public int getNun ()
    {
        return num;
    }
    
    public double getcost ()
    {
        return cost;
    }
    
    public static int mult(int x, double y ,double inTotal)
    {
        num = x;
        cost = y;
        x * y = inTotal;
        return inTotal;
    }
}