public class Homework1
{
    public static void mmain(String[] args)
    {
        for(i=1,i<=5,i++)
        {
            for(j=1,j<i,j++){
            System.out.print(" ");
        }
           for(j=1,j<(2*1)-1,j++){
           System.out.print("*");
        }
        System.out.println();
    }
}