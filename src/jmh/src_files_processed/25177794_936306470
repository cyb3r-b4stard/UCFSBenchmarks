import java.util.*;
public class Factorial
{
    public static void main(String [] args)
    {
        int num, factorial;
        Scanner in = new Scanner(System.in);
        System.out.println("Enter a non negative whole number ");
        num = in.nextInt();
        int fact (factorial);
    
    
    }
    public int factor ( int i, int num)
    {
      int factor, i;
      for (i = 1; i <= num; i++)
      {
          fact = fact * i;
        
        }
        return(factorial);
    
    }


}