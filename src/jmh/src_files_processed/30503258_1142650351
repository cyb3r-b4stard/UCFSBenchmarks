public class Exemplu1{
    public static void main(String [] args){
        Dog d1=new Dog();
        Dog d2=new Dog();
        Dog d3=new Dog();
        
        d1.i=d2.i=8;
        
        d1.says="Wow";
        d2.says="Wow";
        d3=d1;
        System.out.println(d1.1==d3.i);
        System.out.println("d1.says.equals(d2.says) "+d1.says.equals(d2.says));
    }
}