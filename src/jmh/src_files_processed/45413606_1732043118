import java.util.*;
/**
 * 
 * 
 * 
 */
public class DateCalc
{
    private int year;
    private int month;
    private int weekday;

    /**
     * Constructor for objects of class DateCalc
     */
    public GregorianCalendar(year)
    {
        GregorianCalendar today = new GregorianCalendar();
        
        today.add(Calendar.DAY_OF_MONTH, 100);
    }

    
}