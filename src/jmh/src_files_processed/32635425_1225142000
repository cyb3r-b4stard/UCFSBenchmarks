

public class Bubblesort
{
    public int [] feld= new int[8];
  
    public void Bubblesort()
    {
       int n= feld.length;
       while (n>1){
           for(int i=0;i<=n-2;i++){
               if(feld[i]> feld[i+1]){
                   vertausche(feld[i];feld[i+1]);}}
                   n=n-1;}
    }

  
}