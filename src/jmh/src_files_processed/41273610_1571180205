
import static java.lang.System.*;

public class VowelCounter
{
    public static String getNumberString( String s)
    {
        String vowels = "AEIOUaeiou";
        int count = 0;
        for(int start = 0; start <= s.length(); start ++)
        {
            if(vowels.indexOf(s.charAt( >= 0))
            {
                return count++;
            
            }
            
        }
        return "";
    }
}

