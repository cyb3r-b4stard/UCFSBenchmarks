public class Berechnung
{
    private double bmi;
    private double masse;
    private double groeße;
    
 public Berechnung()
 {
     bmi = masse/(groeße*groeße);
    }
    
 public double bmiBerechnen() 
 {
     if (bmi <= 19);
     System.out.println("Sie sind Untergewichtig");}
     
    else
    {
      if (19 < bmi && bmi <= 24);
      {Systhem.out.println("Sie sind Normalgewichtig");}
    }
      else 
       if (24 < bmi && bmi <= 30);
       {Systhem.put.println("Sie sind Übergewichtig");}
       
       else
        if (30 < bmi && bmi < = 40);
        { Systhem.out.println("Sie haben Adipossitas");}
        
        else
        if ( 40 < bmi);
        { Systhem.out.println("Sie haben schwere Adipositas");}
    }
}
    
        
