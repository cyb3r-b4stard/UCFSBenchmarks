 * 
 * @###### (#### ####) 
 * @####### (# ####### ###### ## # ####)
 */
public class Schueler
{
    public class Schueler
{
    private String name;
    private String vorname;
    private String geschlecht;
    private String schriftlichkeit;
    
    /**
     * Constructor for objects of class Schueler
     */
    public Schueler(String pName, String pVorname, String pGeschlecht, String pSchriftlichkeit)
    {
        this.name = pName;
        this.vorname = pVorname;
        this.geschlecht = pGeschlecht;
        this.schriftlichkeit = pSchriftlichkeit;
    }

    /**
     * An example of a method - replace this comment with your own
     * 
     * @param  y   a sample parameter for a method
     * @return     the sum of x and y 
     */
    public String getName()
    {
        return this.name;
    }
    
    public void setName (String pName)
    {
        this.name = pName;
    }
    
    public String getVorname()
    {
        return this.vorname;
    }
    
    public void setVorname(String pVorname)
    {
        this.vorname = pVorname;
    }
    
    public String getGender()
    {
        return this.geschlecht;
    }
    
    public void setGender(String pGeschlecht)
    {
        this.geschlecht = pGeschlecht;
    }
    
    public String getSchriftlichkeit()
    {
        return this.schriftlichkeit;
    }
    
    public void setSchriftlichkeit(String pSchriftlichkeit)
    {
        this.schriftlichkeit = pSchriftlichkeit;
    }
   
}