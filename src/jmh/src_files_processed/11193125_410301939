import java.util.Scanner;
public class CarV5 
{
    private int mySMiles;
    private int myEMiles;
    private String myCarType;
    private double myGallons;
    private double myPricePerGallon;
    public CarV5(int sMiles1, int eMiles1, String carType1, double gallons1)
    {
        mySMiles = sMiles1;
        myEMiles = eMiles1;
        myCarType = carType1;
        myGallons = gallons1;
        myPricePerGallon = totalCost() / gallons1;
    }
    
    public int calcDistance(int sMiles, int eMiles)
    {
        return myEMiles - mySMiles; 
    }
    public double calcGPM(int dist, int gallons1) 
    {
        return dist / gallons1;
    }
    public double totalCost()
    {
    }
    public double calcMPG(int dist, double gallons1)
    {
        return dist * gallons1;
    }
    public static void main(String[]args)
    {        
        CarV5 car1 = new CarV5(134556, 136789, "Toyota Highlander", 13.5); 
        int distance = (int)car1.calcDistance(,eMiles1);
        double mpg = car1.calcMPG(distance,gallons1);
        
        System.out.printf("Gas Mileage Calculations%n%n");
        System.out.printf(" Type of Car        Start Miles     End Miles     Distance     Gallons     Miles/Gals%n%n"); 
        System.out.printf("%12s      %5d        %5d     %8d        %5.2f       %5.2f", name1,sMiles,eMiles,distance,gals,mpg);
        
        
        
    }
}