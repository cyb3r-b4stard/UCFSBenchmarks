import java.util.Random;
public class apple
{
    public int total=0;
    public int i;
    public int larger,rangeSum;
    public boolean largest;
    public int f,l;
    public int number;
    
    Random gen = new Random();
    
    public void printAnswer(double a, double b, double c)
    {
        
    }
    
    public void powersOfTwo ()
    {
        for(i=1;i<=10;i++)
            System.out.println(Math.pow(2,i));
    }
    
    public void alarm(int times)
    {
        if (times<=1)
            System.out.print("Error");
        
        for(i=1;i<=times;i++)
            System.out.println("Alarm");
    }

    public int sum100()
    {
        for (i=1;i<=100;i++)
        total+=i;
        return total;
    }
    
    public int maxOfTwo (int a,int b)
    {
        if(a>b)
            larger=a;
            else
            larger=b;
            return larger;
        }
    
    public int sumRange (int a, int b)
    {
        if (a>b)
        {
            System.out.println("Error: 1st integer must be less than second");
            return 0;
        }
        else
            for(i=a;i<b;i++)
            rangeSum+=i;
        return rangeSum;
    }
    
    public boolean largest (double a, double b)
    {
        return (a>b);
    }
    
    public void hey ()
    {
        System.out.println("HEYYEYYYYEYYYYYEYAAHAHY");
    }
    
    public void setRange(int first, int last)
    {
        f=first;
        l=last;
    }
    
    public int assignNumber()
    {
        number=gen.nextInt*(f,l);
    }
}