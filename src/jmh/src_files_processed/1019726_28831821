public class printVolumes
{
   public static double currency (int knuts);
   int g = knuts/493;
   System.out.println ("G:" + g);
   knuts= knuts%493;
   int s = knuts/17
   System.out.println("S:" + s);
   knuts= s%17;
   int k = knuts/29;
   System.out.println("k:" + k);
}
  