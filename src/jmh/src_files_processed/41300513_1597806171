 * 
 * @###### ######### 
 * @##.##.##
 */
public class Cd {
   
    /**
     * premenne pre udaje o Cd
     */
    
    private static final int MAX_DOBA_POZICANIA = 30;
    private static final int MIX_DOBA_POZICANIA = 0;
    private static final int MAX_MNOZSTVO_POZICANIA_CD = 5; 
    private static final int MIX_MNOZSTVO_POZICANIA_CD = 0;  
    
    private String NazovCd;
    
    public Cd (String nazovCd)  
    private String typCd;
    private int serioveCislo;
    private int DlzkaCd;
    private String interpret;
    private double cenaCd;
    
    
    public String zistitypCd()  
    { return typCd;                     
    }

    
    public double zisticenuCd()     
    {return cenaCd;
    }
    
    public int zistiSerioveCislo()      
    { return serioveCislo;
    }
    
    public int zistiDlzkuCd()        
    { return DlzkaCd;
    }
    
    public void nastavInterpreta (String novyInterpret) 
    {interpret = novyInterpret;
    }
    
    public void nastavCenuCd (int novaCenaCd) 
    {cenaCd = novaCenaCd;
    }
    
    public void nastavTypCd (String novyTypCd)  
    { typCd = novyTypCd;
    }
    
    public void nastavSerioveCislo (int noveSerioveCislo)   
    {serioveCislo = noveSerioveCislo;
    }
    
    public void nastavDlzkuCd (int novaDlzkaCd)       
    { DlzkaCd = novaDlzkaCd;
    }
    
    
        
        
}
    