import gpdraw.*;

public class Benzene
{
private DrawingTool myPencil;
private SketchPad myPaper;
   
public Benzene(){
myPaper = new SketchPad(300, 300);  
myPencil= new DrawingTool(myPaper);
}
 
public void draw(){
myPencil.drawCircle (60);  
myPencil.up ();
myPencil.home();
myPencil.forward(60);
myPencil.forward (40);
myPencil.down();
myPencil.turnRight(110);
myPencil.forward (100);
myPencil.turnRight(65);
myPencil.forward(100);
myPencil.turnRight(65);
myPencil.forward(100);
myPencil.turnRight

  
}
}
