
/**
 * ##### # ########### ## ##### ########## ####.
 * 
 * @###### (#### ####) 
 * @####### (# ####### ###### ## # ####)
 */
public class TheThinker
{
    private int x;
    private int y;
    
    /**
     * default constructor to set default values 
     */
    public TheThinker()
    {
        x=0;
        y=0;
    }
    
    
    /**
     *paramaterized constructor sets the parameterized constructor
     *@param x
     *@param y
     */
    public TheThinker(int x, int y)
    {
        x=0;
        y=0;
    
    }
    
    public boolean isGreaterThanX(double x)
}