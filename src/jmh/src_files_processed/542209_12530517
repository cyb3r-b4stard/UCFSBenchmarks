

public class TableSquareCubes
{
    public static void main(String args[])
    {
        int a = 0;
        int b = 1;
        int c = 3;
        int d = 4;
        int e = 5;
        int f = 6;
        int g = 7;
        int h = 8;
        int i = 9;
        int j = 10;
        int k;
        int l;
        
        System.out.println("number  square  cube");
        a * a = k;
        k*a = l;
        System.out.println(a + "       " + k + "       " + l);
        
    }
}