import java.util.Scanner;
class Addition
{
    public static void main(String args[])
    {
        int x,y,z;
        System.out.println("Enter two integers to find their sum");
        Scanner.in=new;
        x=in.new();
        y=in.new();
        z=x+y;
        System.out.println("Sum of the numbers="+z)
    }
}