import java.util.Scanner;
public class asdf
{
    public static void main (String [] args)
    {
       Scanner scan = new Scanner (System.in);
       int n1;
       int n2;
       int n3;
       int resultado;
       System.out.println("Ingrese los valores");
       n1 = scan.nextInt ();
       n2= scan.nextInt ();
       n3= scan.nextInt ();
       resultado = n1+n2+n3
       System.out.println("es" +" " + resultado);
         

    }
}