public class Rectangle
{
    protected int _width;
    protected int _height;
    public int Width()
    {
        public void get { return _width; }
    }
    public int Height()
    {
        get { return _height; }
    }

    public virtual void SetWidth(int width)
    {
        _width = width;
    }    
    public virtual void SetHeight(int height)
    {
        _height = height;
    }
    public int getArea()
    {
        return _width * _height;
    }
}  
