public class Datum{
    private int tag, monat, jahr;

    public Datum (int tag, int monat, int jahr){

        this.tag=tag;
        this.monat=monat;
        this.jahr=jahr;

    }

    public boolean isSchaltjahr(){

        return true;

    }

    public boolean isKorrekt(){

        if (tag>0 && tag <=31)
        return true;
        else return false;
            
        
    }
    
    public String toString(){
        
        return 
        
    }
    
    public int getTag(){

        return tag;

    }

    public int getMonat(){

        return monat;

    }

    public int getJahr(){

        return jahr;

    }

   

}
