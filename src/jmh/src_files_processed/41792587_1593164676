import java.util.*;

public class DataBase
{
    private ArrayList<Person> listePersonnel;

    public DataBase()
    {
        listePersonnel = new ArrayList<Person>();
    }

    public void listAll()
    {
        Person e;
        for (int i = 0; i< listePersonnel.size();i++)
        {
            System.out.println("\n" + e = (Person) listePersonnel.get(i));
        }
    }
    
    public void addPerson(Person e)
    {
       listePersonnel.add(e); 
    }
    }