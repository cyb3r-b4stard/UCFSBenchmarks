import java.util.ArrayList;

  public class STUDENTS extends University
  {   
    ArrayList myList = new ArrayList();
    int id;
    int courses_grades;
    public STUDENTS()
    {
    }
    
    public STUDENTS(String name, String city, int age,int kimlik,int puan)
    {
     super(name,city,age);
     id=kimlik;
     courses_grades=puan;
     
     /*String added="";
     added=name+" "+city+" "+age+" "+id+" "+courses_grades;*/
     
     
     
     
    }
    public String addStudent(String name, String city, int age,int kimlik,int puan){
        super(name,city,age);
        id=kimlik;
        courses_grades=puan;
        
        String returned = "";
        returned = name+" "+city+" "+age+" "+id+" "+courses_grades;
        
        return returned;
        
    }

    public String toString()
    {   String returned = "";
        returned = name+" "+city+" "+age+" "+id+" "+courses_grades;
        return returned;
    }
    
     public int getID(){
        return id;
    }
    
    public void setID(int a){
        id = a;
    }
    
    public int getCoursesGrades(){
        return courses_grades;
    }
    
    public void setCoursesGrades(int a){
        courses_grades = a;
    }
}