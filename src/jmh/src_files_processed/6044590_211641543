public class Dot
{
    private int x;
    private int y;
    private boolean exists;
    Dot(int x, int y)
    {
        this.x = x;
        this.y = y;
        this.exists = true;
    }   

    public void makeInvisible()
    {
        exists = false;
    }
    
    public void setExists(boolean ex)
    {
        exists = ex;
    }

    public boolean getExists()
    {
        return exists;
    }

    public String toString()
    {
        return "Dot("+x+","+y+")";
        if (exists == false){
            x = (-);
        }
    }
}