import java.util.*;
/**
 * ##### # ########### ## ##### ### ####.
 * 
 * @###### (#### ####) 
 * @####### (# ####### ###### ## # ####)
 */
public class p34
{
    public void evensSum()
    {
       Scanner s = new Scanner(System.in);
       System.out.println("enter a number greater than 2");
       int input = s.nextInt();
       int sum = 0;
       if(input < 2)
       System.out.println("invalid input");
       else
      {
       
       for(int i = 2; i <= input; i++)
       {
          int num = i;
          if((num % 2) == 0)
          sum += num;
          
        
        
    }
    System.out.println("the sum is " + sum);
}
}
public void multtable()
{
    for(int i = 1; i++; i > 13)
    {
        System.out.printn(i * i);
    }
}
}