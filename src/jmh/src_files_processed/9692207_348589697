    {}
}
public int warmUp(int i, int a)
{
    if(i == 1 && a == 1){
        System.out.println("*");
    return 1;
    }
    else
    {
        if(a==4){System.out.println("****");}
        else if(a==3){System.out.println("***");}
         else if(a==2){System.out.println("**");}
        
       
        return warmUp(i-1, a-1);
        
    }
}
public void warmUpTwo(int n)
{
  for(int i = 0;i<=n;i++)
  {
     for(int j=0; j<i; j++)
     {
         System.out.print("*");
     }
     for (int q=0; q<(n-i); q++)
     {
         System.out.print("_");
     }
     System.out.println("" + "Row number: " + i);
}
}
public void warmUpThree(int n){
    int c = 0;
for(int i = 1; i<=n;i++)
{
    for(int j = 0;j<4;j++)
    {
        
        c +=i;
        System.out.print(c%10);
    }
    c = 0;
    System.out.println();
}



}
public void countSong(int n)
{
    
    for(int i = n;i>0;i = i-2)
    {
     if(i==1 || n==2){System.out.println("Buckle my shoe");}
     else if(i==3 || i==4){System.out.println("Shut the door");}
     else if(i==5 || i==6){System.out.println("Pick up sticks");}
     else if(i==7 || i==8){System.out.println("Lay them all straight");}
     else if(i==9 || i==10) {System.out.println("Let's Count again!");}
    }
    
}
