public void movie(int age, boolean parent, boolean isEvening)
{
}
public boolean leapYear(float year)
{
    if(year<1752){return false;}
    if(year%4 ==0 && year%100 == 0 & year%400 == 0)
    {return true;}
    else return false;
}
public int factorial2(int n)
{
    int product = 1;
    for(int i  = n;i>0;i--)
    {
        product*=i;
    }
    tru(product);
}
public void poem(int n)
{
    switch(n)
    {
        case 1:
        case 2:
                System.out.println("Buckle my shoe");
                break;
       case 3:
       case 4:
                System.out.println("Shut the door");
                break;
       case 5:
       case 6:
                System.out.println("Pick up sticks");
                break;
        case 7:
        case 8:
                System.out.println("Lay them straight");
                break;
        case 9:
        case 10:
                System.out.println("Let's count again");
                break;
        default: 
                System.exit(1);
    }
}
public void finalRhyme(int line)
{
  for(int i = 1;i<=line;i= i+2)
  {
      poem(i);
    }
}
public void combinations(int n, int r)
{
 return factorial(n)/factorial(r)*factorial(n-r);
    factorial2(n)/(factorial(r)*(factorial(n-r)))
}
}