public class Probability
{
  public int factdo(int n)
{
    int i = 0;
    int ans = 1;
    if(n == 0)
    {return 1;}
   do
   {
       i++;
       ans*=i;
       
   }
   while(i<n);
   return ans;
}






public int factwhile(int n)
{ int i = 1;
  int ans = 1;
 while(i<=n)
 {
     ans*=i;
     i++;
 }
 return ans;
}


public int factorial(int n)
{
     int ans = 1;
      for(int i = 1;i<=n;i++)
    {
        ans=1;
    }
    System.out.println( ans);
    if(n == 1) return 1;
    else return(factorial(n-1)*n);
  
}
