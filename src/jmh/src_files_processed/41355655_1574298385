
public class AN
{System.out.println("Результат умножения:"); 
int[][] resultat = new int[3][3]; 
for(i=0; i<3; i++) 
{ 
for( j=0; j<3; j++) 
{ 
resultat[i][j] = 0; 
for (int s=0; s<3; s++) 
{ 
resultat[i][j] += C[i][s] * D[s][j]; 
} 
System.out.print(resultat[i][j]+" ");} 
System.out.println();} 

}}
     