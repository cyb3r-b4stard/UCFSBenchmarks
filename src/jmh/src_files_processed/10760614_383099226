import java.util.*; 
public class HundredTableForLoop
{
    public static void main(String [] args)
    {
        Scanner scan = new Scanner(System.in);
        
        System.out.println("");
         
        
        
       
        for(int a = 0, x = 0-; x<=100; x++, a++)
        {
            System.out.print(x+ "\t");
             
            
            if(a==10)
            {
                a = 0;
                System.out.println("\t");
            }
        }
        
        
    }
}
