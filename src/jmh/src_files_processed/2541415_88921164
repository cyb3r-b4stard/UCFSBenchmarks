public class callbyvalue
{
   {

public void CallByValue()

{

int val=10,res;

System.out.println(" Original Value : " + val);

res=change(val);

System.out.println(" Value in the change Method : " + res);

System.out.println(" Original Value in CallByValue Method : " + val);

}

public static int change(int a)

{

a=a*5;

return a;

}

}