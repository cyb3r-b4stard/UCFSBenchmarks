import java.util.Scanner;
public class weight
 {
public static void main(String[] args)
 
 	{
Scanner keyboard=new Scanner(system.in);
	int hight_feet=keyboard.intnext();
	
	int height_inches=keyboard.intnext();		
	 if ((hight_feet > 5)			
{
	int x=hight_feet-5;				
	int inches=x*12;}
	int weight_pounds=(inches*5)+120;	/*here 120 is for 5 inches*/
				/*remaining secs */

	System.out.println("total height = " +weight_pounds) ; 

	}
}

 