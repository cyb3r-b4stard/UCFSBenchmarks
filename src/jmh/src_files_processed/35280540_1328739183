public class Temperature {
    private double degreesF;
    private double degreesC;
    private double degreesK;
    
    public Temperature(double degrees){
        setDegreesF(degrees);
    } 
    public Temperature() {
    this(0.0);} 

    public double getTempC(){
        return degreesC;
    }
    public void setDegreesC(){
    degreesC = degrees;
    degreesF = (degreesC*9/5+32)
    degreesK = (degreesC+273.15)
    }
    public double getTempK(){
        return degreesK;
    }
    public void setDegreesK(){
    degreesK = degrees;
    degreesF= (degreesK-273.15)*(9/5+32)
    degreesC= (degreesK-273.15)
    }
    public double getTempF(){
        return degreesF
    }    
    public void setDegreesF (double degrees) {
        degreesF = degrees;
        degreesC = (degreesF - 32.0) * 5.0 / 9.0; 
        degreesK = degreesC + 273.15;
    }
    
    
    
}