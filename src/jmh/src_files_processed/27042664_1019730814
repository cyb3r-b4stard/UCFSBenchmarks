public class prog215b
{
    public static void main(String[] args){
        
        public final int MUZZLE_VELOCITY = 960;
        final int GRAVITY = 16;
        int t=0;
        int h1=0;
        int h2=0;

        while(h1>h2){
            h1 = h2;
            h2=950 * t - 16 * t * t;

            t++;

        }
        system.out.println(h1)
        
    }
}