import java.util.ArrayList; 

/**
 * ##### ####### ## #### ###########.
 * 
 * @###### (#### ###) 
 * @####### (##/##/####)
 */

public class Club
{
    private ArrayList <Membership> members; 
    /**
     * Constructor for objects of class Club
     */
    public Club ()
    {
      /**
         * Add a new member to the club's collection.
         * @param member The member to be added.
         * /
        members = new ArrayList <membership> ();
       
    }

     
    public void join(Membership member)
    {
        members.add (member);
    }

    /**
     * @return The number of members (Membership objects) in
     *         the club.
     */  
   public int numberOfMembers();
    
        {
            return members.size();
   
        /**
         * the mothod size returns the length of the collection members.
        *@return the current size of member 
        */
        
    }
}
    /** Need to add new method addMember in class to add newe members.
     * 
     */
    public void addMember (string name, int month, int year)
    {
        members.add (new Membership (name, month, year)) ;
    }
}