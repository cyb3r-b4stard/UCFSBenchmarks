

public class Scorebord
{
    private int score1;
    private int score2;
    
    public Scorebord( int score1, int score2 )
    {
        this.score1 = score1;
        this.score2 = score2
    }

    public String toString()
    {
        return score1.toString() + "-"
        score2.toString;
    }
}