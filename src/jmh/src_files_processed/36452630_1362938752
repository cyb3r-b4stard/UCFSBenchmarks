import java.io.*;
class stack_array_func_based
{
    int top,len;
    int arr[];
    stack_array_func_based(int len)
    {
        arr[]=new int(len)
        top=-1;
        for(int i=0;i<=;i++)
        arr[i]=0;
    }
    public void insert(val)
    {
        BufferedReader in=new BufferedReader(new InputStreamReader(System.in));
        System.out.println("enter the value to be entered");
            int val=Integer.parseInt(in.readLine());
            if(top!=(length-1))
            arr[++top]=val;
            else
            System.out.println("Stack overflow");
    }
    public void delete()
    {
         if(top==-1)
            System.out.println("Stack underflow");
            else
            {
            System.out.println("The object"+arr[top]+"has been removed");
            top--;
    }
}
   public void display()
    {
        if(top!=-1)
        {
        System.out.println("Contents of Stack are");
        for(int i=0;i<=top;i++)
        System.out.println(arr[i]);
    }
}
}
   