class contact
{
    public static void main(String na[],int no[],String a)
    {
        int f=0,i;
        for(i=0;i<na.length;i++)
       {
           if(na[i].equals(a))
           {
               f=1;
               break;
            }
        }
        if(f==1)
        System.out.println("name"+a+"\t""contact no."+no);
        else
         System.out.println("invalid");
        }
    }