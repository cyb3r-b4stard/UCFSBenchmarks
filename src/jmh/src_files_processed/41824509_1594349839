import java.util.*;

public class Bibliothek
{
    private ArrayList<Buch>bibl;
    public Bibliothek()
    {
        this.bibl = new ArrayList<Buch>();
    }
    public void addBuch(String titel, String autor)
    {
        Buch neu = new Buch(titel, autor);
        this.bibl.add(neu);
    }
    public Buch entlehne(String titel, String autor)
    {
        for(Buch i: this.bibl)
        {
            if(i.gibTitel().equals(titel)&&i.gibAutor().equals(autor)&&!i.entlehnt())
        }
    }
}