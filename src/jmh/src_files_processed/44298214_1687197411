import java.util.ArrayList;
public class User
{
    ArrayList<String> userArray = new ArrayList<String>(9);
    public User(String uID, String uPW, String uEMail, String uName, String uAddress, String uCity, String uState, String uZip, String uTele)
    {       
    userArray.get[0]=uID;   
    userArray.get[1]=uPW;
    userArray.get[2]=uEmail;
    userArray.get[3]=uName;
    userArray.get[4]=uAddress;
    userArray.get[5]=uCity;
    userArray.get[6]=uState;
    userArray.get[7]=uZip;
    userArray.get[8]=uTele;}
    public String getuID()
    {
        return userArray.get[0]