import java.math.BigInteger;
/**
 * ##### # ########### ## ##### ### ####.
 * 
 * @###### (#### ####) 
 * @####### (# ####### ###### ## # ####)
 */
public class RSAVerschlüsselung
{

    /**
     * Constructor for objects of class bla
     */
    public RSAVerschlüsselung(int Klartextwert)
    {
        BigInteger Hauptmodul = new BigInteger("161");
        BigInteger Verschlüsselungsexponent = new BigInteger("35");
        BigInteger Entschlüsselungsexponent = new BigInteger("83");
        
        BigInteger Klartextwert = int(Klartextwert);
        
        BigInteger verdeckterKlartextwert = Klartextwert.modPow(Verschlüsselungsexponent,Hauptmodul);
        System.out.println("Verschlüsselt Klartextwert: "+verdeckterKlartextwert);
        
        BigInteger offenerKlartextwert = verdeckterKlartextwert.modPow(Entschlüsselungsexponent,Hauptmodul);
        System.out.println("Entschlüsselt Klartextwert: "+offenerKlartextwert);
    }

}
