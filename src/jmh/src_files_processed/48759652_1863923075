import java.util.Scanner;
import static java.lang.System.out;
import static java.lang.System.in;
public class alphabetTacTurn1
{
    public static void main(String [] args)
    {
Scanner keyboard = new Scanner(System.in);
int isxWins = 1;
int isoWins = 1;
int hasxWins = 0; 
int hasoWins = 0;
String secretLetter = "g";
          out.println("Welcome To Alphabet Tac Turn");
          out.println("Are you ready to play?");
          out.println("Let us get started!\n  Guess the secret alphabet!");
          out.println("The first player, player X, makes the first guess.");
          out.println("He/she will enter an alphabet using the  keyboard");
boolean endGame = true;
boolean doesyouXTurn = true;
boolean doesyouOTurn = false;
if(doesyouXTurn == true);
{ 
          out.print("Player X enter an alphabet guess! ");
          secretLetter = keyboard.next();
          secretLetter = "g";
        doesyouXTurn = true;
        hasxWins = isxWins + 1;
        out.println("xGuesses" + hasxWins + "times");
}
       if(endGame != false);
{
         out.print("Play O enter an alphabet guess! ");
         secretLetter = keyboard.next();
          secretLetter = "g";
         doesyouOTurn = false;
         hasxWins = isoWins + 1;
         out.println("oGuesses" + hasoWins + "times");
}
        if(endGame != true);
        {
         out.print("Player X enter an alphabet guess! ");
         secretLetter = keyboard.next();
          secretLetter = "g";
         doesyouOTurn = false;
         hasxWins = isxWins + 1;
         out.println("xGuesses" + hasxWins + "times");
}     
        if(doesyouXTurn == false);
{
         out.print("Player O enter an alphabet guess! ");
         secretLetter = keyboard.next(); 
          secretLetter = "g";
         doesyouOTurn = false;
         doesyouXTurn = true;
         hasxWins = isoWins + 1;
         out.println("oGuesses" + hasoWins + "times");
}  
do
{   
       if(doesyouOTurn == false);
       {
         secretLetter = "g";
        doesyouOTurn = false;
        doesyouXTurn = true;
        hasxWins = isoWins + 1;
        out.println("xGuesses" + hasoWins + "times");
        out.println("You won O!");
        {
while(doesyouXTurn != true);
{
  {
        
         secretLetter = "g";
        doesyouOTurn = false;
        doesyouXTurn = true;
        hasxWins = isxWins + 1;
        out.println("xGuesses" + hasxWins + "times");
        out.println("You won X!");

    }
}
}
}


}