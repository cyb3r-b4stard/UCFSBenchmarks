
public class Time1 {
   /**
    *Represents time - hours:minutes:seconds.
    *Coordinates must be legal
    */
    private int _hour , _minute , _second; {
        _hour = 0-23 ; _minute = 0-59; _second = 0-59;}
    public Time1 (int h, int m, int s) {_hour = h; _minute = m; _second = s;}
     boolean   ( h>23, m>59, s>59){
           (h=0, m=0, s=0);
        }
           
             
        
