
/**
 * ### ####### ## #### ####### ## ## ######### ### ###### ######### ### ######.
 * 
 * #### ##########
 * ##/##/####
 */
public class Co2FootprintV1
{
    double gallons,convert,tons,lbs;
    Co2FootprintV1(double g,double c)
    {
        gallons = g;
        convert = c;
    }
    public double getGallons()
    {
        return gallons;
    }
    public void calcLbs()
    {
        lbs = ((convert*gallons)/0.00045359));
    }
    public double getLbs()
    {
        return lbs;
    }
    public void calcTons()
    {
        tons = (convert*gallons);
    }
    public double getTons()
    {
        return tons;
    }
}