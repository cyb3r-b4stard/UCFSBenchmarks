
import  java.lang.Object;

class Box
{
    double length ;
    double width ;
    double height ;
    Box ( double side )
    {
        width = side ;
        height = side ;
        length = side ;
    }
    Box (double width , double height, double length)
    {
        this.width = width;
        this.height = height;
        this.length = length;
    }
    String (ogkos,epifaneia)
    {
        System.out.ptintln(str);
    }
    double epifaneia(double width, double length)
    {
        return width * length;
    }
    
    double ogkos(double width , double height, double length)
    {
      return calculate() * 2;
    }
    double calculate()
    {
        return (width * height * length)/2 ;
    }
    
    String toString()
    {
        String str = ogkos()+","+epifaneia();
        return str;
    }
}


