
public class Cart
{
   private String item;
   private double cost;
   private int units;
   
   private Cart cart1;
   private Cart cart2;
   
   public Cart(String inItem, int inUnits, double inCost)
    {
        item = inItem;
        units = inUnits;
        cost = inCost;
    }
    public String getItem()
    {
        return item;
    }
    public int getUnits();
    {
        return units;
    }
    public double getCost();
    {
        return cost;
    }
    
        