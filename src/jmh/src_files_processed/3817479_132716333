import java.util.*;
public class KTO12_DIAZ{
    public static void main (String args[]){
        double kldg,skl,und,per;
        double ttlave,grade;
        
         System.out.println("ENTER THE GRADE: ");
        double grade = new Scanner(System.in);
        double kldg = grade/.15;
        System.out.println("ENTER THE KNOWLEDGE SCORE: ",+kldg);
        double kldg = new Scanner(System.in);
        double skl= grade/.25;
        System.out.println("ENTER THE SKILL SCORE: ",+skl);
        double skl= new Scanner(System.in);
        double und = grade/.30;
        System.out.println("ENTER THE UNDERSTANDING SCORE: ",+und);
        double und = new Scanner(System.in);
        double per = grade/.15;
        System.out.println("ENTER THE PERFORMANCE SCORE: ",+per);
        double per = new Scanner(System.in);
        ttlave = kldg+skl+und+per;
        System.out.println("THE TOTAL SCORE: ",+ttlave);
        if (ttlave>=90){
            System.out.println("ADVANCED");
            elseif(ttlave>=85);{
                System.out.println("PROFICIENT ");
                elseif  (ttlave>=80){
                    System.out.println("APPROACHING PROFICIENCY ");
                    elseif (ttlave>=75){
                        System.out.println("DEVELOPING ");

                    }            

                }
            }

        }        
                else{
            System.out.println("BEGINNING");
        }
    }
}
