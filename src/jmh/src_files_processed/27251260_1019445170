 * @###### (#### ####) 
 * @####### (# ####### ###### ## # ####)
 */
public class rechen
{
    public static int merry(int[] a){
        int sum = a[0];
       for(int i = 1; i < a.length; i++){
           sum = sum * a[i];
           }
       }
       return sum;
    }