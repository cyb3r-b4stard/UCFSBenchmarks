

import java.awt.*;
import java.applet.*;


public class GraphicsLab04st extends Applet
{
    static double x = Expo.enterDouble();
    static double y = Expo.enterDouble();
	public void paint(Graphics g)
	{
	    head(g);
	    
	    Body.arms(g);
	    Body.torso(g);
	    Body.legs(g);






	}
	
	public static void head(g)
	{
	  Expo.setColor(g,green);
	  Expo.FillCircle(g,100,100,25);
	   }







}

