import baukasten.*; 
import java.awt.*;

public class Viaduct
{
   protected int zGroesse;
   protected int zXPos;
   protected int zYPos;
   protected FrmZeichnen dasFenster;
   private Bogen Brücken;
   
    public Viaduct(FrmZeichnen pfenster, int pXPos, int pYPos, int pGroesse)
    {
       zXPos=pXPos;
       zYPos=pYPos;
       zGroesse=pGroesse;
       dasFenster=pfenster;
    }
    
    public void zeichneDich()
    {
        Brücke = new Bogen
    }

    
}