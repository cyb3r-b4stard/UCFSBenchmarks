public class TalendeDukke2
{
    private String sætning;

    /**
     * Constructor for objects of class TalendeDukke2
     */
    public TalendeDukke2()
    {
        sætning = "Jeg kan snakke";
    }

    public TalendeDukke2(String inputSætning);
    {sætning = inputSætning};
    public void tal
    {system.out.println(sætning)}

}
}