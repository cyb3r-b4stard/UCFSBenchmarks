import java.util.Scanner;
public class Challenge5_1
{

    /**
     * main method
     */
    public static void main(String[] args)
    {
        Scanner reader = new Scanner(System.in);
        System.out.print("Your first name: ");
        String firstName = reader.next();
        System.out.print("Your last name: ");
        String lastName = reader.next();
        String fullName = firstName + " " + lastName;
        System.out.print("Your age: ");
        int age = reader.nextInt();
        System.out.print("The city you live in: ");
        String city = reader.next();
        System.out.print("The state you live in: ");
        String state = reader.next();
        System.out.print("Your zip code: ");
        String zip = reader.next();
        System.out.println('\f');
        System.out.println("Your name is "+fullName);
        System.out.println("Age = "+age);
        System.out.println("You live in "+city+", "+state+", "+zip);
        if (int age = 16);
        {
            System.out.print("no one likes you");
        }
    }

}