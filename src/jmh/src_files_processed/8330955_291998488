import StdIn()
public class Avergae
{
    private int x;

    /**
     * Constructor for objects of class Avergae
     *
     */
    import StdIn();
    
    public static void main(String[] args)
       {
           double sum=0.0;
           int cnt = 0;
           while (!StdIn.isEmpty())
           {
               sum+=StdIn.readDouble();
               cnt++;
            }
            double avg = sum/cnt;
            StdOut.printIn("Average is %f", avg);
        }
}

