import java.util.*;
public class hwp3
{
    static Scanner reader=new Scanner(System.in);
    public static void main(String[]args)
    {
        int num1, num2, num3, highValue, highValue1, highValue2 ;
        System.out.println("enter num1");
        num1=reader.nextInt();
        System.out.println("enter num2");
        num2=reader.nextInt();
        System.out.println("enter num3");
        num3=reader.nextInt();
        highValue=Math.max(num1,num2);
        highValue1=Math.max(num2,num3);
        highValue2=Math.max(num3,num1);
        System.out.println("highValue="+"highValue2=""highValue3=");
    }
}
 
       