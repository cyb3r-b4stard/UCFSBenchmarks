
import sum.kern.*;
/**
 * @###### 
 * @####### 
 */
public class HI
{
    Bildschirm derBildschirm;
    Stift meinStift;

    public HI()
    {
        derBildschirm = new Bildschirm();
        meinStift = new Stift();
    }

    public void fuehreAus()
    {
       meinStift.hoch();
       meinStift.bewegeBis(700, 100);
       meinStift.runter();
       meinStift.zeichneKreis(20);
       meinStift.zeichneKreis(30);
       meinStift.zeichneKreis(40);
       meinStift.zeichneKreis(30);
       
       meinStift.hoch();
       meinStift.bewegeBis(50, 200);
       meinStift.runter();
       meinStift.dreheBis(10);
       meinStift.bewegeUm(50);
       meinStift.zeichneKreis(3);
       
      
       
      { 
          meinStift.radiere ();
          meinStift.zeicheKreis (3);
          meinStift.bewegeUm (-50);
          meinStift.normal ();
          meinStift.hoch();
          meinStift.bewegeUm(0.2);
          meinStift.runter();
          meinStiftbewegeUm(50);
          meinStift.zeichneKreis (3);
         
    }while (meinStift.hPosition() < 550);