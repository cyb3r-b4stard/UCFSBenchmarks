String s1="Allan Alda";
int count= s1.length();
String a1= s1.substring(2,count-3);
System.out.println(a1);

String s2="John Wayne";
int count= s2.length();
String a2= s2.substring(2,count-3);
System.out.println(a2);

String s3="Gregory Peck";
int count= s3.length();
String a3= s3.substring(1,count-0);
System.out.println(a3);
