import java.io.*;
import java.util.*;


public class SorterTester
{
    public static void main (String [] args) throws IOException
    {
        Scanner scanner = new Scanner(new File("bubbledata.in"));
        int arraySize = scanner.nextInt();
        int[] rawArray = new int[arraySize];
        for (int i = 0; i< arraySize; i++)
        {
            rawArray[i] = scanner.nextInt();
        }

        System.out.println ("last value before sort = "+ rawArray[arraySize-1]);

        sortArray(rawArray);

        System.out.println ("last value after sort = "+ rawArray[arraySize-1]);

        System.out.println( "10th value after sort = " + rawArray[9]);
        System.out.println("  ");  
    }

    public static void swap (int [] arrayName, int copyFrom, int copyTo)
    {
        int a = arrayName[copyFrom];
        arrayName [copyFrom] = arrayName[copyTo];
        arrayName [copyTo] = a
    }

    public static void moveRight(int [] arrayName, int startPos, int endPos)
    {
    }

    public static void moveLeft(int [] arrayName, int startPos, int endPos)
    {
    }

    public static void sortArray(int [] arrayName)
    {
    }

}