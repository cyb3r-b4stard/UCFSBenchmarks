  public static void elements (int [] [] elements)
  {
    Scanner scan = new Scanner(System.in);  
     for (int r=0;r<elements.length-1 ; r++)
     {
      for (int c=+1 ; c<elements[r].length ; c++)
      {
      System.out.println("Enter the int in a ["+r+"]["+c+"]");
      elements[r][c]= scan.nextInt(); 
        
      }
     }
    }
  
   public static void main()
   {
     int[][]elements;
     elements (int [] [] elements);
     
   }
}