

public class FallingObject extends GraphicalElement
{
   
    int speed; 

    /**
     * Constructor for objects of class FallingObject
     */
    public FallingObject(int x, int y)
    {
        super (x, y, 75, 75);
        speed = 5(Math.rand()); 
    }

    /**
     * An example of a method - replace this comment with your own
     * 
     * @param  y   a sample parameter for a method
     * @return     the sum of x and y 
     */
   public void fall()
   {
    setY(getY() + speed);    
   } 
}