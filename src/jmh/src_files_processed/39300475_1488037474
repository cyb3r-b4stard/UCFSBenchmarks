import java.util.ArrayList();
/**
 * ####### ## #### ## ####### #######
 */
public class PhotoAlbum
{
    
    public PhotoAlbum()
    {
        
    }
    /**
     * Draws the pictures in the PhotoAlbum horizontally
     */
    public void draw()
    {
        final int GAP = 10;
        int lastX = 0;
        for (Picture p: photos)
        {
            p.translate(lastX + GAP, 0);
            lastX = p.getMaxX();
            p.draw();
        }
            
    }
}