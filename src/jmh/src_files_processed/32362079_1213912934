class X
{
    private String c;
    
    public X(String c)
    {
        this.c = c;
    }
    
    public String getC(){
        return c;
    }
    
    /**
     * @throws TooLongException
     * @throws TooShortException
     */
    public void setC(String c)throws TooLongException, TooShortException{
        if(c == null){
            throw new TooShortException("null is not allowed; it is fewer than five characters; too short!");
        }else if(c.length() > 5){
            throw new TooLongException("aaaaaaaaaa is more than five characters; too long!");
        }else if(c == ""){
            throw new TooShortException("empty string is fewer than five characters; too short!");
        }else if(c.length() < 5){
            throw new TooShortException("empty string is fewer than five characters; too short!");
        }
        else{
            if(c.length() == 5){
                this.c = c;
            }
    }
    
    
}