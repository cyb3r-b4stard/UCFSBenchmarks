class Radio {
    boolean ukljucen = false ;
    int trenutnaStanica = 0;
    
    void promeniStanicuNavise(){
        trenutnaStanica= trenutnaStanica + 1;
    }
    void promeniStanicuNanize () {
        trenutnaStanica= trenutnaStanica - 1;
}
   int vratiTrenutnuStanicu () {
       return trenutnaStanica;
    }
   void ukljuci () {
       ukljucen true;
    }
}

           
          
            
        
            
        