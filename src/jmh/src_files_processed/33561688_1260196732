
/**
 * ##### # ########### ## ##### ###### ####.
 * 
 * @###### (#### ####) 
 * @####### (# ####### ###### ## # ####)
 */
public class Array3
{
public int countOdd ( int [] count )
{
    int num = 0;
    for (int i = 0; i < count.length; i++)
    {
        if (count[i] % 2 != 0 )
            num += 1;
        }
        return num;
        
}