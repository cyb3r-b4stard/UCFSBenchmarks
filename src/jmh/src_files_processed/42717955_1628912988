import java.util.Scanner;
public class QuizOnForWhile
{
    public static void main(String args[]) { 
        Scanner sc=new Scanner(System.in);

        int sum=0;
        
        
        System.out.println("TASK 1:");
        for(int x=1;x<99;x++){
            
            sum=(x-1)+sum
            
            

        }
        System.out.println("the sum of the odd integers from 1 to 99 is "+sum);
        System.out.print("\n");
    }
}