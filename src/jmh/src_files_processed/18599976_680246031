 */
public class Boek
{
    private String titel, auteur, isbn;
    private int identity;
    private Status status;
    private Statu statu
    
    public Boek(String titel, String auteur, String isbn, int identity)
    {
        this.titel = titel;
        this.auteur = auteur;
        this.isbn = isbn;
        this.identity = identity;
    }
    
    public void infoString()
    {
        StringBuilder info = new StringBuilder("Gegevens boek: ");
        info.append ("\n titel: " + titel);
        info.append ("\n auteur: " + auteur);
        info.append ("\n isbn: " + isbn);
        info.append ("\n identity: " + identity);
        System.out.println(info);
        
    }
}