public class Carrera
{
    private String nombre;
    private double mensualidad;
    private boolean diurna;    
    private int vacantes;
    private int cantidadSemestres;    
   
 public Carrera()
 {
     this.nombre = "desconocido";
     this.mensualidad = 0;
     this.diurna = true;
     this.vacantes = 10;
     this.cantidadSemestres = 4;
 }
 
public Carrera(String nombre, double mensualidad, boolean diurna, int vacantes, int cantidadSemestres)
{
    this.nombre = nombre;
    if (mensualidad>0)
    this.mensualidad = mensualidad;
    else{
    System.out.println("Ingrese un valor positivo para mensualidad");
    }
    this.diurna = diurna;
    if (vacantes>0){
    this.vacantes = vacantes;
    }
    else{
    System.out.println("Ingrese un valor positivo para vacantes");
    }
    this.cantidadSemestres = cantidadSemestres;
}

public String getNombre()
{
    return nombre;
}

public void setNombre(String nombre)
{
    this.nombre = nombre;
}

public double getMensualidad()
{
    return mensualidad;
}

public void setMensualidad(int mensualidad)
{
    this.mensualidad = mensualidad;
}

public boolean isDiurna()
{
    return diurna;
}

public void setDiurna(boolean diurna)
{
    this.diurna=diurna;
}

public int getVacantes()
{
    return vacantes;
}

public void setVacantes(int vacantes)
{
    this.vacantes=vacantes;
}

public int getCantidadSemestres()
{
    return cantidadSemestres;
}

public void setCantidadSemestres(int cantidadSemestres)
{
    this.cantidadSemestres=cantidadSemestres;
}


public void aumentarMensualidad(double aumentarMensualidad)
{
    if(aumentarMensualidad>0)
    this.mensualidad= aumentarMensualidad+mensualidad;
    else
    System.out.println("Error: Ingrese un valor positivo");
}

public void rectificarCarrera()
{
    if(nombre=="Analista"){
    this.nombre="Analista Programador Computacional";
    }
    
    System.out.println("Nombre="+nombre);
    System.out.println("Mensualidad="+mensualidad);
    System.out.println("Diurna="+diurna);
    System.out.println("Vacantes"+vacantes);
    System.out.println("Cantidad de semestres"+cantidadSemestres);
}

public void aumentarMensualidad()
{
    if(nombre=="analista"){
        this.mensualidad=mensualidad*1.1;
    }
    if(nombre=="redes"){
        this.mensualidad=mensualidad*1.2;
    }
    if(nombre=="gastronom√≠a"){
        this.mensualidad=mensualidad*1.25;
    }
    
}