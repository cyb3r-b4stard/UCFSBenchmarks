 * @###### (#### ####) 
 * @####### (# ####### ###### ## # ####)
 */
public class Test {
static int f = 3;
static int g = 5;
int static a;
int static b;
int static c;
int static d;
public static void main( String [ ] args ) {
a = foo(g);
System.out.println(a);
 b=foo();
 System.out.println(b);
 c=foo();
 System.out.println(c);
 d=foo(g);
 System.out.println(d);
System.out.printf("%d", f + g);
}
static void foo(int g) { g++; }
static void foo () { f++; }
} 