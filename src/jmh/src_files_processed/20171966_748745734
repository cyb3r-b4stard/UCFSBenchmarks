
import java.util.Arrays;
import java.util.Scanner;
public class trial
{
    public trial()
    {
    }
public static void main(String[] args)
    {
        int i=0;
        int j=0;
        int x=0;
        String [][] board = new String[3] [3]; 
        while(i<=2){
            board[0][i]= " ";
            board[1][i]= " ";
            board[2][i]= " ";
            i++;
        }
        System.out.println("Please input the x position that you want to go to...");
        Scanner scan = new Scanner(System.in);
        int userposx = scan.nextInt(); 
        
        
        System.out.println("Please input the y position that you want to go to...");
        Scanner scantwo = new Scanner(System.in);
        int userposy = scantwo.nextInt(); 
       
        
        System.out.println("X or O, state your value");
        Scanner scanthree = new Scanner(System.in);
        String value = scanthree.nextLine();
        
        
        
        board[userposx-1] [userposy-1] = value; 
        
        while (j>=9){
            system.out.print(board[x][j]);
            if(i%3=0){
                x++;
            }
            j++;
    }
       
    }
    
}