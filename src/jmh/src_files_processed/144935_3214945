
/**
 * ##### # ########### ## ##### ######## ####.
 * 
 * @###### (#### ####) 
 * @####### (# ####### ###### ## # ####)
 */
public class Alimento
{
    private String Nombre;
    private int Calorias;
    
    public Alimento()
    {
    }

    public String getNombre(){return this.Nombre;}
    public int getCalorias(){return this.Calorias;} 
    
    public void setNombre(String NN){
        if(NN.length() >= 5){
        this.Nombre = NN;}
    }
    public void setCalorias(int NC){
        if(NC >=20 & NC <=500){
        this.Calorias = NC;}
    }
    public boolean igual(alimento o)
    if(this.Nombre.equalsIgnoreCase(o.Nombre) && this.Calorias == o.Calorias)
    {
        return true;
    }
    return false;
}