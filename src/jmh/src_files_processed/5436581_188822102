

public class isTriangle
{
    public static void main (String args [])
    {
        System.out.println(isTriangle(1,2,3));
        System.out.println(isTriangle(0,2,3));
    }
    public static boolean isTriangle(int a, int b, int c)
    {
        int longestside;
        int others;        
        longestside=a;
        others=b+c;
        if (a<=0 || b<=0 || c<=0)
        {
            System.out.println("Invalid Side Lenght or Lengths");
            return "Invalid"
        }
        if (b>a && b>c)
        {
            longestside=b;
            others=c+a;
        }
        if (c>b && c>a)
        {
            longestside=c;   
            others=a+b;
        }
        return longestside<others;
    }
}