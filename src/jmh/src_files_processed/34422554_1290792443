
public class Habitacion
{
    private int nhabitacion, valor;
    private char disponible;

    public Habitacion(int newNhabitacion, int newValor, char newDisponible)
    {
        this.nhabitacion=newNhabitacion;
        this.valor=newValor;
        this.disponible=newDisponible;

    }

    
    public int getNhabitacion(){return this.nhabitacion;}
    public int getValor(){return this.valor};
    public int getDisponible(){return this.disponible};
    
    
    
    
    
    
    
    
    
}