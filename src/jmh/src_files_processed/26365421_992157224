import java.lang.String;
import java.lang.*;
import java.lang.Object;
import java.io.*; 
import java.io.BufferedReader;
import java.io.FileReader;
import java.io.IOException;
import java.io.BufferedWriter;
import java.io.File;
import java.io.FileWriter;
import java.io.IOException;
import java.util.Arrays;
import java.util.List;
 import java.util.LinkedList;
   
import java.io.*;

public class Compare
{
    public static void main(String[] args)throws IOException
    {
        compare("test.csv","test2.csv");
    }
 
    public static void compare(String file1, String file2) throws IOException
    {
   try 
    {
        BufferedReader in1 = new BufferedReader(new FileReader(file1));
        BufferedReader in2 = new BufferedReader(new FileReader(file2));
        
        List<String> list1 = new LinkedList<String>();
        List<String> list2 = new LinkedList<String>();
        
        String [] array2 = new String[file2.length()];
        String count;
        int j = 0; 
        String string;
        while((string = in1.readLine()) !=null){
       list1.add(string);
        j++;
       }
       in1.close();
       j = 0;
       String string2;
        while((string2 = in2.readLine()) !=null){
        list2.add(string2);
        j++;
       }
       in2.close();
       list1.removeAll(file2); 

            if (list1.isEmpty())
                System.out.println("The two files are identical.");
            else
                System.out.println("The two files are NOT identical.");
}
catch(FileNotFoundException e){
}
catch(IOExcepption e){
}
{
    }
}