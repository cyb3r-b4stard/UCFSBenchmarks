public class TreasureMap {
    
    private int px, py, tx, ty, dir;
    
    public TreasureMap (int treasurex, int treasurey)
    {px = 0;
     py = 0;
     tx = treasurex;
     ty = treasurey;
     dir = 0; }
    
    public void moveNorth(int steps)
    {py = py + steps;
     dir = 0; }
    
    public void moveSouth(int steps)
    {py = py - steps;
     dir = 2; }
    
    public void moveEast(int steps)
    {px = px + steps;
     dir = 1; }
    
    
    public void moveWest(int steps)
    {px = px - steps;
     dir = 3; }
    /* pre: steps >= 0 */
     
    public int getXPos()
    {return px; }
    
    public int getYPos()
    {return py; }
    
    public boolean dig()
    {return (px == tx && py == ty); }
    
    public
    
    
    
    
    
    
    
    
    
    
    
    
    
    
}