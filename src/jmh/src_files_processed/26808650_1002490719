
public class clash
{
    public static void main (int n)
    {
        int ,s = 0,p = 1,a;
        while (n > 0)
        {
            a = n % 10
            s = s + a;
            p = p * a;
            
            n = n / 10;
            
        }
        
        system.out.println(s+"  "+p);
        
    }