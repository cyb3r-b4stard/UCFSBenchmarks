
/**
 * ##### ############
 * ##-##### ### ####
 * 
 * @###### ##### ########
 * @####### ##.##.####
 */
public class SoccerPoints
{
    private String teamName;
    private int totalPoints;
    
    /**
     * default constructor
     */
    public SoccerPoints(){
        totalPoints = 0;
        teamName = "";
    }
    
    public SoccerPoints(String teamName){
       if(teamName != null || teamName !=""){
           this.teamName = teamName;
       } else {
           this.teamName = "unknown";
       }
       
       totalPoints = 0;

}