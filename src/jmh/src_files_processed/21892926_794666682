import javax.swing.JOptionPane;

public class exerciceUn
{
    static void MoyenneChiffres()
    {
        int premierChiffre;
        int deuxiemeChiffre;
        int troisiemeChiffre;
        while (premierChiffre < 0 || deuxiemeChiffre < 0|| troisiemeChiffre < 0){
            String reponseUtilisateur1 = JOption.showInputDialog ("Veuillez rentrer votre premier nombre positif");
            premierChiffre = Integer.parseInt(reponseUtilisateur1);
    }
}