
import java.util.Scanner;

public class surfacearea
{

  public static void main(String [] args)
  {
      Scanner reader = new Scanner(System.in);
      int lengthEdge, surfaceArea;
      
      System.out.print("Enter the length of an edge: ");
      lengthEdge = reader.nextInt();
      
  surfaceArea = lengthEdge * lengthEdge * 6;
System.out.println (the surface area of the cube is )+( lengthEdge *lengthEdge);
}
}