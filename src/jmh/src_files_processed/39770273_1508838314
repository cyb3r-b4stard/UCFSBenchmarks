import java.io.*;
public class FRQ
{
    public static void main(String[] args)
    {
        String[] words1 = {"TAN", "ABRACADABRA", "WHOA"}
        
        ArrayList<String> wordList1 = new ArrayList<String>(Arrays.setList(words1));
        
        for(String x:words1)
        {
            System.out.println(scrambleWord(x));
        }
        
        
    }
    public static String ScrambleWord(String word)
    {
        int x = 0;
        String a = "A";
        int b = 0;
        
        while(x < word.length())
        {
            if(word.charAt(x) == 'A')
                b = word.indexOf("A");
                word.replace(a,word.charAt(b));
                System.out.print(word);
            x++;
        } 
        return " ";
    }
    public static String ScrambleorRemove(String word)
    {
        return " ";
    }
}