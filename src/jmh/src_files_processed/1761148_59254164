 */
public class easter{
    public static void main (String [] args)
{  
   {Scanner reader = new Scanner (System.in);
  
        int year;
        int century;
        int gregor;
        int moon;
        int clavian;
        int epact;
        int day;
        int e;
        char month;
   
   System.out.print("Enter any year after 1583, and the date of easter sunday will be calculated");
   
   year = reader.Nextint();
   
   century = (year/100) + 1;
   
   gregor = ((3 * century) / 4) - 12;
   
   moom = (year%19) + 1;
   
   clavian = ((8*century + 5) / 25) - (5 + gregor);
   
   e = ((5 * year)/4) - (gregor+10);
   
   epact = (11*moon+20+clavian)%30;
   
   if (epact == 25 && moon > 11 || epact==24);
       epact = epact + 1;
       
   day = 44-epact;
   
   if (day < 21)
      day = day+30;
      
   day = (day+7)- (day+e)%7;
   
    if (day <= 31)
     System.out.print("March ");
     else
     {day = day-31;
         System.out.print("April ");}
         System.out.println(day);
        }

         
         
    
          
    
