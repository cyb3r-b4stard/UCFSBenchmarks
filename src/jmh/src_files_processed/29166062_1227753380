private void processTrade(Trade t) {
            String status = t.getStatus();
            if (status.equalsIgnoreCase(NEW)) {
                  newTrade(t);
            } else if (status.equalsIgnoreCase(EXECUTE)) {
                  executeTrade(t);
            } else if (status.equalsIgnoreCase(PENDING)) {
                  pendingTrade(t);
            }
}