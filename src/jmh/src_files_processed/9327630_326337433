public class SoccerPlayer
/**
 * Soccer players and their respective stats.
 * 
 * @author (Andy Jespersen) 
 * @version (August 29th, 2014)
 */
{
    private int speed;
    private String playerName;
    private int height;
    private int jersey;    
    public SoccerPlayer(int number,String playerName)
    {
        jersey = number;
        speed = 20;
        height = 74;
        playerName = "name";
    }
    public int getSpeed
    {
        return speed;
    }
    public int getHeight
    {
        return height;
    }
    public int getNumber
    {
        return number
    }
}
