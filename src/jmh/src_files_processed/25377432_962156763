import java.util.Scanner;     
public class RESTDIVISION
{
    static Scanner in = new Scanner(System.in);

    static void tid()
    {
        System.out.println("Hur mÃ¥nga sekunder");
        int sekunder = in.nextInt();
        
        int timmar = sekunder/3600;       
        int minuter = (sekunder%3600)/60; 
        sekunder = sekunder%60;
        
        System.out.println(timmar + " timmar " + minuter + " minuter " + sekunder + " sekunder ");
    
    }
    
    public static int algoritm(int tal)
    {
        int output =0;
        while(tal>0)
        {
            output = output + tal%10;
            tal = tal /10;
        }
        return output;
    }
    
    static void pengar()
    {
        System.out.println(" Hur stor summa?");
        int summa = in.nextInt; 
        
        int 10kr = summa%10;
        int 5kr = (summa%10)/5;
        int 1kr = summa%5;
        
        System.out.println( 10kr + " st tio kronor " + 5kr +" st fem kronor " + 1kr + " st enkronor" )
    }
}
