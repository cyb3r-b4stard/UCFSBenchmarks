class Firma
{
   Pracownik [] tab;
   int ilosc = 0;
   Firma (int rozmiar)
   {
       Pracownik []tab = new Pracownik[rozmiar];
   }
   Pracownik znajdzPracownika (String naz)
   {
       for(int i=0;i<this.ilosc;i++)
       {
           if(naz.equals(tab[i].nazwisko));
                return tab[i];
           
       }
       return null;
   }
   void czymJest(Pracownik prac)
   {
       if(prac instanceof("Urzednik"))
             System.out.println("Urzednik");
       else
             system.out.println("robotnik");
   }
   void nowyPracownik()
   {
      
       this.ilosc++;
   }
}