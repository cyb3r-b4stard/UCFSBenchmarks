public class Formel1
{
    private int pi;

    public Formel1 ()
    {
        pi = 3;

    }

    public int flaecheRechteck (int a, int b)
    {
        int ergebnis = a*b;
        return ergebnis;

    }

    public int summe (int a, int b)
    {
        int summe4 = a+b;
        return summe4;
    }

    public int quotienten (int a, int b)
    {
        int quotienten4 = a/b;
        return quotienten4;

    }

    public int berechneY (int a, int b)
    {
        int r = 3*a + 6*b - a*a;
        int y = 5*r - 2*b;
        return y;

    }

    public int berecheZylinder (int r, int h)
    {
        int g = r*r * pi
        int zylinder = g*h

    }
}