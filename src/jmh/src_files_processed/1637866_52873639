public class newRectangle
{
    private int length;
	private int width;
	private int area;

    public int getArea(int length , int width)
    {
        return length*width;
    }
    
    public int getDiagonal(int length, int width)
    {
        return Math.sqrt(Math.pow(int length,2) + Math.pow(int width,2));
    }
      
        
}
         