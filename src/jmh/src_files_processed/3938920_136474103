
/**
 * @###### #### ###
 * @####### ##.##.####
 */
public class Messwert
{
   private String stationsname;
   private double temperatur;
   private int laufnummer;
   
   public Messwert (String stationsname, double temperatur, int laufnummer)
   {
       this.stationsname=stationsname;
       this.temperatur=temperatur;
       this.laufnummer=laufnummer;
    }
  
    public String gibStationsname()
    {
        return stationsname;
    }
    
    public double gibTemperatur()
    {
        return temperatur;
    }
    
    public int gibLaufnummer()
    {
        return laufnummer;
    }
    
   
   public boolean identischerMesswert(Messwert m)
   {
    if (gibTemperatur() == temperatur && gibLaufnummer() == laufnummer {
        return true;
    }
    else{
        return false;
    }
    
    }
    
   
}