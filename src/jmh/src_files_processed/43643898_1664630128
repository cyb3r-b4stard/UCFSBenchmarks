import kareltherobot.*;
public class Test extends ProjectOriginal
{
    public Test (int street, int avenue, Direction direction, int beepers)
    { super(street, avenue, direction, beepers); }
    
    Public static void main(String args[])
    {
        Test jj = new Test (1, 1, North, 0);
        jj.walkMile();
        jj.turnOff();
    }
}