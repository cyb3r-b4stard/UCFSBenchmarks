class MyRec {
    double width;
    double height;
     MyRec() {
       MyRec (double w, double h) {
           height = h;
           width = w;
        }
        public static void main(String[] args) {
            MyRec rec = new MyRec(10, 20);
            System.out.println("width = " + rec.width + "height" + rec.height);
        }
    }