
public class array2 {
    public static void main(String [] args) {
         int [] x = {3,45,17,2,-1,44,9,23,67,2,-6,-23,-100,12,5,1212};

         
    System.out.println("OUTPUT FOR");
    for	(int i=0; i<x.length;i++) {
		System.out.println(" " + x[i]);}
		
	 int i = x.length;
	System.out.println("OUTPUT FOREACH");
	for	(double e	:	x)
		{	System.out.println((int)e);	}
        
    
    System.out.println("REVERSE WHILE");
    i = x.length -1;
    while (i>=0) {
            System.out.println(x[i]);
            i--;
    }
    
    System.out.print("ODD NUMBER FOR");
    
     for(i=0; i<x.length;i=i+2;) {
		System.out.println(" " + x[i]);}


}
}
  