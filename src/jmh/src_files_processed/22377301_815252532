class freq
{
    public static void main()
    {
        Scanner sc=new Scanner(System.in);
        System.out.println("enter a number");
        int n=sc.nextInt();
        double ctr=0;
        
        while (n!=0)
       
        {
            int z=n%10;
            if (z==1)
            ctr++;
        }
        System.out.println(+z)("Appears"+ctr"times");
    }
}
        