public class CharacterA
{
    public static void main (String[] args)
    {
        int count = 0;
        for (int position = 0; position < name.length(); position++
            if (name.charAt(position) == 'a')
                count++;
        System.out.println ("The character \'a\' appears " + count + " time (s)");
        
        
    }
}