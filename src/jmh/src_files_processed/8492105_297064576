
/**
 * ##### # ########### ## ##### ##### ####.
 * 
 * @###### (#### ####) 
 * @####### (# ####### ###### ## # ####)
 */
public class Shape
{
    String color;
    boolean filled;
    
    public Shape();
    {
                
    }
    
    public Shape(String color, boolean filled);
    {
        this.color = "Green";
        this.filled = true;
    }
    
    public String getColor();
    {
        return color;
    }
    
    public boolean isFilled();
    {
        return filled;
    } 
    
    public void setColor(String color);
    {
        this.color = color;
    }
    
    public void setFilled(boolean filled)
    {
        this.filled = filled;
    }
    
    public String toString();
    {
        if(filled == true)
            strFilled = "filled";
        else
            strFilled = "NOT filled";
        
        String result = ("A Shape with color of " + color + "and " + strFilled);
        
        return result;
    }
}