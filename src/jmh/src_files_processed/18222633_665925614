public class Student {
    private String firstName;
    private String lastName;
    private String studentID;
    private double accumulatedPoints;

    /*
     * Default Constructor
     */
    public Student(){

    }

    /*
     * Non Default Constructor
     */
    public Student(String name, String surname, String studentNum, double points){

    }

    public void setFirstName(String newFirstName){   
        if(newFirstName!=null)
        {
            firstName = newFirstName;
        }else{
            firstName = "";
        }
    }

    public void setLastName(String newLastName){    
        if(newLastName!=null){
            lastName = newLastName;
        }else{
            lastName = "":
        }
    }

    public void setStudentID(String newStudentID){   
        if(newStudentID!=null){
            studentID = newStudentID;
        }else{
            studentID = "";
        }
    }

    public void setAccumulatedPoints(double newAccumulatedPoints){
        if(newAccumulatedPoints > 0){
            accumulatedPoints = newAccumulatedPoints;
        }else{
            accumulatedPoints = 0;
        }
    }
}