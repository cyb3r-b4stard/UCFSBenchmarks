
/**
 * ##### # ########### ## ##### ##### ####.
 * 
 * @###### (#### ####) 
 * @####### (# ####### ###### ## # ####)
 */
public class Klant
{
   private String naam;
   private String gemeente;
   private String technischProbleem;
   private int tijdsduur;
   private double prijs;
   
   public Klant (String naam, String gemeente, String technischProbleem, int tijdsduur, double prijs){
       this.naam = naam;
       this.gemeente = gemeente;
       this.technischProbleem = technischProbleem;
       this.tijdsduur = tijdsduur;
       this.prijs = prijs;
    }
    
   public String getNaam() {
       return naam;
    }
    
   public String getGemeente() {
       return gemeente;
    }
    
   public String gettechnischProbleem() {
       return technischProbleem;
    }
    
   public int gettijdsduur() {
       return tijdsduur;
    }
    
   public double getprijs() {
       return prijs;
    }
    
  public void setNaam(String naam ) {
      this.naam = naam;
    }
    
  public void setGemeente(String gemeente) {
      this.gemeente = gemeente;
    }
    
  public void setTechnischProbleem(String technischProbleem) {
      this.technischProbleem = technischProbleem;
    }
    
  public void setTijdsduur(int tijdsduur) {
      this.tijdsduur = tijdsduur;
    }
    
  public void setPrijs(double prijs) {
      this.prijs = prijs;
    }
    
  
  public void print() {
      System.out.println ("Naam: " + naam); System.out.println ("Gemeente: " + gemeente); System.out.println ("TechnischProbleem: " + technischProbleem); System.out.println ("Tijdsduur: " +tijdsduur "min"); System.out.println ("Prijs: " + prijs "euro");
    }
    
      
}