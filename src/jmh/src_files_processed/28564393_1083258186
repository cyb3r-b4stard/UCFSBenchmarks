public class Map
{
static int r=7;
static int k=7
static int[][] a=new int [r][k];
public Map()
{
    for(int i=0; i<r; i++)
    {
        for(int j=0;j<k;j++)
        {
            a[i][j]=3;
        }
    }
}

public static void ShowMap()
{
    for(int i = 0; i<r ;i++)
    {
        for(int j=0;j<k;j++)
        {
            System.out.print(a[i][j] + " ");
        }
        System.out.println();
    }
}
}