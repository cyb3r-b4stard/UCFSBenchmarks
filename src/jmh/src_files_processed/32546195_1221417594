
/**
 * ## ##### ########## #
 * 
 * @###### ######### ##### 
 * @####### ##### ## ####
 */
public class Loops5
{
   public int GCF (int a, int b)
   {
       int value = 0;
       
       for (int i = a; i > 1; i--)
       {
           if (a % i = 0 && b % i = 0)
           {
               return 1;
           }
           return -1;
       }
   }
}