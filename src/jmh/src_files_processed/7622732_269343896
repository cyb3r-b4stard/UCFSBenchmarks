import java.io.*;
import java.util.*;

public class senior
{
    private TreeNode root;
    private String output = "";
    private Vector myNodes;
    private int index;
    private String s = "";

    public senior()
    {
        root = null;
        output = "";
    }

    public void add(char obj, int num)
    {
        TreeNode newNode = new TreeNode(obj, null, null, num);
        if(root == null)
            root = newNode;
        else
            addNode(root, newNode);
    }
    
    public void addNode(TreeNode cur, TreeNode newNode)
    {
        if(newNode.getValue() <= cur.getValue())
        {
            if(cur.getLeft() == null)
                cur.setLeft(newNode);
            else
                addNode(cur.getLeft(), newNode);
        }
        else 
        {
            if(cur.getRight() == null)
                cur.setRight(newNode);
            else
                addNode(cur.getRight(), newNode);
        }
    }
    
    public TreeNode returnRoot()
    {
        return root;
    }
    
    public void levelorderTraversal(TreeNode root)
    {
        myNodes = new Vector();
        index = 0;
        if(root == null)
            return;
        addIn(root);
        addRightandLeft(index);
}