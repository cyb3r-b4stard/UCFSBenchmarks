public class Test
{
    private Test()
    {
    }
    
    private static void reset(int [] T)
    {
        for (int i = 0; i < T.length; i++)
        {
            T[i] = 0;
        }
    }
    
    private static void displayTab(int [] T)
    {
        for (int i = 0; i < T.length; i++)
        {
            System.out.println (T[i] + ", ");
            System.out.println(T[T.length - 1]);
        }
    }
    
    private static boolean searchTab(int x, int [])
    {
        if (x == T[i]) 
        {
            return true;
        }
    }
   
    public static void test()
    {
       int [] foo = {4, 25, 2, 105};
       
       displayTab(foo);
       reset(foo);
       displayTab(foo);
       System.out.println(searchTab(25, foo));
    }
}