public class Age_Year
{
    public static void main (String [] args) {
       Scanner sc = new Scanner(System.in);
       age;
       int year;
       System.out.println("What year were you born?");
       year = nextInt();
       while (year <= 2017) {
           year ++;
           age (age - 1);
}
