public class number
{
    public static void main (String args [])
    {Int a,b,c;
    Scanner sc = new Scanner (system.in);
    system.out.println ("Enter a number");
    a=sc.next Int ();
    System.out.println("Enter a number");
    b= sc.next Int ();
    c= a-b
    system.out.println ("Difference= " + c);
}
