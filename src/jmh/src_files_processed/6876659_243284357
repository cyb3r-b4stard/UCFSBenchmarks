
/**
 * ##### # ########### ## ##### ########## ####.
 * 
 * @###### (####### #########) 
 * @####### (## ## ##### #### ### #.#)
 */
public class Musculines

{
    private String rut;
    private String nombre;
    private int edad ;
    private boolean estado;
    private int mensualidad;
    private char sexo;
    private int horas; 
    /**
     * 
     */
public Musculines()
{
    this.rut= rut;
    this.nombre= nombre;
    this.edad = edad;
    this.estado= estado;
    this.mensualidad= mensualidad;
    this.sexo = sexo;
    this.horas= horas;
    
}
/**
 * metodo accesador para atributo rut
 */

    public String getRut()
   { 
    return rut;
}
/**
 * metodo accesador para atributo nombre
 */
public String getNombre()
{
    return nombre;
}
/**
 * metodo accesador para atributo edad
 */
public int edad ()
{
    return edad;
}
/**
 * metodo accesador para el atributo estado
 */
public boolean estado()
{
    return estado;
}
/**
 * metodo accesador para el atributo mensualidad 
 */
public int mensualidad ()
{
    return mensualidad;
}
/**
 * metodo accesador para el atributo sexo
 */
public char sexo ()
{
    return sexo;
}
/**
 * metodo accesador para el atributo horas
 */
public int horas ()
{
    return horas;
}
   /**
    * metodo mutador para atributo rut
    */
   public String setRut (String rut) 
   {
    this.rut = rut ;
    }
    /**
     * metodo mutador para atributo nombre
     */
    public String setNombre (String nombre)
    {
        this.nombre = nombre;
    }
    /**
     * metodo mutador para atributo edad
     */
    public String setEdad (String edad)
    {
     this.edad = edad;
    }
    /**
     * metodo mutador para atributo estado
     */
    public String setEstado (String estado)
    {
        this.estado = estado;
    }
    /**
     * metodo mutador para el atributo mensualidad 
     */
    public String setMensualidad (String mensualidad)
    {
        this.mensualidad = mensualidad
    }
    /**
     * 
     */

   
   
} 