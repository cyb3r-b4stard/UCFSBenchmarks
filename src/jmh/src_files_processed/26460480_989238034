public class AssignmentOne
{
   public static void helloWorld() {
       System.out.println("Hello World");
    }
    
   public static void doSomeMath() {
    int a = 20;
    int b = 30;
    System.out.println("The Product of " + a + " and " + b + " is " + a * b);
    }
   public static void escapeFromCT() {
    System.out.println("To: \t Jep \"The Sleeper\" Jepson");
    System.out.println("From: \t Ms. Havisham");
    System.out.println("RE: \t An important message.");
    System.out.println("Wake up! (Or,whatever.)");
    }
   public static void windchill() {
     double t = 20.0;
     double v = 15.0;
     double result = Math.pow(v, 0.16);
     double wc = 35.74 + (0.6215 * t) - (35.75 * result) + ((.4275 * t) * result);
     System.out.println(wc);
    }
   double windchill(double t, double v) {
     double result = Math.pow(v, 0.16);
     double wc = 35.74 + (0.6215 * t) - (35.75 * result) + ((.4275 * t) * result);
     return wc;
    }
   public static void sumIntegers750() {
     int sum = 0;
     for(int i = 0; i < 751; i += 1) {
         sum += i;
     if (i == 750){
         System.out.println(sum);
        }
    }
   }
   public static void sumIntegers(int upperBound){
    int sum = 0;
     for(int i = 0; i < upperBound + 1; i += 1) {
         sum += i;
     if (i == upperBound ){
         System.out.println(sum);
        }
    }
}