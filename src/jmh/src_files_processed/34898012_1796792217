class b
{public b(int n)
    {int d,,c,r,x=n;
        double s;
        while(n>0)
        {d=n%10;
         c++;
         n=n/10;
        }
        n=x;
        while(n>0)
        {r=r%10;
         s=s+(Math.pow(d,c--));
         n=n%10;
        }
        
        if (s==x)
        System.out.println("disarium");
        else
        System.out.println("not disarium");
    }}