import java.util.*;
/**
 * ########
 * 
 * @###### (##### #####) 
 * @####### (#/#/####)
 */
public class Program4
{
  ArrayList<String> grades = new ArrayList<String>();
  Scanner keyboard = new Scanner(System.in);
  int grade;
  public void findGrades()
  {
   System.out.println("Please input your grades that you wish to average out and order them, if you wish to end please use -1");
   do{
    grade = keyboard.nextInt();
   }while(grade != -1);
   grade set grades;
  }
}