
/**
 * ##### # ########### ## ##### ###### ####.
 * 
 * @###### (#### ####) 
 * @####### (# ####### ###### ## # ####)
 */
public class CutRod
{
    private int x;

    /**
     * Constructor for objects of class CutRod
     */
    public CutRod()
    {
        x = 0;
    }

    /**
     * An example of a method - replace this comment with your own
     * 
     * @param  y   a sample parameter for a method
     * @return     the sum of x and y 
     */
    public int BottomUpCutRod(int[] p, int n)
    {
        int[] r = new int[n];
        r[0] = 0;
        double q;
        for(int j = 1; j <= n; j++)
        {
            q = Double.NEGATIVE_INFINITY;
            for(int i = 1; i <= j; i++)
            {
                if(q < p[i] + r[j-i])
                    q = p[i] + r[j-i];
            }
            r[j] = q;
        }
        return 0;
    }
}
}