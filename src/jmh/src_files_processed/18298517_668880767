public class Multiply{
    import comp102x.IO;
public static void main (String args[]){
 int x = 17;
int y = 3;

System.out.println("Enter an integer, x: " + x);

System.out.println("Enter an integer, y: " + y);

int calculated_result = x * y;

System.out.println("Answer = " + calculated_result);
}
}