
/**
 * ##### # ########### ## ##### ################ ####.
 * 
 * @###### (#### ####) 
 * @####### (# ####### ###### ## # ####)
 */
public class ForLoopBeginning
{
    public int forLoopOne()
    {
            int sum = 0;
            for (int count = 1; count <= 100; count = count + 1)
            {
                sum = sum + count;
            }
            
            return sum;
    }