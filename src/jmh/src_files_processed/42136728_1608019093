
public class Phone_Directory
{
    private Entry[] Group;
    private int num;
    private int max;
    
    public Phone_Directory(int amount)
    {
        
        Group = new Entry[amount];
        max = amount;
        num = 0;
    }
    
    public void AddPerson(String last, String first, String address, int Zip, long phone)
    {
        if(num > Group.length)
            System.out.println("No more entries permitted");
            
        Group[num] = new Entry(last, first, address, Zip, phone);
        
        num++;
        for (int i = 0; i < num; i++) 
        {
            for (int j = i + 1; j < num; j++) 
            {
                if (Group[i].toString()compareTo(Group[j].toString)>0)
                {
                    temp = Group[i];
                    Group[i] = Group[j];
                    Group[j] = temp;
                }
            }
        }
    }
    
    public String toString()
    {
        String words = Group[0].toString() + "\n";
        for(int i = 1; i < max; i++)
        {
            words += Group[i].toString() + "\n";
        }
        return words;
    }
}