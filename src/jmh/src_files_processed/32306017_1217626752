public class CollegeStudent
{
    String firstName, lastName;
    String studentID;
    boolean fullTime;
    double[] grades = new double[5];
    public CollegeStudent()
    {
        firstName = "Sam";
        lastName = "Plestudent";
        studentID = "00000000";
        fullTime = true;
        grades[0] = 0.0;
        grades[1] = 0.0;
        grades[2] = 0.0;
        grades[3] = 0.0;
        grades[4] = 0.0;
    }
    public CollegeStudent(String firstName, String lastName, String studentID, boolean fullTime,double eng, double math, double sci, double art, double socSci)
    {
        this.firstName = firstName;
        this.lastName = lastName;
        this.studentID = studentID;
        this.fullTime = fullTime;
        grades[0] = eng;
        grades[1] = math;
        grades[2] = sci;
        grades[3] = art;
        grades[4] = socSci;
    }
    public String toString()
    {
        return "Student Name: " + firstName + " " + lastName + "\n"
             + "StudentID: " + studentID + "\n" 
             + "Full Time Student? " + fullTime;
             + "Student GPA: " + grades.calcGPA();
    }
    public void setGPA(double eng, double math, double sci, double art, double socSci)
    {
        grades[0] = eng;
        grades[1] = math;
        grades[2] = sci;
        grades[3] = art;
        grades[4] = socSci;
    }
    public double calcGPA()
    {
     double sumAll = 0;
        for (int i = 0; i < grades.length; i++)
     {
            sumAll += grades[i];
     }
     double average = sumAll/grades.length;
     return average;
    }
}