
import static java.lang.System.*;

public class OddEven
{
    private int num;
    
    public OddEven()
    {
        num = 0;
    }
    
     public OddEven(int number)
    {
        num = number;
    }
     
    public static boolean isOdd(int number)
    {
        if (number % 2 = 1)
            return true;
        else
            return false;
    }
    
    public static boolean isEven(int num)
    {
        return true;
    }
}