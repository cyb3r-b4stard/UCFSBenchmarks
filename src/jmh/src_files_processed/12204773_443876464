import java.util.Scanner;

public class Decrypt {
    public static final int numChars = 128;

    public static void main(String[] arg)
    {
        String encryptedText = DecryptUtilities.convertFileToString();
        printText(encryptedText);
        
    }
    
    public static void printText(String s){
        System.out.println("The encrypted text is:");
        System.out.print(s);
        System.out.println();
    }
    
    public static int [] frequency(String s){
        int [] freq = new int[numChars];
        for(int i = 0; i<s.length(); i++){
            char temp = s.charAt(i);
            freq[(int)temp]++;
        }
        return freq;
    }
    
    public static void printFreq(int [] table){
        System.out.println("Frequencies of characters.");
        System.out.println("Character - Frequency");
        for(int i = 32; i<127; i++){
            System.out.printf("%c - %d",i , table[i]);
        }
        System.out.println();
    }