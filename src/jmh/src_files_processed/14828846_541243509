public class Containerschiff
{
    private int hoehe;
    private Stack turm1;
    private Stack turm2;
    private Stack turm3;
    private Stack turm4;
    
    public Containerschiff(int phoehe)
    {
        hoehe = phoehe;
        turm1 = new Stack;
        turm2 = new Stack;
        turm3 = new Stack;
        turm4 = new Stack;
        
        
    
    
    }
    
    public boolean schiffLeer()
    {
        if(turm1.isEmpty()==true&&turm2.isEmpty()==true&&turm3.isEmpty()==true&&turm4.isEmpty()==true)
        {
            return true;
        }
        else 
        {
            return false;
        }
        
    }
    
    
    
    
    
}