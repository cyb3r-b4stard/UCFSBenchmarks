            final int max=wrds.length();
            wrds=wrds.toUpperCase();
            char []vowels=new char[max];
            for(int i=0;i<wrds.length()-1;i++)
            {
                scan=wrds.charAt(i);
                if(scan=='A')vowels[i]+=scan;
                else if(scan=='E')vowels[i]+=scan;
                else if(scan=='I')vowels[i]+=scan;
                else if(scan=='O')vowels[i]+=scan;
                else if(scan=='U')vowels[i]+=scan;
            }
