public class Liste 
{
    private Listenelement erster;
    
    public Liste()
    {
        erster = new Abschluss();
    }
    
    public Listenelement ersterGeben()
    {
        return erster;
    }
    
    public int gesamtgewichtGeben()
    {
        return erster.gesamtgewichtGeben();
    }
    
    public void hintenEinfügen(Datenelement knoteninhalt)
    {
        erster = erster.hintenEinfuegen(knoteninhalt);
    }
    
    public Datenelement inhaltErsterGeben()
    {
        return erster.inhaltGeben();
    }
    
    public Datenelement inhaltLetzterGeben()
    {
        return erster.inhaltLetzterGeben(erster.inhaltGeben());
    }
    
    public void vorneEinfügen(Datenelement knoteninhalt)
    {
        Datenknoten n = new Datenknoten(erster, knoteninhalt);
        erster = n;
    }
    
    public Datenelement vorneEntnehmen()
    {
        Datenelement n = erster.inhaltGeben();
        erster = erster.nächsterGeben();
        return n;
    }
    
    public void ausgeben()
    {
        erster.ausgeben();
    }
    
    public void ersterausgeben()
    {
        erster.inhaltausgeben();
    }
    
    public Listenelement asd(Datenelement knoteninhalt)
    {
        return erster.hintenEinfuegen(knoteninhalt)
    }
}