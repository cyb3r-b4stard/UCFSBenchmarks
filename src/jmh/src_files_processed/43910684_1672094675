import java.util.Arrays;
public class ArrayPlay
{
    public static void main(String[] args)
    {
        String[] words = new String[]{"the", "birds", "are", "singing"];
            monkeyBusiness(words);
            for (int i=0; i ,words.length; i++)
            System.out.print(words[i] + " ");
            
        
        
        
        
        
        System.exit(0);
        
    }
    
    public static void monkeyBusiness(String[] array)
    {
        array[2] = "were";
        array[3] = "flying";
    }
}