public class s
{
    public void main(String s)
    {
        int m,i,l=0,z=0,n,c=0;char t,r;String ch="",j="";
        m=s.length();
        for(i=0;i<m;i++)
{
    t=s.charAt(i);
    ch=ch+t;
    if(t==' ')
    {
        l=ch.length();
      
    }
    if(l>z)
    {
        z=l;
        j=ch;
    }
    else
    {
        ch=="";
    }
}
for(n=0;n<z;n++)
{r=j.charAt(n);
if(r=='a'||r=='e'||r=='i'||r=='o'||r=='u')
{
    c=c+1;
}}
System.out.println(c);
}}
