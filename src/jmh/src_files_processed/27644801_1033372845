
public class CSprite implements ISprite
{
    Vector m_vector;
    int m_x, m_y, m_z;
    public static void main(String[] args) 
    {
    }

    public CSprite()
    {
        m_vector = new Vector(0,0);
    }
    public CSprite(int x, int y)
    {
        m_x = x;
        m_y = y;
        m_vector = new Vector(0,0);
    }
public CSprite(int x, int y, int z)
    {
        m_x = x;
        m_y = y;
        m_z = z;
        m_vector = new Vector(0,0);
    }
public CSprite(int x, int y, int z, Vector v)
    {
        m_x = x;
        m_y = y;
        m_z = z;
        m_vector = new v;
    }

    public int get_x(){return m_x;}

    public int get_y(){return m_y;}

    public int get_z(){return m_z;}

    public char[] get_map()
    {
return null;
    }

    public Vector get_Vector()
    {
return m_vector;
    }
}