public class Buch extends Artikel
{
    protected String autor;
    protected int seiten;

    public Buch(String artikelnummer, String bezeichnung, double preis, String autor, int seiten) {
        super(artikelnummer, bezeichnung, preis);
        this.autor = autor;
        this.seiten = seiten;
    }

    public void ausgeben()
    {
        System.out.print("Buch: ");
        System.out.print(" " + artikelnummer + ", " + bezeichnung + ", " + preis)
        System.out.print(" " + this.autor + ", " + this.seiten);
    }
}