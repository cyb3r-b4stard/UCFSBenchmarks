 */
public class Ich_bin_dran
{
    static int [][] values = new int[6][6];
    static char [][] status = new char[6][6];
    public static void main()
    {
        loadStatus;
        loadValues;
        
    }
    
    public static void loadStatus()
    {
        for (int i = 0; i<status.length; i++)
        {
            for(int  o= 0; o<status.length;o ++)
            {
                status[i][o]='*';
            }
        }
    }
    
    public static void loadValues()
    {
        for (int i = 0; i<values.length; i++)
        {
            for(int  o= 0; o<values.length;o ++)
            {
                values[i][o]=(int)(Math.random()*8+1);
            }
        }
    }
    
    
    public static void showValues()
    {
        Out.println(123456);
        for (int i = 0; i<status.length; i++)
        {
            
            for(int  o= 0; o<status.length;o ++)
            {
                Out.print(status[i][o]);
            }
            Out.println();
            Out.print((i+1));
        }
    }
    
}