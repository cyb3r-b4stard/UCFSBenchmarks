import java.util.*;

public class Immobilienverwaltung
{
    private ArrayList<Immobilie> verwaltung;

    
    public Immobilienverwaltung(ArrayList<Immobilie> dieImmobilien)
    {
        verwaltung = new ArrayList<Immobilie> ();
        
        for( int i=0; i<dieImmobilien.size(); i++)
        {
            this.verwaltung.add(dieImmobilien.get(i));
        }
       
    }
    
    public boolean addImmobilie(Immobilie neueImmo)
    {
        for( int j = 0; j < this.verwaltung.size(); j++)
        {
            if(this.verwaltung.get(i).getAdresse().equals(neueImmo.getAdresse()) &&
               this.verwaltung.get(i).getRefNr() == neueImmo.getRefNr())
        }
        
        return false;
    }

 
}