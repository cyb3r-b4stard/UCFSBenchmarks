import java.util.Scanner;

/**
 * ##### # ########### ## ##### ######### ####.
 * 
 * @###### (#### ####) 
 * @####### (# ####### ###### ## # ####)
 */
public class BridgeWX2
{
   private static Scanner kybd = new Scanner(System.in);   
    
    private boolean spJack, spQueen, spKing, spAce;         
    private boolean clJack, clQueen, clKing, clAce;         
    private boolean diJack, diQueen, diKing, diAce;         
    private boolean heJack, heQueen, heKing, heAce;         
    private int spCount, clCount, diCount, heCount;         
    private int points;                                     

    /**
     * Constructor for objects of class BridgeWX2
     */
    public BridgeWX2()
    {
        spJack=spQueen=spKing=spAce=false;          
        clJack=clQueen=clKing=clAce=false;          
        diJack=diQueen=diKing=diAce=false;          
        heJack=heQueen=heKing=heAce=false;          
        spCount=clCount=diCount=heCount=points=0;   
    }
    
       /**
     * program() is the method that is the overall driver for the program. It accepts the cards, note
     * which cards are significant, and computes the points for the hand.
     */
    public void program()
    {
        
    }
  
    /**
     * 
     */
    public void getCards()
    {
         char suit = kybd.next().charAt(0);          
         int  face = kybd.nextInt();                 
         if (suit == 'd' || suit == 'D')
         {
             diCount ++
             diamonds(face);
             
         }
    }
    
    /**
     *
     */
    public void diamonds(String)
    {
        switch(face)                            
                {
                    case 1:  diAce   = true;    break;
                    case 11: diJack  = true;    break;
                    case 12: diQueen = true;    break;
                    case 13: diKing  = true;    break;
                }
    }
    
}