import Java.io.*;
class abc
{int d,n,s,zz;
     void count_no()throws IOException;
     InputStreamReader br= new InputStreamReader(system.in);
     BufferedReader cr= new BufferedReader(br);
     system.out.println("Enter value");
     string s= cr.readLine ();
     int n=Integer.parseInt(s);
     while (n>0)
     {d=n %10;
         n=n/10;
         s++;
        }
        sytem.out.print("no. of digits present"+ s);
        public void main()
        {
            abc obj=new abc();
            zz=obj.count_no();
        }
     


    