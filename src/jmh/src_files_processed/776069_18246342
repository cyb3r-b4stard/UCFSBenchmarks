Public class counter
{
    Private int value;
    Int checker;
    Public int getValue()
    {
        return value;
    }
    Public 
    void click()
    {

        value = value + 1;
        Checker = checker + 1;
    }
    Public 
    void undo()
    {
        Do{
            value = value - 1;
            checker = checker -1;
        }
        While checker > 0;
    }

    public void reset()
    {value = 0;
}
}
