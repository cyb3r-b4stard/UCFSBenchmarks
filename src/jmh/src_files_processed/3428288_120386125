public class TestArray
{

int matrNr[]=new int [1222668];

}

public static int check(int matrNr[]) { int sum = 0; int k = matrNr.length - 1; int alpha = (k + matrNr[k]) % 3;
do {
int digit = matrNr[k];
if (k > 2 && (k % 3) == alpha) { digit *= 2; } sum += digit; k -= (k == 0) ? -5 : 2;
} while (k >= 0);
return sum % 7;
}
