
/**
 * ##### # ########### ## ##### ############ ####.
 * 
 * @###### (#### ####) 
 * @####### (# ####### ###### ## # ####)
 */
public class NestForLoops{
    public static void main(String[] args){
        for (int i=0; i<9; i++){
            System.out.println("*");
            for (int j=-1; j<i; j++);
                System.out.print("*");}
        }
    }
}
