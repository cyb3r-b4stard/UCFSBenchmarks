import java.util.Scanner;
public class Repitition_prov_1
{
    static Scanner in = new Scanner(System.in);
    static void main_andragradare()
    {
        System.out.println("Ge p och q värdena");
        double p = in.nextDouble();
        double q = in.nextDouble();
        
        double[] x_values = andragradare(p, q);
        
        if(x_values==null)
            System.out.println("inga definierade x");
        else if(x_values.length==1)
            System.out.println(x_values[0]);
        else
            System.out.println(x_values[0] + " " + x_values[1]);
    }
    static double[] andragradare(double p, double q)
    {
        if(Math.pow(p/2,2)>q)
        {
            double x1 = (-p/2)+Math.sqrt(Math.pow(p/2,2)-q);
            double x2 = (-p/2)-Math.sqrt(Math.pow(p/2,2)-q);
            return new double[] {x1, x2};
        }
        else if (Math.pow(p/2,2)==q)
        {
            double x1 = (-p/2);
            return new double[] {x1};
        }
        else
        {
            return null;
        }
    }
    
    static void main_hoppresultat()
    {
        double summa = simhopp();
        System.out.println("ditt resultat " + summa);
    }
    static double simhopp()
    {
        System.out.println("Vad ger de 7 domarna dig för poäng");
        double[] results = new double[7];
        for(int i = 0; i<results.length; i++)
        {
            results[i] = in.nextDouble();
        }
        
        double minsta = 11;
        double största = -1;
        double summa = 0;
        for(int i = 0; i<results.length; i++)
        {
            double v = results[i];
            if(v>största)
                största=v;
            if(v<minsta)
                minsta=v;
            summa += v;
        }
        
        summa = summa-(största+minsta);
        summa = summa/5;
        return summa;
    }
    
    /*static void main_prim()
    {
        System.out.println("säg ett tal");
        int tal = in.nextInt();
    }
    static boolean prim(int t)
    {
        double tal_rot = Math.sqrt(tal);
        int i = 0;
        while(i<tal_rot)
        {
            i++;
        }
    }
    */
    static void main_PI()
    {
        int PI_räknat = uppskatt_PI();
        PI_räknat - Math.PI = PI_skillnad;
        System.out.println(PI_skillnad);
    }
    static double uppskatt_PI()
    {
        int sum = 0;
        for(int i = 1 ; i<= 4000; i = i + 4)
        {
            sum = sum + (4/i - 4/i+2);
        }
    }
}