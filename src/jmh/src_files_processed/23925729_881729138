 * @/##/##/####
 */

public class BankAccount 
{
	private String accountNumber;
    private double balance;
    private double interest;
    private double withdrawalFee;
    private double annualInterestRate;
    
	public BankAccount(String accountNumber)
	{
		this.accountNumber = accountNumber;
	}
	
	public BankAccount(String accountNumber, double initialBalance)
	{
		 this.accountNumber = accountNumber;
	     initialBalance = 0.0;
		 balance = initialBalance;
	}
	
	public void deposit(double amount)
	{
          balance = balance + amount;
	}
	
	public void withdraw(double amount)
	{
		balance = balance - amount;
	}
	
	public Double getBalance()
	{
		return balance;
	}
	
	public void annualInterestRate(double amount)

    {
      balance += balance * (1 + (annualInterestRate/10));

        balance = getBalance();
    }

	
	public void withdrawalFee(double amount)
	{
		balance = balance - amount;
	}
	
	public Boolean isOverDrawn()
	{

			if (balance < 0)
			{			   
			   return true;
			}
			else if (balance > 0)
			{
				return false;
			}
			else
			{
				return false;
			}
	}
	
	
	public String toString()
	{
		if (balance < 0)
		{
			return "BankAccount" + " " + "<" + accountNumber + ">" + " " + "<" + "(" + balance + ")" + ">";
		}
		else if (balance >= 0)
		{
		    return "BankAccount" + " " + "<" + accountNumber + ">" + " " + "<" + balance + ">";
		}
			
		return accountNumber;
	}


}
	
	
	public void deposit(double amount)
	{
          balance = balance + amount;
	}
	
	public void withdraw(double amount)
	{
		balance = balance - amount;
	}
	
	public Double getBalance()
	{
		return balance;
	}
	
	public void interest(double amount)

    {
      balance += balance * (1 + (interest/10));

        balance = getBalance();
    }

	
	public void withdrawalFee(double amount)
	{
		balance = balance - amount;
	}
	
	public Boolean isOverDrawn()
	{

			if (balance < 0)
			{			   
			   return true;
			}
			else if (balance > 0)
			{
				return false;
			}
			else
			{
				return false;
			}
	}
	
	
	public String toString()
	{
		if (balance < 0)
		{
			return "BankAccount" + " " + "<" + accountNumber + ">" + " " + "<" + "(" + balance + ")" + ">";
		}
		else if (balance >= 0)
		{
		    return "BankAccount" + " " + "<" + accountNumber + ">" + " " + "<" + balance + ">";
		}
			
		return accountNumber;
	}


}