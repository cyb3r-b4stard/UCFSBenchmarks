public class Aufgaben
{
    private BinaryTree<Integer> baum;
    public Aufgaben()
    {
        baum = new BinaryTree<Integer>();
    }

    public void geordneterBinaerbaum(int pAnzahl)
    {
        for(int i = 0; i < pAnzahl; i++)
        {
            baum.einfuegen((int) (Math.random())*100);
        }
    }

    public void einfuegen(BinaryTree pTree, int pZahl)
    {
        if (pTree.isEmpty())
        {
            pTree.setContenet(pZahl);
        }
        else
        {
            if(pZahl < pTree.getContenet())
            {
                
            }
            else if(pZahl < pTree)
        }
    }
}