import javax.swing.JOptionPane;
public class P3G
   {
   public static void main( String[] args )
      {
      int[] numbSet = new int[10];
      
      numbSet[0] = -9;
      numbSet[1] = 2;
      numbSet[2] = 3;
      numbSet[3] = 4;
      numbSet[4] = 7;
      numbSet[5] = 9;
      numbSet[6] = 10;
      numbSet[7] = 23;
      numbSet[8] = 45;
      numbSet[9] = 67;
      
      for( int index = 0; index < numbSet.length; index++ )
         {
         System.out.print( numbSet[index] + " " );    
         } 
      
      int find = Integer.parseInt( JOptionPane.showInputDialog( "What number are you looking for" ) );
      
      for( int index = 0; index < numbSet.length; index++ )
         {
         if( find == numbSet[index] )
            {
            System.out.println( "\n The index number is " + index );    
            if( index == numbSet.length - 1 && find =! numbSet[numbSet.length - 1] )
               {
               System.out.println( "-1: The number you are looking for is not here" );    
               } 
            } 
         } 
      
      } 
    
   } 
