public class StudentApp
{
    /**
     * print student details
     */
    public static void main(String[] args)
    {
        Student ajq = new Student("Angelo", "Quaglia", "aq211", 100, 95, 79);
        Student tfw = new Student("Terry", "Fuckwit", "tfw666", 10, 9, 7);
        Student fxl = new Student("Foxy", "Lady", "fxl069", 69, 69, 69);
    }
    
    public static void printDetails(student) {
        System.out.println("name " + student.forename);
    }
}