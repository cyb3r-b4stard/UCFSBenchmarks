
import java.util.Scanner;
public class padres
{public static void main (String[]args){
    String nombre, nombreMadre, nombrePadre;
    Scanner teclado=new Scanner(System.in);
    System.out.println("Dime el nombre de tu madre");
    nombreMadre=teclado.nextString;
    System.out.println("Dime el nombre de tu padre");
    nombrePadre=teclado.nextString;
    System.out.println("Dime tu nombre");
    nombre=teclado.nextString;
    System.out.println("Tu nombre y el de tu padre " (nombre.compareTo(nombrePadre)));
    System.out.println("tu nombre y el de tu madre " (nombre.compareTo(nombre Madre)));
}
   }