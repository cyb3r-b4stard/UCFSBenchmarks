import java.util.Scanner;
import java.io.File;
import java.util.ArrayList;
import java.io.FileNotFoundException;
/** 
 * #### ### #### ### #### ####
 * ## ### #### ######## # " , . ; : / \ ? < > [ ] ( ) { } #### ####### ## #### # #####
 * ##### #### #### ##### ####
 * ### # ## ############# ######## ## ###### ##### ####
 * ##### ######### #### ######## '#'
 * ## #### ####### #####, ### ####### # ## #### ########
 * @###### #### ######, #######
 */
public class Q1
{
    public static void main (String[] args)
        throws FileNotFoundException{
            ArrayList<String> words = new ArrayList<String>();
            ArrayList<Integer> count = new ArrayList<Integer>();
            Scanner kb = new Scanner(new File ("README.txt"));
            System.out.println("Word\t\tFrequency");
            int MAX = 0;
              while (kb.hasNext()){
                  String k = kb.next();
                  for (int j=0; j<k.length(); j++){
                      if (k.charAt(j) == '.' || k.charAt(j) == ',' || k.charAt(j) == '.' || k.charAt(j) == ';' || k.charAt(j) == ':' || k.charAt(j) == '/' || k.charAt(j) == '?' || k.charAt(j) == '<' || k.charAt(j) == '>' || k.charAt(j) == '[' || k.charAt(j) == ']' || k.charAt(j) == '(' || k.charAt(j) == ')' || k.charAt(j) == '{' || k.charAt(j) == '}'){
                      k = k.replace(k.charAt(j),' ');
                      k = k.trim();
                    }
                    }
                  if (words.contains(k)){
                    int duplicate = words.indexOf(k);
                    int d1 = count.get(duplicate)+1;
                    count.set(duplicate, d1);
                    System.out.println(d1+" "+count.size())
            }
    }
}

