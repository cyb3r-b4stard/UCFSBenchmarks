public class swich_nuevas
{
  public static void main(Stirng[] args);
 
     int x=2;
     Switch (x)
     {
     case 1:System.out.println("Hello");
     break;
     case 2:System.out.println("Hi");
     break;
     case3:System.out.println("Good");
     break;
     default;System.out.println("no match found");
    }
  }
