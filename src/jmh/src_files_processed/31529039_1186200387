import java.util.Scanner;

public class Opgave1 {

    void start(long a, long x, long b, long y) {
		a+x+b+y;
    }

    public static void main(String[] args) {
        Scanner input = new Scanner(System.in);
        new Opgave1().start(input.nextLong(),input.nextLong(),input.nextLong(),input.nextLong());
    }
}