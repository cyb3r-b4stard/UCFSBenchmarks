class i
{
    public void main (int i[])
    {
        int id ,b,a;
        for(id=0;id<i.length;id++)
        {
            if(i[id]%2==0)
            a=i[id]*i[id]*i[id];
            System.out.println("the cube is="+a);
            else
           b=i[id]*i[id];
            System.out.println("the square is="+b);
        }}}