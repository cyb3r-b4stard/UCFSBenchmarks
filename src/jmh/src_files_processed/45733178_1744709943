import java . io.*;
public class com
{
    public static void main(int sal)
    {
        double comm=0.00 ;
        if(sal>=35000){
          comm(sal*15/100);
         else if (sal>=25000&&sal<35000)
         comm(sal*12/100);
         else if(sal>=15000&&sal<25000)
         comm(sal*10/100);
         else if(sal>=10000&&sal<15000)
         comm(sal*8/100);
         else comm=0;
         System.out.println("Sales="+sal);
         
         System.out.println("Commission="+comm);
        }
    }
}   