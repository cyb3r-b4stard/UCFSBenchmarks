class DREIECK {
    private int Seite=a;
    private int Seite=b;
    private int Seite=c;
    public dreieck(int a, int b, int c){
        Seite a=a;
        Seite b=b;
        Seite c=c;
    }
    public int seitenverhÃ¤ltnisse(){
        a<b+c;
        b<a+c;
        c<b+c;
        a==b+c;
        b==a+c;
        c==b+c;
    }
}