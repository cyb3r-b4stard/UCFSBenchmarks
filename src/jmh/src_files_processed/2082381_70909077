import uulib.GUI;
public class Purchase
{
    public static void main(String[] args)
    {
        double tender_amount;
        double item_price;
        
        item_price = GUI.getDouble ("Enter price of item");
        tender_price = GUI.getDouble ("Enter tender amount");
        
       if (tender_price < item_price);
           {PRINT ("Sorry, you have no given enough money");
           }
            
       if( else change = (tender_price - item_price);
            {PRINT ("Your change is" + change);
            }
            
       END if 
    }
}