public class week4
{
    private int x;

    /**
     * Constructor for objects of class week4
     */

  
       public void whatever(int age, int num)
    {
        System.out.println("My name is stefan, I am " + age + " years old, my favourite number is " + num + ".");
        
    }
    
    /**
     * Constructor for objects of class week4
     */

  
       public double whatever(int a, int b, int c, int area)
    {
         int s = (a+b+c)/2;
         
         area = Math.sqrt(s*(s-a)*(s-b)*(s-c);
         
         return area;
        
    }
  
    /**
     * An example of a method - replace this comment with your own
     * 
     * @param  y   a sample parameter for a method
     * @return     the sum of x and y 
     */
    public int returnN (int x)
    {
        int N = x*x;
        
        return N;
    }
}