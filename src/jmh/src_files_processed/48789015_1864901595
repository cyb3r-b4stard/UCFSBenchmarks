
public class Fir1 extends Thread
{
   @Override
    public void run(){
        try{
        for(int i = 0; i < 70; i=i+2{
            System.out.println(i);
            Thread.sleep(100);
        }
        
     }catch(InterruptedException ex){
         System.out.println(ex.getMessage());
        }
            
        }
        
}
