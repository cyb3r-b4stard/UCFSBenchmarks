public class Bubble_Sort{
public static void Bubble_Sort_Method(int array[])
{
int fim, temp;
boolean troquei;
fim = array.length;
do{
  troquei = false;
  for(int i = 0; )
  {
      if(array[i]>array[i+1])
      {
          temp=array[i];
          array[i]=array[i+1];
          array[i+1]=temp;
        }
  }
  }
}
}