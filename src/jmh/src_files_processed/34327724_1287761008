import java.util.*;
public class One_dim
{
  int a[] = new int [10] ;
  
  Scanner ss= new Scanner (System.in);
  
  for (i=0 ; i< 3 ; i++)
  {
   a[i] = ss.nextInt(); 
   }
  

}