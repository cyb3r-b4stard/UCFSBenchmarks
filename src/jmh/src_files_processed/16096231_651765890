/**
 * 
 */
public class Egg
{
    private boolean Egg;
    public Egg()
    /**
     * Constructs the egg
     */
    {
        Egg = false;
    }
    
    public void Hatch()
    {
        if(Month == 5 && Week == 2)
        {
            Goose bab = new Goose;
            bab.putSelfInGrid();
        }
    }
}