import java.util.Scanner;

public class Method{
    public static void main(String[] args)
    {
        Parcel p1 = new Parcel(1, 4, "75 Oakwood", "75 Oakwood", false, false);
        Parcel p2 = new Parcel(2, 8, "16 Gooby Road", "16 Gooby Road", true, false);
        Parcel p3 = new Parcel(3, 24, "Weenie Hut", "Weenie Hut", true, true);
        Parcel p4 = new Parcel(4, 12, "Weenie Hut Jr", "Weenie Hut Jr", true, false);
        Parcel p5 = new Parcel(5, 21, "Bikini Bottom", "Bikini Bottom", true, false);
        Parcel [] allParcels = {p1, p2, p3, p4, p5};
        
        System.out.println("Here are the parcels that have been delivered but not paid for:");
        dbutnotp(allParcels)
        
    }

    private static void dbutnotp(Parcel [] allParcels)
    {
        for (int i = 0; i < allParcels.length; i++)
        {
            if (allParcels [i].isDelivered){
                if (allParcels [i].isPaid == false){
                    System.out.println(allParcels [i].parcelID);
                }
            }
        }
    }
}