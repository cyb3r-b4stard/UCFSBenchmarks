public class TODO2 extends SpaceSmasherFunctionalAPI {
    
    public void multiBallCheck(){
        boolean isKeyboardButtonDown = true;
        
        if(isKeyboardButtonDown(ONE) == true){
            createNewBall;
            addToBallSetAndSpawn("b");
        }
        else if(isKeyboardButtonDown(TWO) == true){
            createNewBall();
        }
    }
