import java.util.*
public class StackTester extends LinkedListStack
{

public static void main(String[] args) {
    Stack stack = new Stack();
    for (int i = 0; i < 10; i++)
      stack.push(new Integer(i));
     System.out.print("The contents of the stack are: " + stack + "\n"); 
     System.out.println("The size of the stack is: " + stack.size_of_stack());
    System.out.println(stack.pop());
    System.out.println(stack.pop());
    System.out.println(stack.pop());
    System.out.println(stack.pop());
    System.out.println(stack.pop());
    System.out.println(stack.pop());
    System.out.println(stack.pop());
    System.out.println(stack.pop());
    System.out.println(stack.pop());
    System.out.println(stack.pop());
    System.out.println(stack.stack_is_empty());
  }




}