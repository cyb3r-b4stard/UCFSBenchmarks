import javax.swing.JOptionPane;
public class Myif_marbina
{
public static void main(String args[]){
    int iFirst;
    int iSecond;
    int iThird;
    String s1 = JOptionPane.showInputDialog("Enter your first integer:");
    iFirst = Integer.parseInt(s1);
    String s2 = JOptionPane.showInputDialog("Enter your second integer:");
    iSecond = Integer.parseInt(s2);
    String s3 = JOptionPane.showInputDialog("Enter your third integer:");
    iThird = Integer.parseInt(s3);
    if(iFirst>iSecond)
    JOptionPane.showMessageDialog(null,iFirst+ " is greater than "+ iSecond);
    else 
      JOptionPane.showMessageDialog(null,iSecond+ " is greater than "+ iFirst);
      else if(iFirst>iThird)
    JOptionPane.showMessageDialog(null,iFirst+ " is greater than "+ iThird);
    else if
      JOptionPane.showMessageDialog(null,iThird+ " is greater than "+ iFirst);
     if(iFirst>iSecond||iSecond>iThird)
    JOptionPane.showMessageDialog(null,"Order of numbers from biggest to smallest: "+iFirst+" "+iSecond+" "+iThird);
     else if(iThird>iSecond||iSecond>iFirst)
    JOptionPane.showMessageDialog(null,"Order of numbers from biggest to smallest: "+iThird+" "+iSecond+" "+iFirst);
     else if(iFirst>iThird||iThird>iSecond)
    JOptionPane.showMessageDialog(null,"Order of numbers from biggest to smallest: "+iFirst+" "+iThird+" "+iSecond);
     else if(iSecond>iFirst||iFirst>iThird)
    JOptionPane.showMessageDialog(null,"Order of numbers from biggest to smallest: "+iSecond+" "+iFirst+" "+iThird);
     else if(iThird>iFirst||iFirst>iSecond)
    JOptionPane.showMessageDialog(null,"Order of numbers from biggest to smallest: "+iFirst+" "+iSecond+" "+iThird);
      else if(iSecond>iThird||iThird>iFirst)
    JOptionPane.showMessageDialog(null,"Order of numbers from biggest to smallest: "+iSecond+" "+iFirst+" "+iThird);
}
}