public class Point
{
   
   public static Point fwdfarPoint(Point p0, point p1, point p2)
   {
       double d0 = p0.GetDistanceFromOrgin();
       d1;
       d2;
       if( d0 > d1 & d1 > d2)
       {
           return p0
       }
   }
}