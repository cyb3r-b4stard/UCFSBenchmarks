import java.util.Scanner;

public class DominguezRadiamoda16
{
    public static void main(String[] args) 
        {
         Scanner input = new Scanner(System.in);
         System.out.println("Enter an integer between 0 and 127");
         int A = input.nextInt();
          switch (A)
         {
            case 0: System.out.println("NUL"); break;
            case 1: System.out.println("SOH"); break;
            case 2: System.out.println("STX"); break;
            case 3: System.out.println("ETX"); break;
            case 4: System.out.println("EOT"); break;
            case 5: System.out.println("ENQ"); break;
            case 6: System.out.println("ACK"); break;
            case 7: System.out.println("BEL"); break;
            case 8: System.out.println("BS"); break;
            case 9: System.out.println("HT"); break;
            case 10: System.out.println("LF"); break;
            case 11: System.out.println("VT"); break;
            case 12: System.out.println("FF"); break;
            case 13: System.out.println("CR"); break;
            case 14: System.out.println("SO"); break;
            case 15: System.out.println("SI"); break;
            case 16: System.out.println("DLE"); break;
            
            case 17: System.out.println("DC1"); break;
            case 18: System.out.println("DC2"); break;
            case 19: System.out.println("DC3"); break;
            case 20: System.out.println("DC4"); break;
            case 21: System.out.println("NAK"); break;
            case 22: System.out.println("SYN"); break;
            case 23: System.out.println("ETB"); break;
            case 24: System.out.println("CAN"); break;
            case 25: System.out.println("EM"); break;
            case 26: System.out.println("SUB"); break;
            case 27: System.out.println("ESC"); break;
            case 28: System.out.println("FS"); break;
            case 29: System.out.println("GS"); break;
            case 30: System.out.println("RS"); break;
            case 31: System.out.println("US"); break;
            
            case 32: System.out.println(" "); break;
            case 33: System.out.println("!"); break;
            case 34: System.out.println("'"); break;
            case 35: System.out.println("#"); break;
            case 36: System.out.println("$"); break;
            case 37: System.out.println("%"); break;
            case 38: System.out.println("&"); break;
            case 39: System.out.println("'"); break;
            case 40: System.out.println("("); break;
            case 41: System.out.println(")"); break;
            case 42: System.out.println("*"); break;
            case 43: System.out.println("+"); break;
            case 44: System.out.println(","); break;
            case 45: System.out.println("-"); break;
            case 46: System.out.println("."); break;
            case 47: System.out.println("/"); break;
            
            case 48: System.out.println("0"); break;
            case 49: System.out.println("1"); break;
            case 50: System.out.println("2"); break;
            case 51: System.out.println("3"); break;
            case 52: System.out.println("4"); break;
            case 53: System.out.println("5"); break;
            case 54: System.out.println("6"); break;
            case 55: System.out.println("7"); break;
            case 56: System.out.println("8"); break;
            case 57: System.out.println("9"); break;
            case 58: System.out.println(":"); break;
            case 59: System.out.println(";"); break;
            case 60: System.out.println("<"); break;
            case 61: System.out.println("="); break;
            case 62: System.out.println(">"); break;
            case 63: System.out.println("?"); break;
            case 64: System.out.println("@"); break;
            
                        
            case 65: System.out.println("A"); break;
            case 66: System.out.println("B"); break;
            case 67: System.out.println("C"); break;
            case 68: System.out.println("D"); break;
            case 69: System.out.println("E"); break;
            case 70: System.out.println("F"); break;
            case 71: System.out.println("G"); break;
            case 72: System.out.println("H"); break;
            case 73: System.out.println("I"); break;
            case 74: System.out.println("J"); break;
            case 75: System.out.println("K"); break;
            case 76: System.out.println("L"); break;
            case 77: System.out.println("M"); break;
            case 78: System.out.println("N"); break;
            case 79: System.out.println("O"); break;
            case 80: System.out.println("P"); break;
            case 81: System.out.println("Q"); break;
            case 82: System.out.println("R"); break;
            case 83: System.out.println("S"); break;
            case 84: System.out.println("T"); break;
            case 85: System.out.println("U"); break;
            case 86: System.out.println("V"); break;
            case 87: System.out.println("W"); break;
            case 88: System.out.println("X"); break;
            case 89: System.out.println("Y"); break;
            case 90: System.out.println("Z"); break;
           
            case 91: System.out.println("["); break;
            case 93: System.out.println("]"); break;
            case 94: System.out.println("^"); break;
            case 95: System.out.println("_"); break;
            case 96: System.out.println("`"); break;
            
            case 97: System.out.println("a"); break;
            case 98: System.out.println("b"); break;
            case 99: System.out.println("c"); break;
            case 100: System.out.println("d"); break;
            case 101: System.out.println("e"); break;
            case 102: System.out.println("f"); break;
            case 103: System.out.println("g"); break;
            case 104: System.out.println("h"); break;
            case 105: System.out.println("i"); break;
            case 106: System.out.println("j"); break;
            case 107: System.out.println("k"); break;
            case 108: System.out.println("l"); break;
            case 109: System.out.println("m"); break;
            case 110: System.out.println("n"); break;
            case 111: System.out.println("o"); break;
            case 112: System.out.println("p"); break;
            case 113: System.out.println("q"); break;
            case 114: System.out.println("r"); break;
            case 115: System.out.println("s"); break;
            case 116: System.out.println("t"); break;
            case 117: System.out.println("u"); break;
            case 118: System.out.println("v"); break;
            case 119: System.out.println("w"); break;
            case 120: System.out.println("x"); break;
            case 121: System.out.println("y"); break;
            case 122: System.out.println("z"); break;
            
            case 123: System.out.println("{"); break;
            case 124: System.out.println("|"); break;
            case 125: System.out.println("}"); break;
            case 126: System.out.println("~"); break;
            case 127: System.out.println(""); break;
           
           
        
            
            
            System.out.println("The character code for " + A + "is " + A);
        
        
    }
}