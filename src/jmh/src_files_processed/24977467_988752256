import java.util.Scanner;
public class Ueb20151108
{
    public static void main(String[] args)
    {
        double u;
        double b;
        double res2;
        Scanner eingabe = new Scanner(System.in);
        
        System.out.println("Bite geben sie Wert u ein");
        u = eingabe.nextDouble();
                
        System.out.println("Bite geben sie Wert b ein");
        b = eingabe.nextDouble();
        
        static double round(double pValue, int pDecPlace)
        
        System.out.println("Multiplikation von " + u + "mit" + b);
        res2 = round(u * b);
        res2 = Math.round(res2);
        
        
        System.out.println("Das Ergebnis lautet: " +res2);
        
    }
    
}