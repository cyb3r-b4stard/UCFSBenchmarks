import java.awt.*;

public class Vulture extends Bird {
    private boolean hungry;
    private boolean full;
    
    
    public Vulture() {
    }
    
    public Color getColor() {
        return Color.BLACK;
    }

    public boolean eat() {
        hungry = true;
        if (eat())
            full = true;
        else if (win() == )
            full = false;
        while (full)
            hungry = false;
        return hungry;
    }
}
