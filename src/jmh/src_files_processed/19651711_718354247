public class Socio
{
    private String nombre;
    private String sexo;
    private String fechan;
    private int edad;
    private boolean trabaja;
    
    public Socio()
    {
        this.nombre="";
        this.sexo="";
        this.fechan="";
        this.edad=0;
        this.trabaja=false;

    }
    
    public Socio (String nombre, String sexo, String fechan, int edad, boolean trabaja)
    {  this.nombre=nombre;
       this.sexo=sexo;
       this.fechan=fechan;
       this.edad=edad;
       this.trabaja=trabaja;
    }
    
    private String getnombre()
    {
        return nombre;
    }
    private String getsexo()
    {
        return sexo;
    }
    private String getfechan()
    {
        return fechan;
    }
    private int getedad()
    {
        return edad;
    }
    
    private boolean gettrabaja()
    {
        return trabaja;
    }
    
    public void setnombre(String nombre)
    {
        nombre=nombre;
    }
      public void setsexo(String sexo)
    {
        sexo=sexo;
    }
      public void setfechan(String fechan)
    {
        fechan=fechan;
    }
      public void setedad(int edad)
    {
        edad=edad;
    }
    
    public void settrabaja(boolean trabaja)
    {
        trabaja=trabaja;
    }
    
    public ImprimirSocio()
    {
        System.out.println("Nombre del socio: "+nombre);
        System.out.println("Sexo del socio: "+sexo);
        System.out.println("Fecha de nacimiento: "+fechan);
        System.out.println("Edad del socio: "+edad);
        System.out.println("Â¿Se encuentra actualmente activo?: "+trabaja);
    }
    
    public MostrarDatos()
    {
        System.out.println("Nombre: "+nombre);
        System.out.println("Sexo: "+sexo);
        System.out.println("Edad: "+edad);
        Socio.ImprimirAbono;
    }
}