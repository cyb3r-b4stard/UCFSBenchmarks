public class Computer
{
    private String brand;
    private int ram;
    private String card;
    Cpu cpu;
    Computer(String a, int b, String c, Cpu d)
    {brand=a; ram=b; card=c; cpu=d;}
    Computer(String a, int b, String c)
    {brand=a; ram=b; card=c;}
    public void setBrand(String a){brand=a;}
    public void setRam(int a){ram=a;}
    public void setCard(String a){card=a;}
    public void setCpu(Cpu a){cpu=a;}
    public void getBrand(Cpu c, String a){c.getBrand(a)}
    public String getComputer()
    {return brand+" "+ram+" "+card+" "+cpu.getCpu();}
}