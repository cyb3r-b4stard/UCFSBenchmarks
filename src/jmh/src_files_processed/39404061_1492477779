public class Driehoek {
    
    
    public Driehoek() {
          
    }
    
    
    public int Oppervlakte(int a, int b, int c) {
        int s= (a+b+c)/2;
        int f = sqrt(s(s-a)(s-b)(s-c));
        System.out.println("oppervlakte:" + "" + f);
        
    }
    
    
    
   
    
    
    
    

}