import java.util.Random;
import java.util.HashMap;
public class Student
{
    private Random randomGenerator;
    private HashMap<Integer, String> studentMap;
    private static final int RANGE = 5000;
    /**
     * This class creates a student and gives him a number that would represent him in a hashmap.
     * It uses randomly generated numbers to assign v numbers.
     * @author Demond Moore
     * @version 1
     */
    public Student()
    {
        studentMap = new HashMap<Integer, String>();
        randomGenerator = new Random();
    }
    public void newStudent(String sName)
    {
        int vNum = Random<RANGE>;
    }

}