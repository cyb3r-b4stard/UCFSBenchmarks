package CS141.Lab1;


/**
 * ##### # ########### ## ##### ######### ####.
 * 
 * @###### (#### ####) 
 * @####### (# ####### ###### ## # ####)
 */
public class Rectangle
{
    public static void main (String [] args)
    {
        for (int i=1; i<=6; i++)   
    	{
    		if(i==1|i==6) 
    		{
    		    for (int j=1; j<=4); 
    			System.out.println("**********"); /**
    			                                   * then the programme will print out a full line of stars
    			                                   */
       		}
    		else                                   /**
    		                                        * otherwise it will print a line starting with a star followed
    		                                        * by 8 spaces then another star
    		                                        */
    		{
    			System.out.println("*        *");
    	}
    	}
    }
    }
