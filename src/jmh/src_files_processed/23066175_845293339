public class Estudiante extends Persona
{
 private boolean alumnoRegular
 
 public Estudiante()
 {
  super();
  alumnoRegular = false;
 }
 
 public Estudiante(boolean alumnoRegular, String rut, String nombre,
 boolean vacante)
 {
  super(rut,nombre,vacante);
  setAlumnoRegular(alumnoRegular);
 }
 
 public boolean isAlumnoRegula()
 {
  return alumnoRegular;   
 }
 public void setAlumnoRegular(boolean alumnoRegular)
 {
  this.alumnoRegular = alumnoRegular;   
 }
 
 public void prinEstudiante()
 {
  System.out.println("--Informaci√≥n del Estudiante--");
  System.out.println(this);
 }
 
 @Override
 public String siNo()
 {
  return alumnoRegular() ? "Si":"No";   
 }
  
 @Override
 public String toString()
 {
  return "Rut - Nombre: "+super.getRutNombre+"\n"+
  "Vacante: " +super.siNo+"\n"+
  "Es alumno regular: "+siNo;
 }
}