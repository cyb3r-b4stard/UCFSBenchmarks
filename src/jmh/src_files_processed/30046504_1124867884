public class NumberAnalyzer {


    public NumberAnalyzer() {
    }

    public void sameOrNot(int a, int b, int c) {
        if ((a==b) && (b==c)){
            System.out.println("All the same.");
        }
        else if(((a==b) && (b!=c)) || ((a==c) && (a!=b)) || ((b==c) && (a!=b))){
            System.out.println("Exactly two are the same.");
        }

        else 
            System.out.println("None are the same.");
    }

    public int findMax(int a, int b) {
        if(a>b){    
           return a;
        else (a<=b)
            return b;
        
    }

    public void testFindMax() {
        System.out.println("The max of 3 and 4 is: " + findMax(3, 4));
        System.out.println("The max of 4 and 3 is: " + findMax(4, 3));
        System.out.println("The max of 4 and 4 is: " + findMax(4, 4));
        System.out.println("The max of 12 and 19 is: " + findMax(12, 19));
    }
}