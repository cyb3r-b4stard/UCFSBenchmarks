import java.util.*;
import java.lang.*;
/**
 * ########### ### #### ### ###### ####.
 * 
 * @###### (### ####) 
 * @####### (#### ############## #### ### #####)
 */
public class Wort
{

    
    private String satz = "union select join where on not union where or from or select exists from on union and where on select union where where join exists union select or select exists or select and on or on join on join exists or not not on and and exists exists on join select on on union from not exists where union union on not select union or not join select and where exists on select where select or from not join join from or not select or not union where and on join and not and and union select not or union not where and on or not not or union on select join join not select not union select join where from union select from join not from select select exists join from select union where exists not join exists on union exists select where where select exists union union union";
    private ArrayList<String> wortliste;
    private String[] wortArray;

    private HashSet<String> wortliste2;

    public Wort()
    {
        wortliste = new ArrayList<String>(); 
        wortliste2 = new HashSet<String>();
        zerlegeSatz(satz);
        uebergebeWort();

    }
    private String[] zerlegeSatz(String satz)
    {
        wortArray = satz.split(" ");
        System.out.println(wortArray.length);
        return wortArray;
    }

    private void uebergebeWort()
    {
        for(String wort : wortArray){
            wortliste.add(wort);
        }

        for(String wort : wortArray){
            wortliste2.add(wort);
        }
    }

    public void printeWort(){
        for (int i=0; i<wortArray.length; i++) {
            System.out.print(wortArray[i] + "\t" + wortliste.get(i) + "\t") ;

        
            Iterator<String> iter = wortliste2.iterator();
            if(iter.hasNext()){
            String wort = iter.next();
            System.out.print(wort); 
            }
            System..out.println();
            
        }
        

    }

}