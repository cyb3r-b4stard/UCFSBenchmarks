public class Harmonic Sums2
{
    public static void main (String[] args)
    
    {
        int N = Integer.parseInt(args[0]);
        
        float sum1 = 0.0f;
        for (int i = 1; i <= N; i++)
            sum1 = sum1 + 1.0f / i;
        System.out.println(sum1);
        
        double sum3 = 0.0;
        for (int i = 1; i <= N; i++)
            sum3 = sum3 + 1.0 / i;
        System.out.println(sum1);
