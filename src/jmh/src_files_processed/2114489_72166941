import java.util.Scanner;
public class slide24
{
    public static void boolean (String [] args) 
    {
        Scanner k = new Scanner (System.in);
        {
            if (s == null)
                return false;
            if (s.length () < 8)
                return false;
            int counter = 0;
            
            for (int i = 0; i < s.length(); i++)
            {
                if (!Character.isDigit(s.charAt(i)))
                {
                    return false;
                }
                if (Character.isDigit(s.charAt(i))))
                {
                    counter++;
                }
            }
                if (counter >= 2)
                    return true;
            return false;
        }
    }
                
            
            

    }
}
