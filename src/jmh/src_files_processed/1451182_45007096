import java.util.ArrayList;

public class ListenElementEntfernen {
    
    private ArrayList<Integer> eintraege = new ArrayList<Integer>();
    
    public void elementEntfernen() {
        eintraege.add(1);
        eintraege.add(2);
        eintraege.add(3);
        int anzahl = eintraege.size();
        for(int i=0; i<anzahl; i++){
                System.out.println(eintraege.get(i));
            }
        }
    }
}