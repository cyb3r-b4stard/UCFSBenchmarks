class Baum1
{ RECHTECK baumstamm = new RECHTECK();
    KREIS baumkrone = new KREIS();
    Baum1()
    {
        Baumstamm.FarbeSetzen("braun");
        Baumstamm.PositionSetzen(100,250);
        Baumkrone.FarbeSetzen("gruen");
        Baumkrone.PositionSetzen(100,50);
        
    }

{
}