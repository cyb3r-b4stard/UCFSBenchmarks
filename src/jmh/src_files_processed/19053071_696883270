
/**
 * ##### # ########### ## ##### #################### ####.
 * 
 * @###### (#### ####) 
 * @####### (# ####### ###### ## # ####)
 */
public class EntradaDiaEspectador extends Entrada
{
   
    private int precioEspectador;

    /**
     * Constructor for objects of class EntradaDiaEspectador
     */
    public EntradaDiaEspectador(Sesion sesion, int numero){
            super(sesion, numero);
            precioEspectador= super.getPrecio()*(0,6);       
    }

   
}