class CD extends Bank_Snguyen{
    private double balance, ogbalance, i;
    public double balance(){
        balance = ogbalance;
        return balance;
    }
    private double interest(double amount){ 
        i = 0.05; 
        amount = int k;
        i * k = balance;
        return balance;
    }
    public double deposit(double k){
        balance += k;
        return balance;
    }
    public double setBalance(int a){
        a = balance ;
        return balance;
    }
    }