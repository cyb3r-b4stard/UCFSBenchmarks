
public class ArrayStack implements IStack
{
    private int height;
    private double[] theStack;
    private int currentHeight;
    public ArrayStack()
    {
       height = 50;
       double theStack[] = new double[height];
       currentHeight = 0;
    }

    public void push(double value)
    {
       theStack[currentHeight] = value;
       currentHeight+=1;
    }
    
    public double pop()
    {
        double returner = theStack[currentHeight-1];
        theStack[currentHeight-1] = null;
        currentHeight-=1;
        return returner;
    }
    public double peek(){return theStack[currentHeight-1];}
    public int size(){return currentHeight;}
    public boolean isEmpty(){if(currentHeight == 0){return true};return false;}
    public boolean isFull(){return true;}
    public void clear(){}
}