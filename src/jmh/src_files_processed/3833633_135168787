

import static java.lang.System.*;
import java.io.File;
import java.io.IOException;
import java.util.Scanner;
import java.util.ArrayList;
import java.util.Collections;

public class AnotherWay
{
    public static void main(String[] args) throws IOException
    {
        Scanner file = new Scanner(new File("words.txt"));
        int count = 0;

     
        String longest = "";
        String mostVowels = "";
        while (file.hasNextLine())
        {
            String x = file.nextLine();
            if (x.length() > longest.length())
            longest = x;
            
            if (getNumVowels(x) > getNumVowels(mostVowels))
            mostVowels = x;
            count++;
            
       
            
            
        }
        out.println(count);

    
    
    
    
    
    while (file.hasNextLine())
      String y = file.nextLine();
    if (getNumVowels(x) > getNumVowels(mostVowels))
            mostVowels = y;
    }
    
    
    
    
    
    
    
    
    
    
    
        
     public static int getNumVowels(String s)
    {String vowels = "AEIOUaeiou";
     int n = 0;
     for(int i = 0; i<s.length(); i++);
     
    if (vowels.indexOf(s.chartAt(i))-1 > - 1)
    
    {
        n++;
    }
     

    
    
    
    return n;
    
     
    


    }
    }