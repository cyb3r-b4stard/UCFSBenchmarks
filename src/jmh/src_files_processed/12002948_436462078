/*******************************************************************************
 *
 * @######  ### ##### #########
 * @####### #.##.#### 
 */
public class Par implements IKresleny,IPosuvny
{
   
     /** instance platna */
    private static final SpravcePlatna SP = SpravcePlatna.getInstance();
    
    private Rande muz;
    private Rande zena;
    
    /***************************************************************************
     *
     */
    public Par(Rande rande){
        muz.getMuz();
        zena.getZena();
    }
    
     public void nakresli(Kreslitko kreslitko){
        SP.nekresli();{
        } SP.vratKresli();
        
    }
    
    @Override
    public Pozice getPozice() {
        return new Pozice(, zena);
    }

    @Override
    public void setPozice(Pozice p) {
        this.setPozice(p.x, p.y);
    }
    
     @Override
  public void setPozice(int x, int y) {
    SP.nekresli(); {        
      zena.setPozice(x, y);
      muz.setPozice(x + (koruna.getSirka() - kmen.getSirka()) / 2,
                     y + koruna.getVyska());
   } SP.vratKresli();
  } 
    
   
}