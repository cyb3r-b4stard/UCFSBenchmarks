/** 
 * ######## #, #### ####
 *
 */




public class Fitness
{
    private string name;
    
    private int age;
    private double weight;
    private double height;
    
    arraylist name = new array();
    
    public Fitness (string givenName, string middleInitial, string lastName, int age, double weight, double height)
    {
        name = new int[3]
            name[0] = givenName;
            name[1] = middleInitial;
            name[2] = lastName;
        this.age = age;
        this.weight = weight
        this.height = height
    }
    
    public boolean firstNameIs(firstName)
    {
        if (name[0] = firstName);
    
        return true;
    }
    
    public bmi (double height, double weight)
    {
        double x = height * height;
        double y = weight;
        
        double bmi = y / x
        
        return bmi;
    }
    
    public boolean bmiNormal (double bmi)
    {
        if ( bmi < 18.5){
        return false;
    }
       else
       if ( bmi > 25){
        return false;
    }
       else{
        return true;
    } 
    }
}