import java.util.Scanner;
public class Actual
{
public static void main (String args[])
{
    Scanner input=new Scanner(System.in);
    int MathGrade;
    int EnglishGrade;
    int ScienceGrade;
    int FilipinoGrade;
    int Average;
    System.out.println("Enter Math Grade");
    MathGrade=input.nextInt();
    System.out.println("Enter English Grade");
    EnglishGrade=input.nextInt();
    System.out.println("Enter Science Grade");
    ScienceGrade=input.nextInt();
    System.out.println("Enter Filipino Grade");
    FilipinoGrade=input.nextInt();
    Average=(MathGrade+EnglishGrade+ScienceGrade+FilipinoGrade)/4;
    System.out.printf("The Average is %d",+Average);
    
    if (Average <=75) }
        System.out.println("\nPassed");
    }
    Else
    System.out.println("\nFailed");
}
}
    