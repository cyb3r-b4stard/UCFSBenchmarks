public class Summe
{
    /** Attribute */
    double summe=0;


    /** Konstruktor fuer Objekte der Klasse Summe */
    public Summe()
    {
        
    }

    /** Methoden */
    
    public double Summe()
    {
        fot(int i=0;
        {
            i=i+1;
            summe=summe+i;
        }
        
        return summe;
    }

}