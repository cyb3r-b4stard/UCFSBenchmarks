import java.util.Random;

public class Dice {
    
        public static void main( String[] args ){
        
            Random num = new Random();
        
        int d, c;
        
       public int Roll(){
            
            d = num.nextInt(6)+1;
            
            return d;
        }
        
        public int Roll2(){
            
            c = num.nextInt(6)+1;
  
          return c;
}

System.out.print(c + d);
}

        
        
    
   
   