public class Pretekar {
    private String aMeno;
    private int aRokNar;
    private int aSkok1;
    private int aSkok2;
    private int aSkok3;

    public Pretekar(String paMeno,int paRokNar,int paSkok1,int paSkok2,int paSkok3) {
        String aMeno=paMeno;
        this.aRokNar=paRokNar;
        this.aSkok1=paSkok1;
        this.aSkok2=paSkok2;
        this.aSkok3=paSkok3;
    }

    public int meno () {
        return this.aMeno;
    }

    public int maximalnySkok () {
        int max = this.aSkok1;
        if (this.aSkok2>max)
            max=this.aSkok2;
        if(this.aSkok3>max)
            max=this.aSkok3;
        return max;

    }

    public int rokNarodenia () {
        return this.aRokNar;
    }

    public String toString () {
        return this.aMeno+"("+this.aRokNar+") ["+this.aSkok1+", "
        +this.aSkok2+", "this.aSkok3+"], max = "+this.maximalnySkok(); 
    }
}