public class QuadraticEquation
{
    private int a;
    private int b;
    private int c;
    
    public QuadraticEquation (int a,int b,int c)
    {
        a = 0;
        b = 0;
        c = 0;
    }
    
    public QuadraticEquation ()
    {
        this.a = a;
        this.b = b;
        this.c = c;
    }
    
    public int getB ()
    {
        return b;
    }
    
    public void setB ( int b )
    {
        this.b = b;
        return void;
    }
}

