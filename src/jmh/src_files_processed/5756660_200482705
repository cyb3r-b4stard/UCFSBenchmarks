import java.util.*;
public class arm 
{
    public static void main(String args[]);
    int n,t,s=0;
    int r;
    Scanner sc=new scanner(System.in);
    System.out.println("Enter the no");
    n=sc.nextInt();
    t=n;
    while(n>0)
    {
        r=n%10;
        s=s+Math.pow(r,3);
        n=n/10;
    }
    if(t==0)
    System.out.println("the no is armstrong ");
    else 
    System.out.println("the no is not armstrong");
}
}
    
