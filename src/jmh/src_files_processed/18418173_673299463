import java.util.*;

 public class zhanghu 
{
    String haoma;
    String name;
    int remain=0;
    public void getremain()
    {
        System.out.println("余额为"+remain);
    }
    public double save(double a)
    {
        remian += a;
        return remain;
    }
    public double get(double b)
    {
        remain -= b;
        return remain;
    }
    public void change(zhanghu s,zhanghu t,double c)
    {
        a.zhanghu.remain -= c;
        b.zhanghu.remain += c;
    }
}

public class java32 extends zhanghu
{
    zhanghu a =new zhanghu();
    a.haoma = new a.haoma("0001");
    a.name= "zhang";
    a.yu = 0;
    zhanghu b =new zhanghu();
    b.haoma = "002";
    b.name = "yang";
    b.yu = 0;
    a.save(1000);
    a.qu(500);
    a.getyu();
}