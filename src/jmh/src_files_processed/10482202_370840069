public class Estudiante
{
    String rute;
    String nombrees;
    String apellidop;
    String amaterno;
    String fechae;
    int pesoe;
    double estaturae;
    String sexoe;
    String direccione;
    String carrerae;
    
    Estudiante (){
    }
    
    public Estudiante (String rute, String nombrees, String apellidop, String Amaterno, String fechae, int pesoe, double estaturae, String sexoe, String direccione, String carrerae){
      this.rute=rute;
      this.nombrees=nombrees;
      this.apellidop=apellidop;
      this.amaterno=amaterno;
      this.fechae=fechae;
      this.pesoe=pesoe;
      this.estaturae=estaturae;
      this.sexoe=sexoe;
      this.direccione=direccione;
      this.carrerae=carrerae;
   
    } 
    
    public static void main (String [] args){
    
    
    Estudiante es = new estudiante("17.708.506-5 ", )
    
    
    
    }
    
    
    
}