    private int CountAfterZ(String word)
    {
        for(int i=0; i<word.length();i++)
        {
            if (word.charAt(i)=='z')
            {
                if (word.charAt(i)== 'a' || word.charAt(i)== 'e' || word.charAt(i)== 'i' || word.charAt(i)== 'o' || word.charAt(i)== 'u')
                {
                    count ++;
                }    
            }
        }
        return count;
    }