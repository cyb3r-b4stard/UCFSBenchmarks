public class Root
{
    private int count;
    private String[] tests = {"zo", "zablzeaa", "askbgwe", "zearr"};

    public static void main(String[] args)
    {   new Root();  }

    public Root()
    {
        for (String s: tests)
            System.out.println("Vowels = " + count(s));
    }

    private boolean isVowel(char c)
    {
        return "aeiou".contains("" + c); 
    }

    /*private int count (String word)
