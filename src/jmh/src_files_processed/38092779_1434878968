

public class MitternachtsFormel
{

    private int a, b, c;
    private double x1,x2;

 
    public MitternachtsFormel()
    {
     a=1,b=2, c=-2;
    }

 
 
       public double loesungx1()
    {
       x1=(-b+Math.sqrt(b*b-4*c))/(2*a);
        return x1;
    }   public double loesungx2()
    {
       x2=(-b+Math.sqrt(b*b-4*c))/(2*a);
        return x2;
    }
}