 */
public class Robot
{
   private Circle head;
   private Square body;
   private Circle rightArm;
   private Circle leftArm;
   private Triangle legs;
   public Robot()
   {
       head = new Circle
       body = new Square
       rightArm = new Circle
       leftArm = new Circle
       legs = new Triangle
    
}