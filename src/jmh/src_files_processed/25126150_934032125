
public class VOLLKREIS
{  private int xMitte,yMitte,radius,farbnr;
 
    public VOLLKREIS(int xStart, int yStart, int radiusStart, int farbnrStart)
  {xMitte=xStart;yMitte=yStart;
     radius= radiusStart;
     farbnr= farbnrStart; 
    }
    public void zeichne()
    { ZEICHENFENSTER. gibFenster().fuellkreis ( xMitte , yMitte , radius, farbnr)
}