 * ##### # ########### ## ##### ## ####.
 * 
 * @###### (#### ####) 
 * @####### (# ####### ###### ## # ####)
 */
public class kl
{
    public int countEvens(int[] nums) {
        int count =0;
        nums = {53,5,5,5,5,4,5,8,78,20,111};
        for(int i = 0; i<nums.length;i++)
        {
            if(nums[i]%2==0)
            {
                count++;
                return count;
            }

        }
    }
}