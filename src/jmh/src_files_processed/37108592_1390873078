/**
 * ###### ## ###### ######## ####
 */
public class ShoppingCart
{
    private double subtotal;
    private add;
    private remove;

    /**
     * Constructs an online Shopping Cart
     */
    public ShoppingCart()
    {
        subtotal = 0;
    }
    
    /**
     * Adds the cost of a certain item to the Shopping Cart
     * @param cost the cost of the item to add to the Shopping Cart
     * eg (1.25)
     */
    public void add(double cost)
    {
        double costAdd = cost
        subtotal = subtotal + costAdd
    }
}