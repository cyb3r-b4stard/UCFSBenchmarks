import java.io.*;
public class regresija {
    public static void main (String [] args) throws IOException {
        BufferedReader citac=new BufferedReader (new InputStreamReader (System.in));
        
    int i, ;
   double T1 [] = new double  [2];
    double T2 [] = new double [2];
    
     
     for (i=0; i<2; i++) {
         System.out.print (" koordinate prve tocke su : ");
         T1[i] = Double.parseDouble (citac.readLine ());
        }
        
     for (i=0; i<2; i++) {
         System.out.print ( T1[i] + "");
        }
        
      for (i=0; i<2; i++) {
         System.out.print (" koordinate druge tocke su : ");
           T2[i] = Double.parseDouble (citac.readLine ());
        }
        
      for (i=0; i<2; i++) {
         System.out.print ( T2[i] + "");
        }
    }
}
        
      
        
      
    
     
   
    

    
    
     
