import javax.swing.JOptionPane;
public class esercizio1compito
{
    public static void main (String args[])
    {
    int N;
    int K,
    int S;
   
    N=Integer.parseInt(JOptionPane.showInputDialog("dammi un numero"));
    K=0;
    S=0;
    while (K<N)
    {
    K=K+1;
    S=S+K*K;
}
    System.out.println("Il risultato della somma Ã¨" +S)
}
}