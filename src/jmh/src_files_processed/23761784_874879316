
import java.util.Scanner;

public class Matrix
{
     public static void main(String[] args)
     {
      int w = 1123234;
      int x = 1234356;
      int y = 4544553;
      int z = 1235645;
      
      while (x < 100000000)
      {
          System.out.println(x + "\t" + x + "\t" + x + "\t" + x + "\t" x);
       while (y < 10000000)
       {
           System.out.println(y + "\t" + y + "\t" + y + "\t" + y + "\t" + y);
           y = y + 2;
            
       }
          x = x + 1;
    }
  }
}