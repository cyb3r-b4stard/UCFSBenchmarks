public class MathOperations
{ 
    int a;
    int b;
    int c;
    MathOperations (int a1, int b1, int c1) 
    { this.a=a1; 
        this.b=b1;
        this.c=c1;
    }
    int Productumbers ();
    {
    return a*b*c;
    }
    double CalculateRatio (int m, int n);
    {
    return(m/n);
    }
    double AverageNumbers (int m, int n);
    { return ((m+n)/2));
    }
}
