import java.util.Scanner;
public class CurrencyConverter
{
    public static void main (Strings[]argsl)
    {
        Scanner myScanner= new Scanner(System.in);
        System.out.println("Enter today's exchange rate for USD to EURO: ");
        int rows=1
        int cols=30
        for (int i=0; i<rows, i++)
        {
            for (int j=0; j<cols; j++)
            System.out.print("*");
            system.out.println();
        }
        System.out.print("Enter value to convert or q to quit: ");
        String answer=myScanner.nextLine();
        
    }
}