public class Viajes
{
    private int valordia;
    private int cantidaddias;
    private char tipocabina;
    private char destino;
    public Viajes(int valdia, int candias, char tcabina, char des)
    {
        valordia=valdia;
        cantidaddias=candias;
        settipocabina=tcabina;
        setdestino=des;
    }
    public Viajes(){}
    public int getvalordia()
    {
        return valordia;
    }
    public int getcantidaddias()
    {
        return cantidaddias;
    }
    public char tipocabina()
    {
        return tipocabina;
    }
    public char destino()
    {
        return destino;
    }
    public void setvalordia(x)
    {
        valordia=x;
    }
    public void setcantidaddias(x)
    {
        cantidaddias=x;
    }
    public void settipocabina(x)
    {
        tipocabina=X;
    }
    public void setdestino (X)
    {
        destino=x;
    }
}