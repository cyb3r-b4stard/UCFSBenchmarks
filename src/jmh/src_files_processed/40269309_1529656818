class LAPRAK3NO1
{
    public static void main(String[] args)
    {
        for (int nomor=1; nomor<=2; nomor++){
        for (int subNomor=1; subNomor<=2; subNomor++)
            System.out.println(nomor+"."+subNomor+"\n");
        for (int jaja=1; jaja<3; jaja++){
        for (int number=1; number<=2; number++)
        for (int numb1=1; numb1<5; numb1++)
            System.out.println("\t"+jaja+"."+number+"."+numb1);}
    }
}