 */

public class Film
{
    
    public String title;
    private String genre;
    private String language;
    private double ratesByViwers;
    private double ratesByCompany;
    public String staff;
    private String cast;
   

    /**
     * Constructor for objects of class Film
     */
    public Film(String title, String genre, String language)
    {
         this.title = title;
         this.genre = genre;
         this.language = language;
    }
    
    public String getCast() {
     Staff c = new Staff;
     return c.name;
    }
    
    public String getStaff() {
     staff = Staff.name ;
     return staff;
    }
    
    public String getGenre() {
      return genre;
    }
     
    public String getTitle() {
      return title;
    }
    
    public String getLanguage() {
      return language;
    }
    
    public void setRatesByViwers(double rates1){
        if (ratesByViwers <= 10 && ratesByViwers >= 0 ){
         ratesByViwers = rates1  ;
        }
        
        else {
         System.out.println("Invalid rate");
        }
    }

    public void setRatesByCompany(double rates2){
        if (ratesByCompany <= 10 && ratesByCompany >= 0 ){
         ratesByCompany = rates2; 
         }
        
        else {
         System.out.println("Invalid rate");
        }
    }
    
     public double getRAtesByViwers() {
      return ratesByViwers;
    }
    
     public double getRAtesByCompany() {
      return ratesByCompany;
    }
}