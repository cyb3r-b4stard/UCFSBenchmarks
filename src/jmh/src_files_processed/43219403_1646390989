public class Banka
{
    private String nazev;
    private String kod;
    private Ucet[] evidenceUctu;
    private int pocetUctu;
    private static Banka[] evidenceBank;
    private int pocetBank;
   
    public Banka(String n, String k)
    {
        this.nazev = n;
        this.kod = k;
        this.evidenceUctu = new Ucet[100];
        this.pocetUctu = 0;
        this.pocetBank + 1;
    }

    
}