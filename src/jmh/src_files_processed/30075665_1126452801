public class Armstrong
{
    void Input_num(int user)
    {
        int temp=0,method,digit=0,assign,ab,alphabets[];a,b,c,d,e,f,g,h,i,j,k,l,m,n,o,p,q,rs,t,u,v,w,x,y,z;
        while(method>0)
        {
            digit ++;
            temp=temp + digit;
            method = user/10;
        }
        for(ab=0;ab<=temp;ab++)
        {
            alphabets++ = user%10;
        }
        System.out.println("Timepass");
    }
}
        
        
        