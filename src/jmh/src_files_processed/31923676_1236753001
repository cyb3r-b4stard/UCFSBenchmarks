 */
public class test2
{

   public static void main(String args[]){
      Scanner scanner = new Scanner(System.in);
      int[] grades = new int[3];
      grades[1] = 4;
      grades[2] = 3;
      grades[3] = 1;
      
      for(int sum : grades) {
          sum = (int)Math.pow(sum, 3);
      
      System.out.println(sum);
   }

    }
