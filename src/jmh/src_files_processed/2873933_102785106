public class test
{
   public void gutenTag(){
       System.out.println("Guten Tag!");
   }
   public void ausgabeZusammengesetzt(String neuerName){
       int alter = 1000;
       String name = neuerName;
       System.out.println("Mein Name ist " ) + name  + ("und ich bin") +  alter  + ("Jahre alt");
   } 
}