
/**
 * ##### # ########### ## ##### ########## ####.
 * 
 * @###### (#### ####) 
 * @####### (# ####### ###### ## # ####)
 */
public class RodneCislo
{
    private String rodneCislo;

    /**
     * Constructor for objects of class rodneCislo
     */
    public RodneCislo(String rodneCislo)
    {
        this.rodneCislo = rodneCislo;
    }

    public void setRodneCislo() {
        String rodneCislo;
    }
    
    public String getRodneCislo() {
        return rodneCislo;
    }
    
    public void jeZena() {
        if (rodneCislo.charAt(2) >= 5) {
            return true;
        }
        else {
            return false;
        }
        
        }
    }
}
