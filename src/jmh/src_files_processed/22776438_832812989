public class Tester
{
    public static void main(String args[])
    {
        double a= Math.toRadians(187);
        double a1= Math.toRadians(122);
        double p= Math.PI;
        double as= Math.sin(a);
        double a1c= Math.cos(a1);
        System.out.println("d1 = "+(3*p*as+(a1c));

       
    }
}