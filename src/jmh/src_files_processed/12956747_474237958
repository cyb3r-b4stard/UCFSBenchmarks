

public class Fibonacci{
    
    private int x;

    public Fibonacci()
    {
        x = 0;
    }

    
    public static int fibonacciWhile(int n){
        int fibonacci_N1 = 1;
        int fibonacci_N2 = 1;
        int zahlMax = 40;
        int fn;
        
        while(n<=zahlMax) {
            fn=fibonacci_N1+fibonacci_N2;
            fibonacci_N1=fibonacci_N2;
            fibonacci_N2=fn;
            n++;
 
            System.out.println(fn);
    } return n;
}
    
    public static int fibonacciDoWhile(int n){
      	
      	int fibonacci_N1 = 0;
      	int fibonacci_N2 = 1;
      	int wiederholung = 0;
      	int fn = 0;
      	
       do {
           if (wiederholung == 0) {
               System.out.println("Ergebnis: " + fibonacci_N1);
               System.out.println("Ergebnis: " + fibonacci_N2);
            } else {
                fn = fibonacci_N1 + fibonacci_N2;
                System.out.println("Ergebnis: " + fn);
                fibonacci_N1 = fibonacci_N2;
                fibonacci_N2 = fn;
            }
            wiederholung++;
            System.out.println("Wiederholung Nummer: " + wiederholung);
        }
        while ((wiederholung < n) && ((Long.MAX_VALUE - fibonacci_N1) >= fibonacci_N2));
        return n;
    }
    
    
    public static int fibonacciFor(int n){
            int fibonacci_N1 =1;
	        int fibonacci_N2=1;
	        int fn=fibonacci_N1+fibonacci_N2;
	  
	        System.out.print(fibonacci_N1);
	        System.out.print(fibonacci_N2);
	 
	        for( int i=2; i<=20; i++) {
	            System.out.println(fn);
	                 
                fn=(fibonacci_N1+fibonacci_N2);
 
                fibonacci_N1=fibonacci_N2;
	            fibonacci_N2=fn;
	        }return n;
    } 

public static void main (String [] args){
IO.println("Fibonacci While-Schleife: ")
fibonacciWhile(6);
IO.println("Fibonacci Do-While-Schleife: ")
fibonacciDoWhile(3);
IO.println("Fibonacci For-Schleife: ")
fibonacciFor(2);
}

}